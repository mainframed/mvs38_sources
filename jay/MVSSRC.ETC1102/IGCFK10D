         TITLE 'IGCFK10D - REDRIVE PROCESSOR                           *00001000
                        '                                               00002000
IGCFK10D CSECT ,                                                01S0001 00003000
@PROLOG  BALR  @12,0                                            01S0001 00004000
@PSTART  DS    0H                                               01S0001 00005000
         USING @PSTART,@12                                      01S0001 00006000
*                                   /*                                  00007000
IGCFK10D IEDHJN AFTERHJN,HJN                                            00008000
*                                                               01S0295 00009000
*   /*****************************************************************/ 00010000
*   /*                                                               */ 00011000
*   /* INITIALIZATION                                                */ 00012000
*   /*                                                               */ 00013000
*   /*****************************************************************/ 00014000
*                                                               01S0295 00015000
*   ROPCAVT=RPARM;                  /* SET POINTER TO OP. CTL. AVT   */ 00016000
         LR    ROPCAVT,RPARM                                    01S0295 00017000
*   RAVT=OPCAVTPT;                  /* SET POINTER TO TCAM AVT       */ 00018000
         L     RAVT,OPCAVTPT(,ROPCAVT)                          01S0296 00019000
*   RSAVE=ADDR(OPCSAVE);            /* SET POINTER TO SAVE AREA      */ 00020000
         LA    RSAVE,OPCSAVE(,ROPCAVT)                          01S0297 00021000
*                                                               01S0298 00022000
*   /*****************************************************************/ 00023000
*   /*                                                               */ 00024000
*   /* CALCULATE 50% OF TCAM'S LINE UNITS                            */ 00025000
*   /*                                                               */ 00026000
*   /*****************************************************************/ 00027000
*                                                               01S0298 00028000
*   RUNITLVL=AVTNOLBF;              /* # OF TCAM LINE UNITS          */ 00029000
         LH    RUNITLVL,AVTNOLBF(,RAVT)                         01S0298 00030000
*   SRL(RUNITLVL,1);                /* DIVIDE BY TWO FOR 50%         */ 00031000
         SRL   RUNITLVL,1                                       01S0299 00032000
*   REQPRI=FREEREDR;                /* FREE REDRIVE ELEMENT          */ 00033000
         L     @09,OPCCOPCE(,ROPCAVT)                           01S0300 00034000
         MVI   REQPRI(@09),X'00'                                01S0300 00035000
*   IF OPCADGOT>=RUNITLVL THEN      /* IF # BUFFER UNITS A/D USING IS   00036000
*                                      GT OR E TO 1/2 TCAM'S LINE       00037000
*                                      UNITS                         */ 00038000
         LH    @09,OPCADGOT(,ROPCAVT)                           01S0301 00039000
         N     @09,@CF01487                                     01S0301 00040000
         CR    RUNITLVL,@09                                     01S0301 00041000
         BH    @RF00301                                         01S0301 00042000
*     OPCMODID=TO00;                /* SET TO XCTL TO IGC0010D       */ 00043000
         MVC   OPCMODID(2,ROPCAVT),TO00                         01S0302 00044000
*   ELSE                                                        01S0303 00045000
*     DO;                                                       01S0303 00046000
*                                                               01S0303 00047000
         B     @RC00301                                         01S0303 00048000
@RF00301 DS    0H                                               01S0304 00049000
*       /*************************************************************/ 00050000
*       /*                                                           */ 00051000
*       /* OBTAIN LAST OPCE ON A/D BUFFER UNIT WAIT CHAIN            */ 00052000
*       /*                                                           */ 00053000
*       /*************************************************************/ 00054000
*                                                               01S0304 00055000
*/* NOTE: THERE IS ALWAYS AT LEAST 1 OPCE ON CHAIN IF THIS ROUTINE   */ 00056000
*/*       IS CALLED                                                  */ 00057000
*                                                               01S0304 00058000
*       ROPCE=ADDR(OPCBUFWT)-(ADDR(OCLINK)-ADDR(IEDQOPCE));/* INIT.     00059000
*                                      OPCE POINTER SO THAT OCLINK      00060000
*                                      FIELD OF OPCE DSECT OVERLAYS     00061000
*                                      OPCBUFWT - THE WAIT CHAIN        00062000
*                                      ANCHOR                @YM08487*/ 00063000
         LA    @09,OPCBUFWT(,ROPCAVT)                           01S0304 00064000
         LR    @15,ROPCE                                        01S0304 00065000
         SLR   @15,ROPCE                                        01S0304 00066000
         SLR   @09,@15                                          01S0304 00067000
         LR    ROPCE,@09                                        01S0304 00068000
*       DO UNTIL(OCLINK=0);                                     01S0305 00069000
@DL00305 DS    0H                                               01S0306 00070000
*         RNXTLAST=ROPCE;           /* SET PTR. FOR NEXT TO LAST OPCE*/ 00071000
         LR    RNXTLAST,ROPCE                                   01S0306 00072000
*         ROPCE=OCLINK;             /* SET OPCE PTR. TO NEXT OPCE    */ 00073000
         L     ROPCE,OCLINK(,ROPCE)                             01S0307 00074000
*       END;                                                    01S0308 00075000
@DE00305 L     @09,OCLINK(,ROPCE)                               01S0308 00076000
         LTR   @09,@09                                          01S0308 00077000
         BNZ   @DL00305                                         01S0308 00078000
*                                                               02S0309 00079000
*       /*************************************************************/ 00080000
*       /*                                                           */ 00081000
*       /* OBTAIN AS MANY BUFFER UNITS AS THE OPCE NEEDS UP TO 50% OF*/ 00082000
*       /* TCAM'S UNITS. CHAIN THEM OFF OCUNIT FIELD OF THE REDRIVE  */ 00083000
*       /* OPCE.                                                     */ 00084000
*       /*                                                           */ 00085000
*       /*************************************************************/ 00086000
*                                                               02S0309 00087000
*       RFY                                                     02S0309 00088000
*         RZERO RSTD;               /* PARM. FOR ALLOCBUF            */ 00089000
*       RUNITPTR=ADDR(OCUNIT)-EVAL(ADDR(PRFTIC)-ADDR(IEDQPRF));/* SET   00090000
*                                      UNIT PTR. TO POINT INTO OPCE     00091000
*                                      SO THAT THE PRFTIC FIELD OF      00092000
*                                      THE PREFIX DSECT OVERLAYS        00093000
*                                      OCUNIT                        */ 00094000
         LA    @09,OCUNIT(,ROPCE)                               02S0310 00095000
         SL    @09,@CF00045                                     02S0310 00096000
         LR    RUNITPTR,@09                                     02S0310 00097000
*       IF OPCADGOT+OCBUFNED>RUNITLVL THEN/* IF TOTAL OF UNITS A/D      00098000
*                                      HAS AND UNITS NEEDED EXCEEDS     00099000
*                                      THE 50% LEVEL                 */ 00100000
         LH    @09,OPCADGOT(,ROPCAVT)                           02S0311 00101000
         N     @09,@CF01487                                     02S0311 00102000
         LH    @15,OCBUFNED(,ROPCE)                             02S0311 00103000
         N     @15,@CF01487                                     02S0311 00104000
         ALR   @15,@09                                          02S0311 00105000
         CR    @15,RUNITLVL                                     02S0311 00106000
         BNH   @RF00311                                         02S0311 00107000
*         RUNITGET=RUNITLVL-OPCADGOT;/* GET UNITS UP TO 50% LEVEL    */ 00108000
         LCR   @09,@09                                          02S0312 00109000
         ALR   @09,RUNITLVL                                     02S0312 00110000
         LR    RUNITGET,@09                                     02S0312 00111000
*       ELSE                                                    02S0313 00112000
*         RUNITGET=OCBUFNED;        /* GET AS MANY UNITS AS NEEDED   */ 00113000
         B     @RC00311                                         02S0313 00114000
@RF00311 LH    RUNITGET,OCBUFNED(,ROPCE)                        02S0313 00115000
         N     RUNITGET,@CF01487                                02S0313 00116000
*       DO UNTIL(RUNITGET=0);       /* GO TO ALLOCBUF AS MANY TIMES     00117000
*                                      AS NECESSAY TO OBTAIN UNITS   */ 00118000
@RC00311 DS    0H                                               02S0314 00119000
@DL00314 DS    0H                                               02S0315 00120000
*         IF RUNITGET>MAXUNTGT THEN /* IF FK IS TO GET MORE THAN THE    00121000
*                                      # OF UNITS ALLOCBUF CAN GIVE     00122000
*                                      PER CALL                      */ 00123000
         LA    @09,255                                          02S0315 00124000
         CR    RUNITGET,@09                                     02S0315 00125000
         BNH   @RF00315                                         02S0315 00126000
*           RPARM=MAXUNTGT;         /* GET THE MAXIMUM               */ 00127000
         LR    RPARM,@09                                        02S0316 00128000
*         ELSE                                                  02S0317 00129000
*           RPARM=RUNITGET;         /* GET # TO GET                  */ 00130000
         B     @RC00315                                         02S0317 00131000
@RF00315 LR    RPARM,RUNITGET                                   02S0317 00132000
*         CALL ALLOCBUF;            /* GET BUFFER UNITS              */ 00133000
*                                                               02S0318 00134000
@RC00315 L     @15,OPCGETBF(,ROPCAVT)                           02S0318 00135000
         BALR  @14,@15                                          02S0318 00136000
*         /***********************************************************/ 00137000
*         /*                                                         */ 00138000
*         /* LINK UNITS JUST GOTTEN OFF OF OCUNIT AT END OF CURRENT  */ 00139000
*         /* CHAIN OF BUFFERS                                        */ 00140000
*         /*                                                         */ 00141000
*         /***********************************************************/ 00142000
*                                                               02S0319 00143000
*         DO WHILE(TIC=VALTIC);     /* LOOP TO FIND END OF CURRENT      00144000
*                                      CHAIN                 @YM05731*/ 00145000
         B     @DE00319                                         02S0319 00146000
@DL00319 DS    0H                                               02S0320 00147000
*           RUNITPTR=PRFTI0;        /* POINT TO NEXT UNIT            */ 00148000
         L     @09,PRFTI0-1(,RUNITPTR)                          02S0320 00149000
         LA    @09,0(,@09)                                      02S0320 00150000
         LR    RUNITPTR,@09                                     02S0320 00151000
*         END;                                                  02S0321 00152000
@DE00319 TM    TIC(RUNITPTR),B'00000011'                        02S0321 00153000
         BZ    @DL00319                                         02S0321 00154000
*         PRFTI0=RPARM;             /* POINT TO UNITS JUST GOTTEN    */ 00155000
         STCM  RPARM,7,PRFTI0(RUNITPTR)                         02S0322 00156000
*         RUNITGET=RUNITGET-RZERO;  /* UPDATE # OF UNITS TO GET      */ 00157000
         SLR   RUNITGET,RZERO                                   02S0323 00158000
*         OCBUFNED=OCBUFNED-RZERO;  /* UPDATE # OF UNITS OPCE NEEDS  */ 00159000
         LH    @09,OCBUFNED(,ROPCE)                             02S0324 00160000
         N     @09,@CF01487                                     02S0324 00161000
         SLR   @09,RZERO                                        02S0324 00162000
         STH   @09,OCBUFNED(,ROPCE)                             02S0324 00163000
*         OCBUFGOT=OCBUFGOT+RZERO;  /* UPDATE # OF UNITS OPCE HAS    */ 00164000
         LH    @09,OCBUFGOT(,ROPCE)                             02S0325 00165000
         N     @09,@CF01487                                     02S0325 00166000
         ALR   @09,RZERO                                        02S0325 00167000
         STH   @09,OCBUFGOT(,ROPCE)                             02S0325 00168000
*         OPCADGOT=OPCADGOT+RZERO;  /* UPDATE # OF UNITS A/D HAS     */ 00169000
         LH    @09,OPCADGOT(,ROPCAVT)                           02S0326 00170000
         N     @09,@CF01487                                     02S0326 00171000
         ALR   @09,RZERO                                        02S0326 00172000
         STH   @09,OPCADGOT(,ROPCAVT)                           02S0326 00173000
*       END;                                                    02S0327 00174000
@DE00314 SLR   @09,@09                                          02S0327 00175000
         CR    RUNITGET,@09                                     02S0327 00176000
         BNE   @DL00314                                         02S0327 00177000
*       RFY                                                     02S0328 00178000
*         RZERO UNRSTD;                                         02S0328 00179000
*       RNXTLAST->OCLINK=0;         /* DELINK LAST OPCE              */ 00180000
         ST    @09,OCLINK(,RNXTLAST)                            01S0329 00181000
*       OPCCOPCE=ROPCE;             /* AND SPECIFY AS CURRENT OPCE   */ 00182000
         ST    ROPCE,OPCCOPCE(,ROPCAVT)                         01S0330 00183000
*       OPCMODID=OCWTG;             /* SET TO XCTL TO OCWTG MODULE   */ 00184000
         MVC   OPCMODID(2,ROPCAVT),OCWTG(ROPCE)                 01S0331 00185000
*       OCWTG=''B;                  /* CLEAR FOR SUBSEQUENT USE      */ 00186000
         XC    OCWTG(2,ROPCE),OCWTG(ROPCE)                      01S0332 00187000
*     END;                                                      01S0333 00188000
*                                                               01S0333 00189000
*   /*****************************************************************/ 00190000
*   /*                                                               */ 00191000
*   /* XCTL LINKAGE                                                  */ 00192000
*   /*                                                               */ 00193000

*                                                               01S0334 00195000
*   RPARM=ROPCAVT;                  /* SET OP. CTL. AVT AS PARM.     */ 00196000
@RC00301 LR    RPARM,ROPCAVT                                    01S0334 00197000
*   GEN EXIT REFS(ROPCAVT,OPCXCTL);                             01S0335 00198000

*             END;                                              04S0939 00291000
*           ELSE                    /* FINISHED SENDING RESTART 04S0940 00292000
*                                      COMMANDS,                     */ 00293000
*             DO;                   /* SET UP CURRENT OPCE AS ACTLINK   00294000
*                                      OPCE AND XCTL TO IGCFJ10D TO     00295000

         STH   @11,OCBUFNED(,ROPCE)                             04S0952 00364000
*                     END;                                      04S0953 00365000
*                   ELSE            /* ELSE GET ALL NEEDED           */ 00366000
*                     DO;                                       04S0954 00367000
         B     @RC00949                                         04S0954 00368000

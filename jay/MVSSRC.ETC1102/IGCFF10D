         N     @11,@CF02618                                     04S0959 00381000
         ALR   @11,GETNO                                        04S0959 00382000
         STH   @11,OPCADGOT(,ROPCAVT)                           04S0959 00383000
*                 END;                                          04S0960 00384000
*               ELSE                /* ELSE MUST PUT REDRIVE OPCE ON    00385000
*                                      WAIT CHAIN AND WAIT FOR       */ 00386000
*                 DO;               /* AVAILABLE UNITS ON REDRIVE    */ 00387000
         B     @RC00947                                         04S0961 00388000
@RF00947 DS    0H                                               04S0962 00389000
*                   OCWTG=THISMOD;  /* SET RETURN TO THIS MODULE     */ 00390000
         MVC   OCWTG(2,ROPCE),THISMOD                           04S0962 00391000
*                   OCLINK=OPCBUFWT;/* GET CHAIN ANCHOR AND PUT IN      00392000
*                                      LINK FIELD                    */ 00393000

*         OPCMODID=CNTLMOD;         /* SET TO XCTL TO IGC0010D       */ 00118000
         MVC   OPCMODID(2,ROPCAVT),CNTLMOD                      03S0894 00119000
*         RFY                                                   03S0895 00120000
*           REG00 UNRSTD;                                       03S0895 00121000
*         RFY                                                   03S0896 00122000
*           RPARM UNRSTD;                                       03S0896 00123000
*       END;                                                    02S0897 00124000
*     ELSE                          /* THIS IS AN ACTLINK,           */ 00125000
*       DO;                         /* START CKPTR/RTRT PROCESS      */ 00126000
         B     @RC00882                                         02S0898 00127000
@RF00882 DS    0H                                               02S0899 00128000
*                                                               03S0899 00129000
*         /***********************************************************/ 00130000
*         /*                                                         */ 00131000
*         /* PROCESS.START: THIS SEGMENT STARTS THE RESTART          */ 00132000
*         /* PROCESSING BY BUILDING THE RESTART OPCE AND PASSING IT  */ 00133000
*         /* TO CHECKPOINT.                                          */ 00134000
*         /*                                                         */ 00135000
*         /***********************************************************/ 00136000
*                                                               03S0899 00137000
*         OCSWITCH=OCRSTART;        /* SET AS RESTART OPCE           */ 00138000
         MVI   OCSWITCH(ROPCE),X'F1'                            03S0899 00139000
*         OCOPTCDE=OCMODIFY;        /* SET CMND CODE                 */ 00140000
         MVI   OCOPTCDE(ROPCE),X'44'                            03S0900 00141000
*         OCRESNUM=''B;             /* INITIALIZE RESOURCE NUMBER    */ 00142000
         SLR   @11,@11                                          03S0901 00143000
         STH   @11,OCRESNUM(,ROPCE)                             03S0901 00144000
*         OCTRMTBL=OCPTRSAV->OCTRMTBL;/* TTCIN OF NCP                */ 00145000
         L     @11,OCPTRSAV(,ROPCE)                             03S0902 00146000
         LH    @11,OCTRMTBL(,@11)                               03S0902 00147000
         N     @11,@CF02618                                     03S0902 00148000
         STH   @11,OCTRMTBL(,ROPCE)                             03S0902 00149000
*         OCWTG=THISMOD;            /* SET TO RETURN TO THIS MODULE     00150000
*                                      FROM CHECKPOINT               */ 00151000
         MVC   OCWTG(2,ROPCE),THISMOD                           03S0903 00152000
*         OCREDRVE=OFF;             /* INSURE REDRIVE FLAG OFF       */ 00153000
         NI    OCREDRVE(ROPCE),B'11101111'                      03S0904 00154000
*         OPCMODID=CKPTMOD;         /* SET TO XCTL TO CHECKPOINT     */ 00155000
         MVC   OPCMODID(2,ROPCAVT),CKPTMOD                      03S0905 00156000
*       END;                                                    02S0906 00157000
*   ELSE                            /* THIS IS A RESTART OPCE,       */ 00158000
*     DO;                           /* SEND OUT RESTART COMMANDS     */ 00159000
         B     @RC00881                                         02S0907 00160000
@RF00881 DS    0H                                               02S0908 00161000
*                                                               03S0908 00162000
*       /*************************************************************/ 00163000
*       /*                                                           */ 00164000
*       /* PROCESS.SENDER: THIS SEGMENT SENDS THE RESTART COMMANDS TO*/ 00165000
*       /* THE NCP.                                                  */ 00166000
*       /*                                                           */ 00167000
*       /*************************************************************/ 00168000
*                                                               03S0908 00169000
*       RCKPREC=NRACURR;            /* SET BASE FOR CKPT RESOURCE       00170000

         ALR   SIZE,@11                                         05S0932 00272000
@DE00930 CH    SIZE,AVTKEYLE(,RAVT)                             05S0932 00273000
         BH    @DL00930                                         05S0932 00274000
*                   GETNO=GETNO+COUNTER;/* CALC NUM UNITS NEEDED FOR    00275000
*                                      ALL CMNDS                     */ 00276000
         ALR   GETNO,COUNTER                                    05S0933 00277000
*                   OCDVRDCT=COUNTER;/* SAVE NUM UNITS NEEDED FOR       00278000
*                                      DEVICE RECORD CMND IN RESTART    00279000

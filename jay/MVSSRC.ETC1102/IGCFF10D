         ALR   @14,@11                                          04S0949 00344000
         LR    @00,@05                                          04S0949 00345000
         SRDA  @00,32                                           04S0949 00346000
         DR    @00,@06                                          04S0949 00347000
         CR    @14,@01                                          04S0949 00348000
         BNH   @RF00949                                         04S0949 00349000
*                     DO;           /* NO A/D ALREADY HAS IS GT 50%     00350000
*                                      THEN                          */ 00351000
*                       GETNO=((AVTNOLBF/2)-OPCADGOT);/* SET TO GET     00352000
*                                      UP TO 50%                     */ 00353000

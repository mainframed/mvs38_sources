*     ROPCE=OPCCOPCE;               /* SET THE BASE POINTER FOR THE     00050000
*                                      OPERATOR CONTROL ELEMENT      */ 00051000
         L     ROPCE,OPCCOPCE(,ROPCAVT)                         03S0878 00052000
*     FLAGS=''B;                    /* INITIALIZE FLAG BYTE          */ 00053000
         XC    FLAGS(4),FLAGS                                   03S0879 00054000
*   END;                                                        03S0880 00055000
*   IF OCSWITCH^=OCRSTART THEN      /* IF NOT A RESTART OPCE         */ 00056000

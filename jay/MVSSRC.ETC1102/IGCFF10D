         N     GETNO,@CF02618                                   04S0955 00372000
*                       OCBUFNED=0; /* SET NO STILL NEED=0           */ 00373000
         SLR   @11,@11                                          04S0956 00374000
         STH   @11,OCBUFNED(,ROPCE)                             04S0956 00375000
*                     END;                                      04S0957 00376000
*                   OCBUFGOT=GETNO; /* SET NO HAVE =NO TO GET        */ 00377000
@RC00949 STH   GETNO,OCBUFGOT(,ROPCE)                           04S0958 00378000
*                   OPCADGOT=OPCADGOT+GETNO;/* INCREMENT OPCADGOT    */ 00379000

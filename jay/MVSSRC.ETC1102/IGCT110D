         BNO   @RF00290                                            0290 00215000
*     DO;                           /* IF ADDR CHARS CHANGED @Y17XARW*/ 00216000
*       PRURH=TRHFID0;              /* SETUP RH FLAGS        @Y17XARW*/ 00217000
         LR    @03,IEDPRUQP                                        0292 00218000

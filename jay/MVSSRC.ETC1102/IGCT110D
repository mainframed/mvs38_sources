*   GENERATE;                                                      0285 00162000
         LA    RMASK,X'80'             GET MASK FOR EXECUTED INST       00163000
LOOP1    EQU   *                                                        00164000
         CH    RMASK,DIALMSK            IS THIS MASK FOR DIAL DIGITS    00165000
         BE    DIALDIGS                 BRANCH ON YES                   00166000
         CH    RMASK,ADDRMSK            IS THIS MASK FOR ADDR CHARS     00167000
         BE    ADDRCHAR                 BRANCH ON YES                   00168000
         EX    RMASK,TM1                IS THERE A DEV DEP FIELD        00169000
         BNO   SHIFT1                   BRANCH ON NO                    00170000
MOVFLD   EQU   *                                                        00171000
         SR    RLNGTH,RLNGTH            0 LENGTH REGISTER @OZ15650      00172000
         IC    RLNGTH,0(RFLDS)          GET LENGTH OF DATA FIELD        00173000
         BAL   REG12,CHECKAIB           CHECK FOR END OF AIB @G36XRRU*/ 00174000
         EX    RLNGTH,MVC1              MOVE DATA TO USER WORKAREA      00175000
         LA    RTO,1(RLNGTH,RTO)        UPDATE TO AREA POINTER          00176000
         LA    RFLDS,1(RLNGTH,RFLDS)    UPDATE FROM AREA POINTER        00177000
SHIFT1   EQU   *                                                        00178000
         SRL   RMASK,1                  SHIFT MASK 1 POSITION           00179000
         LTR   RMASK,RMASK              ARE THER ANY MORE TO CHECK      00180000
         BNZ   LOOP1                    BRANCH ON YES                   00181000
         LA    RMASK,X'80'             GET MASK FOR EXECUTED INST       00182000
LOOP2    EQU   *                                                        00183000
         EX    RMASK,TM2                EXECUTE TEST UNDER MASK         00184000
         BNO   SHIFT2                   BRANCH IF BIT NOT ON            00185000
MOVFLD2  EQU   *                                                        00186000
         SR    RLNGTH,RLNGTH            0 LENGTH REGISTER @OZ15650      00187000
         IC    RLNGTH,0(RFLDS)          PICK UP FIELD LENGTH            00188000
         BAL   REG12,CHECKAIB           CHECK FOR END OF AIB @G36XRRU   00189000
         EX    RLNGTH,MVC1              MOVE FIELD                      00190000
         LA    RTO,1(RLNGTH,RTO)        UPDATE TO AREA POINTER          00191000
         LA    RFLDS,1(RLNGTH,RFLDS)    UPDATE FROM AREA POINTER        00192000
SHIFT2   EQU   *                                                        00193000
         SRL   RMASK,1                  SHIFT TO NEXT MASK BIT          00194000
         LTR   RMASK,RMASK              ANY MORE BITS TO CHECK          00195000
         BNZ   LOOP2                    BRANCH ON NO                    00196000
         B     SKIP1                                                    00197000
TM1      TM    TRMDEVFL(RFROM),0                                        00198000
TM2      TM    TRMDEVFL+1(RFROM),0                                      00199000
MVC1     MVC   0(0,RTO),0(RFLDS)                                        00200000
SKIP1    EQU   *                                                        00201000
*   IF ELMNT->WENTCD<'08'X          /* IS THIS FOR TCHNG             */ 00202000
*     THEN                                                         0286 00203000
         L     @03,ELMNT(,ROPCAVT)                                 0286 00204000
         CLI   WENTCD(@03),8                                       0286 00205000

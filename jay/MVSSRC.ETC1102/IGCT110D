*     END;                          /*                       @Y17XARW*/ 00244000
*   ELSE                                                           0302 00245000
*     OPCMODID=MODT0;               /* SETUP FOR NEXT MOD    @Y17XARW*/ 00246000
         B     @RC00290                                            0302 00247000
@RF00290 MVC   OPCMODID(2,ROPCAVT),MODT0                           0302 00248000
*EXIT:                                                             0303 00249000
*   OPCFLAG1=0;                     /* CLEAR FLAG FIELDS     @Y17XARW*/ 00250000
@RC00290 DS    0H                                                  0303 00251000
EXIT     MVI   OPCFLAG1(ROPCAVT),X'00'                             0303 00252000
*   REG1=ROPCAVT;                   /* SETUP OP CONTROL AVT  @Y17XARW*/ 00253000
         LR    REG1,ROPCAVT                                        0304 00254000
*   GEN EXIT REFS(OPCXCTL,ROPCAVT);                                0305 00255000
     XCTL    SF=(E,OPCXCTL(ROPCAVT))                           @Y17XARW 00256000
*DIALDIGS:                                                         0306 00257000
*   IF TRMDEVFL(2)='1'B THEN        /* ARE DIAL DIGITS SPECIFIED     */ 00258000
*                                                                  0306 00259000
DIALDIGS TM    TRMDEVFL(RFROM),B'01000000'                         0306 00260000
         BNO   @RF00306                                            0306 00261000
*     /***************************************************************/ 00262000
*     /*                                                             */ 00263000
*     /* WHEN DIAL DIGITS ARE SPECIFIED THE FOLLOWING ACTION IS TAKEN*/ 00264000
*     /* BY THIS MODULE: TCOPY DIGITS IN TTE- DIAL DIGITS TRANSFERRED*/ 00265000
*     /* TO THE USER WORKARE FROM THE TTE DIGITS IN NCP- DIAL DIGITS */ 00266000
*     /* TRANSFERRED TO THE USER WORKAREA FROM THE RESPONSE RU TCHNG */ 00267000
*     /* DIGITS IN TTE- DIAL DIGITS TRANSFERRED TO THE TTE FROM THE  */ 00268000
*     /* USER WORKAREA DIGITS IN NCP- DIAL DIGITS TRANSFERRED TO THE */ 00269000
*     /* RU FROM THE USER WORKAREA AND A REPLACE RU IS SENT TO THE   */ 00270000
*     /* NCP                                                 @YM08051*/ 00271000
*     /*                                                             */ 00272000
*     /***************************************************************/ 00273000
*                                                                  0307 00274000
*     IF TCHNG='1'B THEN            /* IS IT TCHNG?          @YM08051*/ 00275000
*                                                                  0307 00276000
         TM    TCHNG(ROPCAVT),B'01000000'                          0307 00277000
         BNO   @RF00307                                            0307 00278000
*       /*************************************************************/ 00279000
*       /*                                                           */ 00280000
*       /* TCHNG                                             @YM08051*/ 00281000
*       /*                                                           */ 00282000
*       /*************************************************************/ 00283000
*                                                                  0308 00284000
*       IF RTO->FDATA='0F'X THEN    /* AND DIGITS IN NCP     @YM08051*/ 00285000
         CLI   FDATA(RTO),X'0F'                                    0308 00286000
         BE    @RT00308                                            0308 00287000
*         GO TO GETDIAL0;           /* GO TRANSFER DIGITS    @YM08081*/ 00288000
*       ELSE                                                       0310 00289000
*         GO TO MOVFLD;             /* DIGITS IN TTE         @YM08051*/ 00290000
*                                                                  0310 00291000
         B     MOVFLD                                              0310 00292000
*   /*****************************************************************/ 00293000
*   /*                                                               */ 00294000
*   /* TCOPY                                                 @YM08051*/ 00295000
*   /*                                                               */ 00296000
*   /*****************************************************************/ 00297000
*                                                                  0311 00298000
*     ELSE                                                         0311 00299000
*       IF RFLDS->FDATA='0F'X       /* NO ARE DIGITS IN NCP  @YM08051*/ 00300000
*         THEN                                                     0311 00301000
@RF00307 CLI   FDATA(RFLDS),X'0F'                                  0311 00302000
         BE    @RT00311                                            0311 00303000
*         GO TO GETDIAL;            /* YES, DIGITS ARE IN RESPONSE   */ 00304000
*       ELSE                                                       0313 00305000
*         GO TO MOVFLD;             /* NO, DIGITS ARE IN TERM ENTRY  */ 00306000
         B     MOVFLD                                              0313 00307000
*   GO TO SHIFT1;                   /* GO CHECK NEXT BIT             */ 00308000
         B     SHIFT1                                              0314 00309000
*GETDIAL0:                                                         0315 00310000
*   RTO=RTO+2;                      /* INCREMENT TTE REG     @YM08081*/ 00311000
GETDIAL0 LA    RTO,2(,RTO)                                         0315 00312000
*   GO TO GETDIAL1;                 /* GO MOVE DATA          @YM08081*/ 00313000
         B     GETDIAL1                                            0316 00314000
*GETDIAL:                                                          0317 00315000
*   RFLDS=RFLDS+2;                  /* UPDATE 'FROM' DEV DEP PTR     */ 00316000
GETDIAL  LA    RFLDS,2(,RFLDS)                                     0317 00317000
*GETDIAL1:                                                         0318 00318000
*   IF DIALCHAR=0                   /* IS THERE ANY DIAL DATA        */ 00319000
*     THEN                                                         0318 00320000
GETDIAL1 L     @03,OPCWORK(,ROPCAVT)                               0318 00321000
         L     @03,DIALCHAR(,@03)                                  0318 00322000
         LTR   @03,@03                                             0318 00323000
         BZ    @RT00318                                            0318 00324000
*     GO TO SHIFT1;                 /* NO, GO CHECK NEXT BIT         */ 00325000
*   CHANGE='1'B;                    /* INDICATE DIAL DIGITS MUST   0320 00326000
*                                      CHANGE IN NCP         @Y17XARW*/ 00327000
         OI    CHANGE(ROPCAVT),B'10000000'                         0320 00328000
*   RDATA=DIALCHAR;                 /* ADDRESS OF DIAL DIGITS      0321 00329000
*                                                            @Y17XARW*/ 00330000
         STCM  @03,7,RDATA(ROPCAVT)                                0321 00331000
*   IF TCHNG='1'B THEN              /* IS IT TCHNG           @YM08051*/ 00332000
         TM    TCHNG(ROPCAVT),B'01000000'                          0322 00333000
         BO    @RT00322                                            0322 00334000
*     GO TO MOVIT0;                 /* YES GO MOVE DIGITS TO RU    0323 00335000
*                                                            @YM08051*/ 00336000
*MOVDATA:                                                          0324 00337000
*   RLNGTH=RDATA->FCOUNT+1;         /* LENGTH OF FLD TO MOVE @Y17XARW*/ 00338000
MOVDATA  L     @03,RDATA-1(,ROPCAVT)                               0324 00339000
         SLR   @15,@15                                             0324 00340000
         IC    @15,FCOUNT(,@03)                                    0324 00341000
         LA    @15,1(,@15)                                         0324 00342000
         LR    RLNGTH,@15                                          0324 00343000
*MOVIT1:                                                           0325 00344000
*   GENERATE;                                                      0325 00345000
MOVIT1   DS    0H                                                  0325 00346000
*                                  /*                        @G36XRRU*/ 00347000
         BAL   REG12,CHECKAIB      /* CHECK FOR END OF AIB   @G36XRRU*/ 00348000
*   RTO->FIELD=RDATA->FIELD;        /* MOVE ONE BYTE OF DATA @Y17XARW*/ 00349000
         L     @03,RDATA-1(,ROPCAVT)                               0326 00350000
         LA    @03,0(,@03)                                         0326 00351000
         MVC   FIELD(1,RTO),FIELD(@03)                             0326 00352000
*   RLNGTH=RLNGTH-1;                /* REDUCE COUNT TO MOVE          */ 00353000
         BCTR  RLNGTH,0                                            0327 00354000
*   RTO=RTO+1;                      /* UPDATE 'TO' AREA POINTER      */ 00355000
         LA    @15,1                                               0328 00356000
         ALR   RTO,@15                                             0328 00357000
*   RDATA=RDATA+1;                  /* UPDATE 'FROM' AREA PTR      0329 00358000
*                                                            @Y17XARW*/ 00359000
         ALR   @03,@15                                             0329 00360000
         STCM  @03,7,RDATA(ROPCAVT)                                0329 00361000
*   GENERATE;                                                      0330 00362000
         BAL   REG12,CHKUNIT       /* CHECK END OF UNIT EXCEEDED     */ 00363000
*   IF RLNGTH=0                     /* HAS ALL DATA BEEN MOVED       */ 00364000
*     THEN                                                         0331 00365000
         LTR   RLNGTH,RLNGTH                                       0331 00366000
         BZ    @RT00331                                            0331 00367000

         SPACE 2                                                        31100010
         USING *,RENTRY                 ADDRESSABILITY.          Y01004 31200010
         DC    2H'0'                    PADDING FOR POINT MACRO         31500010
IGG019RM IEDHJN SAVEREGS                                                32000010
         STM   RETURN,RBASE,E12(RSAVE)  SAVE CALLER'S REGISTERS. Y01004 32400010
         LR    RBASE,RENTRY             GET ENTRY POINT.         Y01004 32450010
         USING IGG019RM,RBASE           SET ADDRESSABILITY.      Y01004 32500010
         DROP  RENTRY                   DISCARD OLD BASE.        Y01004 32700010
         USING IEDQPECB,RPECB           PECB ADDRESSABILITY      Y02027 36000010
         LR    RDCB,RPARM               ADDRESS OF DCB.          Y01004 36530010
         LR    RWORK,RZERO              ADDRESS OF RETRIEVE DATA Y01004 36560010
         USING IHADCB,RDCB              DCB BASE REGISTER               40000010
         LA    RENTRY,INVSEQ            POSSIBLE SEQ # ERROR     Y02027 40500010
         CLI   EIGHT(RWORK),BLANK       RETRIEVE TERMINATION     Y02027 43500010
         BE    CONT                     BRANCH IF YES            Y02027 46500010
         ICM   R10,HALF,SEQNO(RWORK)    GET SEQUENCE NUMBER FROM Y01004 54500010
*                                       RETRIEVE DATA.           Y01004 55000010
         BZ    EXIT                     BRANCH IF SEQ. NUMBER=0  Y02027 55500010
CONT     EQU   *                                                 Y02027 57500010
         L     RPECB,DCBPECB            GET PECB ADDRESS         Y02027 59500010
*                                                                Y02027 61500010
*              SET UP PARAMETER LIST                             Y02027 63500010
*                                                                Y02027 65500010
         LA    R10,PECBECB              GET ECB ADDRESS AND      Y02027 67500010
         ST    R10,PECBPARM             STORE IN PARAMETER LIST  Y02027 69500010
         MVI   PECBPARM,PTCODE          SET POINT CODE IN PARM   Y02027 71500010
*                                       LIST                     Y02027 73500010
         LA    RPARM,PECBPARM           SET UP PARAMETER REG     Y02027 75500010
         AQCTL                                                   Y02027 77500010
EXIT     EQU   *                                                 Y02027 79500010
         RETURN (14,12),RC=(15)         RESTORE USER'S REGISTERS Y01004X83013010
                                        AND RETURN CONTROL TO    Y01004X83014010
                                        NSI FOLLOWING POINT      Y01004X83015010
                                        MACRO.                   Y01004 83016010
         EJECT                                                          83017010
* E Q U A T E S                                                  Y01004 83600010
         SPACE                                                          83700010
BLANK    EQU   X'40'                                                    84000010

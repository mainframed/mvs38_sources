         BE    @RT00352                                            0352 00372000
         CLI   DIGIT(RSPTR),C' '                                   0352 00373000
         BNE   @RF00352                                            0352 00374000
@RT00352 DS    0H                                                  0353 00375000
*       DO;                         /* OR A BLANK                    */ 00376000
*         OCBACKUP=' ';             /* YES, BLANK RCVING FIELD       */ 00377000
         MVI   OCBACKUP+1(ROPCE),C' '                              0354 00378000
         MVC   OCBACKUP+2(6,ROPCE),OCBACKUP+1(ROPCE)               0354 00379000
         MVI   OCBACKUP(ROPCE),C' '                                0354 00380000
*         OCBACKUP=FIELD(1:RCNT);   /* MOVE TERMINAL NAME            */ 00381000
         MVI   OCBACKUP+1(ROPCE),C' '                              0355 00382000
         MVC   OCBACKUP+2(6,ROPCE),OCBACKUP+1(ROPCE)               0355 00383000

*SCAN:                                                             0341 00341000
*   PROC OPTIONS(DONTSAVE,NOSAVEAREA);                             0341 00342000
SCAN     DS    0H                                                  0342 00343000
*   GEN(EJECT) ;                                                   0342 00344000
*                                                                  0342 00345000
         EJECT                                                          00346000
*/* THIS ROUTINE SCANS FOR A COMMA OR A BLANK FROM WHERE THE SCAN    */ 00347000
*/* POINTER (REG 7) IS POINTING. IF FOUND, IT INITIALIZES OCBACKUP   */ 00348000
*/* WITH THE TERMINAL NAME. OTHERWISE AN ERROR MSG IS ISSUED.        */ 00349000
*                                                                  0343 00350000
*   RESTRICT(ROPCE,ROPCAVT);                                       0343 00351000
*   RESTRICT(RSPTR,RSCAN,RCNT);                                    0344 00352000
*   GEN(SPACE 2) ;                                                 0345 00353000
         SPACE 2                                                        00354000
*   RESPECIFY                                                      0346 00355000
*     RLINK RSTD;                   /*                       @Y17XART*/ 00356000
*   OPCSAVE=RLINK;                  /* SAVE RETURN REG               */ 00357000
         ST    RLINK,OPCSAVE(,ROPCAVT)                             0347 00358000
*   RESPECIFY                                                      0348 00359000
*     RLINK UNRSTD;                 /*                       @Y17XART*/ 00360000
*   RSPTR=RSCAN;                    /* INIT SCAN PTR                 */ 00361000
         LR    RSPTR,RSCAN                                         0349 00362000
*   DO RCNT=0 TO 8 BY 1;            /* SCAN FOR A COMMA              */ 00363000
         SLR   RCNT,RCNT                                           0350 00364000
@DL00350 DS    0H                                                  0351 00365000
*     RSPTR=RSCAN+RCNT;             /* UPDATE SCAN PTR               */ 00366000
         LR    RSPTR,RSCAN                                         0351 00367000
         ALR   RSPTR,RCNT                                          0351 00368000
*     IF DIGIT=','|                 /* IS THIS CHARACTER A COMMA     */ 00369000
*         DIGIT=' ' THEN                                           0352 00370000

*                           RFY                                 03S0532 00391000
*                             RTNCODE UNRSTD;                   03S0532 00392000
*                           RFY                                 03S0533 00393000
*                             IEDPRUQD BASED(UNITSAVE);         03S0533 00394000
*                           RFY                                 03S0534 00395000
*                             TICFLD BASED(ADDR(PRUTIC));       03S0534 00396000
*                                                               03S0534 00397000
*                           /*****************************************/ 00398000
*                           /*                                       */ 00399000
*                           /* IF THE NUMBER OF DIAL DIGITS IN THE   */ 00400000
*                           /* TTE IS TOO LARGE FOR CURRENT RU, GET  */ 00401000
*                           /* ANOTHER UNIT.                         */ 00402000
*                           /*                                       */ 00403000
*                           /*****************************************/ 00404000
*                                                               03S0535 00405000
*                           IF TIC^=VALTIC THEN/* IF START OF DIAL      00406000
*                                      DATA IS IN THE LAST UNIT      */ 00407000
         L     @05,UNITSAVE                                     03S0535 00408000
         TM    TIC+8(@05),B'00000011'                           03S0535 00409000
         BZ    @RF00535                                         03S0535 00410000
*                             IF(DATASAVE-UNITSAVE)+(COUNT+1)+(TTEDIALN 00411000
*                                 -ACTUAL)>(AVTKEYLE+12) THEN/* IS      00412000
*                                      ANOTHER UNIT NEEDED           */ 00413000
         LCR   @05,@05                                          03S0536 00414000
         AL    @05,DATASAVE                                     03S0536 00415000
         LA    @15,1                                            03S0536 00416000
         LA    @14,DIALSP                                       03S0536 00417000
         SLR   @07,@07                                          03S0536 00418000
         IC    @07,COUNT(,@14)                                  03S0536 00419000
         ALR   @07,@15                                          03S0536 00420000

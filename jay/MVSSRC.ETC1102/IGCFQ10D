         B     @RC00519                                         03S0521 00344000
@RF00519 LA    @14,DIALSP                                       03S0521 00345000
         CLC   TTEDIALN(1,DEVDPADR),ACTUAL(@14)                 03S0521 00346000
         BNE   @RF00521                                         03S0521 00347000
*                         DO;       /* CONVERT NCP'S DIGITS TO BINARY   00348000
*                                      AND COMPARE WITH TCAM'S  03S0522 00349000
*                                                            @YM06517*/ 00350000
*                           DIGITS(1:1)=DIGITS(1:1)&CLEARHI;/*  03S0523 00351000
*                                      CONVERT FIRST BYTE    @YM06517*/ 00352000
         NI    DIGITS(@14),X'0F'                                03S0523 00353000

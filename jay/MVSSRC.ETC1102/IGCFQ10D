*                           ELSE                                03S0529 00381000
*                             BLRPLFLG=ON;/* SET FLAG TO BUILD  03S0529 00382000
*                                      REPLACE                       */ 00383000
         B     @RC00527                                         03S0529 00384000
@RF00527 OI    BLRPLFLG,B'01000000'                             03S0529 00385000
*                         END;      /*                       @YM06517*/ 00386000
*                       ELSE                                    03S0531 00387000
*                         DO;                                   03S0531 00388000
         B     @RC00521                                         03S0531 00389000

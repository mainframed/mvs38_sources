         AH    @05,@CH00071                                     04S0496 00194000
         ST    @05,PLMVSD(,@14)                                 04S0496 00195000
*                 PLMVTD=ADDR(PRUDATA)+LENGTH(IEDBDU);/* TARGET DATA    00196000
*                                      ADDR.                         */ 00197000
         ST    @05,PLMVTD(,@14)                                 04S0497 00198000
*                 PLMVLNTH=DSIPOLNM+LENGTH(IEDDSI1);/* NUMBER OF        00199000

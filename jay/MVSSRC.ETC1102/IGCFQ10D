*   /*                                                               */ 01096000
*   /*****************************************************************/ 01097000
*                                                               01S0708 01098000
*                                                               01S0708 01099000
*/********************************************************************/ 01100000
*/*              REDRIVE SUBROUTINE                                  */ 01101000
*/********************************************************************/ 01102000
*/*                                                                  */ 01103000
*/* THIS ROUTINE PERFORMS THE REDRIVE PROCESSING:                    */ 01104000
*/* 1. THE RVT IS SCANNED STARTING WITH THE ENTRY ADDR. (ENTRYRVT)   */ 01105000
*/*    CONTAINED IN OCMODNME OF THE REDRIVE OPCE. ON THE FIRST PASS, */ 01106000
*/*    THIS IS SET UP WITH THE 2ND ENTRY IN THE RVT.                 */ 01107000
*/* 2. USING RVTINDEX, THE TTE IS OBTAINED AND THE TERMINAL IS       */ 01108000
*/*    VERIFIED AS A BSC-SS DIAL TERM. WITH AUTOCALL SPECIFIED.      */ 01109000
*/* 3. IF SO, A COPY DEVICE SESSION INFORMATION OPCE IS BUILT IN     */ 01110000
*/*    THE UNIT POINTED TO BY OCUNIT OF THE REDRIVE OPCE. THE NEXT   */ 01111000
*/*    UNIT HAS A PACKED RU BUILT IN IT. OCUNIT OF THE REDRIVE OPCE  */ 01112000
*/*    IS SET TO THE UNIT FOLLOWING THE SECOND UNIT.                 */ 01113000
*/* 4. THE OPCE=RU PAIR JUST BUILT IS PUT INTO THE CHAIN FOR THE     */ 01114000
*/*    POST TO AVTOPCOB.                                             */ 01115000
*/* 5. IF THERE ARE 2 OR MORE UNITS LEFT, THE RVT SCAN IS CONTINED   */ 01116000
*/*    AND STEPS 2-4 REPEATED.                                       */ 01117000
*/*                                                                  */ 01118000
*/********************************************************************/ 01119000
*                                                               01S0708 01120000
*REDRIVE:                                                       01S0708 01121000
*   PROC OPTIONS(RETREG,SAVE(14));                              01S0708 01122000
REDRIVE  ST    @14,@SA00002                                     01S0708 01123000
*   IF OCBUFGOT>=2 THEN             /* IF TWO OR MORE UNITS ATTACHED */ 01124000
         CLC   OCBUFGOT(2,ROPCE),@CH00131                       01S0709 01125000
         BL    @RF00709                                         01S0709 01126000
*     DO;                                                       01S0710 01127000
*                                                               01S0710 01128000
*       /*************************************************************/ 01129000
*       /*                                                           */ 01130000
*       /* THERE ARE ENOUGH UNITS                                    */ 01131000
*       /*                                                           */ 01132000
*       /*************************************************************/ 01133000
*                                                               01S0711 01134000
*       OPCAQWD1=OCUNIT;            /* SET FIRST WORD OF AQCTL PARM.    01135000
*                                      LIST TO POINT TO FIRST UNIT   */ 01136000
         MVC   OPCAQWD1(4,ROPCAVT),OCUNIT(ROPCE)                01S0711 01137000
*       DO WHILE(OCBUFGOT>=2);      /* LOOP WHILE ENOUGH UNITS REMAIN*/ 01138000
         B     @DE00712                                         01S0712 01139000
@DL00712 DS    0H                                               01S0713 01140000
*         RFY                                                   01S0713 01141000
*           IEDQOPCE BASED(UNITPTR);/* REDEFINE BASE FOR LOOP        */ 01142000
*         UNITPTR=ROPCE->OCUNIT;    /* POINT TO FIRST UNIT           */ 01143000
         L     UNITPTR,OCUNIT(,ROPCE)                           01S0714 01144000
*         ROPCE->OCUNIT=OCUNIT->OCUNIT;/* SET OCUNIT OF REDRIVE TO      01145000
*                                      POINT TO UNIT FOLLOWING UNIT     01146000
*                                      FOLLOWING UNIT POINTED TO BY     01147000
*                                      UNITPTR                       */ 01148000
         L     @14,OCUNIT(,UNITPTR)                             01S0715 01149000
         L     @11,OCUNIT(,@14)                                 01S0715 01150000
         ST    @11,OCUNIT(,ROPCE)                               01S0715 01151000
*         OCELEM=ROPCE->OCUNIT;     /* MOVE LINK FOR POST TO AVTOPCOB*/ 01152000
         ST    @11,OCELEM(,UNITPTR)                             01S0716 01153000
*         PRUTIC=PRUINTIC;          /* SET TIC FIELD OF RU INVALID   */ 01154000
*                                                               01S0717 01155000
         MVC   PRUTIC(4,@14),@CB01345                           01S0717 01156000
*         /***********************************************************/ 01157000
*         /*                                                         */ 01158000
*         /* SET COMMON FIELDS IN OPCE                               */ 01159000
*         /*                                                         */ 01160000
*         /***********************************************************/ 01161000
*                                                               01S0718 01162000
*         OCPTRSAV=ROPCE->OCPTRSAV; /* SET ADDR. OF ORIG. OPCE       */ 01163000
         MVC   OCPTRSAV(4,UNITPTR),OCPTRSAV(ROPCE)              01S0718 01164000
*         OCSWITCH=OCOPYDSI;        /* SET COMMAND CODE              */ 01165000
         MVI   OCSWITCH(UNITPTR),X'E3'                          01S0719 01166000
*         OCLINK=ADDR(AVTOPCOB);    /* SET QCB ADDR.                 */ 01167000
         LA    @14,AVTOPCOB(,RAVT)                              01S0720 01168000
         ST    @14,OCLINK(,UNITPTR)                             01S0720 01169000
*         OCELMPRI=PRIMEDFG;        /* SET UP POST PRIORITY          */ 01170000
         MVI   OCELMPRI(UNITPTR),X'DF'                          01S0721 01171000
*         OCPRIFLG=OPR02;           /* SET TO ROUTE TO RU DISPATCHER */ 01172000
         MVI   OCPRIFLG(UNITPTR),X'08'                          01S0722 01173000
*         OCWTG=THISMOD;            /* SET TO RETURN TO THIS MOD.    */ 01174000
         MVC   OCWTG(2,UNITPTR),THISMOD                         01S0723 01175000
*         OCACTREQ=ON;              /* ACTIVATE REQUEST              */ 01176000
         OI    OCACTREQ(UNITPTR),B'00001000'                    01S0724 01177000
*         OPCAQWD2=UNITPTR;         /* 'END OF CHAIN' ADDR. IN AQCTL    01178000
*                                      PARM. LIST                    */ 01179000
*                                                               01S0725 01180000
         ST    UNITPTR,OPCAQWD2(,ROPCAVT)                       01S0725 01181000
*         /***********************************************************/ 01182000
*         /*                                                         */ 01183000
*         /* SCAN RVT UNTIL THE TTE OF A DIAL TERM. WITH AUTOCALL AND*/ 01184000
*         /* VALID DIAL DIGITS IS FOUND. FINISH BUILDING OPCE AND    */ 01185000
*         /* PACKED RU.                                              */ 01186000
*         /*                                                         */ 01187000
*         /***********************************************************/ 01188000
*                                                               01S0726 01189000
*                                                               02S0726 01190000
*         /***********************************************************/ 01191000
*         /*                                                         */ 01192000
*         /* ------- THIS SEGMENT BUILDS "COPY DEVICE SESSION        */ 01193000
*         /* INFORMATION" OPCE- PACKED RU PAIRS. -----               */ 01194000
*         /*                                                         */ 01195000
*         /***********************************************************/ 01196000
*                                                               02S0726 01197000
*         RFY                                                   02S0726 01198000
*           IEDQTRM BASED(RPARM);   /* SET TTE BASE                  */ 01199000
*         RFY                                                   02S0727 01200000
*           IEDQOPCE BASED(ROPCE);  /* SET BASE AS REDRIVE OPCE      */ 01201000
*         DO UNTIL(BUILTFLG=ON);    /* DO UNTIL CMND. BUILT          */ 01202000
*                                                               02S0728 01203000
@DL00728 DS    0H                                               02S0729 01204000
*           /*********************************************************/ 01205000
*           /*                                                       */ 01206000
*           /* OBTAIN NEXT RVT ENTRY ADDR. FROM REDRIVE OPCE AND SET */ 01207000
*           /* OCTRMTBL OF OPCE BEING BUILT TO TTCIN CONTAINED IN RVT*/ 01208000
*           /* ENTRY.                                                */ 01209000
*           /*                                                       */ 01210000
*           /*********************************************************/ 01211000
*                                                               02S0729 01212000
*           UNITPTR->OCTRMTBL=RVTINDEX;/* SET TTCIN FOR OPCE BEING      01213000
*                                      BUILT                         */ 01214000
         L     @14,ENTRYRVT(,ROPCE)                             02S0729 01215000
         LH    @11,RVTINDEX(,@14)                               02S0729 01216000
         STH   @11,OCTRMTBL(,UNITPTR)                           02S0729 01217000
*           ENTRYRVT=ENTRYRVT+LENGTH(RVTINDEX);/* UPDATE RVT ENTRY      01218000
*                                      PTR. IN R/D OPCE TO POINT TO     01219000
*                                      NEXT ENTRY                    */ 01220000
         AH    @14,@CH00131                                     02S0730 01221000
         ST    @14,ENTRYRVT(,ROPCE)                             02S0730 01222000
*           RPARM=UNITPTR->OCTRMTBL;/* OBTAIN ADDR. OF ....          */ 01223000
         LR    RPARM,@11                                        02S0731 01224000
*           CALL IEDQTNT;           /* .... TTE                      */ 01225000
         L     @15,AVTRNMPT(,RAVT)                              02S0732 01226000
         BALR  @14,@15                                          02S0732 01227000
*           IF TRMTYPE=TRMPSNA&TRMAUTOC=ON&TRMDDIG=ON THEN/* IF 02S0733 01228000
*                                      PRE-SNA TERM. WITH AUTOCALL      01229000
*                                      AND DIAL DIGITS               */ 01230000
         LR    @14,RPARM                                        02S0733 01231000
         SH    @14,@CH01700                                     02S0733 01232000
         CLI   TRMTYPE(@14),X'4F'                               02S0733 01233000
         BNE   @RF00733                                         02S0733 01234000
         TM    TRMAUTOC(@14),B'00100000'                        02S0733 01235000
         BNO   @RF00733                                         02S0733 01236000
         TM    TRMDDIG(RPARM),B'01000000'                       02S0733 01237000
         BNO   @RF00733                                         02S0733 01238000
*             DO;                                               02S0734 01239000
*                                                               02S0734 01240000
*               /*****************************************************/ 01241000
*               /*                                                   */ 01242000
*               /* BUILD COPY DEVICE SESSION INFO. CMND. IF THIS     */ 01243000
*               /* TERMINAL HAS VALID DIAL DIGITS.                   */ 01244000
*               /*                                                   */ 01245000
*               /*****************************************************/ 01246000
*                                                               02S0735 01247000
*               RZERO=TRMDDIGA;     /* OBTAIN ADDR. OF ....          */ 01248000
         LH    RZERO,@CH01535                                   02S0735 01249000
*               CALL IEDQTL;        /* .... DEVICE DEPENDENT FIELD   */ 01250000
         L     @15,AVTDDFT(,RAVT)                               02S0736 01251000
         BALR  @14,@15                                          02S0736 01252000
*               DEVDPADR=RTNCODE;   /* COPY ADDR. OF FIELD           */ 01253000
         LR    DEVDPADR,RTNCODE                                 02S0737 01254000
*               IF TTEDIALN=1&TTEDIALD(1:1)='0F'X THEN/* IF INVALID  */ 01255000
         CLI   TTEDIALN(DEVDPADR),1                             02S0738 01256000
         BNE   @RF00738                                         02S0738 01257000
         CLI   TTEDIALD(DEVDPADR),X'0F'                         02S0738 01258000
         BNE   @RF00738                                         02S0738 01259000
*                 BUILTFLG=OFF;     /* CMND. NOT BUILT FOR THIS TERM.   01260000
*                                      - CHECK NEXT RVT ENTRY        */ 01261000
         NI    BUILTFLG,B'11101111'                             02S0739 01262000
*               ELSE                                            02S0740 01263000
*                 DO;                                           02S0740 01264000
*                                                               02S0740 01265000
         B     @RC00738                                         02S0740 01266000
@RF00738 DS    0H                                               02S0741 01267000
*                   /*************************************************/ 01268000
*                   /*                                               */ 01269000
*                   /* BUILD PACKED RU                               */ 01270000
*                   /*                                               */ 01271000
*                   /*************************************************/ 01272000
*                                                               02S0741 01273000
*                   RFY                                         02S0741 01274000
*                     IEDQOPCE BASED(UNITPTR);/* RESET BASE AS OPCE     01275000
*                                      BEING BUILT                   */ 01276000
*                   PRUTTCIN=OCTRMTBL;/* SET RESOURCE TTCIN          */ 01277000
         L     @14,OCUNIT(,UNITPTR)                             02S0742 01278000
         MVC   PRUTTCIN(2,@14),OCTRMTBL(UNITPTR)                02S0742 01279000
*                   PRUDATCT=LENGTH(IEDBDU);/* DATA LENGTH IS BDU       01280000
*                                      LENGTH                        */ 01281000
         MVC   PRUDATCT(2,@14),@CH00071                         02S0743 01282000
*                   BDUCMND=CD0CTRL;/* CONTROL BDU                   */ 01283000
         LA    @11,12                                           02S0744 01284000
         ALR   @11,@14                                          02S0744 01285000
         MVI   BDUCMND(@11),X'08'                               02S0744 01286000
*                   BDUMODIF=CD0CCDSI;/* COPY DSI MODIFIER           */ 01287000
         MVI   BDUMODIF(@11),X'21'                              02S0745 01288000
*                   BDUHEADR=ON;    /* TURN ON HEADER FLAG   @YM06147*/ 01289000
         OI    BDUHEADR(@11),B'01000000'                        02S0746 01290000
*                   PRURHTH(1:3)=TRHFID0;/* SET FID0 RH FLAGS        */ 01291000
         SH    @14,@CH00045                                     02S0747 01292000
         MVC   PRURHTH(3,@14),@CB01378                          02S0747 01293000
*                   PRURHTH(4:4)=TTH0BYT0;/* SET FID0 TH FLAGS       */ 01294000
         MVI   PRURHTH+3(@14),X'0C'                             02S0748 01295000
*                   BUILTFLG=ON;    /* RU BUILT, RETURN TO REDRIVE   */ 01296000
         OI    BUILTFLG,B'00010000'                             02S0749 01297000
*                 END;                                          02S0750 01298000
*             END;                                              02S0751 01299000
*           ELSE                                                02S0752 01300000
*             BUILTFLG=OFF;         /* CMND. NOT BUILT FOR THIS TERM.   01301000
*                                      - CHECK NEXT RVT ENTRY        */ 01302000
         B     @RC00733                                         02S0752 01303000
@RF00733 NI    BUILTFLG,B'11101111'                             02S0752 01304000
*         END;                                                  02S0753 01305000
@RC00733 DS    0H                                               02S0753 01306000
@DE00728 TM    BUILTFLG,B'00010000'                             02S0753 01307000
         BNO   @DL00728                                         02S0753 01308000
*         RFY                                                   02S0754 01309000
*           IEDQTRM BASED(RTRM);    /* RESET BASE FOR TTE            */ 01310000
*         ROPCE->OCBUFGOT=ROPCE->OCBUFGOT-2;/* DECREMENT # UNITS GOT */ 01311000
         SLR   @14,@14                                          01S0755 01312000
         ICM   @14,3,OCBUFGOT(ROPCE)                            01S0755 01313000
         BCTR  @14,0                                            01S0755 01314000
         BCTR  @14,0                                            01S0755 01315000
         STH   @14,OCBUFGOT(,ROPCE)                             01S0755 01316000
*         RFY                                                   01S0756 01317000
*           IEDQOPCE BASED(ROPCE);  /* RESET BASE AS REDRIVE OPCE    */ 01318000
*       END;                                                    01S0757 01319000
*                                                               01S0757 01320000
@DE00712 CLC   OCBUFGOT(2,ROPCE),@CH00131                       01S0757 01321000
         BNL   @DL00712                                         01S0757 01322000
*       /*************************************************************/ 01323000
*       /*                                                           */ 01324000
*       /* POST OPCE'S TO AVTOPCOB                                   */ 01325000
*       /*                                                           */ 01326000
*       /*************************************************************/ 01327000
*                                                               01S0758 01328000
*       OPCAQREQ=CHAINFLG;          /* FINISH BUILDING PARM. LIST BY    01329000
*                                      SETTING FLAG FOR CHAIN POST   */ 01330000
         MVI   OPCAQREQ(ROPCAVT),X'B4'                          01S0758 01331000
*       RPARM=ADDR(OPCAQCTL);       /* SET REG 1 TO POINT TO PARM.      01332000
*                                      LIST                          */ 01333000
         LA    RPARM,OPCAQCTL(,ROPCAVT)                         01S0759 01334000
*       SVC(102);                   /* POST IT                       */ 01335000
         SVC   102                                              01S0760 01336000
*     END;                          /* END OF REDRIVE PROCESSING     */ 01337000
*   ELSE                                                        01S0762 01338000
*     ;                             /* NOT ENOUGH UNITS, DON'T DO       01339000
*                                      REDRIVE                       */ 01340000
*                                                               01S0762 01341000
@RF00709 DS    0H                                               01S0763 01342000
*   /*****************************************************************/ 01343000
*   /*                                                               */ 01344000
*   /* IF STILL MORE OPCE - RU'S TO BUILD, PLACE REDRIVE ELEMENT BACK*/ 01345000
*   /* ON WAIT CHAIN                                                 */ 01346000
*   /*                                                               */ 01347000
*   /*****************************************************************/ 01348000
*                                                               01S0763 01349000
*   IF OCBUFNED=0 THEN              /* IF PROCESSING DONE            */ 01350000
         ICM   @14,3,OCBUFNED(ROPCE)                            01S0763 01351000
         BNZ   @RF00763                                         01S0763 01352000
*     DO;                                                       01S0764 01353000
*       RPARM=ROPCE;                /* FREE REDRIVE ....             */ 01354000
         LR    RPARM,ROPCE                                      01S0765 01355000
*       CALL FREEBUFF;              /* .... OPCE                     */ 01356000
         L     @15,OPCFREBF(,ROPCAVT)                           01S0766 01357000
         BALR  @14,@15                                          01S0766 01358000
*       RZERO=1;                    /* DECREMENT OPCADGOT AND ....   */ 01359000
         LA    RZERO,1                                          01S0767 01360000
*       CALL DECRCT;                /* .... INITIATE REDRIVE    01S0768 01361000
*                                      PROCESSING FOR ANOTHER REDRIVE   01362000
*                                      OPCE                          */ 01363000
         L     @15,OPCDECR(,ROPCAVT)                            01S0768 01364000
         BALR  @14,@15                                          01S0768 01365000
*     END;                                                      01S0769 01366000
*   ELSE                            /* PROCESSING NOT FINISHED       */ 01367000
*     DO;                                                       01S0770 01368000
         B     @RC00763                                         01S0770 01369000
@RF00763 DS    0H                                               01S0771 01370000
*       OCWTG=THISMOD;              /* SET TO RETURN                 */ 01371000
         MVC   OCWTG(2,ROPCE),THISMOD                           01S0771 01372000
*       OCLINK=OPCBUFWT;            /* PLACE REDRIVE OPCE ....       */ 01373000
         MVC   OCLINK(4,ROPCE),OPCBUFWT(ROPCAVT)                01S0772 01374000
*       OPCBUFWT=ROPCE;             /* .... BACK ON WAIT CHAIN       */ 01375000
         ST    ROPCE,OPCBUFWT(,ROPCAVT)                         01S0773 01376000
*     END;                                                      01S0774 01377000
*                                                               01S0774 01378000
*   /*****************************************************************/ 01379000
*   /*                                                               */ 01380000
*   /* SET TO XCTL TO THE CONTROL MOD. - IGC0010D                    */ 01381000
*   /*                                                               */ 01382000
*   /*****************************************************************/ 01383000
*                                                               01S0775 01384000
*   OPCMODID=CNTLMOD;                                           01S0775 01385000
@RC00763 MVC   OPCMODID(2,ROPCAVT),CNTLMOD                      01S0775 01386000
*   END REDRIVE;                                                01S0776 01387000
@EL00002 DS    0H                                               01S0776 01388000
@EF00002 DS    0H                                               01S0776 01389000
@ER00002 L     @14,@SA00002                                     01S0776 01390000
         BR    @14                                              01S0776 01391000
*   END IGCFQ10D                                                01S0777 01392000
*                                                               01S0777 01393000

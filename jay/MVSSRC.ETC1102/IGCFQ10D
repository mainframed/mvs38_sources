         EX    @05,@SC01713                                     03S0527 00376000
         BNE   @RF00527                                         03S0527 00377000
*                             FREUTFLG=ON;/* REPLACE DOES NOT NEED TO   00378000
*                                      BE DONE - FREE OPCE AND RU    */ 00379000

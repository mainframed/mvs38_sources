         L     @15,SAVTMVBF(,@05)                               04S0514 00311000
         BALR  @14,@15                                          04S0514 00312000
*                     RFY                                       04S0515 00313000
*                       IEDDSI1 BASED(ADDR(PRUDATA)+LENGTH(IEDBDU));    00314000
*                                   /* RESET BASE FOR START OF DATA     00315000
*                                      RECORD                        */ 00316000
*                   END;            /*                       @YM07430*/ 00317000
*                                   /* ... WORK AREA FOR EASY   03S0517 00318000
*                                      HANDLING ADDR. OF DIAL FIELD     00319000
*                                      IN RU PLACED IN RUDIALAD      */ 00320000
*                 IF FREUTFLG=OFF THEN/*                     @YM07670*/ 00321000

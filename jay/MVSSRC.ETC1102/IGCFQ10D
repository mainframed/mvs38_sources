         CLC   TTEDIALN(1,DEVDPADR),MAXIMUM(@14)                03S0519 00328000
         BNH   @RF00519                                         03S0519 00329000
*                       FREUTFLG=ON;/* SET TO FREE OPCE AND RU       */ 00330000
         OI    FREUTFLG,B'10000000'                             03S0520 00331000
*                     ELSE                                      03S0521 00332000
*                                                               03S0521 00333000
*                       /*********************************************/ 00334000
*                       /*                                           */ 00335000
*                       /* REPLACE CAN BE DONE IF DIAL DIGITS DO NOT */ 00336000
*                       /* MATCH.                                    */ 00337000
*                       /*                                           */ 00338000
*                       /*********************************************/ 00339000
*                                                               03S0521 00340000
*                       IF TTEDIALN=ACTUAL THEN/* IF NUMBER OF TTE      00341000
*                                      DIAL DIGITS IS SAME AS NO. OF    00342000

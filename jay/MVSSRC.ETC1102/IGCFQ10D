*                 /* USED TO FIND ADDR. OF DIAL DATA BY MOVING BYTES */ 00178000
*                 /* INTO THEMSELVES THUS MOVING POINTER.            */ 00179000
*                 /*                                                 */ 00180000
*                 /***************************************************/ 00181000
*                                                               04S0493 00182000
*                 IEDPLMV=''B;      /* CLEAR PARM. LIST AREA         */ 00183000
         LA    @14,PARMLSP                                      04S0493 00184000
         XC    IEDPLMV(24,@14),IEDPLMV(@14)                     04S0493 00185000
*                 PLMVSU=OCUNIT;    /* SOURCE UNIT ADDR.             */ 00186000
         L     @05,OCUNIT(,ROPCE)                               04S0494 00187000
         ST    @05,PLMVSU(,@14)                                 04S0494 00188000
*                 PLMVTU=OCUNIT;    /* TARGET UNIT ADDR.             */ 00189000
         ST    @05,PLMVTU(,@14)                                 04S0495 00190000
*                 PLMVSD=ADDR(PRUDATA)+LENGTH(IEDBDU);/* SOURCE DATA    00191000
*                                      ADDR.                         */ 00192000

*   RWORK7=OPCWAITV;                /* GET 1ST ELEM ON CHAIN         */ 00787000
         L     RWORK7,OPCWAITV(,ROPCAVT)                           0776 00788000
*   RWORK7=RWORK7&MASKBYTE;         /* CLEAR HIGH ORDER BY   @YM06021*/ 00789000
         N     RWORK7,@CF02517                                     0777 00790000
*   DO WHILE(RWORK7^=0&RWORK7->OCBACKUP^=RPARM->GRPNAME);/* SEARCH 0778 00791000

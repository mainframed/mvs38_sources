*                                                                  0307 00371000
*/********************************************************************/ 00372000
*/* GET A BUFFER UNIT INTO WHICH A BTU WILL BE BUILT                 */ 00373000
*/********************************************************************/ 00374000
*                                                                  0307 00375000
*   RPARM=1;                        /* GET ONE BUFFER UNIT           */ 00376000
@RC00272 LA    RPARM,1                                             0307 00377000
*   RBRANCH=OPCGETBF;               /* ADDR OF BUFFER ALLOCATION     */ 00378000
         L     RBRANCH,OPCGETBF(,ROPCAVT)                          0308 00379000
*   GEN( BALR RRETURN,RBRANCH );    /* CALL ALLOCATION ROUTINE       */ 00380000
          BALR RRETURN,RBRANCH                                          00381000
*   OCUNIT=RPARM;                   /* SAVE ADDR OF UNIT             */ 00382000
         ST    RPARM,OCUNIT(,ROPCE)                                0310 00383000
*/********************************************************************/ 00384000
*/* BUILD THE PRU                                            @Y17XARX*/ 00385000
*/********************************************************************/ 00386000
*                                                                  0311 00387000
*   PRURHTH=FID1PRUH;               /* SET UP FID1 RH/TH     @Y17XARX*/ 00388000
         L     @15,OCUNIT(,ROPCE)                                  0311 00389000
         LR    @14,@15                                             0311 00390000
         AL    @14,@CF01293                                        0311 00391000
         MVC   PRURHTH(4,@14),FID1PRUH                             0311 00392000

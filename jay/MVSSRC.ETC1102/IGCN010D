         LR    @01,IEDQTPTR                                        0258 00235000
         AL    @01,@CF00992                                        0258 00236000
         CLI   TRMTYPE-11(@01),X'20'                               0258 00237000
         BE    @RF00258                                            0258 00238000
@RT00258 DS    0H                                                  0259 00239000
*     DO;                           /* YES, INVALID ENTRY            */ 00240000
*       RMSGNUM=443;                /* SET 'NOT A 3705 LINE' MSG     */ 00241000
         LA    RMSGNUM,443                                         0260 00242000
*       GOTO ERREXIT;               /* WRITE THE MESSAGE             */ 00243000
         B     ERREXIT                                             0261 00244000
*     END;                                                         0262 00245000
*                                                                  0262 00246000
*/********************************************************************/ 00247000
*/* DETERMINE THAT LINE IS INACTIVE                                  */ 00248000
*/********************************************************************/ 00249000
*                                                                  0263 00250000
*   IF TRMRSACT='1'B THEN           /* IS LINE ACTIVE        @Y17XARX*/ 00251000
@RF00258 LR    @01,IEDQTPTR                                        0263 00252000
         AL    @01,@CF01289                                        0263 00253000
         TM    TRMRSACT-2(@01),B'10000000'                         0263 00254000
         BNO   @RF00263                                            0263 00255000
*     DO;                           /* YES, DO                       */ 00256000
*       RMSGNUM=446;                /* SET 'LINE ACTIVE' MSG         */ 00257000
         LA    RMSGNUM,446                                         0265 00258000
*       GOTO ERREXIT;               /* WRITE THE MESSAGE             */ 00259000
         B     ERREXIT                                             0266 00260000
*     END;                                                         0267 00261000
*   RESPECIFY                                                      0268 00262000
*     RZERO RSTD;                   /*                       @Y17XARX*/ 00263000
@RF00263 DS    0H                                                  0269 00264000
*   RESPECIFY                                                      0269 00265000
*     RPARM RSTD;                   /*                       @Y17XARX*/ 00266000
*   ERRFLAG='0'B;                   /* RESET ERROR FLAG      @Y17XARX*/ 00267000
         NI    ERRFLAG(ROPCAVT),B'01111111'                        0270 00268000
*   RZERO=TRMSUBST;                 /* GET TTCIN OF FIRST TERM     0271 00269000
*                                                            @Y17XARX*/ 00270000
         LR    @15,IEDQTPTR                                        0271 00271000
         BCTR  @15,0                                               0271 00272000
         BCTR  @15,0                                               0271 00273000
         LH    @15,TRMSUBST-12(,@15)                               0271 00274000
         N     @15,@CF01291                                        0271 00275000
         LR    RZERO,@15                                           0271 00276000
*   IF TRMSUBST^=0 THEN             /* HEIRAIRCHY PRESENT?   @Y17XARR*/ 00277000

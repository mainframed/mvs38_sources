         TITLE 'IGCN010D, 3705-SWITCH LINE MODE                        *00001000
                        '                                               00002000
IGCN010D CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IGCN010D  77.215'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  BALR  @12,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@12                                         0001 00012000
*                                                                  0200 00013000
*/********************************************************************/ 00014000
*/* INITIALIZE CONTROL BLOCK REGISTERS.                              */ 00015000
*/********************************************************************/ 00016000
*                                                                  0200 00017000
*   ROPCAVT=RPARM;                  /* SET BASE FOR OP CTL AVT       */ 00018000
         LR    ROPCAVT,RPARM                                       0200 00019000
*   IEDQAPTR=OPCAVTPT;              /* SET BASE FOR TCAM AVT         */ 00020000
         L     IEDQAPTR,OPCAVTPT(,ROPCAVT)                         0201 00021000
*   ROPCE=OPCCOPCE;                 /* PICK UP ELEMENT POINTER       */ 00022000
         L     ROPCE,OPCCOPCE(,ROPCAVT)                            0202 00023000
*   RSAVE=ADDR(OPCSAVE);            /* GET SAVE AREA ADDRESS         */ 00024000
         LA    RSAVE,OPCSAVE(,ROPCAVT)                             0203 00025000
*   IF OPCRSTRT='1'B THEN           /* RESTART IN PROGRESS           */ 00026000
         TM    OPCRSTRT(ROPCAVT),B'00100000'                       0204 00027000
         BNO   @RF00204                                            0204 00028000
*     DO;                           /* YES                           */ 00029000
*       OPCDOUBL=OCBACKUP;          /* RESET GRPNAME                 */ 00030000
         MVC   OPCDOUBL(8,ROPCAVT),OCBACKUP(ROPCE)                 0206 00031000
*       RPARM=OCRLN;                /* RESET RLN                     */ 00032000
         SLR   RPARM,RPARM                                         0207 00033000
         IC    RPARM,OCRLN(,ROPCE)                                 0207 00034000
*       GOTO RESTART;               /* GET LINE ENTRY ADDRESS        */ 00035000
         B     RESTART                                             0208 00036000
*     END;                                                         0209 00037000
*                                                                  0209 00038000
*/********************************************************************/ 00039000
*/* CHECK FOR RESPONSE BTU:                                          */ 00040000
*/*                OCRESP=OFF - ELEMENT IS ASSOCIATED WITH           */ 00041000
*/*                             INITIAL SWITCH LINE MODE             */ 00042000
*/*                             COMMAND WHICH NEEDS OPERAND          */ 00043000
*/*                             CHECKING AND A BTU SENT.             */ 00044000
*/*                OCRESP=ON  - ELEMENT IS ASSOCIATED WITH A         */ 00045000
*/*                             RESPONSE BTU SENT ON A PREVIOUS      */ 00046000
*/*                             ENTRY INTO THIS MODULE.              */ 00047000
*/********************************************************************/ 00048000
*                                                                  0210 00049000
*   IF OCRESP='1'B THEN             /* DOES ELEMENT POINT TO A     0210 00050000
*                                      RESPONSE BTU ?                */ 00051000
@RF00204 TM    OCRESP(ROPCE),B'00000001'                           0210 00052000
         BO    @RT00210                                            0210 00053000
*     GOTO PROCBTU;                 /* YES, PROCESS THE BTU          */ 00054000
*   OCMODNME=BLANKS;                /* CLEAR OCMODNME TO BLANKS      */ 00055000
         MVC   OCMODNME(8,ROPCE),BLANKS                            0212 00056000
*                                                                  0213 00057000
*/********************************************************************/ 00058000
*/* SCAN THE FIRST OPERAND FOR VALID NUMERIC OF ONE TO THREE DIGITS. */ 00059000
*/* IF THE OPERAND IS A VALID NUMERIC, PUT THE RLN INTO RPARM AND    */ 00060000
*/* CALL THE 'GRPNAME,RLN' CODE TO FIND THE LINE ENTRY. 'GRPNAME'    */ 00061000
*/* IS IN OPCDOUBL AT ENTRY.                                         */ 00062000
*/********************************************************************/ 00063000
*                                                                  0213 00064000
*   IF RLEN=0 THEN                  /* ANOTHER OPERAND ?             */ 00065000
         LTR   RLEN,RLEN                                           0213 00066000
         BZ    @RT00213                                            0213 00067000
*     GOTO CMDINV;                  /* NO, COMMAND INVALID           */ 00068000
*   OCBACKUP=OPCDOUBL;              /* SAVE GROUP NAME               */ 00069000
         MVC   OCBACKUP(8,ROPCE),OPCDOUBL(ROPCAVT)                 0215 00070000
*   CALL SCAN1;                     /* SCAN THE OPERAND              */ 00071000
         BAL   @14,SCAN1                                           0216 00072000
*   IF RBYSCN=0|                    /* WAS OPERAND SKIPPED OR        */ 00073000
*       RBYSCN>AVTHA3|              /* OPERAND LENGTH > 3 OR         */ 00074000
*       RLEN=0 THEN                 /* LAST OPERAND ?                */ 00075000
         SLR   @15,@15                                             0217 00076000
         CR    RBYSCN,@15                                          0217 00077000
         BE    @RT00217                                            0217 00078000
         CH    RBYSCN,AVTHA3(,IEDQAPTR)                            0217 00079000
         BH    @RT00217                                            0217 00080000
         CR    RLEN,@15                                            0217 00081000
         BE    @RT00217                                            0217 00082000
*     GOTO CMDINV;                  /* YES TO ANY OF ABOVE - COMMAND    00083000
*                                      INVALID                       */ 00084000
*   RBYSCN=RBYSCN-1;                /* GET NUMBER OF DIGITS LESS 1 0219 00085000
*                                      FOR EXECUTE INSTRUCTION       */ 00086000
         BCTR  RBYSCN,0                                            0219 00087000
*/********************************************************************/ 00088000
*/* CONVERT RLN FROM EBCDIC TO BINARY AND TEST FOR VALID RLN --      */ 00089000
*/*               0 < RLN =< 255                                     */ 00090000
*/********************************************************************/ 00091000
*                                                                  0220 00092000
*   GEN;                                                           0220 00093000
         EX    RBYSCN,MOVERLN           PUT RLN IN WORK AREA            00094000
         EX    RBYSCN,PACK              PACK RLN IN WORK AREA           00095000
         XC    OPCSAVE(EQU6,ROPCAVT),OPCSAVE(ROPCAVT)                   00096000
         CVB   RPARM,OPCSAVE(ROPCAVT)   CONVERT RLN TO BINARY           00097000
*   IF RPARM=0|                     /* RLN = 0 OR                    */ 00098000
*       RPARM>255 THEN              /* RLN GREATER THAN MAXIMUM ?    */ 00099000
         LTR   RPARM,RPARM                                         0221 00100000
         BZ    @RT00221                                            0221 00101000
         C     RPARM,@CF01239                                      0221 00102000
         BH    @RT00221                                            0221 00103000
*     GOTO CMDINV;                  /* YES, COMMAND INVALID          */ 00104000
*                                                                  0223 00105000
*/********************************************************************/ 00106000
*/* CALL IEDQCA AT ENTRY POINT FOR 'GRPNAME,RLN' LOOKUP. RETURN WILL */ 00107000
*/* BE THE FOLLOWING:                                                */ 00108000
*/*                     RRET+0 -- SUCCESSFUL                         */ 00109000
*/*                     RRET+4 -- UNSUCCESSFUL                       */ 00110000
*/* IF UNSUCCESSFUL, ONE OF THE FOLLOWING RETURN CODES WILL BE IN    */ 00111000
*/* RZERO:                                                           */ 00112000
*/*                  X'01' - LGB NOT OPEN (NOTOPEN)                  */ 00113000
*/*                  X'04' - NAME NOT FOUND (NONAME)                 */ 00114000
*/*                  X'08' - INVALID RLN (INVRLN)                    */ 00115000
*/*                  X'20' - NOT A 3705 LINE (NOT3705L)              */ 00116000
*/********************************************************************/ 00117000
*                                                                  0223 00118000
*RESTART:                                                          0223 00119000
*   CALL DCBLOCAT;                  /* FIND TERM ENTRY       @Y17XARX*/ 00120000
RESTART  L     @15,OPCDCBLK(,ROPCAVT)                              0223 00121000
         BALR  @14,@15                                             0223 00122000
*   GOTO SAVELINE;                  /* GOOD RETURN - SAVE LINE ENTRY    00123000
*                                      ADDRESS                       */ 00124000
         B     SAVELINE                                            0224 00125000
*DLABEL:                            /*                       @Y17XARX*/ 00126000
*   IF RZERO=1|                     /* LGB NOT OPEN OR               */ 00127000
*       RZERO=8 THEN                /* RLN IS INVALID ?              */ 00128000
DLABEL   C     RZERO,@CF00062                                      0225 00129000
         BE    @RT00225                                            0225 00130000
         C     RZERO,@CF00053                                      0225 00131000
         BNE   @RF00225                                            0225 00132000
@RT00225 DS    0H                                                  0226 00133000
*     RMSGNUM=17;                   /* YES, SET 'LINE NOT OPEN'    0226 00134000
*                                      MESSAGE                       */ 00135000
         LA    RMSGNUM,17                                          0226 00136000
*   ELSE                                                           0227 00137000
*     RMSGNUM=173;                  /* SET 'NOT A 3705 RESOURCE'   0227 00138000
*                                      MESSAGE                       */ 00139000
         B     @RC00225                                            0227 00140000
@RF00225 LA    RMSGNUM,173                                         0227 00141000
*   GOTO ERREXIT;                   /* WRITE THE MESSAGE             */ 00142000
         B     ERREXIT                                             0228 00143000
*SAVELINE:                                                         0229 00144000
*   OCELEM=RZERO;                   /* SAVE LINE ENTRY ADDRESS       */ 00145000
SAVELINE ST    RZERO,OCELEM(,ROPCE)                                0229 00146000
*   IF OPCRSTRT='1'B THEN           /* RESTART IN PROGRESS           */ 00147000
         TM    OPCRSTRT(ROPCAVT),B'00100000'                       0230 00148000
         BNO   @RF00230                                            0230 00149000
*     DO;                           /* YES                           */ 00150000
*       IEDQTPTR=OCELEM;            /* SET LINE ENTRY BASE           */ 00151000
         L     @15,OCELEM(,ROPCE)                                  0232 00152000
         LR    IEDQTPTR,@15                                        0232 00153000
*       GOTO RESTART1;              /* GO SET EP MODE BIT            */ 00154000
         B     RESTART1                                            0233 00155000
*     END;                                                         0234 00156000
*                                                                  0235 00157000
*/********************************************************************/ 00158000
*/* SCAN COMMAND FOR EP OR NCP OPERAND                               */ 00159000
*/*          'EP'  - INDICATES SWITCH TO EP MODE                     */ 00160000
*/*          'NCP' - INDICATES SWITCH TO NCP MODE                    */ 00161000
*/********************************************************************/ 00162000
*                                                                  0235 00163000
*   IF RLEN=0 THEN                  /* ANOTHER OPERAND               */ 00164000
@RF00230 LTR   RLEN,RLEN                                           0235 00165000
         BZ    @RT00235                                            0235 00166000
*     GOTO CMDINV;                  /* NO, COMMAND INVALID           */ 00167000
*   CALL SCAN;                      /* SCAN THE OPERAND              */ 00168000
         BAL   @14,SCAN                                            0237 00169000
*   IF RBYSCN>AVTHA3|               /* OPERAND LENGTH > 3 OR         */ 00170000
*       RBYSCN<AVTHA2|              /* OPERAND LENGTH . 2 OR         */ 00171000
*       LASTCHAR=','|               /* LAST CHAR A COMMA OR          */ 00172000
*       RLEN^=0 THEN                /* ANOTHER OPERAND               */ 00173000
         CH    RBYSCN,AVTHA3(,IEDQAPTR)                            0238 00174000
         BH    @RT00238                                            0238 00175000
         LH    @15,AVTHA2(,IEDQAPTR)                               0238 00176000
         CR    RBYSCN,@15                                          0238 00177000
         BL    @RT00238                                            0238 00178000
         LR    @01,RAPTR                                           0238 00179000
         BCTR  @01,0                                               0238 00180000
         CLI   LASTCHAR+1(@01),C','                                0238 00181000
         BE    @RT00238                                            0238 00182000
         LTR   RLEN,RLEN                                           0238 00183000
         BNZ   @RT00238                                            0238 00184000
*     GOTO CMDINV;                  /* YES TO ANY OF ABOVE - COMMAND    00185000
*                                      INVALID                       */ 00186000
*   IF RBYSCN=AVTHA2 THEN           /* OPERAND LENGTH OF 2           */ 00187000
         CR    RBYSCN,@15                                          0240 00188000
         BNE   @RF00240                                            0240 00189000
*     DO;                           /* YES, DO                       */ 00190000
*       IF OPEREP='EP' THEN         /* OPERAND = EP                  */ 00191000
         CLC   OPEREP(2,RRPTR),@CC01249                            0242 00192000
         BNE   @RF00242                                            0242 00193000
*         OCMODNME=EPMODE;          /* INDICATE EP MODE              */ 00194000
         MVC   OCMODNME(8,ROPCE),EPMODE                            0243 00195000
*       ELSE                        /*                               */ 00196000
*         GOTO CMDINV;              /* NO, COMMAND INVALID           */ 00197000
*     END;                          /*                               */ 00198000
*   ELSE                            /* OPERAND LENGTH OF 3           */ 00199000
*     IF OPERNCP='NCP' THEN         /* OPERAND = NCP                 */ 00200000
         B     @RC00240                                            0246 00201000
@RF00240 CLC   OPERNCP(3,RRPTR),@CC01250                           0246 00202000
         BNE   @RF00246                                            0246 00203000
*       DO;                         /* YES, DO                       */ 00204000
*         OCSWITCH='28'X;           /* INDICATE NCP SWITCH           */ 00205000
         MVI   OCSWITCH(ROPCE),X'28'                               0248 00206000
*         OCMODNME=NCPMODE;         /* INDICATE NCP MODE             */ 00207000
         MVC   OCMODNME(8,ROPCE),NCPMODE                           0249 00208000
*       END;                                                       0250 00209000
*     ELSE                                                         0251 00210000
*       GOTO CMDINV;                /* NO, COMMAND INVALID           */ 00211000
*                                                                  0252 00212000
*/********************************************************************/ 00213000
*/* CHECK LINE ENTRY TO DETERMINE THAT IT IS VALID LINE ENTRY        */ 00214000
*/* ATTACHED TO A 3705.                                              */ 00215000
*/********************************************************************/ 00216000
*                                                                  0252 00217000
*   IEDQTPTR=OCELEM;                /* SET LINE ENTRY BASE           */ 00218000
@RC00240 L     IEDQTPTR,OCELEM(,ROPCE)                             0252 00219000
*   IF TRMSTOTE='1'B THEN           /* TOTE CONTROLLING RESOURCE     */ 00220000
         LR    @01,IEDQTPTR                                        0253 00221000
         AL    @01,@CF01288                                        0253 00222000
         TM    TRMSTOTE-3(@01),B'00000001'                         0253 00223000
         BNO   @RF00253                                            0253 00224000
*     DO;                           /* YES                           */ 00225000
*       RMSGNUM=49;                 /* SET 'TOTE CONTROLLING' MSG    */ 00226000
         LA    RMSGNUM,49                                          0255 00227000
*       GOTO ERREXIT;               /* WRITE THE MESSAGE             */ 00228000
         B     ERREXIT                                             0256 00229000
*     END;                                                         0257 00230000
*   IF TRMPREF='0'B|TRMTYPE^=TRMNSDLC THEN/* ENTRY NOT A 3705 NON-SNA   00231000
*                                      LINE                  @Y17XARX*/ 00232000
@RF00253 TM    TRMPREF(IEDQTPTR),B'00010000'                       0258 00233000

         BZ    @RF00272                                            0272 00279000
*     DO;                           /* YES                   @Y17XARR*/ 00280000
*       DO UNTIL(RZERO=OCTRMTBL|ERRFLAG='1'B);/* FOR ALL TERMS OR  0274 00281000
*                                      UNTIL ERROR           @Y17XARX*/ 00282000
@DL00274 DS    0H                                                  0275 00283000
*         RWORKB=RZERO;             /* SAVE TTCIN OF TERM    @Y17XARX*/ 00284000
         LR    RWORKB,RZERO                                        0275 00285000
*         RPARM=RZERO;              /* GET TTE FOR TTCIN     @Y17XARX*/ 00286000
         LR    RPARM,RZERO                                         0276 00287000
*         CALL TNTCODE;             /*                       @Y17XARX*/ 00288000

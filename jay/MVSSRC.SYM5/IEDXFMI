         TITLE 'IEDXFMI, PIU TO BUFFER TRANSFORM                       *00001000
                        '                                               00002000
IEDXFMI  CSECT ,                                                   0001 00003000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00004000
         BALR  @12,0                                               0001 00005000
@PSTART  DS    0H                                                  0001 00006000
         USING @PSTART,@12                                         0001 00007000
IEDXFMI  IEDHJN START,HJN                                               00008000
*                                                                  0144 00009000
*/********************************************************************/ 00010000
*/*                                                                  */ 00011000
*/*     M A I N     R O U T I N E                                    */ 00012000
*/*                                                                  */ 00013000
*/********************************************************************/ 00014000
*                                                                  0144 00015000
*   PIUBFRP=RPARM;                  /* PIU BUFFER ADDRESS            */ 00016000
         LR    PIUBFRP,RPARM                                       0144 00017000
*   PIUSTART=ADDR(PRFPIUO)+PRFPIUO; /* ADDRESS OF PIU DATA(TH-RH-RU) */ 00018000
         LA    PIUSTART,PRFPIUO(,IEDQPPTR)                         0145 00019000
         SLR   @15,@15                                             0145 00020000
         IC    @15,PRFPIUO(,IEDQPPTR)                              0145 00021000
         ALR   PIUSTART,@15                                        0145 00022000
*/*                                                                  */ 00023000
*/* INITIALIZE WORK AREA                                             */ 00024000
*                                                                  0146 00025000
*   WAPIUO=PRFPIUO;                 /* SAVE PIU OFFSET IN WORK AREA  */ 00026000
         STC   @15,WAPIUO                                          0146 00027000
*   WATH=IEDTH;                     /* SAVE TH IN WORK AREA          */ 00028000
         MVC   WATH(10),IEDTH(PIUSTART)                            0147 00029000
*   WARH=IEDRH;                     /* SAVE 3-BYTE RH IN WORK AREA   */ 00030000
         MVC   WARH(3),IEDRH+10(PIUSTART)                          0148 00031000
*/*                                                                  */ 00032000
*/* ZERO OUT HEADER PORTION OF BUFFER FROM PRFNBUNT UP TO (BUT NOT   */ 00033000
*/* INCLUDING) PRFHQBCK. THIS IS ALL BUT THE HEADER ONLY PART OF A   */ 00034000
*/* TCAM BUFFER.                                                     */ 00035000
*                                                                  0149 00036000
*   IEDQPRF(ADDR(PRFNBUNT)-ADDR(IEDQPRF)+1:ADDR(PRFHQBCK)-ADDR(IEDQPRF) 00037000
*       )=''B;                      /* ZERO OUT HDR                  */ 00038000
         XC    IEDQPRF+12(23,IEDQPPTR),IEDQPRF+12(IEDQPPTR)        0149 00039000
*/*                                                                  */ 00040000
*/* ZERO OUT THE -8 BUFFER PREFIX                                    */ 00041000
*                                                                  0150 00042000
*   IEDPF1=IEDPF1&&IEDPF1;          /* ZERO OUT PREFIX               */ 00043000
         LR    @15,IEDQPPTR                                        0150 00044000
         AL    @15,@CF00425                                        0150 00045000
         XC    IEDPF1(8,@15),IEDPF1(@15)                           0150 00046000
*/*                                                                  */ 00047000
*                                                                  0151 00048000
*   CALL COMMON;                    /* SET BUFFER FIELDS COMMON TO 0151 00049000
*                                      BOTH FID0 AND FID1            */ 00050000
         BAL   @14,COMMON                                          0151 00051000
*/*                                                                  */ 00052000
*                                                                  0152 00053000
*   IF WATHPTR->TTHFTYPE=TTHFID0 THEN/* FID0                         */ 00054000
         L     @15,WATHPTR                                         0152 00055000
         TM    TTHFTYPE(@15),B'11110000'                           0152 00056000
         BNZ   @RF00152                                            0152 00057000
*     CALL FID0BLD;                 /* FID0 BUFFER BUILD             */ 00058000
         BAL   @14,FID0BLD                                         0153 00059000
*   ELSE                            /* FID1                          */ 00060000
*     CALL FID1BLD;                 /* FID1 BUFFER BUILD             */ 00061000
         B     @RC00152                                            0154 00062000
@RF00152 BAL   @14,FID1BLD                                         0154 00063000
*/*                                                                  */ 00064000
*                                                                  0155 00065000
*   RETURN;                         /* RETURN TO CALLER              */ 00066000
@EL00001 DS    0H                                                  0155 00067000
@EF00001 DS    0H                                                  0155 00068000
@ER00001 LM    @14,@12,12(@13)                                     0155 00069000
         BR    @14                                                 0155 00070000
*                                                                  0156 00071000
*/********************************************************************/ 00072000
*/*                                                                  */ 00073000
*/* THIS ROUTINE FILLS IN TCAM BUFFER FIELDS COMMON TO BOTH FID0 AND */ 00074000
*/* FID1.                                                            */ 00075000
*/*                                                                  */ 00076000
*/********************************************************************/ 00077000
*                                                                  0156 00078000
*COMMON:                                                           0156 00079000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0156 00080000
         B     @EL00001                                            0156 00081000
COMMON   DS    0H                                                  0157 00082000
*   PRF1RH=WARHPTR->IEDRH;          /* MOVE IN 3-BYTES OF RH         */ 00083000

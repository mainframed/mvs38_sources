         MVC   PRF1SQID(2,@15),TTHSNF(@11)                         0159 00094000
*   PRFSRCE=WATHPTR->TTHOAF;        /* SET SOURCE                    */ 00095000
         MVC   PRFSRCE(2,IEDQPPTR),TTHOAF(@11)                     0160 00096000
*   PRFSIZE=WAPIUO+LENGTH(IEDTH)+WATHPTR->TTHDCF;/* CALCULATE SIZE   */ 00097000
         LA    @15,10                                              0161 00098000
         SLR   @00,@00                                             0161 00099000
         IC    @00,WAPIUO                                          0161 00100000
         ALR   @15,@00                                             0161 00101000
         SLR   @00,@00                                             0161 00102000
         ICM   @00,3,TTHDCF(@11)                                   0161 00103000
         ALR   @15,@00                                             0161 00104000
         STH   @15,PRFSIZE(,IEDQPPTR)                              0161 00105000
*/*                                                                  */ 00106000
*/* CALCULATE THE NUMBER OF UNITS IN THE BUFFER                      */ 00107000
*                                                                  0162 00108000
*   UNITCTR=1;                      /* SET UNIT COUNTER TO 1         */ 00109000
         LA    UNITCTR,1                                           0162 00110000
*   UNITPTR=PIUBFRP;                /* SET UNIT POINTER TO 1ST UNIT     00111000
*                                      OF BUFFER                     */ 00112000
         LR    UNITPTR,PIUBFRP                                     0163 00113000
*   DO WHILE(UNITPTR->PRFTIC^=INVALTIC);/* UNIT COUNT LOOP           */ 00114000
         B     @DE00164                                            0164 00115000
@DL00164 DS    0H                                                  0165 00116000
*     UNITPTR=UNITPTR->PRFTIC;      /* GET NEXT UNIT ADDRESS         */ 00117000
         L     UNITPTR,PRFTIC(,UNITPTR)                            0165 00118000
*     UNITCTR=UNITCTR+1;            /* INCREMENT UNIT COUNTER        */ 00119000
         LA    UNITCTR,1(,UNITCTR)                                 0166 00120000
*   END;                                                           0167 00121000
*                                                                  0167 00122000
@DE00164 CLC   PRFTIC(4,UNITPTR),@CB00312                          0167 00123000
         BNE   @DL00164                                            0167 00124000
*/*                                                                  */ 00125000
*                                                                  0168 00126000
*   PRFNBUNT=UNITCTR;               /* SET NUMBER OF UNITS           */ 00127000
         STC   UNITCTR,PRFNBUNT(,IEDQPPTR)                         0168 00128000
*/*                                                                  */ 00129000
*                                                                  0169 00130000
*   RETURN;                         /* RETURN TO CALLER              */ 00131000
@EL00002 DS    0H                                                  0169 00132000
@EF00002 DS    0H                                                  0169 00133000
@ER00002 BR    @14                                                 0169 00134000
*   END COMMON;                                                    0170 00135000
*                                                                  0171 00136000
*/********************************************************************/ 00137000
*/*                                                                  */ 00138000
*/* THIS ROUTINE FILLS IN THE REMAINDER OF THE BUFFER FOR FID0.      */ 00139000
*/*                                                                  */ 00140000

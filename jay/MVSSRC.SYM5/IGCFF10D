*                       OCBUFNED=OCBUFNED-GETNO;/* SET NO STILL NEED    00359000
*                                      =REMAINDER                    */ 00360000
         LH    @11,OCBUFNED(,ROPCE)                             04S0746 00361000
         N     @11,@CF02048                                     04S0746 00362000
         SLR   @11,GETNO                                        04S0746 00363000
         STH   @11,OCBUFNED(,ROPCE)                             04S0746 00364000
*                     END;                                      04S0747 00365000
*                   ELSE            /* ELSE GET ALL NEEDED           */ 00366000
*                     DO;                                       04S0748 00367000
         B     @RC00743                                         04S0748 00368000
@RF00743 DS    0H                                               04S0749 00369000
*                       GETNO=OCBUFNED;/* SET TO GET ALL             */ 00370000
         LH    GETNO,OCBUFNED(,ROPCE)                           04S0749 00371000
         N     GETNO,@CF02048                                   04S0749 00372000
*                       OCBUFNED=0; /* SET NO STILL NEED=0           */ 00373000
         SLR   @11,@11                                          04S0750 00374000
         STH   @11,OCBUFNED(,ROPCE)                             04S0750 00375000
*                     END;                                      04S0751 00376000
*                   OCBUFGOT=GETNO; /* SET NO HAVE =NO TO GET        */ 00377000
@RC00743 STH   GETNO,OCBUFGOT(,ROPCE)                           04S0752 00378000
*                   OPCADGOT=OPCADGOT+GETNO;/* INCREMENT OPCADGOT    */ 00379000
         LH    @11,OPCADGOT(,ROPCAVT)                           04S0753 00380000
         N     @11,@CF02048                                     04S0753 00381000
         ALR   @11,GETNO                                        04S0753 00382000
         STH   @11,OPCADGOT(,ROPCAVT)                           04S0753 00383000
*                 END;                                          04S0754 00384000
*               ELSE                /* ELSE MUST PUT REDRIVE OPCE ON    00385000
*                                      WAIT CHAIN AND WAIT FOR       */ 00386000
*                 DO;               /* AVAILABLE UNITS ON REDRIVE    */ 00387000
         B     @RC00741                                         04S0755 00388000
@RF00741 DS    0H                                               04S0756 00389000
*                   OCWTG=THISMOD;  /* SET RETURN TO THIS MODULE     */ 00390000
         MVC   OCWTG(2,ROPCE),THISMOD                           04S0756 00391000
*                   OCLINK=OPCBUFWT;/* GET CHAIN ANCHOR AND PUT IN      00392000
*                                      LINK FIELD                    */ 00393000

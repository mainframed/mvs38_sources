         TITLE 'IGCFF10D, ACTIVATE CHECKPOINT/RESTART MODULE           *00001000
                        '                                               00002000
IGCFF10D CSECT ,                                                01S0001 00003000
@PROLOG  BALR  @12,0                                            01S0001 00004000
@PSTART  DS    0H                                               01S0001 00005000
         USING @PSTART,@12                                      01S0001 00006000
*                                   /*                                  00007000
IGCFF10D IEDHJN AFTERHJN,HJN           MODULE IDENTIFICATION MACRO      00008000
*   ROPCAVT=RPARM;                  /* SET THE BASE POINTER FOR THE     00009000
*                                      OPERATOR CONTROL ADDRESS 02S0665 00010000
*                                      VECTOR TABLE                  */ 00011000
         LR    ROPCAVT,RPARM                                    02S0665 00012000
*   RFY                                                         02S0666 00013000
*     RPARM UNRSTD;                 /* RELEASE THE PARAMETER REGISTER   00014000
*                                      FOR USE BY THE COMPILER       */ 00015000
*   RWRKAREA=OPCWORK;               /* SET THE BASE REGISTER TO BE      00016000
*                                      USED FOR ADDRESSING AUTOMATIC    00017000
*                                      DATA WITH THE POINTER TO THE     00018000
*                                      OPERATOR CONTROL RESIDENT WORK   00019000
*                                      AREA                          */ 00020000
         L     RWRKAREA,OPCWORK(,ROPCAVT)                       02S0667 00021000
*   GEN REFS(RWRKAREA) NOSETS;                                  02S0668 00022000
*                                   /*                                  00023000
         USING @DATD,RWRKAREA          INFORM THE ASSEMBLER OF THE      00024000
*                                      BASE REGISTER TO BE USED FOR     00025000
*                                      ADDRESSING AUTOMATIC DATA        00026000
*   RSAVE=ADDR(OPCSAVE);            /* SET THE SAVE AREA POINTER        00027000
*                                      REGISTER WITH THE POINTER TO     00028000
*                                      THE OPERATOR CONTROL SAVE AREA*/ 00029000
         LA    RSAVE,OPCSAVE(,ROPCAVT)                          02S0669 00030000
*                                                               02S0670 00031000
*   /*****************************************************************/ 00032000
*   /*                                                               */ 00033000
*   /* PROCESS: THIS SEGMENT DOES THE PROCESSING THAT IS NECESSARY TO*/ 00034000
*   /* PERFORM THE FUNCTION OF THE MODULE.                           */ 00035000
*   /*                                                               */ 00036000
*   /*****************************************************************/ 00037000
*                                                               02S0670 00038000
*                                                               03S0670 00039000
*   /*****************************************************************/ 00040000
*   /*                                                               */ 00041000
*   /* PROCESS.INITIAL: THIS SEGMENT DOES THE INITIAL PROCESSING.    */ 00042000
*   /*                                                               */ 00043000
*   /*****************************************************************/ 00044000
*                                                               03S0670 00045000
*   DO;                                                         03S0670 00046000
*     RAVT=OPCAVTPT;                /* SET THE BASE POINTER FOR THE     00047000
*                                      TCAM ADDRESS VECTOR TABLE     */ 00048000
         L     RAVT,OPCAVTPT(,ROPCAVT)                          03S0671 00049000
*     ROPCE=OPCCOPCE;               /* SET THE BASE POINTER FOR THE     00050000
*                                      OPERATOR CONTROL ELEMENT      */ 00051000
         L     ROPCE,OPCCOPCE(,ROPCAVT)                         03S0672 00052000
*     FLAGS=''B;                    /* INITIALIZE FLAG BYTE          */ 00053000
         XC    FLAGS(4),FLAGS                                   03S0673 00054000
*   END;                                                        03S0674 00055000
*   IF OCSWITCH^=OCRSTART THEN      /* IF NOT A RESTART OPCE         */ 00056000
         CLI   OCSWITCH(ROPCE),X'F1'                            02S0675 00057000
         BE    @RF00675                                         02S0675 00058000
*     IF OCSWITCH^=OCACTLK THEN     /* IF NOT AN ACTLINK OPCE        */ 00059000
         CLI   OCSWITCH(ROPCE),X'50'                            02S0676 00060000
         BE    @RF00676                                         02S0676 00061000
*       DO;                         /* PROCESS RESPONSES TO COMMANDS    00062000
*                                      SENT AS PART OF 3705 RESTART  */ 00063000
*                                                               03S0678 00064000
*         /***********************************************************/ 00065000
*         /*                                                         */ 00066000
*         /* PROCESS.RESPONSE: THIS SEGMENT PROCESSES THE RESPONSES  */ 00067000
*         /* TO THE RESTART COMMANDS. NO ERROR PROCESSING IS         */ 00068000
*         /* PERFORMED. THE UNITS CONTAINING THE OPCE AND ATTACHED RU*/ 00069000
*         /* ARE FREED AND TRANSFER OF CONTROL IS SET UP FOR         */ 00070000
*         /* IGC0010D.                                               */ 00071000
*         /*                                                         */ 00072000
*         /***********************************************************/ 00073000
*                                                               03S0678 00074000
*         RFY                                                   03S0678 00075000
*           REG00 RSTD;                                         03S0678 00076000
*         RFY                                                   03S0679 00077000
*           RPARM RSTD;                                         03S0679 00078000
*         IF OCSWITCH^=OCSND3 THEN  /* IF NOT REPLACE DEVICE RECORD     00079000
*                                      RESPONSE                      */ 00080000
         CLI   OCSWITCH(ROPCE),X'F4'                            03S0680 00081000
         BE    @RF00680                                         03S0680 00082000
*           IF OCSWITCH=OCSND1|OCSWITCH=OCSND2 THEN/* IF BHSET OR       00083000
*                                      MODEM SPEED SELECT RESPONSE      00084000
*                                      (FID0)                        */ 00085000
         CLI   OCSWITCH(ROPCE),X'F2'                            03S0681 00086000
         BE    @RT00681                                         03S0681 00087000
         CLI   OCSWITCH(ROPCE),X'F3'                            03S0681 00088000
         BNE   @RF00681                                         03S0681 00089000
@RT00681 DS    0H                                               03S0682 00090000
*             REG00=2;              /* SET TO DECREMENT OPCADGOT BY 2   00091000
*                                      FOR THE OPCE AND RU           */ 00092000
         LA    REG00,2                                          03S0682 00093000
*           ELSE                    /* ELSE FID1 RESPONSE            */ 00094000
*             REG00=1;              /* SET TO DECREMENT OPCADGOT BY 1   00095000
*                                      FOR THE OPCE                  */ 00096000
         B     @RC00681                                         03S0683 00097000
@RF00681 LA    REG00,1                                          03S0683 00098000
*         ELSE                      /* REPLACE DEVICE RECORD RESPONSE*/ 00099000
*           REG00=OCDVRDCT;         /* SET TO DECREMENT OPCADGOT BY     00100000
*                                      THE COUNT OF UNITS WHICH WERE    00101000
*                                      NECESSARY TO BUILD THE OPCE      00102000
*                                      AND THE REPLACE DEVICE RECORD    00103000
*                                      RU                            */ 00104000
         B     @RC00680                                         03S0684 00105000
@RF00680 LH    REG00,OCDVRDCT(,ROPCE)                           03S0684 00106000
*         CALL DECRCT;              /* DECREMENT OPCADGOT AND INIT.     00107000
*                                      REDRIVE ELEMENT IF R/D OPCE'S    00108000
*                                      ON OPCBUFWT CHAIN             */ 00109000
@RC00680 L     @15,OPCDECR(,ROPCAVT)                            03S0685 00110000
         BALR  @14,@15                                          03S0685 00111000
*         RPARM=ROPCE;              /* POINT TO CURRENT OPCE         */ 00112000
         LR    RPARM,ROPCE                                      03S0686 00113000
*         CALL FREEBUFF;            /* FREE THE OPCE AND ANY ATTACHED   00114000
*                                      RU'S                          */ 00115000
         L     @15,OPCFREBF(,ROPCAVT)                           03S0687 00116000
         BALR  @14,@15                                          03S0687 00117000
*         OPCMODID=CNTLMOD;         /* SET TO XCTL TO IGC0010D       */ 00118000
         MVC   OPCMODID(2,ROPCAVT),CNTLMOD                      03S0688 00119000
*         RFY                                                   03S0689 00120000
*           REG00 UNRSTD;                                       03S0689 00121000
*         RFY                                                   03S0690 00122000
*           RPARM UNRSTD;                                       03S0690 00123000
*       END;                                                    02S0691 00124000
*     ELSE                          /* THIS IS AN ACTLINK,           */ 00125000
*       DO;                         /* START CKPTR/RTRT PROCESS      */ 00126000
         B     @RC00676                                         02S0692 00127000
@RF00676 DS    0H                                               02S0693 00128000
*                                                               03S0693 00129000
*         /***********************************************************/ 00130000
*         /*                                                         */ 00131000
*         /* PROCESS.START: THIS SEGMENT STARTS THE RESTART          */ 00132000
*         /* PROCESSING BY BUILDING THE RESTART OPCE AND PASSING IT  */ 00133000
*         /* TO CHECKPOINT.                                          */ 00134000
*         /*                                                         */ 00135000
*         /***********************************************************/ 00136000
*                                                               03S0693 00137000
*         OCSWITCH=OCRSTART;        /* SET AS RESTART OPCE           */ 00138000
         MVI   OCSWITCH(ROPCE),X'F1'                            03S0693 00139000
*         OCOPTCDE=OCMODIFY;        /* SET CMND CODE                 */ 00140000
         MVI   OCOPTCDE(ROPCE),X'44'                            03S0694 00141000
*         OCRESNUM=''B;             /* INITIALIZE RESOURCE NUMBER    */ 00142000
         SLR   @11,@11                                          03S0695 00143000
         STH   @11,OCRESNUM(,ROPCE)                             03S0695 00144000
*         OCTRMTBL=OCPTRSAV->OCTRMTBL;/* TTCIN OF NCP                */ 00145000
         L     @11,OCPTRSAV(,ROPCE)                             03S0696 00146000
         LH    @11,OCTRMTBL(,@11)                               03S0696 00147000
         N     @11,@CF02048                                     03S0696 00148000
         STH   @11,OCTRMTBL(,ROPCE)                             03S0696 00149000
*         OCWTG=THISMOD;            /* SET TO RETURN TO THIS MODULE     00150000
*                                      FROM CHECKPOINT               */ 00151000
         MVC   OCWTG(2,ROPCE),THISMOD                           03S0697 00152000
*         OCREDRVE=OFF;             /* INSURE REDRIVE FLAG OFF       */ 00153000
         NI    OCREDRVE(ROPCE),B'11101111'                      03S0698 00154000
*         OPCMODID=CKPTMOD;         /* SET TO XCTL TO CHECKPOINT     */ 00155000
         MVC   OPCMODID(2,ROPCAVT),CKPTMOD                      03S0699 00156000
*       END;                                                    02S0700 00157000
*   ELSE                            /* THIS IS A RESTART OPCE,       */ 00158000
*     DO;                           /* SEND OUT RESTART COMMANDS     */ 00159000
         B     @RC00675                                         02S0701 00160000
@RF00675 DS    0H                                               02S0702 00161000
*                                                               03S0702 00162000
*       /*************************************************************/ 00163000
*       /*                                                           */ 00164000
*       /* PROCESS.SENDER: THIS SEGMENT SENDS THE RESTART COMMANDS TO*/ 00165000
*       /* THE NCP.                                                  */ 00166000
*       /*                                                           */ 00167000
*       /*************************************************************/ 00168000
*                                                               03S0702 00169000
*       RCKPREC=NRACURR;            /* SET BASE FOR CKPT RESOURCE       00170000
*                                      RECORD                        */ 00171000
         L     @11,OCRSTGTM(,ROPCE)                             03S0702 00172000
         L     RCKPREC,NRACURR(,@11)                            03S0702 00173000
*       IF OCREDRVE=OFF THEN        /* IF NOT REDRIVE                */ 00174000
         TM    OCREDRVE(ROPCE),B'00010000'                      03S0703 00175000
         BNZ   @RF00703                                         03S0703 00176000
*         DO;                       /* DETERMINE NUMBER OF COMMANDS     00177000
*                                      TO SEND AND, IF ANY, GET AS      00178000
*                                      MANY UNITS AS POSSIBLE IN        00179000
*                                      WHICH TO BUILD OPCE'S AND RU'S   00180000
*                                      AND HANG OFF OCUNIT           */ 00181000
*                                                               04S0705 00182000
*           /*********************************************************/ 00183000
*           /*                                                       */ 00184000
*           /* PROCESS.SENDER.CHKR: THIS SEGMENT DETERMINES THE      */ 00185000
*           /* NUMBER OF RESTART COMMANDS TO BE SENT BASED ON THE    */ 00186000
*           /* SETTINGS OF THE BITS IN THE RESOURCE RECORD. IF THE   */ 00187000
*           /* RESOURCE RECORD NUMBER IS 0 THEN RESTART PROCESSING IS*/ 00188000
*           /* OVER FOR THIS 3705 AND RESTART OPCE IS RESET OF RETURN*/ 00189000
*           /* TO IGCFJ10D TO START ACTLINK PROCESSING.              */ 00190000
*           /*                                                       */ 00191000
*           /*********************************************************/ 00192000
*                                                               04S0705 00193000
*           IF OCRESNUM^=0 THEN     /* IF RESTART PROCESSING NOT OVER*/ 00194000
         LH    @11,OCRESNUM(,ROPCE)                             04S0705 00195000
         N     @11,@CF02048                                     04S0705 00196000
         LTR   @11,@11                                          04S0705 00197000
         BZ    @RF00705                                         04S0705 00198000
*             DO;                   /* DETERMINE NUMBER OF UNITS TO     00199000
*                                      SEND RESTART COMMANDS FOR THIS   00200000
*                                      RESOURCE                      */ 00201000
*               GETNO=''B;          /* INIT. CTR. FOR NUMBER OF UNITS   00202000
*                                      NEEDED                        */ 00203000
         SLR   GETNO,GETNO                                      04S0707 00204000
*               IF CNRFBHS=ON|CNRFBHSC=ON THEN/* IF BHSET FLAG ON       00205000
*                                                            @YM07713*/ 00206000

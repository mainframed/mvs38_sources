*                 GETNO=GETNO+2;    /* NEED 2 UNITS                  */ 00238000
         AL    GETNO,@CF00132                                   04S0719 00239000
*               IF CNRFRDR=ON THEN  /* IF DEVICE RECORD FLAG ON      */ 00240000
@RF00718 TM    CNRFRDR(RCKPREC),B'00000001'                     04S0720 00241000
         BNO   @RF00720                                         04S0720 00242000
*                 DO;               /* DETERMINE NUMBER OF UNITS TO     00243000
*                                      BUILD REPLACE DEVICE RECORD RU*/ 00244000
*                                                               05S0722 00245000
*                   /*************************************************/ 00246000
*                   /*                                               */ 00247000
*                   /* PROCESS.SENDER.CALCRDR: THIS SEGMENT CALULATES*/ 00248000
*                   /* THE NUMBER OF UNITS NEEDED TO BUILD THE CHANGE*/ 00249000
*                   /* DEVICE RECORD OPCE-PACKED RU.                 */ 00250000
*                   /*                                               */ 00251000
*                   /*************************************************/ 00252000
*                                                               05S0722 00253000
*                   RFY                                         05S0722 00254000
*                     IEDPRUQD BASED(ADDR(CNRDEVRC));/* BASE PACKED     00255000
*                                      RU STRUCTURE ON ADDR OF DEVICE   00256000
*                                      RECORD IN RESOURCE RECORD     */ 00257000
*                   COUNTER=2;      /* INIT TO MIN NUM OF UNITS TO      00258000
*                                      BUILD FID0 CMND               */ 00259000
         LA    COUNTER,2                                        05S0723 00260000
*                   DO SIZE=PRUDATCT BY(-AVTKEYLE) WHILE(SIZE>AVTKEYLE) 00261000
*                         ;         /* LOOP WHILE SIZE IS STILL 05S0724 00262000
*                                      GREATER THAN TCAM UNIT LENGTH */ 00263000
         LH    SIZE,PRUDATCT+19(,RCKPREC)                       05S0724 00264000
         B     @DE00724                                         05S0724 00265000
@DL00724 DS    0H                                               05S0725 00266000
*                     COUNTER=COUNTER+1;/* INCREMENT NUM UNITS NEEDED*/ 00267000
         AL    COUNTER,@CF00061                                 05S0725 00268000
*                   END;                                        05S0726 00269000
         LH    @11,AVTKEYLE(,RAVT)                              05S0726 00270000
         LCR   @11,@11                                          05S0726 00271000
         ALR   SIZE,@11                                         05S0726 00272000
@DE00724 CH    SIZE,AVTKEYLE(,RAVT)                             05S0726 00273000
         BH    @DL00724                                         05S0726 00274000
*                   GETNO=GETNO+COUNTER;/* CALC NUM UNITS NEEDED FOR    00275000
*                                      ALL CMNDS                     */ 00276000
         ALR   GETNO,COUNTER                                    05S0727 00277000
*                   OCDVRDCT=COUNTER;/* SAVE NUM UNITS NEEDED FOR       00278000
*                                      DEVICE RECORD CMND IN RESTART    00279000
*                                      OPCE                          */ 00280000
         STH   COUNTER,OCDVRDCT(,ROPCE)                         05S0728 00281000
*                   RFY                                         05S0729 00282000
*                     IEDPRUQD BASED;                           05S0729 00283000
*                 END;                                          04S0730 00284000
*               OCBUFNED=GETNO;     /* SET NUMBER OF BUFFER UNITS       00285000
*                                      NEEDED                        */ 00286000
@RF00720 STH   GETNO,OCBUFNED(,ROPCE)                           04S0731 00287000
*               OCBUFGOT=''B;       /* SET NUMBER OF BUFFER UNITS GOT*/ 00288000
         SLR   @11,@11                                          04S0732 00289000
         STH   @11,OCBUFGOT(,ROPCE)                             04S0732 00290000
*             END;                                              04S0733 00291000
*           ELSE                    /* FINISHED SENDING RESTART 04S0734 00292000
*                                      COMMANDS,                     */ 00293000
*             DO;                   /* SET UP CURRENT OPCE AS ACTLINK   00294000
*                                      OPCE AND XCTL TO IGCFJ10D TO     00295000
*                                      BUILD ACTLINKS FOR ALL LINES     00296000
*                                      WITH REMEMBER BIT ON          */ 00297000
         B     @RC00705                                         04S0734 00298000
@RF00705 DS    0H                                               04S0735 00299000
*               OUTFLAG=ON;         /* SET FLAG TO BYPASS REMAINDER     00300000
*                                      OF PROCESSING                 */ 00301000
         LA    @11,1                                            04S0735 00302000
         ST    @11,OUTFLAG                                      04S0735 00303000
*               OCSWITCH=''B;       /* SET VALUE OF 0 TO INDICATE TO    00304000
*                                      IGCFJ10D THAT OPCE IS FROM       00305000
*                                      RESTART                       */ 00306000
         MVI   OCSWITCH(ROPCE),X'00'                            04S0736 00307000
*               OPCMODID=RPNCPMOD;  /* SET TO XCTL TO IGCFJ10D       */ 00308000
         MVC   OPCMODID(2,ROPCAVT),RPNCPMOD                     04S0737 00309000
*             END;                                              04S0738 00310000
*           IF OUTFLAG=OFF THEN     /* IF COMMANDS TO BE SENT,       */ 00311000
@RC00705 L     @11,OUTFLAG                                      03S0739 00312000
         LTR   @11,@11                                          03S0739 00313000
         BNZ   @RF00739                                         03S0739 00314000
*             DO;                   /* GET AS MANY UNITS AS POSSIBLE    00315000
*                                      TO START BUILDING THOSE CMNDS */ 00316000
*                                                               04S0741 00317000
*               /*****************************************************/ 00318000
*               /*                                                   */ 00319000
*               /* PROCESS.SENDER.GETBUFS: THIS SEGMENT DETERMINES IF*/ 00320000
*               /* ANY UNITS MAY BE OBTAINED, AND IF SO, HOW MANY,   */ 00321000
*               /* AND THEN GETS THEM AND ATTACHES THEM TO THE       */ 00322000
*               /* REDRIVE OPCE                                      */ 00323000
*               /*                                                   */ 00324000
*               /*****************************************************/ 00325000
*                                                               04S0741 00326000
*               IF OPCADGOT<(AVTNOLBF/2) THEN/* IF ACT/DEACT HAS LT     00327000
*                                      50%                           */ 00328000
         LH    @11,OPCADGOT(,ROPCAVT)                           04S0741 00329000
         N     @11,@CF02048                                     04S0741 00330000
         LA    @06,2                                            04S0741 00331000
         LH    @05,AVTNOLBF(,RAVT)                              04S0741 00332000
         LR    @00,@05                                          04S0741 00333000
         SRDA  @00,32                                           04S0741 00334000
         DR    @00,@06                                          04S0741 00335000
         CR    @11,@01                                          04S0741 00336000
         BNL   @RF00741                                         04S0741 00337000
*                 DO;               /* THEN                          */ 00338000
*                   IF(OCBUFNED+OPCADGOT)>(AVTNOLBF/2) THEN/* IF NO     00339000
*                                      NEED PLUS                     */ 00340000
         LH    @15,OCBUFNED(,ROPCE)                             04S0743 00341000
         N     @15,@CF02048                                     04S0743 00342000
         LR    @14,@15                                          04S0743 00343000
         ALR   @14,@11                                          04S0743 00344000
         LR    @00,@05                                          04S0743 00345000
         SRDA  @00,32                                           04S0743 00346000
         DR    @00,@06                                          04S0743 00347000
         CR    @14,@01                                          04S0743 00348000
         BNH   @RF00743                                         04S0743 00349000
*                     DO;           /* NO A/D ALREADY HAS IS GT 50%     00350000
*                                      THEN                          */ 00351000
*                       GETNO=((AVTNOLBF/2)-OPCADGOT);/* SET TO GET     00352000
*                                      UP TO 50%                     */ 00353000
         LH    @14,AVTNOLBF(,RAVT)                              04S0745 00354000
         SRDA  @14,32                                           04S0745 00355000
         DR    @14,@06                                          04S0745 00356000

         BZ    @RF00708                                         04S0708 00208000
*                 GETNO=GETNO+2;    /* FID0 COMMAND, NEED TWO UNITS     00209000
*                                      FOR OPCE AND RU               */ 00210000
         AL    GETNO,@CF00132                                   04S0709 00211000
*               IF CNRFSSP=ON THEN  /* IF SERVICE SEEK PAUSE         */ 00212000
@RF00708 TM    CNRFSSP(RCKPREC),B'00100000'                     04S0710 00213000
         BNO   @RF00710                                         04S0710 00214000
*                 GETNO=GETNO+1;    /* NEED 1 UNIT                   */ 00215000
         AL    GETNO,@CF00061                                   04S0711 00216000
*               IF CNRFNPL=ON THEN  /* IF CHANGE NEG. POLL LIMIT FLAG   00217000
*                                      ON                            */ 00218000
@RF00710 TM    CNRFNPL(RCKPREC),B'00010000'                     04S0712 00219000
         BNO   @RF00712                                         04S0712 00220000
*                 GETNO=GETNO+1;    /* NEED 1 UNIT                   */ 00221000
         AL    GETNO,@CF00061                                   04S0713 00222000
*               IF CNRFCSL=ON THEN  /* IF CHANGE SESSION LIMIT FLAG     00223000
*                                      ON                            */ 00224000
@RF00712 TM    CNRFCSL(RCKPREC),B'00001000'                     04S0714 00225000
         BNO   @RF00714                                         04S0714 00226000
*                 GETNO=GETNO+1;    /* NEED 1 UNIT                   */ 00227000
         AL    GETNO,@CF00061                                   04S0715 00228000
*               IF CNRFCTL=ON THEN  /* IF CHANGE TRANSMISSION LIMIT     00229000
*                                      FLAG ON                       */ 00230000
@RF00714 TM    CNRFCTL(RCKPREC),B'00000100'                     04S0716 00231000
         BNO   @RF00716                                         04S0716 00232000
*                 GETNO=GETNO+1;    /* NEED 1 UNIT                   */ 00233000
         AL    GETNO,@CF00061                                   04S0717 00234000
*               IF CNRFMSS=ON THEN  /* IF MODEM SPEED SELECT FLAG ON */ 00235000
@RF00716 TM    CNRFMSS(RCKPREC),B'00000010'                     04S0718 00236000

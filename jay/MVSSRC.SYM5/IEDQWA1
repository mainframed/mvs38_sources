*               CALL DEVFLMSG;      /* PRINT MESSAGE                 */ 00130000
         BAL   @14,DEVFLMSG                                     03S0535 00131000
*             END;                  /* END OF SEVERE CT/AP PROBLEM      00132000
*                                      HDLG                          */ 00133000
*       ELSE                        /* INBOUND DATA FROM SNA DEVICE  */ 00134000
*         DO;                       /* MAY BE CECOM REPLY OR TEST       00135000
*                                      DATA                          */ 00136000

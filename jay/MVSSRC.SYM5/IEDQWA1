*               CALL RTNBUF;        /* RETURN BUFFER AND LCB         */ 00267000
         BAL   @14,RTNBUF                                       03S0579 00268000
*             END;                  /* END OF LAST BUF HANDLING      */ 00269000
*         RFY                                                   03S0581 00270000
*           IEDQLCB BASED;          /* SET AS ON ENTRY TO SEGMENT    */ 00271000

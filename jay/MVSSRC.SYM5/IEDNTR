*       GEN NOREFS NOSETS;                                         0124 00158000
*                  /*                                        @Y17XASI*/ 00159000
         BCTR  RLEN,0                                        @YM05745   00160000
         EX    RLEN,CLRDATA             CLEAR VARIABLE DATA  @Y17XASI   00161000
*     END;                          /*                       @Y17XASI*/ 00162000
*   RFY                                                            0126 00163000
*     RPARM RSTD;                   /* RESTRICT REGISTER 1   @Y17XASI*/ 00164000
@RF00120 DS    0H                                                  0127 00165000
*   IF CENTRY=MENTRY THEN           /* 1ST HALF OF TABLE FULL?       */ 00166000
         L     @07,MENTRY(,RTBL)                                   0127 00167000
         C     @07,CENTRY(,RTBL)                                   0127 00168000
         BNE   @RF00127                                            0127 00169000
*     DO;                           /* YES                           */ 00170000
*       COUNTER=COUNTER+ONE;        /* INCREMENT COUNTER             */ 00171000
         LH    @00,ONE                                             0129 00172000

         BNO   @RF00172                                            0172 00303000
*             DO;                   /* YES, SET 1ST-HALF PARMS       */ 00304000
*               PARMAREA=FENTRY;    /* DATA ADDRESS                  */ 00305000
         L     @07,FENTRY(,RTBL)                                   0174 00306000

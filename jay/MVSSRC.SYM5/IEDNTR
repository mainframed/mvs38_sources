*             END;                                                 0182 00327000
*           PARMFLAG=''B;           /* RESET COMPLETION FLAG         */ 00328000
@RC00172 MVI   PARMFLAG,X'00'                                      0183 00329000
*           RPARM=ADDR(AQPARM);     /* ADDR OF SVC PARAMETER LIST    */ 00330000
         LA    RPARM,AQPARM                                        0184 00331000
*           GEN(AQCTL);             /* SVC TO POST COMWRITE ECB      */ 00332000
         AQCTL                                                          00333000
*         END;                                                     0186 00334000
*     END;                                                         0187 00335000
*   RETURN;                         /* RETURN TO CALLER      @Y17XASI*/ 00336000

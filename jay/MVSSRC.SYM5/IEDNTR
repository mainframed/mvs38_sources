*               PARMLNTH=TLENGTH;   /* DATA LENGTH                   */ 00322000
         L     @07,TLENGTH(,RTBL)                                  0180 00323000
         STH   @07,PARMLNTH                                        0180 00324000
*               TBL2FULL=OFF;       /* RESET TABLE-FULL SWITCH       */ 00325000

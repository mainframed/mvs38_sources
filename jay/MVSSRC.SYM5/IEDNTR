*       CENTRY=CENTRY+ENTRYSZ;      /* BUMP POINTER TO NEXT ENTRY    */ 00237000
         B     @RC00140                                            0153 00238000
@RF00140 LH    @07,ENTRYSZ                                         0153 00239000
         AL    @07,CENTRY(,RTBL)                                   0153 00240000
         ST    @07,CENTRY(,RTBL)                                   0153 00241000
*   IF TRCOMWRT=ON&                 /* ARE WE USING COMWRITE? ...    */ 00242000
*       (TBL1FULL=ON|TBL2FULL=ON) THEN/* ... AND IS EITHER... ...  0154 00243000
*                                      HALF-TABLE FULL?              */ 00244000
@RC00140 DS    0H                                                  0154 00245000
@RC00127 TM    TRCOMWRT(RTBL),B'00001000'                          0154 00246000
         BNO   @RF00154                                            0154 00247000
         TM    TBL1FULL(RTBL),B'11000000'                          0154 00248000
         BZ    @RF00154                                            0154 00249000
*     DO;                           /* YES                           */ 00250000
*/********************************************************************/ 00251000
*/*                                                                  */ 00252000
*/*      THIS SUBROUTINE TESTS WHETHER EITHER OF THE 2 COMWRITE ECBS */ 00253000
*/*      IS AVAILABLE FOR USE.  IF AN ECB IS AVAILABLE, THE SWITCH   */ 00254000
*/*      ECBAVAIL IS TURNED ON, THE ECB ADDRESS IS PLACED IN RPARM   */ 00255000
*/*      AND THE CORRESPONDING COMWRITE PARAMETER LIST ADDRESS IS    */ 00256000
*/*      INITIALIZED.                                                */ 00257000
*/*                                                                  */ 00258000
*/********************************************************************/ 00259000
*                                                                  0156 00260000
*TESTECBS:                                                         0156 00261000
*       ECBAVAIL=OFF;               /* ASSUME NO ECB AVAILABLE       */ 00262000
TESTECBS NI    ECBAVAIL(RTBL),B'11011111'                          0156 00263000
*       TS(AVTCWTS1);               /* COMWRITE ECB 1 AVAILABLE?     */ 00264000
         TS    AVTCWTS1(RAVT)                                      0157 00265000
*       GEN(BZ ECB1);               /* BRANCH IF YES                 */ 00266000
         BZ ECB1                                                        00267000
*       TS(AVTCWTS2);               /* COMWRITE ECB 2 AVAILABLE?     */ 00268000
         TS    AVTCWTS2(RAVT)                                      0159 00269000
*       GEN(BNZ ENDTEST);           /* BRANCH IF NO, CAN'T PASS TO 0160 00270000
*                                      COMWRITE NOW                  */ 00271000
         BNZ ENDTEST                                                    00272000
*ECB2:                                                             0161 00273000
*       AVTCWPM2=ADDR(PARMLIST);    /* PARAMETER LIST ADDRESS        */ 00274000
ECB2     LA    @07,PARMLIST                                        0161 00275000
         ST    @07,AVTCWPM2(,RAVT)                                 0161 00276000
*       RPARM=ADDR(AVTCWEC2);       /* ADDR OF COMWRITE ECB 2        */ 00277000
         LA    RPARM,AVTCWEC2(,RAVT)                               0162 00278000
*       ECBAVAIL=ON;                /* INDICATE ECB AVAILABLE        */ 00279000
         OI    ECBAVAIL(RTBL),B'00100000'                          0163 00280000
*       GO TO ENDTEST;              /* EXIT                          */ 00281000
         B     ENDTEST                                             0164 00282000
*ECB1:                                                             0165 00283000
*       AVTCWPM1=ADDR(PARMLIST);    /* PARAMETER LIST ADDRESS        */ 00284000
ECB1     LA    @07,PARMLIST                                        0165 00285000
         ST    @07,AVTCWPM1(,RAVT)                                 0165 00286000
*       RPARM=ADDR(AVTCWEC1);       /* ADDR OF COMWRITE ECB 1        */ 00287000
         LA    RPARM,AVTCWEC1(,RAVT)                               0166 00288000
*       ECBAVAIL=ON;                /* INDICATE ECB AVAILABLE        */ 00289000
         OI    ECBAVAIL(RTBL),B'00100000'                          0167 00290000
*ENDTEST:                                                          0168 00291000
*       IF ECBAVAIL=ON THEN         /* IS AN ECB AVAILABLE?          */ 00292000
ENDTEST  TM    ECBAVAIL(RTBL),B'00100000'                          0168 00293000
         BNO   @RF00168                                            0168 00294000
*         DO;                       /* YES                           */ 00295000
*           AQECB=RPARM;            /* COMWRITE ECB ADDRESS          */ 00296000
         STCM  RPARM,7,AQECB                                       0170 00297000
*           AQTCB=AVTCWTCB;         /* COMWRITE TCB ADDRESS          */ 00298000

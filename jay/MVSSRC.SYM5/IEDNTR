*               PARMLNTH=TLENGTH;   /* DATA LENGTH                   */ 00308000
         L     @07,TLENGTH(,RTBL)                                  0175 00309000
         STH   @07,PARMLNTH                                        0175 00310000
*               TBL1FULL=OFF;       /* RESET TABLE-FULL SWITCH       */ 00311000
         NI    TBL1FULL(RTBL),B'01111111'                          0176 00312000
*             END;                                                 0177 00313000
*           ELSE                    /* 2ND HALF-TABLE IS FULL        */ 00314000
*             DO;                   /* SET 2ND-HALF PARAMETERS       */ 00315000
         B     @RC00172                                            0178 00316000
@RF00172 DS    0H                                                  0179 00317000
*               PARMAREA=MENTRY+ENTRYSZ;/* DATA ADDRESS              */ 00318000
         LH    @07,ENTRYSZ                                         0179 00319000

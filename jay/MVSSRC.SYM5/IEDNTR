         EX    RREM,MOVEDATA            MOVE VARIABLE DATA     @Y17XASI 00131000
*       RENT=CENTRY;                /* RESTORE CURRENT ENTRY ADDRESS    00132000
*                                                            @Y17XASI*/ 00133000
         L     RENT,CENTRY(,RTBL)                                  0117 00134000
*     END;                          /*                       @Y17XASI*/ 00135000
*   RFY                                                            0119 00136000
*    (RWRK8,                                                       0119 00137000
*     RREM) UNRSTD;                 /* FREE UP WORK REGISTERS FOR PLS   00138000
*                                                            @Y17XASI*/ 00139000
@RF00110 DS    0H                                                  0120 00140000
*   IF RLEN<LENGTH(TRPIU) THEN      /* IF TOTAL PIU LENGTH IS LESS 0120 00141000
*                                      THAN TRACE ENTRY SIZE THE   0120 00142000

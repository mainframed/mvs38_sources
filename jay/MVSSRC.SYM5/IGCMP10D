*READREG:                           /*                       @Y17XARL*/ 00236000
*   RENDADR=RBUF1->MAPB&TOFF;       /* ISOLATE ENDING ADDRESS FOR THE   00237000
*                                      DUMP                          */ 00238000
READREG  L     RENDADR,MAPB(,RBUF1)                                0435 00239000
         N     RENDADR,TOFF                                        0435 00240000
*   CPA=CCWAREA;                    /* SET UP FOR DATA TRANSFER    0436 00241000

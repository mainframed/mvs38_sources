         L     @15,OCTRDVD(,@15)                                   0455 00293000
         ICM   RDCB,15,NCPDPDCB(@15)                               0455 00294000
*   GEN;                                                           0456 00295000
*             /*                                             @Y17XARL*/ 00296000
    WRITE  COMDECB(RCOM),SF,(4),(6),MF=E                     @Y17XARL   00297000
    CHECK  COMDECB(RCOM)                                     @Y17XARL   00298000
*   RFY                                                            0457 00299000

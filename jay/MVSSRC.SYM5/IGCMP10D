         L     @00,OCIPLWA(,@04)                                   0463 00328000
         ST    @00,CPACOM(,RLCB)                                   0463 00329000
*   COMOPCE=OPCCOPCE;               /* SAVE OPCE ADDRESS     @Y17XARL*/ 00330000
         ST    @04,COMOPCE(,RCOM)                                  0464 00331000

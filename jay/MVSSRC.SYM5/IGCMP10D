*   REG1=ADDR(LCBQEBPL);            /* PARM LIST FOR QEB     @ZM47618*/ 00347000
         LA    REG1,LCBQEBPL(,RLCB)                                0470 00348000
*   GEN(AQCTL);                     /* GO TO SIO TO 3705     @ZM47618*/ 00349000
         AQCTL                                                          00350000
*   OCTRIP=ON;                      /* IN PROGRESS           @Y17XARL*/ 00351000

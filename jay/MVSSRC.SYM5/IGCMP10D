*   LCBSIO=SIOCODE;                 /* INDICATE SIO TO QEB   @ZM47618*/ 00341000
         MVI   LCBSIO(RLCB),X'30'                                  0468 00342000
*   LCBCP=LCBSTART;                 /* START OF CHAN PGM     @ZM47618*/ 00343000
         L     @04,LCBSTART-1(,RLCB)                               0469 00344000

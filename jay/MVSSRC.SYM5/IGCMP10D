*   IF NCPDPDCB=ZER0                /* WAS DUMP DCB SPECIFIED      0384 00061000
*                                                            @Y17XARL*/ 00062000
*     THEN                          /* NO                            */ 00063000
         L     @15,OCTRDVD(,@15)                                   0384 00064000

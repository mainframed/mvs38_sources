*       WKREG=RBUF1+FIVE;           /* IF THIS MACHINE IS 48K OR     */ 00259000
         LA    WKREG,5                                             0443 00260000
         ALR   WKREG,RBUF1                                         0443 00261000
*       RCTR=EIGHT;                 /* SMALLER THERE MAY BE          */ 00262000
         LA    RCTR,8                                              0444 00263000
*LOOP:                              /* EXTRA BITS ON IN THE AREA     */ 00264000
*       WKREG->MAPC='00'X;          /* WHERE THE GROUP ZERO REGS   0445 00265000
*                                                            @Y17XARL*/ 00266000
LOOP     MVI   MAPC(WKREG),X'00'                                   0445 00267000
*       WKREG=WKREG+FOUR;           /* WERE STORED. THESE BITS       */ 00268000
         AL    WKREG,@CF00059                                      0446 00269000
*       GEN(BCT RCTR,LOOP);         /* MUST BE TURNED OFF            */ 00270000
         BCT RCTR,LOOP                                                  00271000
*       RFY                                                        0448 00272000
*         RCTR UNRSTD;              /*                       @Y17XARL*/ 00273000
*     END;                                                         0449 00274000
*IOLOOP:                                                           0450 00275000
*   CPADATA2=RBUF2;                 /* UPDATE BUFFER POINTERS      0450 00276000
*                                                            @Y17XARL*/ 00277000
@RF00440 DS    0H                                                  0450 00278000
IOLOOP   STCM  RBUF2,7,CPADATA2(RLCB)                              0450 00279000
*   RBUF2=RBUF1;                    /* RBUF2 IS NOW FILLED BUFFER    */ 00280000

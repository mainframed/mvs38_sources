         USING @PSTART,@12                                         0001 00012000
         PRINT OFF                                                      00013000
         PRINT ON                                                       00014000
         USING @DATD,DATREG                                             00015000
*   DATREG=ADDR(MLCMHDAT);                                         0037 00016000
         LA    DATREG,MLCMHDAT(,MLCAPTR)                           0037 00017000
*   SCANPTR=PARMR;                                                 0038 00018000
         LR    SCANPTR,PARMR                                       0038 00019000
*   PARMSAVE=PARMR;                 /* SAVE PARM REG                 */ 00020000
         LR    PARMSAVE,PARMR                                      0039 00021000
*   TABLEPTR=TABPTR;                                               0040 00022000
         LR    TABLEPTR,TABPTR                                     0040 00023000
*   MLCNAME=BLANK;                  /* BLANK MLCNAME                 */ 00024000
         MVI   MLCNAME+1(MLCAPTR),C' '                             0041 00025000
         MVC   MLCNAME+2(6,MLCAPTR),MLCNAME+1(MLCAPTR)             0041 00026000
         MVI   MLCNAME(MLCAPTR),C' '                               0041 00027000
*   MLCMACRO=BLANK;                 /* BLANK MLCMACRO                */ 00028000
         MVI   MLCMACRO+1(MLCAPTR),C' '                            0042 00029000
         MVC   MLCMACRO+2(6,MLCAPTR),MLCMACRO+1(MLCAPTR)           0042 00030000
         MVI   MLCMACRO(MLCAPTR),C' '                              0042 00031000
*   NAMELEN=0;                      /* SET CTR TO 0                  */ 00032000
         SLR   NAMELEN,NAMELEN                                     0043 00033000
*/********************************************************************/ 00034000
*/* IF THERE IS NO NAME ON THE MACRO SET UP THE MLCNAME AS BLANKS    */ 00035000
*/* OTHERWISE MOVE IN THE NAME FROM THE MACRO. HTIS NAME WILL BE USED*/ 00036000
*/* TO TELL THAT THIS MACRO CAUSED AN ERROR IF ONE IS DETECTED       */ 00037000
*/********************************************************************/ 00038000
*                                                                  0044 00039000
*   IF CHARCOMP^=BLANK THEN         /* 1ST BUFFER CHAR A BLANK?      */ 00040000
         CLI   CHARCOMP(SCANPTR),C' '                              0044 00041000
         BE    @RF00044                                            0044 00042000
*     DO;                                                          0045 00043000
*       DO WHILE CHARCOMP^=BLANK;   /* NO,FIND BLANK & COUNT CHAR    */ 00044000
         B     @DE00046                                            0046 00045000
@DL00046 DS    0H                                                  0047 00046000
*         NAMELEN=NAMELEN+1;        /* COUNT CHARACTERS              */ 00047000
         LA    @08,1                                               0047 00048000
         ALR   NAMELEN,@08                                         0047 00049000
*         SCANPTR=SCANPTR+1;        /* STEP UP PTR                   */ 00050000
         ALR   SCANPTR,@08                                         0048 00051000
*       END;                                                       0049 00052000
@DE00046 CLI   CHARCOMP(SCANPTR),C' '                              0049 00053000
         BNE   @DL00046                                            0049 00054000
*       SCANPTR=PARMSAVE;           /* RESTORE POINTER               */ 00055000
         LR    SCANPTR,PARMSAVE                                    0050 00056000
*       IF NAMELEN<=8 THEN          /* NAME LONGER THAN 8            */ 00057000
         CH    NAMELEN,@CH00044                                    0051 00058000
         BH    @RF00051                                            0051 00059000
*         DO;                                                      0052 00060000
*           MLCNAME(1:NAMELEN)=TEXTBUF(1:NAMELEN);/* MOVE IN NAME    */ 00061000
         LR    @07,NAMELEN                                         0053 00062000
         BCTR  @07,0                                               0053 00063000
         EX    @07,@SM00222                                        0053 00064000
*           PARMSAVE=PARMR;         /* SAVE REGS 1 & 2 OVER TRT      */ 00065000
         LR    PARMSAVE,PARMR                                      0054 00066000
*           TWOSAVE=R2;                                            0055 00067000
         LR    TWOSAVE,R2                                          0055 00068000
*           PARMR=0;                /* SET REGS 1 & 2 TO 0           */ 00069000
         SLR   PARMR,PARMR                                         0056 00070000
*           R2=0;                                                  0057 00071000
         SLR   R2,R2                                               0057 00072000
*           TRT(CHARCOMP,TTABLE1);  /* TRT FIRST CHARACTER           */ 00073000
         TRT   CHARCOMP(1,SCANPTR),TTABLE1                         0058 00074000
*           IF PARMR=0 THEN         /* FIRST CHAR VALID?             */ 00075000
         LTR   PARMR,PARMR                                         0059 00076000
         BNZ   @RF00059                                            0059 00077000
*             DO;                                                  0060 00078000
*               SCANPTR=SCANPTR+1;  /* YES, ADVANCE PTR              */ 00079000
         AH    SCANPTR,@CH00063                                    0061 00080000
*               TRT(TEXTBUF,TTABLE2);/* TRT REST OF NAME             */ 00081000
         TRT   TEXTBUF(256,SCANPTR),TTABLE2                        0062 00082000
*               IF R2^=1 THEN       /* TRT STOPPED BY BLANK?         */ 00083000
         CH    R2,@CH00063                                         0063 00084000
         BE    @RF00063                                            0063 00085000
*                 DO;                                              0064 00086000
*                   RCSAVE=8;       /* NO, ERROR CONDITION           */ 00087000
         LA    RCSAVE,8                                            0065 00088000
*                 END;                                             0066 00089000
*               ELSE                /* R2 = 1, GO ON                 */ 00090000
*                 RCSAVE=0;         /* INDICATE NO ERROR             */ 00091000
         B     @RC00063                                            0067 00092000
@RF00063 SLR   RCSAVE,RCSAVE                                       0067 00093000
*             END;                                                 0068 00094000
*           ELSE                                                   0069 00095000
*             RCSAVE=8;             /* PARMR NOT = 0                 */ 00096000
         B     @RC00059                                            0069 00097000
@RF00059 LA    RCSAVE,8                                            0069 00098000
*         END;                                                     0070 00099000
*       ELSE                        /* NAME > 8                      */ 00100000
*         DO;                                                      0071 00101000
         B     @RC00051                                            0071 00102000
@RF00051 DS    0H                                                  0072 00103000
*           MLCNAME(1:8)=TEXTBUF(1:8);/* MOVE IN 1ST 8 CHAR          */ 00104000
         MVC   MLCNAME(8,MLCAPTR),TEXTBUF(SCANPTR)                 0072 00105000
*           RCSAVE=8;               /* ERROR CONDITION               */ 00106000
         LA    RCSAVE,8                                            0073 00107000
*         END;                                                     0074 00108000
*     END;                                                         0075 00109000
*   ELSE                            /* FIRST CHAR WAS BLANK          */ 00110000
*     DO;                                                          0076 00111000
         B     @RC00044                                            0076 00112000
@RF00044 DS    0H                                                  0077 00113000
*       RCSAVE=0;                   /* SIGNIFY NO ERROR              */ 00114000
         SLR   RCSAVE,RCSAVE                                       0077 00115000
*     END;                                                         0078 00116000
*   IF RCSAVE=0 THEN                                               0079 00117000
@RC00044 LTR   RCSAVE,RCSAVE                                       0079 00118000
         BNZ   @RF00079                                            0079 00119000
*     DO;                                                          0080 00120000
*                                                                  0080 00121000
*/********************************************************************/ 00122000
*/* NOW SCAN THE INPUT FOR A MACRO NAME.  WHEN IT IS FOUND BEGIN A   */ 00123000
*/* SCAN OF THE MACRO NAME TABLE AND COMPARE THE NAME OF THE MACRO   */ 00124000
*/* THAT IS ON THE INPUT TO THE NAME IN THE TABLE IF THE NAMES ARE   */ 00125000
*/* NOT THE SAME CONTINUE. IF THE NAME IS FOUND THEN PICK UP THE     */ 00126000
*/* ADDRESS OF THE ROUTINE ASSOCIATED WITH THE MACRO AND PUT IT IN   */ 00127000
*/* R0 TO RETURN TO THE CALLER                                       */ 00128000
*/********************************************************************/ 00129000
*                                                                  0081 00130000
*       SCANPTR=PARMSAVE+NAMELEN+1; /* RESET PTR                     */ 00131000
         LR    SCANPTR,PARMSAVE                                    0081 00132000
         ALR   SCANPTR,NAMELEN                                     0081 00133000

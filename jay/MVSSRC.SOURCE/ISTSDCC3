*         DO;                                                      0083 00138000
*           MACLEN=0;               /* ZERO OUT CTR                  */ 00139000
         SLR   MACLEN,MACLEN                                       0084 00140000
*           DO WHILE CHARCOMP^=BLANK;/* SCAN FOR BLANKS              */ 00141000
         B     @DE00085                                            0085 00142000
@DL00085 DS    0H                                                  0086 00143000
*             MACLEN=MACLEN+1;      /* KEEP COUNT OF CHAR            */ 00144000

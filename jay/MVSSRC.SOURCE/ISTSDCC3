         B     @DE00096                                            0096 00173000
@DL00096 DS    0H                                                  0097 00174000
*                 IF MLCMACRO=MHTMACRO THEN                        0097 00175000
         CLC   MLCMACRO(8,MLCAPTR),MHTMACRO(MHTEPTR)               0097 00176000
         BNE   @RF00097                                            0097 00177000
*                   DO;                                            0098 00178000
*                     TABPTR=MHTVALUE;/* PUT PTR TO MACRO IN R0      */ 00179000
         L     TABPTR,MHTVALUE(,MHTEPTR)                           0099 00180000
*                     RCSAVE=0;     /* INDICATE NO ERROR             */ 00181000
         SLR   RCSAVE,RCSAVE                                       0100 00182000
*                   END;                                           0101 00183000
*                 ELSE              /* MLCMACRO^=MHTMACRO,GO ON      */ 00184000
*                   DO;                                            0102 00185000
         B     @RC00097                                            0102 00186000
@RF00097 DS    0H                                                  0103 00187000
*                     ENTCOUNT=ENTCOUNT-1;/* STEP DOWN CTR           */ 00188000
         BCTR  ENTCOUNT,0                                          0103 00189000
*                     MHTEPTR=MHTEPTR+12;/* STEP UP PTR              */ 00190000
         AH    MHTEPTR,@CH00194                                    0104 00191000
*                   END;                                           0105 00192000
*               END;                                               0106 00193000
@RC00097 DS    0H                                                  0106 00194000
@DE00096 LTR   ENTCOUNT,ENTCOUNT                                   0106 00195000
         BZ    @DC00096                                            0106 00196000
         CH    RCSAVE,@CH00065                                     0106 00197000
         BE    @DL00096                                            0106 00198000
@DC00096 DS    0H                                                  0107 00199000
*             END;                                                 0107 00200000
*           ELSE                    /* MACLEN > 8                    */ 00201000
*             DO;                                                  0108 00202000
         B     @RC00090                                            0108 00203000
@RF00090 DS    0H                                                  0109 00204000
*               MLCMACRO(1:8)=MACOP(1:8);/* MOVE IN FIRST 8 CHAR     */ 00205000

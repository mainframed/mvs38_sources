         TITLE 'IFCEVENT-EVENT HISTORY PROCESSING                      *00001000
                        '                                               00002000
IFCEVENT CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IFCEVENT  78.251'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  ST    @14,12(,@13)                                        0001 00010000
         STM   @00,@12,20(@13)                                     0001 00011000
         BALR  @10,0                                               0001 00012000
@PSTART  LA    @11,4095(,@10)                                      0001 00013000
         LA    @12,4095(,@11)                                      0001 00014000
         USING @PSTART,@10                                         0001 00015000
         USING @PSTART+4095,@11                                    0001 00016000
         USING @PSTART+8190,@12                                    0001 00017000
         ST    @13,@SA00001+4                                      0001 00018000
         LA    @14,@SA00001                                        0001 00019000
         ST    @14,8(,@13)                                         0001 00020000
         LR    @13,@14                                             0001 00021000
         B     @PB00002                                            0198 00022000
TEMPLATE STM   @14,@12,@SA00002                                    0198 00023000
*  SPACING = UPSPACE;                                      /*@Z427VLS*/ 00024000
         MVC   SPACING(1),UPSPACE                                  0199 00025000
*  CALL PRINTIT;                                           /*@Z427VLS*/ 00026000
         BAL   @14,PRINTIT                                         0200 00027000
*  SPACING = DOUBLE;                                       /*@Z427VLS*/ 00028000
         MVC   SPACING(1),DOUBLE                                   0201 00029000
*  TOPTEMP = LINE01;             /*MOVE THE TOP LINE OF TEMPLATE IN  */ 00030000
         MVC   TOPTEMP(23),LINE01                                  0202 00031000
*  CALL PRINTIT;                 /*PRINT OUT THE HEADING FOR TEMPLATE*/ 00032000
         BAL   @14,PRINTIT                                         0203 00033000
*  DO N = 1 BY 1 TO 9;           /*PRINT 9 PAIR OF LINES     @Z427VLS*/ 00034000
         LA    @06,1                                               0204 00035000
         ST    @06,N                                               0204 00036000
@DL00204 DS    0H                                                  0205 00037000
*    GEN(MVI  TIME1,X'40');                                /*@Z427VLS*/ 00038000
         MVI  TIME1,X'40'                                               00039000
*    GEN(MVC  TIME1+1(130),TIME1);                         /*@Z427VLS*/ 00040000
         MVC  TIME1+1(130),TIME1                                        00041000
*    SPACING = SINGLE;                                     /*@Z427VLS*/ 00042000
         MVC   SPACING(1),SINGLE                                   0207 00043000
*    CALL PRINTIT;               /*PRINT SEPT LINE           @Z427VLS*/ 00044000
         BAL   @14,PRINTIT                                         0208 00045000
*    LINE002 = LINE02;           /*MOVE COMMON PART OF LINE IN       */ 00046000
         MVC   LINE002(20),LINE02                                  0209 00047000
*    R8 = ADDRTAB1(N);           /*WHERE THE DATA IS AT      @Z427VLS*/ 00048000
         L     @06,N                                               0210 00049000
         SLA   @06,2                                               0210 00050000
         L     R8,ADDRTAB1-4(@06)                                  0210 00051000
*    R9 = NUMCHAR1(N);           /*NUMBER OF BYTES TO MOVE   @Z427VLS*/ 00052000
         L     R9,NUMCHAR1-4(@06)                                  0211 00053000
*    GEN(EX   R9,EXECMVC);       /*EXEC THE MOVE CHAR        @Z427VLS*/ 00054000
         EX   R9,EXECMVC                                                00055000
*    SPACING = DOUBLE;                                     /*@Z427VLS*/ 00056000
         MVC   SPACING(1),DOUBLE                                   0213 00057000
*    CALL PRINTIT;               /*GO PRINT THE DATA         @Z427VLS*/ 00058000
         BAL   @14,PRINTIT                                         0214 00059000
*    DUPLICAT  = LINE04;         /*MOVE IN THE COMMON PART OF MESSAGE*/ 00060000
         MVC   DUPLICAT(33),LINE04                                 0215 00061000
*    R8 = ADDRTAB2(N);           /*SET UP WHERE THE DATA IS  @Z427VLS*/ 00062000
         L     @06,N                                               0216 00063000
         SLA   @06,2                                               0216 00064000
         L     R8,ADDRTAB2-4(@06)                                  0216 00065000
*    R9 = NUMCHAR2(N);           /*NUMBER OF BYTES TO MOVE   @Z427VLS*/ 00066000
         L     R9,NUMCHAR2-4(@06)                                  0217 00067000
*    GEN(EX    R9,EXECMVC2);     /*EXEC THE MVC INSTRUCTION  @Z427VLS*/ 00068000
         EX    R9,EXECMVC2                                              00069000
*    SPACING = DOUBLE;           /*SET TO SINGLE SPACE ON THIS LINE  */ 00070000
         MVC   SPACING(1),DOUBLE                                   0219 00071000
*    CALL PRINTIT;               /*GO RPINT THE BUFFER       @Z427VLS*/ 00072000
         BAL   @14,PRINTIT                                         0220 00073000
*   END;                                                   /*@Z427VLS*/ 00074000
         LA    @06,1                                               0221 00075000
         AL    @06,N                                               0221 00076000
         ST    @06,N                                               0221 00077000
         C     @06,@CF00085                                        0221 00078000

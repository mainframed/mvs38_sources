*/* * * * * * * * * * * * * * * * * * * * * * *  * * * * * * * * * * */ 00093000
*/*    THIS PROC WILL CONVERT THE BINARY NUMBER IN ITEM BINWORD TO   */ 00094000
*/*    PRINTABLE IF THE NUMBER (CONVERTED) IS > THAN THE NUMBER OF   */ 00095000
*/*    CHARACTERS REQUESTED (IN ITEM BNDECNUM) A NINE WILL BE PLACED */ 00096000
*/*    IN EACH PRINT POSITION                                @Z427VLS*/ 00097000
*/*                                                          @Z427VLS*/ 00098000
*CONVBIN:  PROC;                                           /*@Z427VLS*/ 00099000
CONVBIN  STM   @14,@12,@SA00003                                    0227 00100000
* R8 = BINWORD;                  /*LOAD THE ITEM TO CONVERT  @Z427VLS*/ 00101000
         L     R8,BINWORD                                          0228 00102000
* GEN(CVD   R8,DBLWORD);         /*CONVERT THE NUMBER        @Z427VLS*/ 00103000
         CVD   R8,DBLWORD                                               00104000
* GEN(UNPK   WORK(8),DBLWORD);   /*UNPACK THE CONVERTED NUMBER       */ 00105000
         UNPK   WORK(8),DBLWORD                                         00106000
* GEN(OI    WORK+7,X'F0');       /*INSURE SIGN IS NO PROBLEM @Z427VLS*/ 00107000
         OI    WORK+7,X'F0'                                             00108000
* SWITCH2 = OFF;                 /*CLEAR THE SWITCH PRIOR TO START   */ 00109000
         NI    SWITCH2,B'10111111'                                 0232 00110000
* DO L = 1 BY 1 TO 7 WHILE (SWITCH2 = OFF);                /*@Z427VLS*/ 00111000
         LA    @06,1                                               0233 00112000
         ST    @06,L                                               0233 00113000
@DL00233 TM    SWITCH2,B'01000000'                                 0233 00114000
         BNZ   @DC00233                                            0233 00115000
*  IF WORKOVER(L)  = 'F0'X       /*IS IT A 0 IF SO MAKE IT A BLANK   */ 00116000
*    THEN                                                          0234 00117000
         LA    @03,WORKOVER-1(@06)                                 0234 00118000
         CLI   0(@03),X'F0'                                        0234 00119000
         BNE   @RF00234                                            0234 00120000
*      WORKOVER(L) = '40'X;      /*SET TO A BLANK            @Z427VLS*/ 00121000
         LA    @03,WORKOVER-1(@06)                                 0235 00122000
         MVI   0(@03),X'40'                                        0235 00123000
*    ELSE                                                          0236 00124000
*      SWITCH2 = ON;             /*SET SWITCH TO TERMINATE DO LOOP   */ 00125000
         B     @RC00234                                            0236 00126000
@RF00234 OI    SWITCH2,B'01000000'                                 0236 00127000
* END;                                                     /*@Z427VLS*/ 00128000
@RC00234 LA    @06,1                                               0237 00129000
         AL    @06,L                                               0237 00130000
         ST    @06,L                                               0237 00131000
         C     @06,@CF00082                                        0237 00132000
         BNH   @DL00233                                            0237 00133000
@DC00233 DS    0H                                                  0238 00134000
* IF (TEN   - L) > BNDECNUM      /*IS THE CONVERTED NUMBER TO LARGE  */ 00135000
*    THEN                                                          0238 00136000
         L     @06,TEN                                             0238 00137000
         SL    @06,L                                               0238 00138000
         C     @06,BNDECNUM                                        0238 00139000
         BNH   @RF00238                                            0238 00140000
*      DO;                                                 /*@Z427VLS*/ 00141000
*        GEN(MVI   WORKOVER,X'F9');  /*SET FIRST NINE IN THE MEASSAGE*/ 00142000
         MVI   WORKOVER,X'F9'                                           00143000
*        GEN(MVC   WORKOVER+1(7),WORKOVER);/*FILL WITH NINES @Z427VLS*/ 00144000
         MVC   WORKOVER+1(7),WORKOVER                                   00145000
*      END;                                                /*@Z427VLS*/ 00146000
* R8 = ADDR(WORKOVER) + (8-BNDECNUM);/*SET WHERE TO MOVE FROM ADDRESS*/ 00147000
@RF00238 LA    R8,WORKOVER                                         0243 00148000
         L     @06,BNDECNUM                                        0243 00149000
         LA    @03,8                                               0243 00150000
         SLR   @03,@06                                             0243 00151000
         ALR   R8,@03                                              0243 00152000
* R9 = BNDECNUM - ONE;           /*NUMBER OF BYTES TO MOVE   @Z427VLS*/ 00153000
         SL    @06,ONE                                             0244 00154000
         LR    R9,@06                                              0244 00155000
* RESPECIFY (R6) RESTRICTED;                               /*@Z427VLS*/ 00156000
* R6 = DECADDR;                  /*ADDRESS TO MOVE TO        @Z427VLS*/ 00157000
         L     R6,DECADDR                                          0246 00158000
* GEN(EX    R9,MVCDATA);         /*MOVE DATA TO REQUESTED LOCATION   */ 00159000
         EX    R9,MVCDATA                                               00160000
* GENERATE DATA;                                           /*@Z427VLS*/ 00161000
* RESPECIFY (R6) UNRESTRICTED;                             /*@Z427VLS*/ 00162000
*END;                                                      /*@Z427VLS*/ 00163000
@EL00003 DS    0H                                                  0250 00164000
@EF00003 DS    0H                                                  0250 00165000
@ER00003 LM    @14,@12,@SA00003                                    0250 00166000
         BR    @14                                                 0250 00167000
*/********************************************************************/ 00168000
*/* * * * * * * * * * * * * * * * * * * * * * *  * * * * * * * * * * */ 00169000
*/*    THIS PROC WILL CONVERT THE HEX DATA INTO PRINTABLE    @Z427VLS*/ 00170000
*/*                                                          @Z427VLS*/ 00171000
*/*    IF IT IS NECESSARY TO CONVERT ONLY 1/2 OF FIRST BYTE(CUA FOR  */ 00172000
*/*    EXP. THEN THE USER SHOULD SET THE SWITCH1 TO THEON CONDITION  */ 00173000
*CONVERT: PROC;                                            /*@Z427VLS*/ 00174000
CONVERT  STM   @14,@12,12(@13)                                     0251 00175000
* DO P = NUMBER BY -1 TO 1;      /*# = # OF BYTES TO CONVERT TO PRINT*/ 00176000
         L     @06,NUMBER                                          0252 00177000
         ST    @06,P                                               0252 00178000
         B     @DE00252                                            0252 00179000
@DL00252 DS    0H                                                  0253 00180000
*   R8 = NEXTBYTE(P);            /*LOAD THE NEXT BYTE TO CONVERT     */ 00181000
         L     @03,NEXTBY                                          0253 00182000
         BCTR  @03,0                                               0253 00183000
         SLR   R8,R8                                               0253 00184000
         IC    R8,NEXTBYTE(@06,@03)                                0253 00185000
*   X =  TWO * P;                /*OUTPUT INDEX MUST BE TWICE INPUT  */ 00186000
         L     @01,TWO                                             0254 00187000
         MR    @00,@06                                             0254 00188000
         ST    @01,X                                               0254 00189000
*   SRDL  (R8,FOUR);             /*MOVE 1/2 IN TO NEXT REG   @Z427VLS*/ 00190000
         SRDL  R8,4                                                0255 00191000
*   SRL   (R9,TWOEIGHT);         /*SHIFT THE REG 28 BITS     @Z427VLS*/ 00192000
         L     @03,TWOEIGHT                                        0256 00193000
         SRL   R9,0(@03)                                           0256 00194000
*   R9  =  R9 + NUMBER1;         /*INSURE INDEX BY 0 DOES NOT AFFECT */ 00195000
         L     @03,NUMBER1                                         0257 00196000
         ALR   R9,@03                                              0257 00197000
*   R8  =  R8 + NUMBER1;         /*SAME                      @Z427VLS*/ 00198000
         ALR   R8,@03                                              0258 00199000
*   IF SWITCH1 = ON & P = ONE    /*DO WE NOT PUT THE HALF BYTE OUT   */ 00200000
*    THEN                                                          0259 00201000
         TM    SWITCH1,B'10000000'                                 0259 00202000
         BNO   @RF00259                                            0259 00203000
         C     @06,ONE                                             0259 00204000
         BNE   @RF00259                                            0259 00205000
*     SWITCH1 = OFF;             /*TURN OFF THE SWITCH       @Z427VLS*/ 00206000
         NI    SWITCH1,B'01111111'                                 0260 00207000
*    ELSE                                                          0261 00208000
*     ADDNUM(X-ONE) = CHARS(R8);/*PUT THE PRINT CHAR INTO THE OUTPUT*/  00209000
         B     @RC00259                                            0261 00210000
@RF00259 L     @06,X                                               0261 00211000
         SL    @06,ONE                                             0261 00212000
         L     @03,ADDNXT                                          0261 00213000
         ALR   @03,@06                                             0261 00214000
         BCTR  @03,0                                               0261 00215000
         LA    @06,CHARS-1(R8)                                     0261 00216000
         MVC   ADDNUM(1,@03),0(@06)                                0261 00217000
*     ADDNUM(X)  = CHARS(R9);   /*FINSH THE BYTE             @Z427VLS*/ 00218000
@RC00259 L     @06,ADDNXT                                          0262 00219000
         AL    @06,X                                               0262 00220000
         BCTR  @06,0                                               0262 00221000
         LA    @03,CHARS-1(R9)                                     0262 00222000
         MVC   ADDNUM(1,@06),0(@03)                                0262 00223000
* END;                                                     /*@Z427VLS*/ 00224000
         SLR   @06,@06                                             0263 00225000
         BCTR  @06,0                                               0263 00226000
         AL    @06,P                                               0263 00227000
         ST    @06,P                                               0263 00228000
@DE00252 LTR   @06,@06                                             0263 00229000
         BP    @DL00252                                            0263 00230000
*END;                                                      /*@Z427VLS*/ 00231000
@EL00004 DS    0H                                                  0264 00232000
@EF00004 DS    0H                                                  0264 00233000
@ER00004 LM    @14,@12,12(@13)                                     0264 00234000
         BR    @14                                                 0264 00235000
*/********************************************************************/ 00236000
*/* * * * * * * * * * * * * * * * * * * * * * *  * * * * * * * * * * */ 00237000
*/*    THIS PROC WILL ADD TOTALS OF EACH TYPE RECORD AND PRINT THE   */ 00238000
*/*    TOTALS                                                @Z427VLS*/ 00239000
*/*    THIS PROC IS BASED ON THERE BEING 23 MINOR TYPES OF RECORDS   */ 00240000
*/*    (UNDER THE 9 MAIN) ANY CHANGE IN THE NUMBER OF TYPES WILL     */ 00241000
*/*    RESULT IN BAD THINGS                                  @Z427VLS*/ 00242000
*/*    ALL 23 LENGHT ITEMS WILL JUST NEED THERE LENGTH ADJUSTED ALSO */ 00243000
*/*    THE TYPES WILL NEED ADJUSTING AND THE TYPE COUNTERS THESE ARE */ 00244000
*/*    NOT IN A TABLE AND START AFTER ITEM NUMCHAR2          @Z427VLS*/ 00245000
*/*                                                          @Z427VLS*/ 00246000
*PRTTOTAL:  PROC;                                          /*@Z427VLS*/ 00247000
PRTTOTAL STM   @14,@12,@SA00005                                    0265 00248000
* SPACING = UPSPACE;             /*START ON A FRESH PAGE     @Z427VLS*/ 00249000
         MVC   SPACING(1),UPSPACE                                  0266 00250000
* CALL PRINTIT;                                            /*@Z427VLS*/ 00251000
         BAL   @14,PRINTIT                                         0267 00252000
*  SWITCH5 = ON;    /*SET TO UP SPACE DURING TOTALS IF REQ   @0001VLS*/ 00253000
         OI    SWITCH5,B'00001000'                                 0268 00254000
* SPACING = DOUBLE;              /*RESET TO SINGLE SPACING   @Z427VLS*/ 00255000
         MVC   SPACING(1),DOUBLE                                   0269 00256000
* Z = ONE;                       /*SET INDEX TO STEP THROUGH @Z427VLS*/ 00257000
         L     @06,ONE                                             0270 00258000
         ST    @06,Z                                               0270 00259000
* TOTALHD2 = TOTALHD1;           /*MOVE IN WORDS RECORD TYPES        */ 00260000
         MVC   TOTALHD2(12),TOTALHD1                               0271 00261000
* RESPECIFY (R6) RESTRICTED;                               /*@Z427VLS*/ 00262000
* R6 = ADDR(TOTALHD3) ;                                    /*@Z427VLS*/ 00263000
         LA    R6,TOTALHD3                                         0273 00264000
* R8 = ADDR(TIME1) + 26;                                   /*@Z427VLS*/ 00265000
         LA    R8,TIME1                                            0274 00266000
         AL    R8,@CF01065                                         0274 00267000
* R9 = 7     + SIX * EVNUM;                                /*@Z427VLS*/ 00268000
         L     @03,EVPTR(,R2)                                      0275 00269000
         L     R9,SIX                                              0275 00270000
         MH    R9,EVNUM(,@03)                                      0275 00271000
         AL    R9,@CF00082                                         0275 00272000
* GEN(EX  R9,MOVECHAR);                                    /*@Z427VLS*/ 00273000
         EX  R9,MOVECHAR                                                00274000
* RESPECIFY (R6) UNRESTRICTED;                             /*@Z427VLS*/ 00275000
* CALL PRINTIT;                  /*PRINT THE HEADING FOR THE TOTALS  */ 00276000
         BAL   @14,PRINTIT                                         0278 00277000
* SPACING = SINGLE;              /*PRINT THE HEADING FOR THE TOTALS  */ 00278000
         MVC   SPACING(1),SINGLE                                   0279 00279000
* DO J = 1 BY 1 TO 9 ;           /*MAIN LOOP FOR 9 TYPES OF RECORDS  */ 00280000
         LA    @06,1                                               0280 00281000
         ST    @06,J                                               0280 00282000
@DL00280 DS    0H                                                  0281 00283000
*  DO  WHILE(LETTER(Z) = LETTER1);                         /*@Z427VLS*/ 00284000
         B     @DE00281                                            0281 00285000
@DL00281 DS    0H                                                  0282 00286000
*   BNDECNUM = SIX;              /*SET FOR SIX CHARACTERS OUT        */ 00287000
         L     @06,SIX                                             0282 00288000
         ST    @06,BNDECNUM                                        0282 00289000
*   TOTALNME = NMETOTAL(Z);      /*WILL MOVE IN 7 CHAR SUBNAME       */ 00290000
         L     @06,Z                                               0283 00291000
         MH    @06,@CH00082                                        0283 00292000
         LA    @03,NMETOTAL-7(@06)                                 0283 00293000
         MVC   TOTALNME(7),0(@03)                                  0283 00294000
*   BINWORD = ZERO;              /*SET TO 0 PRIOR TO START   @Z427VLS*/ 00295000
         SLR   @06,@06                                             0284 00296000
         ST    @06,BINWORD                                         0284 00297000
*   DO K = 1 BY 1 TO EVNUM;   /*EVNUM# OF CPUS FOUND THIS JOB  */  0285 00298000
         LA    @06,1                                               0285 00299000
         ST    @06,K                                               0285 00300000
         B     @DE00285                                            0285 00301000
@DL00285 DS    0H                                                  0286 00302000
*    BINWORD = COUNT(Z,K) + BINWORD;/*ADD UP THE TOTAL THIS SUBTYPE  */ 00303000
         L     @03,Z                                               0286 00304000
         SLA   @03,4                                               0286 00305000
         ALR   @03,@06                                             0286 00306000
         SLA   @03,2                                               0286 00307000
         L     @03,COUNT-68(@03)                                   0286 00308000
         AL    @03,BINWORD                                         0286 00309000
         ST    @03,BINWORD                                         0286 00310000
*   END;                                                   /*@Z427VLS*/ 00311000
         AL    @06,@CF00054                                        0287 00312000
         ST    @06,K                                               0287 00313000
@DE00285 L     @03,EVPTR(,R2)                                      0287 00314000
         CH    @06,EVNUM(,@03)                                     0287 00315000
         BNH   @DL00285                                            0287 00316000
*   TOTAL = TOTAL + BINWORD;     /*SAVE TO HAVE THE TOTAL OF THIS LNE*/ 00317000
         L     @06,BINWORD                                         0288 00318000
         L     @03,TOTAL                                           0288 00319000
         ALR   @03,@06                                             0288 00320000
         ST    @03,TOTAL                                           0288 00321000
*   TOTOFTOT = TOTOFTOT + BINWORD;                         /*@Z427VLS*/ 00322000
         AL    @06,TOTOFTOT                                        0289 00323000
         ST    @06,TOTOFTOT                                        0289 00324000
*   DECADDR = ADDR(TOTALVAL);    /*SET WHERE TO PUT CONVERTED VALUE  */ 00325000
         LA    @06,TOTALVAL                                        0290 00326000
         ST    @06,DECADDR                                         0290 00327000
*   CALL CONVBIN;                /*GO CONVERT THE ITEM TO PRINTABLE  */ 00328000
         BAL   @14,CONVBIN                                         0291 00329000
*   BNDECNUM = FIVE;             /*CHANGE INDIVIDUAL ITEMS TO 5 CHAR */ 00330000
         L     @06,FIVE                                            0292 00331000
         ST    @06,BNDECNUM                                        0292 00332000
*   DECADDR = DECADDR + SEVEN;   /*MOVE DOWN THE PRINT LINE BY 7 CHAR*/ 00333000
         L     @06,DECADDR                                         0293 00334000
         AL    @06,SEVEN                                           0293 00335000
         ST    @06,DECADDR                                         0293 00336000
*   DO K = 1 BY 1 TO EVNUM;   /*SET BY FILTER 4 TO # OF CPUS @Z427VLS*/ 00337000
         LA    @06,1                                               0294 00338000
         ST    @06,K                                               0294 00339000
         B     @DE00294                                            0294 00340000
@DL00294 DS    0H                                                  0295 00341000
*    BINWORD = COUNT(Z,K);       /*PULL UP THE FIRST COUNT   @Z427VLS*/ 00342000
         L     @03,Z                                               0295 00343000
         SLA   @03,4                                               0295 00344000
         ALR   @03,@06                                             0295 00345000
         SLA   @03,2                                               0295 00346000
         L     @03,COUNT-68(@03)                                   0295 00347000
         ST    @03,BINWORD                                         0295 00348000
*    TOTALS(K) = TOTALS(K) + BINWORD;/*CPU TOTALS FOR THIS LINE      */ 00349000
         SLA   @06,2                                               0296 00350000
         L     @15,TOTALS-4(@06)                                   0296 00351000
         ALR   @15,@03                                             0296 00352000
         ST    @15,TOTALS-4(@06)                                   0296 00353000
*   SUBTOT(K) = SUBTOT(K) + BINWORD;/*FOR OVER ALL TOTAL     @Z427VLS*/ 00354000
         AL    @03,SUBTOT-4(@06)                                   0297 00355000
         ST    @03,SUBTOT-4(@06)                                   0297 00356000
*    CALL CONVBIN;               /*GO CONVERT TO PRINTABLE 5 CHARS   */ 00357000
         BAL   @14,CONVBIN                                         0298 00358000
*    DECADDR = DECADDR + SIX;    /*MOVE DOWN PRINT TO NEXT CPU       */ 00359000
         L     @06,DECADDR                                         0299 00360000
         AL    @06,SIX                                             0299 00361000
         ST    @06,DECADDR                                         0299 00362000
*   END;                                                   /*@Z427VLS*/ 00363000
         LA    @06,1                                               0300 00364000
         AL    @06,K                                               0300 00365000
         ST    @06,K                                               0300 00366000
@DE00294 L     @03,EVPTR(,R2)                                      0300 00367000
         CH    @06,EVNUM(,@03)                                     0300 00368000
         BNH   @DL00294                                            0300 00369000
*   CALL PRINTIT;                /*PRINT TOTAL AND INDIVIDULE COUNT  */ 00370000
         BAL   @14,PRINTIT                                         0301 00371000
*   Z = Z + ONE;                 /*MOVE TO THE NEXT SUB CASE @Z427VLS*/ 00372000
         L     @06,Z                                               0302 00373000
         AL    @06,ONE                                             0302 00374000
         ST    @06,Z                                               0302 00375000
*  END;                                                    /*@Z427VLS*/ 00376000
@DE00281 L     @06,Z                                               0303 00377000
         LA    @03,LETTER-1(@06)                                   0303 00378000
         CLC   0(1,@03),LETTER1                                    0303 00379000

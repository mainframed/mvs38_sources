.*                            THIS MACRO GENERATES A NORMAL EDIT ENTRY  43600002
.*                                 FOR PROCESSING BY THE SUMMARY MODULE 43700002
.*                            &LOGITEM - NAME OF DATA ITEM TO BE SUMMED 43800002
.*                            &MASK - BYTE MASK FOR BITS TO BE TESTED.  43900002
.*                                 THIS OPERAND SHOULD BE OMITTED FOR   44000002
.*                                 SINGLE BIT ITEMS IN WHICH CASE THE   44100002
.*                                 MASK WILL BE SET ACCORDING TO THE    44200002
.*                                 OFF BYTE BIT DISPLACEMENT OF THE     44300002
.*                                ITEM AS DEFINED BY THE MACRO 'DSGEN'  44400002
         GBLA  &COUNTR,&ITEMNBR,&BITS(1000),&SHIFT(1000)                44500002
         GBLC  &ITEM(1000),&DSECT                                       44600002
         LCLA  &IBITS,&NDX,&ISHIFT,&M,&BITNBR                           44700002
.LOOP    ANOP                                                           44800002
&NDX     SETA  &NDX+1                                                   44900002
         AIF   ('&ITEM(&NDX)' EQ '&LOGITEM').FOUND                      45000002
         AIF   (&NDX LT &ITEMNBR).LOOP                                  45100002
         MNOTE *,'LOGITEM NOT FOUND'                                    45200002
         MEXIT                                                          45300002
.FOUND   ANOP                                                           45400002
         DC    AL1(0)                                                   45500002
&ISHIFT  SETA  &SHIFT(&NDX)                                             45600002
&IBITS   SETA  &BITS(&NDX)                                              45700002
         AIF   ('&MASK' EQ '').FORM                                     45800002
         DC    B'&MASK'                                                 45900002
         AGO   .ITEM                                                    46000002
.FORM    ANOP                                                           46100002
&BITNBR  SETA  &ISHIFT-7                                                46200002
&BITNBR  SETA  0-&BITNBR                                                46300002
&M       SETA  1                                                        46400002
.LOOP2   ANOP                                                           46500002
         AIF   (&BITNBR EQ 0).GOTIT                                     46600002
&M       SETA  &M*2                                                     46700002
&BITNBR  SETA  &BITNBR-1                                                46800002
         AGO   .LOOP2                                                   46900002
.GOTIT   ANOP                                                           47000002
         DC    AL1(&M)                                                  47100002
.ITEM    DC    AL2(&LOGITEM-&DSECT)                                     47200002
         DC    AL2(&COUNTR)                                             47300002
&COUNTR  SETA  &COUNTR+1                                                47400002
         MEND                                                           47500002
         GBLC  &DSECT,&CSECT                                            47600002
&CSECT   SETC       'IFCEUKNO'                                          47700002
&DSECT   SETC       'COMMON'                                            47800002
IFCEUKNO CSECT                                                          47900002
*                                                                       48000002
*                                                                       48100002
         USING      COMMON,R2                                           48200002
         USING      *,R9                                                48300002
         DC         A(ENTRIES)                                          48400002
         DC         CL8'IFCEUKNO'                                       48500002
R9PTR    DS         2F                                                  48600002
*                                                                       48700002
         EJECT                                                          48800002
*                                                                       48900002
*        COMMON DSECT                                                   49000002
COMMON   DSECT                                                          49100002

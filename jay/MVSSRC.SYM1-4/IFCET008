         MACRO                                                          00210002
&NAME    DSGEN &X1,&X2,&X3,&X4,&X5,&X6,&X7,&X8,&X9,&X10,&X11,&X12,&X13,X00280002
               &X14,&X15,&X16,&X17,&X18,&X19,&X20,&X21,&X22,&X23,&X24, X00350002
               &X25,&X26,&X27,&X28,&X29,&X30,&X31,&X32                  00420002
.*                            THIS MACRO GENERATES ONE DSECT WORD       00490002
.*                                 FORMATTED FOR THE SUBJECT LOG RECORD 00560002
.*                                 EACH OPERAND HAS TWO SUB LISTS. THE  00630002
.*                                 FIRST IS THE NAME OF THE LOG ITEM    00700002
.*                                 WHILE THE SECOND IS THE # OF BITS TO 00770002
.*                                 ALLOCATE FOR THIS ITEM. ITEMS MUST   00840002
.*                                 BE DEFINED CONTIGUOUSLY WITHOUT GAPS 00910002
.*                                 UNUSED BIT GROUPS MUST BE GIVEN      00980002
.*                                 DUMMY NAMES. THE NAMES PROVIDED FOR  01050002
.*                                 THE LOG ITEMS MAY THEN BE USED IN    01120002
.*                                 THE CONVERSION MACRO INSTRUCTIONS.   01190002
         GBLC &ITEM(1000)                                               01260002
         GBLA &ITEMNBR,&BITS(1000),&SHIFT(1000)                         01330002
         LCLC  &SYMBOL                                                  01400002
         LCLA  &OP,&COUNT,&BYTE                                         01470002
.LOOP    ANOP                                                           01540002
&OP      SETA  &OP+1                                                    01610002
&ITEMNBR SETA  &ITEMNBR+1                                               01680002
&SYMBOL  SETC  '&SYSLIST(&OP,1)'                                        01750002
         AIF   ('&SYSLIST(&OP,2)' NE '').OK1                            01820002
         MNOTE *,'NULL BIT COUNT FIELD'                                 01890002
         MEXIT                                                          01960002
.OK1     ANOP                                                           02030002
         AIF   ('&SYMBOL' NE '').OK                                     02100002
         MNOTE 'LOG ITEM SYMBOL NOT PROVIDED'                           02170002
         AGO   .CNT                                                     02240002
.OK      ANOP                                                           02310002
         AIF   ('&SYMBOL'(1,1) EQ '''').CNT                *TEMP*       02380002
&SYMBOL  EQU   *+&BYTE                                                  02450002
&ITEM(&ITEMNBR)  SETC '&SYSLIST(&OP,1)'                                 02520002
&BITS(&ITEMNBR) SETA &SYSLIST(&OP,2)                                    02590002
&SHIFT(&ITEMNBR) SETA &COUNT-&BYTE*8                                    02660002
.CNT     ANOP                                                           02730002
&COUNT   SETA  &COUNT+&SYSLIST(&OP,2)                                   02800002
&BYTE    SETA  &COUNT/8                                                 02870002
         AIF (&OP LT N'&SYSLIST).LOOP                                   02940002
         AIF   (&COUNT NE 32).NOT32                                     03010002
&NAME    DS    4C                                                       03080002
         MEXIT                                                          03150002
.NOT32   AIF   (&COUNT NE 64).ERR                                       03220002
&NAME    DS    8C                                                       03290002
         MEXIT                                                          03360002
.ERR     MNOTE '**BIT COUNT FIELDS DO NOT TOTAL 32 OR 64**'             03430002
         MEND                                                           03500002
         EJECT                                                          03570002
*  LINE MACRO     LINE MACRO     LINE MACRO     LINE MACRO            * 03640002
         SPACE 2                                                        03710002
         MACRO                                                          03780002
&NAME    LINE  &DATA,&LABEL,&SKIP=                                      03850002
.*                            THIS MACRO ESTABLISHES THE MARGINS OR     03920002
.*                                 COLUMN SETTINGS FOR EACH LINE. IT    03990002
.*                                 GENERATES NECESSARY CARRIAGE CONTROL 04060002
.*                                 ENTRIES. LASTLY IT DETERMINES THE    04130002
.*                                 NUMBER OF ENTRIES ASSOCIATED WITH    04200002
.*                                 THE PREVIOUS LINE AND STORES THAT #  04270002
.*                                 INTO THE APPROPRIATE BYTE.           04340002
.*                            &DATA(1) - CHARACTER POSITION TO BEGIN    04410002
.*                                 DATA.                                04480002
.*                            &DATA(2) - # OF SPACES BETWEEN SUCCESSIVE 04550002
.*                                 DATA COLUMN STARTING CHARACTERS.     04620002
.*                            &LABEL(1) - CHAR. POS. TO BEGIN LABELS.   04690002
.*                            &LABEL(2) - # SPACES BETWEEN LINE'S LABEL 04760002
.*                                 STARTING CHARACTERS.                 04830002
.*                            &SKIP= - # OF LINES TO ADVANCE CARRIAGE   04900002
.*                                 TO BEGIN NEXT PRINTED LINE.          04970002
.*                            NEW ENTRIES ARE GENERATED ONLY WHEN       05040002
.*                                 NECESSARY. OMISSION OF ITEMS MEANS   05110002
.*                                 THEY HAVE NOT CHANGED SINCE PREVIOUS 05180002
.*                                 SETTINGS. A LINE MACRO IS REQUIRED   05250002
.*                                 FOR EACH PRINTED LINE. IF NO SETTING 05320002
.*                                 CHANGES ARE REQUIRED ALL OPERANDS    05390002
.*                                 ARE OMITTED IN WHICH CASE ONLY THE   05460002
.*                                 # OF ENTRIES FOR THE PREVIOUS LINE   05530002
.*                                 IS COMPUTED. THIS MACRO MUST BE THE  05600002
.*                                 FIRST MACRO INSTRUCTION ISSUED FOR   05670002
.*                                 THE SUBJECT LINE                     05740002
         GBLA  &DB,&DT,&LB,&LT,&NRENTS                                  05810002
         GBLA  &LASTCC,&INITIAL                                         05880002
         GBLC  &NRENTSX                                                 05950002
         GBLC  &CARRCON                                                 06020002
         GBLB  &TEST                                                    06090002
         LCLA  &S                                                       06160002
&NAME    DS    0C                                                       06230002
         AIF   (&INITIAL EQ 0).FIRSTLN                                  06300002
         LINEND                                                         06370002
         AGO   .NTFIRST                                                 06440002
.FIRSTLN ANOP                                                           06510002
&NRENTSX SETC  'N&SYSNDX'                                               06580002
&INITIAL SETA  1                                                        06650002
N&SYSNDX EQU   *                                                        06720002
.NTFIRST ANOP                                                           06790002
         DC    AL1(0)                                                   06860002
         AIF   ('&DATA(1)' EQ '').CKDTABS                               06930002
         AIF   (&DATA(1) EQ &DB).CKDTABS                                07000002
&DB      SETA  &DATA(1)                                                 07070002
         AGO   .AA                                                      07140002
.CKDTABS AIF   ('&DATA(2)' EQ '').NCDATA                                07210002
         AIF   (&DATA(2) EQ &DT).NCDATA                                 07280002
         AGO   .CC                                                      07350002
.AA      AIF   ('&DATA(2)' EQ '').BB                                    07420002
         AIF   (&DATA(2) EQ &DT).BB                                     07490002
.CC      ANOP                                                           07560002
&DT      SETA  &DATA(2)                                                 07630002
.BB      DC    XL2'FFFE'                                                07700002
         DC    FL1'&DB'                                                 07770002
         DC    FL1'&DT'                                                 07840002
&NRENTS  SETA  &NRENTS+1                                                07910002
.NCDATA  ANOP                                                           07980002
         AIF   ('&LABEL(1)' EQ '').CKLTABS                              08050002
         AIF   (&LABEL(1) EQ &LB).CKLTABS                               08120002
&LB      SETA  &LABEL(1)                                                08190002
         AGO   .AAA                                                     08260002
.CKLTABS AIF   ('&LABEL(2)' EQ '').NCLABEL                              08330002
         AIF   (&LABEL(2) EQ &LT).NCLABEL                               08400002
         AGO   .CCC                                                     08470002
.AAA     AIF   ('&LABEL(2)' EQ '').BBB                                  08540002
         AIF   (&LABEL(2) EQ &LT).BBB                                   08610002
.CCC     ANOP                                                           08680002
&LT      SETA  &LABEL(2)                                                08750002
.BBB     DC    XL2'FFFF'                                                08820002
         DC    FL1'&LB'                                                 08890002
         DC    FL1'&LT'                                                 08960002
&NRENTS  SETA  &NRENTS+1                                                09030002
.NCLABEL ANOP                                                           09100002
         AIF   ('&SKIP' NE '').CONT                                     09170002
         AIF   (&TEST EQ 0).NOCHANG                                     09240002
&S       SETA  &LASTCC                                                  09310002
         AGO   .DEFAULT                                                 09380002
.CONT    ANOP                                                           09450002
         AIF   (&TEST EQ 0).AB                                          09520002
&S       SETA  &SKIP                                                    09590002
.DEFAULT ANOP                                                           09660002
         ORG   &CARRCON+3                                               09730002
         DC    AL.3(0),AL.2(&S),AL.3(1)                                 09800002
         ORG                                                            09870002
&LASTCC  SETA  &S                                                       09940002
&TEST    SETB  0                                                        10010002
         AGO   .DD                                                      10080002
.AB      AIF   (&SKIP EQ &LASTCC).NOCHANG                               10150002
A&SYSNDX DC    XL2'FFFB'                                                10220002
         DC    AL.3(0),AL.2(&SKIP),AL.3(1)                              10290002
         DC    FL1'9'                                                   10360002
&LASTCC  SETA  &SKIP                                                    10430002
&TEST    SETB  1                                                        10500002
&CARRCON SETC  'A&SYSNDX'                                               10570002
&NRENTS  SETA  &NRENTS+1                                                10640002
.DD      ANOP                                                           10710002
.NOCHANG ANOP                                                           10780002
         MEND                                                           10850002
         EJECT                                                          10920002
*  BIN MACRO     BIN MACRO     BIN MACRO     BIN MACRO     BIN MACRO  * 10990002
         SPACE 2                                                        11060002
         MACRO                                                          11130002
         BIN   &LOGITEM,&SKIP$OR                                        11200002
.*                                 &LOGITEM - NAME OF ITEM IN LOG       11270002
.*                                      RECORD DSECT                    11340002
.*                                 &SKIP$OR - NBR OF SPACES TO SKIP ON  11410002
.*                                 MULTIPLE PRINT OR NBR OF SPACES      11480002
.*                                      TO BACK UP FROM COLUMN FOR      11550002
.*                                      START OF PRINTING. THE LETTER   11620002
.*                                      B PRECEEDS THE NUMBER (B5) IF   11690002
.*                                      THE LATTER IS DESIRED. OMISSION 11760002
.*                                      OF THIS OPERAND FORCES OPTION 2 11830002
.*                                      WITH BACKUP SPACING EQUAL TO    11900002
.*                                 THE NUMBER OF BITS IN THE ITEM       11970002
.*                                 DEFINITION (VIA MACRO 'DSGEN').      12040002
.*                            A LIMIT OF 15 BITS IS IMPOSED FOR THE     12110002
.*                                 TOTAL NUMBER OF BITS TO PRINT.       12180002
         GBLC &ITEM(1000)                                               12250002
         GBLA &ITEMNBR,&BITS(1000),&SHIFT(1000),&NRENTS                 12320002
         GBLC  &DSECT                                                   12390002
         LCLB  &B                                                       12460002
         LCLA  &IBITS,&NDX,&ISHIFT,&K                                   12530002
         LCLA  &TEMP,&T                                                 12600002
         LCLC  &S                                                       12670002
.LOOP    ANOP                                                           12740002
&NDX     SETA  &NDX+1                                                   12810002
         AIF   ('&ITEM(&NDX)' EQ '&LOGITEM').FOUND                      12880002
         AIF   (&NDX LT &ITEMNBR).LOOP                                  12950002
         MNOTE *,'YOU BLEW IT'                                          13020002
         ROUTINE DUM,SKIPPER                                            13090002
         MEXIT                                                          13160002
.FOUND   ANOP                                                           13230002
&ISHIFT  SETA  &SHIFT(&NDX)                                             13300002
&IBITS   SETA  &BITS(&NDX)                                              13370002
&T       SETA  &IBITS-1                                                 13440002
&B       SETB  0                                                        13510002
         AIF   ('&SKIP$OR' EQ '').BACKUP                                13580002
&K       SETA  K'&SKIP$OR                                               13650002
&B       SETB  ('&SKIP$OR'(1,1) EQ 'B')                                 13720002
&S       SETC  '&SKIP$OR'(&B+1,&K-&B)                                   13790002
&T       SETA  &S                                                       13860002
.BACKUP  ANOP                                                           13930002
&NRENTS  SETA  &NRENTS+1                                                14000002
&TEMP    SETA  &ISHIFT                                                  14070002
A&SYSNDX DC    AL.12(&LOGITEM-&DSECT),XL.4'&TEMP'                       14140002
         AIF (&IBITS EQ 1 AND &B EQ 0).OUT                              14210002
         ORG   A&SYSNDX                                                 14280002
&B       SETB  1                                                        14350002
         DC    AL.12(&LOGITEM-&DSECT),XL.4'C'                           14420002
         DC    BL.1'&B',AL.7(&T),AL.4(&IBITS),FL.4'&ISHIFT'             14490002
.OUT     ANOP                                                           14560002
         MEND                                                           14630002
         EJECT                                                          14700002
*  HEX MACRO     HEX MACRO     HEX MACRO    HEXN MACRO     HEX MACRO  * 14770002
         SPACE 2                                                        14840002
         MACRO                                                          14910002
         HEX   &LOGITEM,&SKIP$OR,&BYTES                                 14980002
.*                            THIS MACRO FACILITATES GENERATION OF THE  15050002
.*                                 TABLE ENTRIES SPECIFYING HEX TYPE    15120002
.*                                 CONVERSIONS.                         15190002
.*                            &LOGITEM - NAME OF DATA ITEM TO BE        15260002
.*                                 CONVERTED                            15330002
.*                            &SKIP$OR - NUMBER OF SPACES TO SKIP ON    15400002
.*                                 MULTIPLE PRINT OR NBR OF SPACES TO   15470002
.*                                 BACK UP FROM COLUMN FOR START OF     15540002
.*                                 PRINTING. THE LETTER B PRECEEDS THE  15610002
.*                                 NUMBER (B8) IF THE LATTER OPTION IS  15680002
.*                                 DESIRED. OMISSION OF THIS OPERAND    15750002
.*                                 FORCES OPTION 2 WITH BACKUP SPACING  15820002
.*                                 EQUAL TO THE NUMBER OF BYTES SPECI-  15890002
.*                                 FIED BY THE NEXT OPERAND (TWICE THIS 15960002
.*                                 NUMBER IF SUFFIX IS EXPLICITLY OR    16030002
.*                                 IMPLICITLY B).                       16100002
.*                            &BYTES - NUMBER OF TOTAL INPUT BYTES TO   16170002
.*                                 PROCESS. THIS NUMBER IS SUFFIXED     16240002
.*                                 WITH AN L,R, OR B TO STIPULATE       16310002
.*                                 PRINTING OF THE LEFT HALF, RIGHT     16380002
.*                                 HALF, OR BOTH HALVES OF THE BYTE     16450002
.*                                 RESPECTIVELY. OMISSION OF THIS       16520002
.*                                 OPERAND DEFAULTS TO B WITH THE NUM-  16590002
.*                                 BER OF BYTES EQUAL TO THE ITEM LNGTH 16660002
.*                            A NEW ENTRY IS GENERATED ONLY WHEN NECES- 16730002
.*                                 SARY. THIS MACRO MUST BE THE FIRST   16800002
.*                                 MACRO INSTRUCTION ISSUED FOR THE     16870002
.*                                 SUBJECT LINE(S).                     16940002
         GBLC &ITEM(1000),&DSECT                                        17010002
         GBLA &ITEMNBR,&BITS(1000),&SHIFT(1000),&NRENTS                 17080002
         LCLB  &B                                                       17150002
         LCLA  &IBITS,&NDX,&ISHIFT,&K,&T,&CODE,&BYTESP                  17220002
         LCLC  &S,&LETTER                                               17290002
.LOOP    ANOP                                                           17360002
&NDX     SETA  &NDX+1                                                   17430002
         AIF   ('&ITEM(&NDX)' EQ '&LOGITEM').FOUND                      17500002
         AIF   (&NDX LT &ITEMNBR).LOOP                                  17570002
         MNOTE *,'LOG ITEM NOT DEFINED'                                 17640002
         MEXIT                                                          17710002
.FOUND   ANOP                                                           17780002
&ISHIFT  SETA  &SHIFT(&NDX)                                             17850002
&IBITS   SETA  &BITS(&NDX)                                              17920002
&CODE    SETA  1                                                        17990002
&K       SETA  K'&BYTES                                                 18060002
         AIF   (&IBITS-(&IBITS/4)*4 EQ 0).OK                            18130002
         MNOTE *,'BIT LENGTH SPECIFICATION INCONSISTENT WITH CONTROL'   18200002
         MEXIT                                                          18270002
.OK      AIF   ('&BYTES' EQ '').USEDEFN                                 18340002
&LETTER  SETC  '&BYTES'(&K,1)                                           18410002
&S       SETC  '&BYTES'(1,&K-1)                                         18480002
&BYTESP  SETA  &S                                                       18550002
         AIF   ('&LETTER' EQ 'B').BOTH                                  18620002
         AIF   (&ISHIFT EQ 0 OR &ISHIFT EQ 4).OK2                       18690002
.ERR     MNOTE *,'DATA NOT ALLIGNED ON HEX OR BYTE BOUNDARY'            18760002
         MEXIT                                                          18830002
.USEDEFN ANOP                                                           18900002
&BYTESP  SETA  &IBITS/8                                                 18970002
.BOTH    AIF   (&ISHIFT NE 0).ERR                                       19040002
&CODE    SETA  3                                                        19110002
&T       SETA  &BYTESP*2-1                                              19180002
&S       SETC  '&T'                                                     19250002
.OK2     AIF   (&ISHIFT EQ 0).L                                         19320002
&CODE    SETA  2                                                        19390002
.L       ANOP                                                           19460002
&B       SETB  1                                                        19530002
         AIF   ('&SKIP$OR' EQ '').BACKUP                                19600002
&K       SETA  K'&SKIP$OR                                               19670002
&B       SETB  ('&SKIP$OR'(1,1) EQ 'B')                                 19740002
&S       SETC  '&SKIP$OR'(&B+1,&K-&B)                                   19810002
.BACKUP  ANOP                                                           19880002
&NRENTS  SETA  &NRENTS+1                                                19950002
         DC    AL.12(&LOGITEM-&DSECT),XL.4'A'                           20020002
         DC    BL.1'&B',FL.7'&S',FL.4'&BYTESP',FL.4'&CODE'              20090002
.OUT     ANOP                                                           20160002
         MEND                                                           20230002
         SPACE 2                                                        20300002

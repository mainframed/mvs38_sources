         MACRO                                                          20580002
&NAME    LABEL &LABEL1,&LABEL2,&LABEL3,&LABEL4,&LABEL5,&LABEL6,        C20640002
               &LABEL7,&LABEL8                                          20700002
.*                            THIS MACRO ALLOWS UP TO ACHT LABELS TO    20760002
.*                                 BE SPECIFIED FOR A GIVEN LINE. THE   20820002
.*                                 LABELS SHOULD BE ENCLOSED IN QUOTA-  20880002
.*                                 TION MARKS. THE COLUMN AND LINE END  20940002
.*                                 CONTROLS WILL BE INSERTED BY THE     21000002
.*                                 MACRO. OMITTED OPERANDS MEAN NO      21060002
.*                                 LABEL REQUIRED FOR RESPECTIVE        21120002
.*                                 COLUMN. THIS MACRO WILL ALSO ALERT   21180002
.*                                 THE PROGRAMMER TO EXACTLY WHERE THE  21240002
.*                                 TABLE MUST BE BROKEN IF NECESSARY    21300002
.*                                 TO AVOID EXCEEDING THE MAXIMUM LIMIT 21360002
         GBLC &LABELS(2000)                                             21420002
         GBLC  &LBL(100)                                                21480002
         GBLA  &LBLCTR(100)                                             21540002
         GBLA  &LABELNR                                                 21600002
         GBLA  &NRENTS,&LPTR                                            21660002
         LCLC  &MARGIN,&TACK                                            21720002
         LCLC  &TEXT                                                    21780002
         LCLA  &OP,&K,&START,&NRCHAR,&REMAIN                            21840002
&MARGIN  SETC  '*'                                                      21900002
         AIF   ('&NAME' EQ '').LOOPOUT                                  21960002
&LPTR    SETA  &LPTR+1                                                  22020002
&LBL(&LPTR) SETC '&NAME'                                                22080002
&LBLCTR(&LPTR) SETA &LABELNR+1                                          22140002
.LOOPOUT ANOP                                                           22200002
&OP      SETA  &OP+1                                                    22260002
&K       SETA  K'&SYSLIST(&OP)                                          22320002
         AIF (&K GE 2).NORM                                             22380002
&K       SETA  2                                                        22440002
.NORM    ANOP                                                           22500002
&START   SETA  2                                                        22560002
         AIF (&OP LT N'&SYSLIST).NOTLAST                                22620002
&MARGIN  SETC  '$'                                                      22680002
.NOTLAST ANOP                                                           22740002
&NRCHAR  SETA  8                                                        22800002
&TACK    SETC  ''                                                       22860002
.LOOPIN  ANOP                                                           22920002
&REMAIN  SETA  &K-&START                                                22980002
         AIF (&REMAIN GE 8).MORE                                        23040002
&TACK    SETC  '&MARGIN'                                                23100002
&NRCHAR  SETA  &REMAIN                                                  23160002
.MORE    ANOP                                                           23220002
&TEXT    SETC  ''                                                       23280002
         AIF (&REMAIN EQ 0).ENDTEXT                                     23340002
&TEXT    SETC  '&SYSLIST(&OP)'(&START,&NRCHAR)                          23400002
.ENDTEXT ANOP                                                           23460002
&LABELNR SETA  &LABELNR+1                                               23520002
&LABELS(&LABELNR) SETC '&TEXT'.'&TACK'                                  23580002
&START   SETA  &START+8                                                 23640002
         AIF ('&TACK' EQ '').LOOPIN                                     23700002
         AIF ('&TACK' EQ '*').LOOPOUT                                   23760002
         MEND                                                           23820002
         SPACE 2                                                        23880002
         MACRO                                                          23940002
         LSTART                                                         24000002

         MACRO                                                          13500002
         HEX   &LOGITEM,&SKIP$OR,&BYTES                                 13560002
.*                                                                      13620002
.*                                 THIS MACRO FACILITATES GENERATION    13680002
.*                                 OF THE TABLE ENTRIES SPECIFYING HEX  13740002
.*                                 TYPE CONVERSIONS.                    13800002
.*                                                                      13860002
.*                                 &LOGITEM - NAME OF DATA ITEM TO BE   13920002
.*                                 CONVERTED.                           13980002
.*                                                                      14040002
.*                                 &SKIP$OR - NUMBER OF SPACES TO SKIP  14100002
.*                                 ON MULTIPLE PRINT OF NUMBER OF       14160002
.*                                 SPACES TO BACK UP FROM COLUMN FOR    14220002
.*                                 START OF PRINTING. THE LETTER B      14280002
.*                                 PRECEEDS THE NUMBER (B8) IF THE      14340002
.*                                 LATTER OPTION IS DESIRED. OMISSION   14400002
.*                                 OF THIS OPERAND FORCES OPTION 2 WITH 14460002
.*                                 BACKUP SPACING EQUAL TO THE NUMBER   14520002
.*                                 OF BYTES SPECIFIED BY THE NEXT       14580002
.*                                 OPERAND (TWICE THIS NUMBER IF SUFFIX 14640002
.*                                 IS EXPLICITLY OR IMPLICITLY B).      14700002
.*                                                                      14760002
.*                                 &BYTES - NUMBER OF TOTAL INPUT BYTES 14820002
.*                                 TO PROCESS. THIS NUMBER IS SUFFIXED  14880002
.*                                 WITH A L,R, OR B TO STIPULATE        14940002
.*                                 PRINTING OF THE LEFT HALF, RIGHT     15000002
.*                                 HALF, OR BOTH HALVES OF THE BYTE     15060002
.*                                 RESPECTIVELY. OMISSION OF THIS       15120002
.*                                 OPERAND DEFAULTS TO B WITH THE       15180002
.*                                 NUMBER OF BYTES EQUAL TO THE ITEM    15240002
.*                                 LENGTH.                              15300002
.*                                                                      15360002
.*                                 A NEW ENTRY IS GENERATED ONLY WHEN   15420002
.*                                 NECESSARY. THIS MACRO MUST BE THE    15480002
.*                                 FIRST MACRO INSTRUCTION ISSUED FOR   15540002
.*                                 THE SUBJECT LINE(S)                  15600002
         GBLC  &ITEM(2000),&DSECT                                       15660002
         GBLA  &ITEMNBR,&BITS(2000),&SHIFT(2000),&NRENTS                15720002
         LCLB  &B                                                       15780002
         LCLA  &IBITS,&NDX,&ISHIFT,&K,&T,&CODE,&BYTESP                  15840002
         LCLC  &S,&LETTER                                               15900002
.LOOP    ANOP                                                           15960002
&NDX     SETA  &NDX+1                                                   16020002
         AIF   ('&ITEM(&NDX)' EQ '&LOGITEM').FOUND                      16080002
         AIF   (&NDX LT &ITEMNBR).LOOP                                  16140002
         MNOTE '**LOG ITEM NOT FOUND**'                                 16200002
         MEXIT                                                          16260002
.FOUND   ANOP                                                           16320002
&ISHIFT  SETA  &SHIFT(&NDX)                                             16380002
&IBITS   SETA  &BITS(&NDX)                                              16440002
&CODE    SETA  1                                                        16500002
&K       SETA  K'&BYTES                                                 16560002
         AIF   (&IBITS-(&IBITS/4)*4 EQ 0).OK                            16620002
         MNOTE 'BIT LENGTH SPECIFICATION INCONSISTENT WITH CONTROL'     16680002
         MEXIT                                                          16740002
.OK      AIF   ('&BYTES' EQ '').USEDEFN                                 16800002
&LETTER  SETC  '&BYTES'(&K,1)                                           16860002
&S       SETC  '&BYTES'(1,&K-1)                                         16920002
&BYTESP  SETA  &S                                                       16980002
         AIF   ('&LETTER' EQ 'B').BOTH                                  17040002
         AIF   ('&LETTER' EQ 'L').LEFT                                  17100002
&CODE    SETA  2                                                        17160002
         AGO   .L                                                       17220002
.LEFT    ANOP                                                           17280002
&CODE    SETA  1                                                        17340002
         AGO   .L                                                       17400002
         AIF   (&ISHIFT EQ 0 OR &ISHIFT EQ 4).OK2                       17460002
.ERR     MNOTE 'DATA NOT ALIGNED ON HEX OR BYTE BOUNDARY'               17520002
         MEXIT                                                          17580002
.USEDEFN ANOP                                                           17640002
&BYTESP  SETA  &IBITS/8                                                 17700002
.BOTH    AIF   (&ISHIFT NE 0).ERR                                       17760002

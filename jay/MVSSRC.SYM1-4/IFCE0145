         TITLE 'IFCE0145 MODEL  145 MACHINE CHECK EDIT'                 00060002
*  DSGEN MACRO     DSGEN MACRO     DSGEN MACRO     DSGEN MACRO          00120002
         SPACE 2                                                        00180002
         MACRO                                                          00240002
&NAME    DSGEN                                                          00300002
.*                            THIS MACRO GENERATES ONE DSECT WORD       00320002
.*                                 FORMATTED FOR THE SUBJECT LOG RECORD 00340002
.*                                 EACH OPERAND HAS TWO SUB LISTS. THE  00360002
.*                                 FIRST IS THE NAME OF THE LOG ITEM    00380002
.*                                 WHILE THE SECOND IS THE # OF BITS TO 00400002
.*                                 ALLOCATE FOR THIS ITEM. ITEMS MUST   00420002
.*                                 BE DEFINED CONTIGUOUSLY WITHOUT GAPS 00440002
.*                                 UNUSED BIT GROUPS MUST BE GIVEN      00460002
.*                                 DUMMY NAMES. THE NAMES PROVIDED FOR  00480002
.*                                 THE LOG ITEMS MAY THEN BE USED IN    00500002
.*                                 THE CONVERSION MACRO INSTRUCTIONS.   00520002
         GBLC &ITEM(2000)                                               00540002
         GBLA &ITEMNBR,&BITS(2000),&SHIFT(2000)                         00560002
         LCLB  &ONLY1,&CTIND                                            00580002
         LCLA  &START1,&START2,&END1,&END2                              00600002
         LCLC  &SYMBOL,&EQUSYM                                          00620002
         LCLA  &OP,&COUNT,&BYTE,&EQUCNT                                 00640002
         ACTR  100000                                                   00660002
.START   AIF   (N'&SYSLIST(&OP+1) LT 3).LOOP                            00680002
         AIF   ('&SYSLIST(&OP+1,3)' EQ 'EQU').LOOP     TEST FOR EQU     00700002
  AIF   ('&SYSLIST(&OP+1,3)' EQ '' AND '&SYSLIST(&OP+1,4)' EQ '').ERR1  00720002
  AIF   ('&SYSLIST(&OP+1,3)' EQ '' OR '&SYSLIST(&OP+1,4)' EQ '').ERR1   00740002
         AIF   ('&SYSLIST(&OP+1,3)' GT '&SYSLIST(&OP+1,4)').ERR3        00760002
&START1  SETA  &SYSLIST(&OP+1,3)        GET START COUNT OF 1            00780002
&START2  SETA  &SYSLIST(&OP+1,4)        GET FIRST END COUNT             00800002
         AGO   .SET2A                   BRANCH AROUND                   00820002
.ERR1    MNOTE '**FIRST SYMBOL CONCATENATION INCOMPLETE**'              00840002
         MEXIT                                                          00860002
.SET2A   AIF   ('&SYSLIST(&OP+1,5)' EQ ''AND '&SYSLIST(&OP+1,6)' EQ '' X00880002
               ).SET2                   TEST FOR NO SECOND              00900002
  AIF   ('&SYSLIST(&OP+1,5)' EQ '' OR '&SYSLIST(&OP+1,6)' EQ '').ERR2   00920002
         AIF   ('&SYSLIST(&OP+1,5)' GT '&SYSLIST(&OP+1,6)').ERR3        00940002
&END1    SETA  &SYSLIST(&OP+1,5)        GET START COUNT OF 2            00960002
&END2    SETA  &SYSLIST(&OP+1,6)        GET END COUNT OF 2              00980002
         AGO   .LOOP                                                    01000002
.SET2    ANOP                                                           01020002
&ONLY1   SETB  1                        INDICATE ONLY ONE CONCATENATION 01040002
         AGO   .LOOP                                                    01060002
.ERR2    MNOTE '**SECOND SYMBOL CONCATENATION INCOMPLETE**'             01080002
         MEXIT                                                          01100002
.ERR3    MNOTE '**START GREATER THAN END CONCATENATION**'               01120002
         MEXIT                                                          01140002
.LOOP    ANOP                                                           01160002
&OP      SETA  &OP+1                                                    01180002
         AIF   (&OP GT N'&SYSLIST).DONE      TEST IF ALL DONE           01200002
&ITEMNBR SETA  &ITEMNBR+1                                               01220002
         AIF   ('&SYSLIST(&OP,2)' NE '').OK1                            01240002
         MNOTE '**NULL BIT COUNT FIELD**'                               01260002
         MEXIT                                                          01280002
.OK1     ANOP                                                           01300002
&SYMBOL  SETC  '&SYSLIST(&OP,1)'                                        01320002
         AIF   ('&SYMBOL' NE '').OK                                     01340002
*              *****     LOG ITEM SYMBOL NOT PROVIDED     *****         01360002
         AIF   ('&SYSLIST(&OP,3)' NE 'EQU').CNT                         01380002
         AGO   .START                   GET NEXT                        01400002
.OK      AIF   (N'&SYSLIST(&OP) LT 3).NO3    TEST FOR OLD FORMAT        01420002
         AIF   ('&SYSLIST(&OP,3)' EQ 'EQU').EQU   TEST FOR EQU          01440002
         AIF   (&ONLY1).OK2             TEST FOR ONLY ONE CONCAT        01460002
&SYMBOL  SETC  '&SYMBOL.&START1.&END1'       GET 1 AND 2 CONCAT         01480002
         AGO   .NO3                     BRANCH AROUND                   01500002
.OK2     ANOP                                                           01520002
&SYMBOL  SETC  '&SYMBOL.&START1'        GET 1 CONCATENATED              01540002
.NO3     ANOP                                                           01560002
&SYMBOL  EQU   *+&BYTE                                                  01580002
&ITEM(&ITEMNBR) SETC '&SYMBOL'          GET LABEL IN GLOBAL             01600002
&BITS(&ITEMNBR) SETA &SYSLIST(&OP,2)                                    01620002
&SHIFT(&ITEMNBR) SETA &COUNT-&BYTE*8                                    01640002
.CNT     ANOP                                                           01660002
&CTIND   SETB  1                        SET COUNT INDICATOR             01680002
&COUNT   SETA  &COUNT+&SYSLIST(&OP,2)                                   01700002
&BYTE    SETA  &COUNT/8                                                 01720002
         AIF   (N'&SYSLIST(&OP) LT 3).START  TEST FOR OLD FORMAT        01740002
         AIF   (&ONLY1).TSTST           TEST FOR ONLY ONE CONCAT        01760002
         AIF   (&END1 EQ &END2).TSTST   TEST FOR SECOND END             01780002
&END1    SETA  &END1+1                  INCREMENT COUNT                 01800002
&OP      SETA  &OP-1                    DECREMENT INCREMENT             01820002
         AGO   .LOOP                    GENERATE NEXT                   01840002
.TSTST   ANOP                                                           01860002
         AIF   (&START1 EQ &START2).LPDONE   TEST FOR END               01880002
         AIF   (&ONLY1).NSET1           TEST FOR ONLY ONE CONCAT        01900002
&END1    SETA  &SYSLIST(&OP,5)          RE-INIT SECOND START COUNT      01920002
.NSET1   ANOP                                                           01940002
&OP      SETA  &OP-1                    DECREMENT INDEX                 01960002
&START1  SETA  &START1+1                INCREMENT FIRST COUNT           01980002
         AGO   .LOOP                    GO DO NEXT                      02000002
.LPDONE  ANOP                                                           02020002
&ONLY1   SETB  0                        CLEAR INDICATOR                 02040002
         AGO   .START                   GO TEST FOR MORE SUB-LISTS      02060002
.DONE    AIF   (&COUNT-&COUNT/8*8 NE 0).ERR  TEST FOR MULTIPLE OF EIGHT 02080002
         AIF   (&CTIND EQ 0).END        NO DS IF NO COUNT               02100002
&COUNT   SETA  &COUNT/8                 GET NUMBER OF BYTES             02120002
&NAME    DS    &COUNT.C                                                 02140002
.END     MEXIT                                                          02160002
.ERR     MNOTE '**BIT COUNT FLDS DO NOT TOTAL MULTIPLE OF 8**'          02180002
         MEXIT                                                          02200002
.EQU     AIF   ('&SYSLIST(&OP,4)' NE '').EQU1     TEST FOR NULL LABEL   02220002
.ERR4    MNOTE '**EQU OPERAND OMITTED OR NOT FOUND**'                   02240002
         MEXIT                                                          02260002
.EQU1    AIF   (&SYSLIST(&OP,2) LT 16).EQU2  COUNT OVER FIFTEEN         02280002
*****               THE FOLLOWING STATEMENT                *****        02300002
*****     NUMBER OF BITS EXCEED 15 - DO NOT USE AS BIN     *****        02320002
.EQU2    ANOP                                                           02340002
&EQUCNT  SETA  0                        CLEAR COUNTER                   02360002
&EQUSYM  SETC  '&SYSLIST(&OP,4)'        GET EQU OPERAND                 02380002
.EQULOP  ANOP                                                           02400002
&EQUCNT  SETA  &EQUCNT+1                INCREMENT INDEX                 02420002
         AIF   ('&ITEM(&EQUCNT)' EQ '&EQUSYM').EQUOUT  LABEL FOUND?     02440002
         AIF   (&EQUCNT EQ &ITEMNBR).ERR4    ALL TESTED?                02460002
         AGO   .EQULOP                  NO-GET NEXT                     02480002
.EQUOUT  ANOP                                                           02500002
&ITEM(&ITEMNBR) SETC '&SYMBOL'          GET LABEL NAME                  02520002
&BITS(&ITEMNBR) SETA &SYSLIST(&OP,2)    GET NUMBER OF BITS              02540002
&SHIFT(&ITEMNBR) SETA &SHIFT(&EQUCNT)   GET SHIFT COUNT                 02560002
&SYMBOL  EQU   &EQUSYM                  THIS SYMBOL EQUATED             02580002
         AGO   .START                   RETURN FOR NEXT                 02600002
         MEND                                                           02620002
*                                                                       03120002
*                                                                       03180002
         MACRO                                                          03240002
&NAME    LINE  &DATA,&LABEL,&SKIP=                                      03300002
.*                            THIS MACRO ESTABLISHES THE MARGINS OR     03360002
.*                                 COLUMN SETTINGS FOR EACH LINE. IT    03420002
.*                                 GENERATES NECESSARY CARRIAGE CONTROL 03480002
.*                                 ENTRIES. LASTLY IT DETERMINES THE    03540002
.*                                 NUMBER OF ENTRIES ASSOCIATED WITH    03600002
.*                                 THE PREVIOUS LINE AND STORES THAT #  03660002
.*                                 INTO THE APPROPRIATE BYTE.           03720002
.*                                                                      03780002
.*                                 &DATA(1) - CHARACTER POSITION TO     03840002
.*                                 BEGIN DATA.                          03900002
.*                                                                      03960002
.*                                 &DATA(2) - # OF SPACES BETWEEN       04020002
.*                                 SUCCESSIVE DATA COLUMN STARTING      04080002
.*                                 CHARACTERS.                          04140002
.*                                                                      04200002
.*                                 &LABEL(1) - CHARACTER POSITION       04260002
.*                                 TO BEGIN LABELS.                     04320002
.*                                                                      04380002
.*                                 &LABEL(2) - # OF SPACES BETWEEN      04440002
.*                                 LINE'S LABEL STARTING CHARACTERS.    04500002
.*                                                                      04560002
.*                                 &SKIP= - # OF LINES TO ADVANCE       04620002
.*                                 CARRIAGE TO BEGIN NEXT PRINTED LINE. 04680002
.*                                                                      04740002
.*                                 NEW ENTRIES ARE GENERATED ONLY WHEN  04800002
.*                                 NECESSARY. OMISSION OF ITEMS MEANS   04860002
.*                                 THEY HAVE NOT CHANGED SINCE PREVIOUS 04920002
.*                                 SETTINGS. A LINE MACRO IS REQUIRED   04980002
.*                                 FOR EACH PRINTED LINE. IF NO SETTING 05040002
.*                                 CHANGES ARE REQUIRED ALL OPERANDS    05100002
.*                                 ARE OMITTED IN WHICH CASE ONLY THE   05160002
.*                                 # OF ENTRIES FOR THE PREVIOUS LINE   05220002
.*                                 IS COMPUTED. THIS MACRO MUST BE THE  05280002
.*                                 FIRST MACRO INSTRUCTION ISSUED FOR   05340002
.*                                 THE SUBJECT LINE.                    05400002
         GBLA  &DB,&DT,&LB,&LT,&NRENTS                                  05460002
         GBLA  &LASTCC,&INITIAL                                         05520002
         GBLC  &NRENTSX                                                 05580002
         GBLC  &CARRCON                                                 05640002
         GBLB  &TEST                                                    05700002
         LCLA  &S                                                       05760002
&NAME    DS    0C                                                       05820002
         AIF   (&INITIAL EQ 0).FIRSTLN                                  05880002
         LINEND                                                         05940002
         AGO   .NTFIRST                                                 06000002
.FIRSTLN ANOP                                                           06060002
&NRENTSX SETC  'N&SYSNDX'                                               06120002
&INITIAL SETA  1                                                        06180002
N&SYSNDX EQU   *                                                        06240002
.NTFIRST ANOP                                                           06300002
         DC    AL1(0)                                                   06360002
         AIF   ('&DATA(1)' EQ '').CKDTABS                               06420002
         AIF   (&DATA(1) EQ &DB).CKDTABS                                06480002
&DB      SETA  &DATA(1)                                                 06540002
         AGO   .AA                                                      06600002
.CKDTABS AIF   ('&DATA(2)' EQ '').NCDATA                                06660002
         AIF   (&DATA(2) EQ &DT).NCDATA                                 06720002
         AGO   .CC                                                      06780002
.AA      AIF   ('&DATA(2)' EQ '').BB                                    06840002
         AIF   (&DATA(2) EQ &DT).BB                                     06900002
.CC      ANOP                                                           06960002
&DT      SETA  &DATA(2)                                                 07020002
.BB      DC    XL2'FFFE'                                                07080002
         DC    FL1'&DB'                                                 07140002
         DC    FL1'&DT'                                                 07200002
&NRENTS  SETA  &NRENTS+1                                                07260002
.NCDATA  ANOP                                                           07320002
         AIF   ('&LABEL(1)' EQ '').CKLTABS                              07380002
         AIF   (&LABEL(1) EQ &LB).CKLTABS                               07440002
&LB      SETA  &LABEL(1)                                                07500002
         AGO   .AAA                                                     07560002
.CKLTABS AIF   ('&LABEL(2)' EQ '').NCLABEL                              07620002
         AIF   (&LABEL(2) EQ &LT).NCLABEL                               07680002
         AGO   .CCC                                                     07740002
.AAA     AIF   ('&LABEL(2)' EQ '').BBB                                  07800002
         AIF   (&LABEL(2) EQ &LT).BBB                                   07860002
.CCC     ANOP                                                           07920002
&LT      SETA  &LABEL(2)                                                07980002
.BBB     DC    XL2'FFFF'                                                08040002
         DC    FL1'&LB'                                                 08100002
         DC    FL1'&LT'                                                 08160002
&NRENTS  SETA  &NRENTS+1                                                08220002
.NCLABEL ANOP                                                           08280002
         AIF   ('&SKIP' NE '').CONT                                     08340002
         AIF   (&TEST EQ 0).NOCHANG                                     08400002
&S       SETA  &LASTCC                                                  08460002
         AGO   .DEFAULT                                                 08520002
.CONT    ANOP                                                           08580002
         AIF   (&TEST EQ 0).AB                                          08640002
&S       SETA  &SKIP                                                    08700002
.DEFAULT ANOP                                                           08760002
         ORG   &CARRCON+3                                               08820002
         DC    AL.3(0),AL.2(&S),AL.3(1)                                 08880002
         ORG                                                            08940002
&LASTCC  SETA  &S                                                       09000002
&TEST    SETB  0                                                        09060002
         AGO   .DD                                                      09120002
.AB      AIF   (&SKIP EQ &LASTCC).NOCHANG                               09180002
A&SYSNDX DC    XL2'FFFB'                                                09240002
         DC    AL.3(0),AL.2(&SKIP),AL.3(1)                              09300002
         DC    FL1'9'                                                   09360002
&LASTCC  SETA  &SKIP                                                    09420002
&TEST    SETB  1                                                        09480002
&CARRCON SETC  'A&SYSNDX'                                               09540002
&NRENTS  SETA  &NRENTS+1                                                09600002
.DD      ANOP                                                           09660002
.NOCHANG ANOP                                                           09720002
         MEND                                                           09780002

         MACRO                                                 @YL02021 10080602
         BIN   ,                                               @YL02021 10081202
.*                                 &SYSLIST(&OP,1) - NAME OF LOG ITEM   10081802
.*                                      RECORD DSECT           @YL02021 10082402
.*                                 $SYSLIST(&OP,2) - NBR OF SPACES      10083002
.*                                      TO SKIP ON MULTIPLE    @YL02021 10083602
.*                                      PRINT OR NBR OF SPACES @YL02021 10084202
.*                                      TO BACK UP FROM COLUMN FOR      10084802
.*                                      START OF PRINTING. THE LETTER   10085402
.*                                      B PRECEEDS THE NUMBER (B5) IF   10086002
.*                                      THE LATTER IS DESIRED. OMISSION 10086602
.*                                      OF THIS OPERAND FORCES OPTION 2 10087202
.*                                      WITH BACKUP SPACING EQUAL TO    10087802
.*                                 THE NUMBER OF BITS IN THE ITEM       10088402
.*                                 DEFINITION (VIA MACRO 'DSGEN').      10089002
.*                            A LIMIT OF 15 BITS IS IMPOSED FOR THE     10089602
.*                                 TOTAL NUMBER OF BITS TO PRINT.       10090202
         GBLA  &INITIAL                                        @YL02021 10090802
         GBLB  &DIAGTST,&DIAGSET,&DIAGLIN                      @YL02021 10091402
         GBLC  &ITEM(2000),&DSECT                              @YL02021 10092002
         GBLA  &BITS(2000),&ITEMNBR,&SHIFT(2000),&NRENTS       @YL02021 10092602
         LCLB  &B,&SUB                                         @YL02021 10093202
         LCLA  &IBITS,&NDX,&ISHIFT,&K                          @YL02021 10093802
         LCLA  &TEMP,&T,&OP                                    @YL02021 10094402
         LCLC  &S                                              @YL02021 10095002
         ACTR  500000                                          @YL02021 10095602
         AIF   (&DIAGTST EQ 0).NOMSG                           @YL02021 10096202
         AIF   (&INITIAL EQ 0).NOMSG                           @YL02021 10096802
         AIF   (&DIAGLIN EQ 1).NOMSG                           @YL02021 10097402
         MNOTE '**NO LINE ASSOCIATED WITH THIS DATA**'         @YL02021 10098002
*****THE FOLLOWING GENERATED FOR TEST PURPOSES*****            @YL02021 10098602
*        LINE                                                  @YL02021 10099202
         LINE                                                  @YL02021 10099802
.NOMSG   ANOP                                                  @YL02021 10100402
         AIF   (K'&SYSLIST(1) EQ K'&SYSLIST(1,1)).LOOP         @YL02021 10101002
&SUB     SETB  1                                               @YL02021 10101602
.LOOP    ANOP                                                  @YL02021 10102202
&OP      SETA  &OP+1                                           @YL02021 10102802
.LOOP1   ANOP                                                  @YL02021 10103402
&NDX     SETA  &NDX+1                                          @YL02021 10104002
         AIF   ('&ITEM(&NDX)' EQ '&SYSLIST(&OP,1)').FOUND      @YL02021 10104602
         AIF   (&NDX LT &ITEMNBR).LOOP1                        @YL02021 10105202
         MNOTE '**LOG ITEM NOT DEFINED**'                      @YL02021 10105802
         MEXIT                                                 @YL02021 10106402
.FOUND   ANOP                                                  @YL02021 10107002
&ISHIFT  SETA  &SHIFT(&NDX)                                    @YL02021 10107602
&IBITS   SETA  &BITS(&NDX)                                     @YL02021 10108202
         AIF   (&SUB EQ 1).SUB00                               @YL02021 10108802
         AIF   ('&SYSLIST(3)' EQ '').SETT                      @YL02021 10109402
&IBITS   SETA  &SYSLIST(3)                                     @YL02021 10110002
         AGO   .SETT                                           @YL02021 10110602
.SUB00   AIF   ('&SYSLIST(&OP,3)' EQ '').SETT                  @YL02021 10111202
&IBITS   SETA  &SYSLIST(&OP,3)                                 @YL02021 10111802
.SETT    ANOP                                                  @YL02021 10112402
&T       SETA  &IBITS-1                                        @YL02021 10113002
&B       SETB  0                                               @YL02021 10113602
         AIF   (&IBITS GT 0 AND &IBITS LT 16).SUB0             @YL02021 10114202
         MNOTE '**NO. OF BITS OUTSIDE RANGE OF 1-15**'         @YL02021 10114802
         MEXIT                                                 @YL02021 10115402
.SUB0    AIF   (&SUB EQ 1).SUBA                                @YL02021 10116002
         AIF   ('&SYSLIST(2)' EQ '').BACKUP                    @YL02021 10116602
&K       SETA  K'&SYSLIST(2)                                   @YL02021 10117202
&B       SETB  ('&SYSLIST(2)'(1,1) EQ 'B')                     @YL02021 10117802
&S       SETC  '&SYSLIST(2)'(&B+1,&K-&B)                       @YL02021 10118402
         AGO   .SUBB                                           @YL02021 10119002
.SUBA    ANOP                                                  @YL02021 10119602
         AIF   ('&SYSLIST(&OP,2)' EQ '').BACKUP                @YL02021 10120202
&K       SETA  K'&SYSLIST(&OP,2)                               @YL02021 10120802
&B       SETB  ('&SYSLIST(&OP,2)'(1,1) EQ 'B')                 @YL02021 10121402
&S       SETC  '&SYSLIST(&OP,2)'(&B+1,&K-&B)                   @YL02021 10122002
.SUBB    ANOP                                                  @YL02021 10122602
&T       SETA  &S                                              @YL02021 10123202
.BACKUP  ANOP                                                  @YL02021 10123802
&NRENTS  SETA  &NRENTS+1                                       @YL02021 10124402
&TEMP    SETA  &ISHIFT                                         @YL02021 10125002
         AIF   (&IBITS NE 1 OR &B NE 0).BIN2                   @YL02021 10125602
         DC    AL.12(&SYSLIST(&OP,1)-&DSECT),XL.4'&TEMP'       @YL02021 10126202
         AGO   .OUT                                            @YL02021 10126802
.BIN2    AIF   (&SUB EQ 1).BIN2A                               @YL02021 10127402
         AIF   ('&SYSLIST(2)' NE '').BIN3                      @YL02021 10128002
         AGO   .BIN2B                                          @YL02021 10128602
.BIN2A   AIF   ('&SYSLIST(&OP,2)' NE '').BIN3                  @YL02021 10129202
.BIN2B   ANOP                                                  @YL02021 10129802
&B       SETB  1                                               @YL02021 10130402
.BIN3    ANOP                                                  @YL02021 10131002
         DC    AL.12(&SYSLIST(&OP,1)-&DSECT),XL.4'C'           @YL02021 10131602
         DC    BL.1'&B',AL.7(&T),AL.4(&IBITS),FL.4'&ISHIFT'    @YL02021 10132202
.OUT     ANOP                                                  @YL02021 10132802
&NDX     SETA  0                                               @YL02021 10133402
         AIF   (&SUB EQ 0).OUTA                                @YL02021 10134002
         AIF   (&OP LT N'&SYSLIST).LOOP                        @YL02021 10134602
.OUTA    ANOP                                                  @YL02021 10135202
         MEND                                                  @YL02021 10135802

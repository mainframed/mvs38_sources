*  BIN MACRO     BIN MACRO     BIN MACRO     BIN MACRO     BIN MACRO  * 27200002
         SPACE 2                                                        27300002
         MACRO                                                          27400002
         BIN   &LOGITEM,&SKIP$OR                                        27500002
.*                                 &LOGITEM - NAME OF ITEM IN LOG       27600002
.*                                      RECORD DSECT                    27700002
.*                                 &SKIP$OR - NBR OF SPACES TO SKIP ON  27800002
.*                                 MULTIPLE PRINT OR NBR OF SPACES      27900002
.*                                      TO BACK UP FROM COLUMN FOR      28000002
.*                                      START OF PRINTING. THE LETTER   28100002
.*                                      B PRECEEDS THE NUMBER (B5) IF   28200002
.*                                      THE LATTER IS DESIRED. OMISSION 28300002
.*                                      OF THIS OPERAND FORCES OPTION 2 28400002
.*                                      WITH BACKUP SPACING EQUAL TO    28500002
.*                                 THE NUMBER OF BITS IN THE ITEM       28600002
.*                                 DEFINITION (VIA MACRO 'DSGEN').      28700002
.*                            A LIMIT OF 15 BITS IS IMPOSED FOR THE     28800002
.*                                 TOTAL NUMBER OF BITS TO PRINT.       28900002
         GBLC &ITEM(1000)                                               29000002
         GBLA &ITEMNBR,&BITS(1000),&SHIFT(1000),&NRENTS                 29100002
         GBLC  &DSECT                                                   29200002
         LCLB  &B                                                       29300002
         LCLA  &IBITS,&NDX,&ISHIFT,&K                                   29400002
         LCLA  &TEMP,&T                                                 29500002
         LCLC  &S                                                       29600002
.LOOP    ANOP                                                           29700002
&NDX     SETA  &NDX+1                                                   29800002
         AIF   ('&ITEM(&NDX)' EQ '&LOGITEM').FOUND                      29900002
         AIF   (&NDX LT &ITEMNBR).LOOP                                  30000002
         MNOTE *,'YOU BLEW IT'                                          30100002
         ROUTINE DUM,SKIPPER                                            30200002
         MEXIT                                                          30300002
.FOUND   ANOP                                                           30400002
&ISHIFT  SETA  &SHIFT(&NDX)                                             30500002
&IBITS   SETA  &BITS(&NDX)                                              30600002
&T       SETA  &IBITS-1                                                 30700002
&B       SETB  0                                                        30800002
         AIF   ('&SKIP$OR' EQ '').BACKUP                                30900002
&K       SETA  K'&SKIP$OR                                               31000002
&B       SETB  ('&SKIP$OR'(1,1) EQ 'B')                                 31100002
&S       SETC  '&SKIP$OR'(&B+1,&K-&B)                                   31200002
&T       SETA  &S                                                       31300002
.BACKUP  ANOP                                                           31400002
&NRENTS  SETA  &NRENTS+1                                                31500002
&TEMP    SETA  &ISHIFT                                                  31600002
A&SYSNDX DC    AL.12(&LOGITEM-&DSECT),XL.4'&TEMP'                       31700002
         AIF (&IBITS EQ 1 AND &B EQ 0).OUT                              31800002
         ORG   A&SYSNDX                                                 31900002
&B       SETB  1                                                        32000002
         DC    AL.12(&LOGITEM-&DSECT),XL.4'C'                           32100002
         DC    BL.1'&B',AL.7(&T),AL.4(&IBITS),FL.4'&ISHIFT'             32200002
.OUT     ANOP                                                           32300002
         MEND                                                           32400002
         EJECT                                                          32500002
*  HEX MACRO     HEX MACRO     HEX MACRO    HEXN MACRO     HEX MACRO  * 32600002
         SPACE 2                                                        32700002
         MACRO                                                          32800002
         HEX   &LOGITEM,&SKIP$OR,&BYTES                                 32900002
.*                            THIS MACRO FACILITATES GENERATION OF THE  33000002
.*                                 TABLE ENTRIES SPECIFYING HEX TYPE    33100002
.*                                 CONVERSIONS.                         33200002
.*                            &LOGITEM - NAME OF DATA ITEM TO BE        33300002
.*                                 CONVERTED                            33400002
.*                            &SKIP$OR - NUMBER OF SPACES TO SKIP ON    33500002
.*                                 MULTIPLE PRINT OR NBR OF SPACES TO   33600002
.*                                 BACK UP FROM COLUMN FOR START OF     33700002
.*                                 PRINTING. THE LETTER B PRECEEDS THE  33800002
.*                                 NUMBER (B8) IF THE LATTER OPTION IS  33900002
.*                                 DESIRED. OMISSION OF THIS OPERAND    34000002
.*                                 FORCES OPTION 2 WITH BACKUP SPACING  34100002
.*                                 EQUAL TO THE NUMBER OF BYTES SPECI-  34200002
.*                                 FIED BY THE NEXT OPERAND (TWICE THIS 34300002
.*                                 NUMBER IF SUFFIX IS EXPLICITLY OR    34400002
.*                                 IMPLICITLY B).                       34500002
.*                            &BYTES - NUMBER OF TOTAL INPUT BYTES TO   34600002
.*                                 PROCESS. THIS NUMBER IS SUFFIXED     34700002
.*                                 WITH AN L,R, OR B TO STIPULATE       34800002
.*                                 PRINTING OF THE LEFT HALF, RIGHT     34900002
.*                                 HALF, OR BOTH HALVES OF THE BYTE     35000002
.*                                 RESPECTIVELY. OMISSION OF THIS       35100002
.*                                 OPERAND DEFAULTS TO B WITH THE NUM-  35200002
.*                                 BER OF BYTES EQUAL TO THE ITEM LNGTH 35300002
.*                            A NEW ENTRY IS GENERATED ONLY WHEN NECES- 35400002
.*                                 SARY. THIS MACRO MUST BE THE FIRST   35500002
.*                                 MACRO INSTRUCTION ISSUED FOR THE     35600002
.*                                 SUBJECT LINE(S).                     35700002
         GBLC &ITEM(1000),&DSECT                                        35800002
         GBLA &ITEMNBR,&BITS(1000),&SHIFT(1000),&NRENTS                 35900002
         LCLB  &B                                                       36000002
         LCLA  &IBITS,&NDX,&ISHIFT,&K,&T,&CODE,&BYTESP                  36100002
         LCLC  &S,&LETTER                                               36200002
.LOOP    ANOP                                                           36300002
&NDX     SETA  &NDX+1                                                   36400002
         AIF   ('&ITEM(&NDX)' EQ '&LOGITEM').FOUND                      36500002
         AIF   (&NDX LT &ITEMNBR).LOOP                                  36600002
         MNOTE *,'LOG ITEM NOT DEFINED'                                 36700002
         MEXIT                                                          36800002
.FOUND   ANOP                                                           36900002
&ISHIFT  SETA  &SHIFT(&NDX)                                             37000002
&IBITS   SETA  &BITS(&NDX)                                              37100002
&CODE    SETA  1                                                        37200002
&K       SETA  K'&BYTES                                                 37300002
         AIF   (&IBITS-(&IBITS/4)*4 EQ 0).OK                            37400002
         MNOTE *,'BIT LENGTH SPECIFICATION INCONSISTENT WITH CONTROL'   37500002
         MEXIT                                                          37600002
.OK      AIF   ('&BYTES' EQ '').USEDEFN                                 37700002
&LETTER  SETC  '&BYTES'(&K,1)                                           37800002
&S       SETC  '&BYTES'(1,&K-1)                                         37900002
&BYTESP  SETA  &S                                                       38000002
         AIF   ('&LETTER' EQ 'B').BOTH                                  38100002
         AIF   (&ISHIFT EQ 0 OR &ISHIFT EQ 4).OK2                       38200002
.ERR     MNOTE *,'DATA NOT ALLIGNED ON HEX OR BYTE BOUNDARY'            38300002
         MEXIT                                                          38400002
.USEDEFN ANOP                                                           38500002
&BYTESP  SETA  &IBITS/8                                                 38600002
.BOTH    AIF   (&ISHIFT NE 0).ERR                                       38700002
&CODE    SETA  3                                                        38800002
&T       SETA  &BYTESP*2-1                                              38900002
&S       SETC  '&T'                                                     39000002
.OK2     AIF   (&ISHIFT EQ 0).L                                         39100002
&CODE    SETA  2                                                        39200002
.L       ANOP                                                           39300002
&B       SETB  1                                                        39400002
         AIF   ('&SKIP$OR' EQ '').BACKUP                                39500002
&K       SETA  K'&SKIP$OR                                               39600002
&B       SETB  ('&SKIP$OR'(1,1) EQ 'B')                                 39700002
&S       SETC  '&SKIP$OR'(&B+1,&K-&B)                                   39800002
.BACKUP  ANOP                                                           39900002
&NRENTS  SETA  &NRENTS+1                                                40000002
         DC    AL.12(&LOGITEM-&DSECT),XL.4'A'                           40100002
         DC    BL.1'&B',FL.7'&S',FL.4'&BYTESP',FL.4'&CODE'              40200002
.OUT     ANOP                                                           40300002
         MEND                                                           40400002
         GBLC  &DSECT,&CSECT                                            40500002

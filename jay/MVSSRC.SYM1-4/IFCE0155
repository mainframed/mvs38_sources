         MACRO                                                          18360002
&NAME    LINE  &DATA,&LABEL,&SKIP=                                      18420002
.*                            THIS MACRO ESTABLISHES THE MARGINS OR     18480002
.*                                 COLUMN SETTINGS FOR EACH LINE. IT    18540002
.*                                 GENERATES NECESSARY CARRIAGE CONTROL 18600002
.*                                 ENTRIES. LASTLY IT DETERMINES THE    18660002
.*                                 NUMBER OF ENTRIES ASSOCIATED WITH    18720002
.*                                 THE PREVIOUS LINE AND STORES THAT #  18780002
.*                                 INTO THE APPROPRIATE BYTE.           18840002
.*                            &DATA(1) - CHARACTER POSITION TO BEGIN    18900002
.*                                 DATA.                                18960002
.*                            &DATA(2) - # OF SPACES BETWEEN SUCCESSIVE 19020002
.*                                 DATA COLUMN STARTING CHARACTERS.     19080002
.*                            &LABEL(1) - CHAR. POS. TO BEGIN LABELS.   19140002
.*                            &LABEL(2) - # SPACES BETWEEN LINE'S LABEL 19200002
.*                                 STARTING CHARACTERS.                 19260002
.*                            &SKIP= - # OF LINES TO ADVANCE CARRIAGE   19320002
.*                                 TO BEGIN NEXT PRINTED LINE.          19380002
.*                            NEW ENTRIES ARE GENERATED ONLY WHEN       19440002
.*                                 NECESSARY. OMISSION OF ITEMS MEANS   19500002
.*                                 THEY HAVE NOT CHANGED SINCE PREVIOUS 19560002
.*                                 SETTINGS. A LINE MACRO IS REQUIRED   19620002
.*                                 FOR EACH PRINTED LINE. IF NO SETTING 19680002
.*                                 CHANGES ARE REQUIRED ALL OPERANDS    19740002
.*                                 ARE OMITTED IN WHICH CASE ONLY THE   19800002
.*                                 # OF ENTRIES FOR THE PREVIOUS LINE   19860002
.*                                 IS COMPUTED. THIS MACRO MUST BE THE  19920002
.*                                 FIRST MACRO INSTRUCTION ISSUED FOR   19980002
.*                                 THE SUBJECT LINE                     20040002
         GBLA  &DB,&DT,&LB,&LT,&NRENTS                                  20100002
         GBLA  &LASTCC,&INITIAL                                         20160002
         GBLC  &NRENTSX                                                 20220002
         GBLC  &CARRCON                                                 20280002
         GBLB  &TEST                                                    20340002
         GBLB  &DIAGTST,&DIAGSET,&DIAGLIN                               20400002
         LCLA  &S                                                       20460002
         AIF   (&INITIAL EQ 0 OR &DIAGTST EQ 0).DIAGSET                 20520002
         AIF   (&DIAGSET EQ 0).DIAGSET                                  20580002
         MNOTE '**NO LABEL ASSOCIATED WITH LAST SET**'                  20640002
***** THE FOLLOWING GENERATED FOR TEST PURPOSES *****                   20700002
*        LABEL '%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%'       20760002
               LABEL '%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%' 20820002
.DIAGSET ANOP                                                           20880002
&DIAGSET SETB  1                                                        20940002
&DIAGLIN SETB  1                                                        21000002
&NAME    DS    0C                                                       21060002
         AIF   (&INITIAL EQ 0).FIRSTLN                                  21120002
         LINEND                                                         21180002
         AGO   .NTFIRST                                                 21240002
.FIRSTLN ANOP                                                           21300002
&NRENTSX SETC  'N&SYSNDX'                                               21360002
&INITIAL SETA  1                                                        21420002
N&SYSNDX EQU   *                                                        21480002
.NTFIRST ANOP                                                           21540002
         DC    AL1(0)                                                   21600002
         AIF   ('&DATA(1)' EQ '').CKDTABS                               21660002
         AIF   (&DATA(1) EQ &DB).CKDTABS                                21720002
&DB      SETA  &DATA(1)                                                 21780002
         AGO   .AA                                                      21840002
.CKDTABS AIF   ('&DATA(2)' EQ '').NCDATA                                21900002
         AIF   (&DATA(2) EQ &DT).NCDATA                                 21960002
         AGO   .CC                                                      22020002
.AA      AIF   ('&DATA(2)' EQ '').BB                                    22080002
         AIF   (&DATA(2) EQ &DT).BB                                     22140002
.CC      ANOP                                                           22200002
&DT      SETA  &DATA(2)                                                 22260002
.BB      DC    XL2'FFFE'                                                22320002
         DC    FL1'&DB'                                                 22380002
         DC    FL1'&DT'                                                 22440002
&NRENTS  SETA  &NRENTS+1                                                22500002
.NCDATA  ANOP                                                           22560002
         AIF   ('&LABEL(1)' EQ '').CKLTABS                              22620002
         AIF   (&LABEL(1) EQ &LB).CKLTABS                               22680002
&LB      SETA  &LABEL(1)                                                22740002
         AGO   .AAA                                                     22800002
.CKLTABS AIF   ('&LABEL(2)' EQ '').NCLABEL                              22860002
         AIF   (&LABEL(2) EQ &LT).NCLABEL                               22920002
         AGO   .CCC                                                     22980002
.AAA     AIF   ('&LABEL(2)' EQ '').BBB                                  23040002
         AIF   (&LABEL(2) EQ &LT).BBB                                   23100002
.CCC     ANOP                                                           23160002
&LT      SETA  &LABEL(2)                                                23220002
.BBB     DC    XL2'FFFF'                                                23280002
         DC    FL1'&LB'                                                 23340002
         DC    FL1'&LT'                                                 23400002
&NRENTS  SETA  &NRENTS+1                                                23460002
.NCLABEL ANOP                                                           23520002
         AIF   ('&SKIP' NE '').CONT                                     23580002
         AIF   (&TEST EQ 0).NOCHANG                                     23640002
         AGO   .DEFALTA                                                 23700002
.CONT    ANOP                                                           23760002
         AIF   (&TEST EQ 0).AB                                          23820002
         AIF   ('&SKIP' EQ 'E').AB                                      23880002
&S       SETA  &SKIP                                                    23940002
.DEFAULT ANOP                                                           24000002
         AIF   (&SKIP EQ &LASTCC).DEFALTA                               24060002
         ORG   &CARRCON+3                                               24120002
         DC    AL.3(0),AL.2(&S),AL.3(1)                                 24180002
         ORG                                                            24240002
&LASTCC  SETA  &S                                                       24300002
.DEFALTA ANOP                                                           24360002
&TEST    SETB  0                                                        24420002
         AGO   .DD                                                      24480002
.AB      AIF   ('&SKIP' NE 'E').ABB                                     24540002
A&SYSNDX DC    X'FFFB8B00'              EJECT                           24600002
         AGO   .ABC                                                     24660002
.ABB     AIF   (&SKIP EQ &LASTCC).NOCHANG                               24720002
A&SYSNDX DC    XL2'FFFB'                                                24780002
         DC    AL.3(0),AL.2(&SKIP),AL.3(1)                              24840002
         DC    AL.3(0),AL.2(&SKIP),AL.3(1)                              24900002
&LASTCC  SETA  &SKIP                                                    24960002
&TEST    SETB  1                                                        25020002
.ABC     ANOP                                                           25080002
&CARRCON SETC  'A&SYSNDX'                                               25140002
&NRENTS  SETA  &NRENTS+1                                                25200002
.DD      ANOP                                                           25260002
.NOCHANG ANOP                                                           25320002
         MEND                                                           25380002
         MACRO                                                          25440002
&NAME    DSGEN                                                          25500002
.*                            THIS MACRO GENERATES ONE DSECT WORD       25560002
.*                                 FORMATTED FOR THE SUBJECT LOG RECORD 25620002
.*                                 EACH OPERAND HAS TWO SUB LISTS. THE  25680002
.*                                 FIRST IS THE NAME OF THE LOG ITEM    25740002
.*                                 WHILE THE SECOND IS THE # OF BITS TO 25800002
.*                                 ALLOCATE FOR THIS ITEM. ITEMS MUST   25860002
.*                                 BE DEFINED CONTIGUOUSLY WITHOUT GAPS 25920002
.*                                 UNUSED BIT GROUPS MUST BE GIVEN      25980002

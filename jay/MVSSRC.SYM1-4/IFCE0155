         AGO   .SUBB                                                    08940002
.SUBA    ANOP                                                           09000002
&K       SETA  K'&SYSLIST(&OP,3)                                        09060002
.SUBB    ANOP                                                           09120002
         AIF   (&IBITS-(&IBITS/4)*4 EQ 0).OK                            09180002
         MNOTE '**BIT LENGTH SPECIFICATION INCONSISTENT WITH CONTROL**' 09240002
         MEXIT                                                          09300002
.OK      AIF   (&SUB EQ 1).SUBC                                         09360002
         AIF   ('&SYSLIST(3)' EQ '').USEDEFN                            09420002
&LETTER  SETC  '&SYSLIST(3)'(&K,1)                                      09480002
&S       SETC  '&SYSLIST(3)'(1,&K-1)                                    09540002
         AGO   .SUBD                                                    09600002
.SUBC    ANOP                                                           09660002
         AIF   ('&SYSLIST(&OP,3)' EQ '').USEDEFN                        09720002

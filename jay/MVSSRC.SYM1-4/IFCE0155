&S       SETC  '&SYSLIST(&OP,3)'(1,&K-1)                                09840002
.SUBD    ANOP                                                           09900002
&BYTESP  SETA  &S                                                       09960002
         AIF   ('&LETTER' EQ 'B').BOTH                                  10020002
         AIF   ('&LETTER' EQ 'L').LEFT                                  10080002
&CODE    SETA  2                                                        10140002
         AGO   .L                                                       10200002
.LEFT    ANOP                                                           10260002
&CODE    SETA  1                                                        10320002
         AGO   .L                                                       10380002
.USEDEFN ANOP                                                           10440002
&BYTESP  SETA  &IBITS/8                                                 10500002
.BOTH    AIF   (&ISHIFT NE 0).ERR                                       10560002
&CODE    SETA  3                                                        10620002
&T       SETA  &BYTESP*2-1                                              10680002
&S       SETC  '&T'                                                     10740002
.OK2     AIF   (&ISHIFT EQ 0).L                                         10800002
&CODE    SETA  2                                                        10860002
.L       ANOP                                                           10920002
&B       SETB  1                                                        10980002
         AIF   (&SUB EQ 1).SUBE                                         11040002
         AIF   ('&SYSLIST(2)' EQ '').BACKUP                             11100002
&K       SETA  K'&SYSLIST(2)                                            11160002
&B       SETB  ('&SYSLIST(2)'(1,1) EQ 'B')                              11220002
&S       SETC  '&SYSLIST(2)'(&B+1,&K-&B)                                11280002
         AGO   .SUBF                                                    11340002
.SUBE    ANOP                                                           11400002
         AIF   ('&SYSLIST(&OP,2)' EQ '').BACKUP                         11460002
&K       SETA  K'&SYSLIST(&OP,2)                                        11520002
&B       SETB  ('&SYSLIST(&OP,2)'(1,1) EQ 'B')                          11580002
&S       SETC  '&SYSLIST(&OP,2)'(&B+1,&K-&B)                            11640002
.SUBF    ANOP                                                           11700002
.BACKUP  ANOP                                                           11760002
&NRENTS  SETA  &NRENTS+1                                                11820002
         AIF   (&BYTESP GT 0 AND &BYTESP LT 16).SUB0                    11880002
         MNOTE '**NO. OF BYTES OUTSIDE RANGE OF 1-15**'                 11940002
         MEXIT                                                          12000002

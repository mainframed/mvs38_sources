         MACRO                                                          00120002
         BIN                                                            00180002
.*                                 &SYSLIST(&OP,1) - NAME OF LOG ITEM   00240002
.*                                      RECORD DSECT                    00300002
.*                                 $SYSLIST(&OP,2) - NBR OF SPACES      00360002
.*                                      TO SKIP ON MULTIPLE             00420002
.*                                      PRINT OR NBR OF SPACES          00480002
.*                                      TO BACK UP FROM COLUMN FOR      00540002
.*                                      START OF PRINTING. THE LETTER   00600002
.*                                      B PRECEEDS THE NUMBER (B5) IF   00660002
.*                                      THE LATTER IS DESIRED. OMISSION 00720002
.*                                      OF THIS OPERAND FORCES OPTION 2 00780002
.*                                      WITH BACKUP SPACING EQUAL TO    00840002
.*                                 THE NUMBER OF BITS IN THE ITEM       00900002
.*                                 DEFINITION (VIA MACRO 'DSGEN').      00960002
.*                            A LIMIT OF 15 BITS IS IMPOSED FOR THE     01020002
.*                                 TOTAL NUMBER OF BITS TO PRINT.       01080002
         GBLA  &INITIAL                                                 01140002
         GBLB  &DIAGTST,&DIAGSET,&DIAGLIN                               01200002
         GBLC &ITEM(3000)                                               01260002
         GBLA &ITEMNBR,&BITS(3000),&SHIFT(3000),&NRENTS                 01320002
         GBLC  &DSECT                                                   01380002
         LCLB  &B,&SUB                                                  01440002
         LCLA  &IBITS,&NDX,&ISHIFT,&K                                   01500002
         LCLA  &TEMP,&T,&OP                                             01560002
         LCLC  &S                                                       01620002
         AIF   (&DIAGTST EQ 0).NOMSG                                    01680002
         AIF   (&INITIAL EQ 0).NOMSG                                    01740002
         AIF   (&DIAGLIN EQ 1).NOMSG                                    01800002
         MNOTE '**NO LINE ASSOCIATED WITH THIS DATA**'                  01860002
*****THE FOLLOWING GENERATED FOR TEST PURPOSES*****                     01920002
*        LINE                                                           01980002
         LINE                                                           02040002
.NOMSG   ANOP                                                           02100002
         AIF   (K'&SYSLIST(1) EQ K'&SYSLIST(1,1)).LOOP                  02160002
&SUB     SETB  1                                                        02220002
.LOOP    ANOP                                                           02280002
&OP      SETA  &OP+1                                                    02340002
.LOOP1   ANOP                                                           02400002
&NDX     SETA  &NDX+1                                                   02460002
         AIF   ('&ITEM(&NDX)' EQ '&SYSLIST(&OP,1)').FOUND               02520002
         AIF   (&NDX LT &ITEMNBR).LOOP1                                 02580002
         MNOTE '**LOG ITEM NOT DEFINED**'                               02640002
         MEXIT                                                          02700002
.FOUND   ANOP                                                           02760002
&ISHIFT  SETA  &SHIFT(&NDX)                                             02820002
&IBITS   SETA  &BITS(&NDX)                                              02880002
&T       SETA  &IBITS-1                                                 02940002
&B       SETB  0                                                        03000002
         AIF   (&IBITS GT 0 AND &IBITS LT 16).SUB0                      03060002
         MNOTE '**NO. OF BITS OUTSIDE RANGE OF 1-15**'                  03120002
         MEXIT                                                          03180002
.SUB0    AIF   (&SUB EQ 1).SUBA                                         03240002
         AIF   ('&SYSLIST(2)' EQ '').BACKUP                             03300002
&K       SETA  K'&SYSLIST(2)                                            03360002
&B       SETB  ('&SYSLIST(2)'(1,1) EQ 'B')                              03420002
&S       SETC  '&SYSLIST(2)'(&B+1,&K-&B)                                03480002
         AGO   .SUBB                                                    03540002
.SUBA    ANOP                                                           03600002
         AIF   ('&SYSLIST(&OP,2)' EQ '').BACKUP                         03660002
&K       SETA  K'&SYSLIST(&OP,2)                                        03720002
&B       SETB  ('&SYSLIST(&OP,2)'(1,1) EQ 'B')                          03780002
&S       SETC  '&SYSLIST(&OP,2)'(&B+1,&K-&B)                            03840002
.SUBB    ANOP                                                           03900002
&T       SETA  &S                                                       03960002
.BACKUP  ANOP                                                           04020002
&NRENTS  SETA  &NRENTS+1                                                04080002
&TEMP    SETA  &ISHIFT                                                  04140002
         AIF   (&IBITS NE 1 OR &B NE 0).BIN2                            04200002
         DC    AL.12(&SYSLIST(&OP,1)-&DSECT),XL.4'&TEMP'                04260002
         AGO   .OUT                                                     04320002
.BIN2    AIF   ('&SYSLIST(&OP,2)' NE '').BIN3                           04380002
&B       SETB  1                                                        04440002
.BIN3    ANOP                                                           04500002
         DC    AL.12(&SYSLIST(&OP,1)-&DSECT),XL.4'C'                    04560002
         DC    BL.1'&B',AL.7(&T),AL.4(&IBITS),FL.4'&ISHIFT'             04620002
.OUT     ANOP                                                           04680002
&NDX     SETA  0                                                        04740002
         AIF   (&SUB EQ 0).OUTA                                         04800002
         AIF   (&OP LT N'&SYSLIST).LOOP                                 04860002
.OUTA    ANOP                                                           04920002
         MEND                                                           04980002
         MACRO                                                          05040002
         HEX                                                            05100002
.*                            THIS MACRO FACILITATES GENERATION OF THE  05160002
.*                                 TABLE ENTRIES SPECIFYING HEX TYPE    05220002
.*                                 CONVERSIONS.                         05280002
.*                            &SYSLIST(&OP,1) - NAME OF DATA ITEM TO BE 05340002
.*                                 CONVERTED                            05400002
.*                            &SYSLIST(&OP,2) - # OF SPACES TO SKIP ON  05460002
.*                                 MULTIPLE PRINT OR NBR OF SPACES TO   05520002
.*                                 BACK UP FROM COLUMN FOR START OF     05580002
.*                                 PRINTING. THE LETTER B PRECEEDS THE  05640002
.*                                 NUMBER (B8) IF THE LATTER OPTION IS  05700002
.*                                 DESIRED. OMISSION OF THIS OPERAND    05760002
.*                                 FORCES OPTION 2 WITH BACKUP SPACING  05820002
.*                                 EQUAL TO THE NUMBER OF BYTES SPECI-  05880002
.*                                 FIED BY THE NEXT OPERAND (TWICE THIS 05940002
.*                                 NUMBER IF SUFFIX IS EXPLICITLY OR    06000002
.*                                 IMPLICITLY B).                       06060002
.*                            &SYSLIST(&OP,3) - NUMBER OF BYTES TO      06120002
.*                                 PROCESS. THIS NUMBER IS SUFFIXED     06180002
.*                                 WITH AN L,R, OR B TO STIPULATE       06240002
.*                                 PRINTING OF THE LEFT HALF, RIGHT     06300002
.*                                 HALF, OR BOTH HALVES OF THE BYTE     06360002
.*                                 RESPECTIVELY. OMISSION OF THIS       06420002
.*                                 OPERAND DEFAULTS TO B WITH THE NUM-  06480002
.*                                 BER OF BYTES EQUAL TO THE ITEM LNGTH 06540002
.*                            A NEW ENTRY IS GENERATED ONLY WHEN NECES- 06600002
.*                                 SARY. THIS MACRO MUST BE THE FIRST   06660002
.*                                 MACRO INSTRUCTION ISSUED FOR THE     06720002
.*                                 SUBJECT LINE(S).                     06780002
         GBLC &ITEM(3000),&DSECT                                        06840002
         GBLA &ITEMNBR,&BITS(3000),&SHIFT(3000),&NRENTS                 06900002
         GBLA  &INITIAL                                                 06960002
         GBLB  &DIAGTST,&DIAGSET,&DIAGLIN                               07020002

.SUBH    ANOP                                                           12360002
         DC    BL.1'&B',FL.7'&S',AL.4(&BYTESP),FL.4'&CODE'              12420002
.OUT     ANOP                                                           12480002
&NDX     SETA  0                                                        12540002
         AIF   (&SUB EQ 0).OUTA                                         12600002
         AIF   (&OP LT N'&SYSLIST).LOOP                                 12660002
.OUTA    ANOP                                                           12720002

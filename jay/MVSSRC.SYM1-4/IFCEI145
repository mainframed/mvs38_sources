         TITLE 'IFCEI145 MODEL 145 CHANNEL EDIT'                        00047002
         MACRO                                                          00094002
&NAME    DSGEN                                                          00141002
.*                            THIS MACRO GENERATES ONE DSECT WORD       00188002
.*                                 FORMATTED FOR THE SUBJECT LOG RECORD 00235002
.*                                 EACH OPERAND HAS TWO SUB LISTS. THE  00282002
.*                                 FIRST IS THE NAME OF THE LOG ITEM    00329002
.*                                 WHILE THE SECOND IS THE # OF BITS TO 00376002
.*                                 ALLOCATE FOR THIS ITEM. ITEMS MUST   00423002
.*                                 BE DEFINED CONTIGUOUSLY WITHOUT GAPS 00470002
.*                                 UNUSED BIT GROUPS MUST BE GIVEN      00517002
.*                                 DUMMY NAMES. THE NAMES PROVIDED FOR  00564002
.*                                 THE LOG ITEMS MAY THEN BE USED IN    00611002
.*                                 THE CONVERSION MACRO INSTRUCTIONS.   00658002
         GBLC &ITEM(1500)                                               00705002
         GBLA &ITEMNBR,&BITS(1500),&SHIFT(1500)                         00752002
         LCLB  &ONLY1,&CTIND                                            00799002
         LCLA  &START1,&START2,&END1,&END2                              00846002
         LCLC  &SYMBOL,&EQUSYM                                          00893002
         LCLA  &OP,&COUNT,&BYTE,&EQUCNT                                 00940002
         ACTR  100000                                                   00987002
.START   AIF   (N'&SYSLIST(&OP+1) LT 3).LOOP                            01034002
         AIF   ('&SYSLIST(&OP+1,3)' EQ 'EQU').LOOP     TEST FOR EQU     01081002
  AIF   ('&SYSLIST(&OP+1,3)' EQ '' AND '&SYSLIST(&OP+1,4)' EQ '').ERR1  01128002
  AIF   ('&SYSLIST(&OP+1,3)' EQ '' OR '&SYSLIST(&OP+1,4)' EQ '').ERR1   01175002
         AIF   ('&SYSLIST(&OP+1,3)' GT '&SYSLIST(&OP+1,4)').ERR3        01222002
&START1  SETA  &SYSLIST(&OP+1,3)        GET START COUNT OF 1            01269002
&START2  SETA  &SYSLIST(&OP+1,4)        GET FIRST END COUNT             01316002
         AGO   .SET2A                   BRANCH AROUND                   01363002
.ERR1    MNOTE '**FIRST SYMBOL CONCATENATION INCOMPLETE**'              01410002
         MEXIT                                                          01457002
.SET2A   AIF   ('&SYSLIST(&OP+1,5)' EQ ''AND '&SYSLIST(&OP+1,6)' EQ '' X01504002
               ).SET2                   TEST FOR NO SECOND              01551002
  AIF   ('&SYSLIST(&OP+1,5)' EQ '' OR '&SYSLIST(&OP+1,6)' EQ '').ERR2   01598002
         AIF   ('&SYSLIST(&OP+1,5)' GT '&SYSLIST(&OP+1,6)').ERR3        01645002
&END1    SETA  &SYSLIST(&OP+1,5)        GET START COUNT OF 2            01692002
&END2    SETA  &SYSLIST(&OP+1,6)        GET END COUNT OF 2              01739002
         AGO   .LOOP                                                    01786002
.SET2    ANOP                                                           01833002
&ONLY1   SETB  1                        INDICATE ONLY ONE CONCATENATION 01880002
         AGO   .LOOP                                                    01927002
.ERR2    MNOTE '**SECOND SYMBOL CONCATENATION INCOMPLETE**'             01974002
         MEXIT                                                          02021002
.ERR3    MNOTE '**START GREATER THAN END CONCATENATION**'               02068002
         MEXIT                                                          02115002
.LOOP    ANOP                                                           02162002
&OP      SETA  &OP+1                                                    02209002
         AIF   (&OP GT N'&SYSLIST).DONE      TEST IF ALL DONE           02256002
&ITEMNBR SETA  &ITEMNBR+1                                               02303002
         AIF   ('&SYSLIST(&OP,2)' NE '').OK1                            02350002
         MNOTE '**NULL BIT COUNT FIELD**'                               02397002
         MEXIT                                                          02444002
.OK1     ANOP                                                           02491002
&SYMBOL  SETC  '&SYSLIST(&OP,1)'                                        02538002
         AIF   ('&SYMBOL' NE '').OK                                     02585002
*              *****     LOG ITEM SYMBOL NOT PROVIDED     *****         02632002
         AIF   ('&SYSLIST(&OP,3)' NE 'EQU').CNT                         02679002
         AGO   .START                   GET NEXT                        02726002
.OK      AIF   (N'&SYSLIST(&OP) LT 3).NO3    TEST FOR OLD FORMAT        02773002
         AIF   ('&SYSLIST(&OP,3)' EQ 'EQU').EQU   TEST FOR EQU          02820002
         AIF   (&ONLY1).OK2             TEST FOR ONLY ONE CONCAT        02867002
&SYMBOL  SETC  '&SYMBOL.&START1.&END1'       GET 1 AND 2 CONCAT         02914002
         AGO   .NO3                     BRANCH AROUND                   02961002
.OK2     ANOP                                                           03008002
&SYMBOL  SETC  '&SYMBOL.&START1'        GET 1 CONCATENATED              03055002
.NO3     ANOP                                                           03102002
&SYMBOL  EQU   *+&BYTE                                                  03149002
&ITEM(&ITEMNBR) SETC '&SYMBOL'          GET LABEL IN GLOBAL             03196002
&BITS(&ITEMNBR) SETA &SYSLIST(&OP,2)                                    03243002
&SHIFT(&ITEMNBR) SETA &COUNT-&BYTE*8                                    03290002
.CNT     ANOP                                                           03337002
&CTIND   SETB  1                        SET COUNT INDICATOR             03384002
&COUNT   SETA  &COUNT+&SYSLIST(&OP,2)                                   03431002
&BYTE    SETA  &COUNT/8                                                 03478002
         AIF   (N'&SYSLIST(&OP) LT 3).START  TEST FOR OLD FORMAT        03525002
         AIF   (&ONLY1).TSTST           TEST FOR ONLY ONE CONCAT        03572002
         AIF   (&END1 EQ &END2).TSTST   TEST FOR SECOND END             03619002
&END1    SETA  &END1+1                  INCREMENT COUNT                 03666002
&OP      SETA  &OP-1                    DECREMENT INCREMENT             03713002
         AGO   .LOOP                    GENERATE NEXT                   03760002
.TSTST   ANOP                                                           03807002
         AIF   (&START1 EQ &START2).LPDONE   TEST FOR END               03854002
         AIF   (&ONLY1).NSET1           TEST FOR ONLY ONE CONCAT        03901002
&END1    SETA  &SYSLIST(&OP,5)          RE-INIT SECOND START COUNT      03948002
.NSET1   ANOP                                                           03995002
&OP      SETA  &OP-1                    DECREMENT INDEX                 04042002
&START1  SETA  &START1+1                INCREMENT FIRST COUNT           04089002
         AGO   .LOOP                    GO DO NEXT                      04136002
.LPDONE  ANOP                                                           04183002
&ONLY1   SETB  0                        CLEAR INDICATOR                 04230002
         AGO   .START                   GO TEST FOR MORE SUB-LISTS      04277002
.DONE    AIF   (&COUNT-&COUNT/8*8 NE 0).ERR  TEST FOR MULTIPLE OF EIGHT 04324002
         AIF   (&CTIND EQ 0).END        NO DS IF NO COUNT               04371002
&COUNT   SETA  &COUNT/8                 GET NUMBER OF BYTES             04418002
&NAME    DS    &COUNT.C                                                 04465002
.END     MEXIT                                                          04512002
.ERR     MNOTE '**BIT COUNT FLDS DO NOT TOTAL MULTIPLE OF 8**'          04559002
         MEXIT                                                          04606002
.EQU     AIF   ('&SYSLIST(&OP,4)' NE '').EQU1     TEST FOR NULL LABEL   04653002
.ERR4    MNOTE '**EQU OPERAND OMITTED OR NOT FOUND**'                   04700002
         MEXIT                                                          04747002
.EQU1    AIF   (&SYSLIST(&OP,2) LT 16).EQU2  COUNT OVER FIFTEEN         04794002
*****               THE FOLLOWING STATEMENT                *****        04841002
*****     NUMBER OF BITS EXCEED 15 - DO NOT USE AS BIN     *****        04888002
.EQU2    ANOP                                                           04935002
&EQUCNT  SETA  0                        CLEAR COUNTER                   04982002
&EQUSYM  SETC  '&SYSLIST(&OP,4)'        GET EQU OPERAND                 05029002
.EQULOP  ANOP                                                           05076002
&EQUCNT  SETA  &EQUCNT+1                INCREMENT INDEX                 05123002
         AIF   ('&ITEM(&EQUCNT)' EQ '&EQUSYM').EQUOUT  LABEL FOUND?     05170002
         AIF   (&EQUCNT EQ &ITEMNBR).ERR4    ALL TESTED?                05217002
         AGO   .EQULOP                  NO-GET NEXT                     05264002
.EQUOUT  ANOP                                                           05311002
&ITEM(&ITEMNBR) SETC '&SYMBOL'          GET LABEL NAME                  05358002
&BITS(&ITEMNBR) SETA &SYSLIST(&OP,2)    GET NUMBER OF BITS              05405002
&SHIFT(&ITEMNBR) SETA &SHIFT(&EQUCNT)   GET SHIFT COUNT                 05452002
&SYMBOL  EQU   &EQUSYM                  THIS SYMBOL EQUATED             05499002
         AGO   .START                   RETURN FOR NEXT                 05546002
         MEND                                                           05593002
         EJECT                                                          05640002
*  LINE MACRO     LINE MACRO     LINE MACRO     LINE MACRO            * 05687002
         SPACE 2                                                        05734002
         MACRO                                                          05781002
&NAME    LINE  &DATA,&LABEL,&SKIP=                                      05828002
.*                            THIS MACRO ESTABLISHES THE MARGINS OR     05875002
.*                                 COLUMN SETTINGS FOR EACH LINE. IT    05922002
.*                                 GENERATES NECESSARY CARRIAGE CONTROL 05969002
.*                                 ENTRIES. LASTLY IT DETERMINES THE    06016002
.*                                 NUMBER OF ENTRIES ASSOCIATED WITH    06063002
.*                                 THE PREVIOUS LINE AND STORES THAT #  06110002
.*                                 INTO THE APPROPRIATE BYTE.           06157002
.*                            &DATA(1) - CHARACTER POSITION TO BEGIN    06204002
.*                                 DATA.                                06251002
.*                            &DATA(2) - # OF SPACES BETWEEN SUCCESSIVE 06298002
.*                                 DATA COLUMN STARTING CHARACTERS.     06345002
.*                            &LABEL(1) - CHAR. POS. TO BEGIN LABELS.   06392002
.*                            &LABEL(2) - # SPACES BETWEEN LINE'S LABEL 06439002
.*                                 STARTING CHARACTERS.                 06486002
.*                            &SKIP= - # OF LINES TO ADVANCE CARRIAGE   06533002
.*                                 TO BEGIN NEXT PRINTED LINE.          06580002
.*                            NEW ENTRIES ARE GENERATED ONLY WHEN       06627002
.*                                 NECESSARY. OMISSION OF ITEMS MEANS   06674002
.*                                 THEY HAVE NOT CHANGED SINCE PREVIOUS 06721002
.*                                 SETTINGS. A LINE MACRO IS REQUIRED   06768002
.*                                 FOR EACH PRINTED LINE. IF NO SETTING 06815002
.*                                 CHANGES ARE REQUIRED ALL OPERANDS    06862002
.*                                 ARE OMITTED IN WHICH CASE ONLY THE   06909002
.*                                 # OF ENTRIES FOR THE PREVIOUS LINE   06956002
.*                                 IS COMPUTED. THIS MACRO MUST BE THE  07003002
.*                                 FIRST MACRO INSTRUCTION ISSUED FOR   07050002
.*                                 THE SUBJECT LINE                     07097002
         GBLA  &DB,&DT,&LB,&LT,&NRENTS                                  07144002
         GBLA  &LASTCC,&INITIAL                                         07191002
         GBLC  &NRENTSX                                                 07238002
         GBLC  &CARRCON                                                 07285002
         GBLB  &TEST                                                    07332002
         LCLA  &S                                                       07379002
&NAME    DS    0C                                                       07426002
         AIF   (&INITIAL EQ 0).FIRSTLN                                  07473002
         LINEND                                                         07520002
         AGO   .NTFIRST                                                 07567002
.FIRSTLN ANOP                                                           07614002
&NRENTSX SETC  'N&SYSNDX'                                               07661002
&INITIAL SETA  1                                                        07708002
N&SYSNDX EQU   *                                                        07755002
.NTFIRST ANOP                                                           07802002
         DC    AL1(0)                                                   07849002
         AIF   ('&DATA(1)' EQ '').CKDTABS                               07896002
         AIF   (&DATA(1) EQ &DB).CKDTABS                                07943002
&DB      SETA  &DATA(1)                                                 07990002
         AGO   .AA                                                      08037002
.CKDTABS AIF   ('&DATA(2)' EQ '').NCDATA                                08084002
         AIF   (&DATA(2) EQ &DT).NCDATA                                 08131002
         AGO   .CC                                                      08178002
.AA      AIF   ('&DATA(2)' EQ '').BB                                    08225002
         AIF   (&DATA(2) EQ &DT).BB                                     08272002
.CC      ANOP                                                           08319002
&DT      SETA  &DATA(2)                                                 08366002
.BB      DC    XL2'FFFE'                                                08413002
         DC    FL1'&DB'                                                 08460002
         DC    FL1'&DT'                                                 08507002
&NRENTS  SETA  &NRENTS+1                                                08554002
.NCDATA  ANOP                                                           08601002
         AIF   ('&LABEL(1)' EQ '').CKLTABS                              08648002
         AIF   (&LABEL(1) EQ &LB).CKLTABS                               08695002
&LB      SETA  &LABEL(1)                                                08742002
         AGO   .AAA                                                     08789002
.CKLTABS AIF   ('&LABEL(2)' EQ '').NCLABEL                              08836002
         AIF   (&LABEL(2) EQ &LT).NCLABEL                               08883002
         AGO   .CCC                                                     08930002
.AAA     AIF   ('&LABEL(2)' EQ '').BBB                                  08977002
         AIF   (&LABEL(2) EQ &LT).BBB                                   09024002
.CCC     ANOP                                                           09071002
&LT      SETA  &LABEL(2)                                                09118002
.BBB     DC    XL2'FFFF'                                                09165002
         DC    FL1'&LB'                                                 09212002
         DC    FL1'&LT'                                                 09259002
&NRENTS  SETA  &NRENTS+1                                                09306002
.NCLABEL ANOP                                                           09353002
         AIF   ('&SKIP' NE '').CONT                                     09400002
         AIF   (&TEST EQ 0).NOCHANG                                     09447002
&S       SETA  &LASTCC                                                  09494002
         AGO   .DEFAULT                                                 09541002
.CONT    ANOP                                                           09588002
         AIF   (&TEST EQ 0).AB                                          09635002
&S       SETA  &SKIP                                                    09682002
.DEFAULT ANOP                                                           09729002
         ORG   &CARRCON+3                                               09776002
         DC    AL.3(0),AL.2(&S),AL.3(1)                                 09823002
         ORG                                                            09870002
&LASTCC  SETA  &S                                                       09917002
&TEST    SETB  0                                                        09964002
         AGO   .DD                                                      10011002
.AB      AIF   (&SKIP EQ &LASTCC).NOCHANG                               10058002
A&SYSNDX DC    XL2'FFFB'                                                10105002
         DC    AL.3(0),AL.2(&SKIP),AL.3(1)                              10152002
         DC    FL1'9'                                                   10199002
&LASTCC  SETA  &SKIP                                                    10246002
&TEST    SETB  1                                                        10293002
&CARRCON SETC  'A&SYSNDX'                                               10340002
&NRENTS  SETA  &NRENTS+1                                                10387002
.DD      ANOP                                                           10434002
.NOCHANG ANOP                                                           10481002
         MEND                                                           10528002
         EJECT                                                          10575002
         MACRO                                                          10622002
         BIN                                                            10669002
.*                                 &LOGITEM - NAME OF ITEM IN LOG       10716002
.*                                      RECORD DSECT                    10763002
.*                                 &SKIP$OR - NBR OF SPACES TO SKIP ON  10810002
.*                                 MULTIPLE PRINT OR NBR OF SPACES      10857002
.*                                      TO BACK UP FROM COLUMN FOR      10904002
.*                                      START OF PRINTING. THE LETTER   10951002
.*                                      B PRECEEDS THE NUMBER (B5) IF   10998002
.*                                      THE LATTER IS DESIRED. OMISSION 11045002
.*                                      OF THIS OPERAND FORCES OPTION 2 11092002
.*                                      WITH BACKUP SPACING EQUAL TO    11139002
.*                                 THE NUMBER OF BITS IN THE ITEM       11186002
.*                                 DEFINITION (VIA MACRO 'DSGEN').      11233002
.*                            A LIMIT OF 15 BITS IS IMPOSED FOR THE     11280002
.*                                 TOTAL NUMBER OF BITS TO PRINT.       11327002
         GBLC &ITEM(1500)                                               11374002
         GBLA &ITEMNBR,&BITS(1500),&SHIFT(1500),&NRENTS                 11421002
         GBLC  &DSECT                                                   11468002
         LCLB  &B                                                       11515002
         LCLA  &IBITS,&NDX,&ISHIFT,&K                                   11562002
         LCLA  &TEMP,&T,&OP                                             11609002
         LCLC  &S                                                       11656002
.LOOP    ANOP                                                           11703002
&OP      SETA  &OP+1                                                    11750002
.LOOP1   ANOP                                                           11797002
&NDX     SETA  &NDX+1                                                   11844002
         AIF   ('&ITEM(&NDX)' EQ '&SYSLIST(&OP,1)').FOUND               11891002
         AIF   (&NDX LT &ITEMNBR).LOOP1                                 11938002
         MNOTE *,'YOU BLEW IT'                                          11985002
         ROUTINE DUM,SKIPPER                                            12032002
         MEXIT                                                          12079002
.FOUND   ANOP                                                           12126002
&ISHIFT  SETA  &SHIFT(&NDX)                                             12173002
&IBITS   SETA  &BITS(&NDX)                                              12220002
&T       SETA  &IBITS-1                                                 12267002
&B       SETB  0                                                        12314002
         AIF   ('&SYSLIST(&OP,2)' EQ '').BACKUP                         12361002
&K       SETA  K'&SYSLIST(&OP,2)                                        12408002
&B       SETB  ('&SYSLIST(&OP,2)'(1,1) EQ 'B')                          12455002
&S       SETC  '&SYSLIST(&OP,2)'(&B+1,&K-&B)                            12502002
&T       SETA  &S                                                       12549002
.BACKUP  ANOP                                                           12596002
&NRENTS  SETA  &NRENTS+1                                                12643002
&TEMP    SETA  &ISHIFT                                                  12690002
         AIF   (&IBITS NE 1 OR &B NE 0).BIN2                            12737002
         DC    AL.12(&SYSLIST(&OP,1)-&DSECT),XL.4'&TEMP'                12784002
         AGO   .OUT                                                     12831002
.BIN2    AIF   ('&SYSLIST(&OP,2)' NE '').BIN3                           12878002
&B       SETB  1                                                        12925002
.BIN3    ANOP                                                           12972002
         DC    AL.12(&SYSLIST(&OP,1)-&DSECT),XL.4'C'                    13019002
         DC    BL.1'&B',AL.7(&T),AL.4(&IBITS),FL.4'&ISHIFT'             13066002
.OUT     ANOP                                                           13113002
&NDX     SETA  0                                                        13160002
         AIF   (&OP LT N'&SYSLIST).LOOP                                 13207002
         MEND                                                           13254002
         EJECT                                                          13301002
         MACRO                                                 @YM2954P 13348002
         HEX   ,                                               @YM2954P 13368002
.*                            THIS MACRO FACILITATES GENERATION OF THE  13388002
.*                                 TABLE ENTRIES SPECIFYING HEX TYPE    13408002
.*                                 CONVERSIONS.                @YM2954P 13428002
.*                            &SYSLIST(&OP,1) - NAME OF DATA ITEM TO BE 13448002
.*                                 CONVERTED                   @YM2954P 13468002
.*                            &SYSLIST(&OP,2) - # OF SPACES TO SKIP ON  13488002
.*                                 MULTIPLE PRINT OR NBR OF SPACES TO   13508002
.*                                 BACK UP FROM COLUMN FOR START OF     13528002
.*                                 PRINTING. THE LETTER B PRECEEDS THE  13548002
.*                                 NUMBER (B8) IF THE LATTER OPTION IS  13568002
.*                                 DESIRED. OMISSION OF THIS OPERAND    13588002
.*                                 FORCES OPTION 2 WITH BACKUP SPACING  13608002
.*                                 EQUAL TO THE NUMBER OF BYTES SPECI-  13628002
.*                                 FIED BY THE NEXT OPERAND (TWICE THIS 13648002
.*                                 NUMBER IF SUFFIX IS EXPLICITLY OR    13668002
.*                                 IMPLICITLY B).              @YM2954P 13688002
.*                            &SYSLIST(&OP,3) - NUMBER OF BYTES TO      13708002
.*                                 PROCESS. THIS NUMBER IS SUFFIXED     13728002
.*                                 WITH AN L,R, OR B TO STIPULATE       13748002
.*                                 PRINTING OF THE LEFT HALF, RIGHT     13768002
.*                                 HALF, OR BOTH HALVES OF THE BYTE     13788002
.*                                 RESPECTIVELY. OMISSION OF THIS       13808002
.*                                 OPERAND DEFAULTS TO B WITH THE NUM-  13828002
.*                                 BER OF BYTES EQUAL TO THE ITEM LNGTH 13848002
.*                            A NEW ENTRY IS GENERATED ONLY WHEN NECES- 13868002
.*                                 SARY. THIS MACRO MUST BE THE FIRST   13888002
.*                                 MACRO INSTRUCTION ISSUED FOR THE     13908002
.*                                 SUBJECT LINE(S).            @YM2954P 13928002
         GBLA  &BITS(1500),&ITEMNBR,&SHIFT(1500),&NRENTS       @YM2954P 13948002
         GBLC  &ITEM(1500),&DSECT                              @YM2954P 13968002
         GBLA  &INITIAL                                        @YM2954P 13988002
         GBLB  &DIAGTST,&DIAGSET,&DIAGLIN                      @YM2954P 14008002
         LCLB  &B,&SUB                                         @YM2954P 14028002
         LCLA  &IBITS,&NDX,&ISHIFT,&K,&T,&CODE,&BYTESP,&OP     @YM2954P 14048002
         LCLC  &S,&LETTER                                      @YM2954P 14068002
         ACTR  500000                                          @YM2954P 14088002
         AIF   (&DIAGTST EQ 0).NOMSG                           @YM2954P 14108002
         AIF   (&INITIAL EQ 0).NOMSG                           @YM2954P 14128002
         AIF   (&DIAGLIN EQ 1).NOMSG                           @YM2954P 14148002
         MNOTE '**NO LINE ASSOCIATED WITH THIS DATA**'         @YM2954P 14168002
*****THE FOLLOWING GENERATED FOR TEST PURPOSES*****            @YM2954P 14188002
*        LINE                                                  @YM2954P 14208002
         LINE                                                  @YM2954P 14228002
.NOMSG   ANOP                                                  @YM2954P 14248002
         AIF   (K'&SYSLIST(1) EQ K'&SYSLIST(1,1)).LOOP         @YM2954P 14268002
&SUB     SETB  1                                               @YM2954P 14288002
.LOOP    ANOP                                                  @YM2954P 14308002
&OP      SETA  &OP+1                                           @YM2954P 14328002
.LOOP1   ANOP                                                  @YM2954P 14348002
&NDX     SETA  &NDX+1                                          @YM2954P 14368002
         AIF   ('&ITEM(&NDX)' EQ '&SYSLIST(&OP,1)').FOUND      @YM2954P 14388002
         AIF   (&NDX LT &ITEMNBR).LOOP1                        @YM2954P 14408002
         MNOTE '**LOG ITEM NOT DEFINED**'                      @YM2954P 14428002
         MEXIT                                                 @YM2954P 14448002
.FOUND   ANOP                                                  @YM2954P 14468002
&ISHIFT  SETA  &SHIFT(&NDX)                                    @YM2954P 14488002
&IBITS   SETA  &BITS(&NDX)                                     @YM2954P 14508002
         AIF   (&ISHIFT EQ 0 OR &ISHIFT EQ 4).ALIGN            @YM2954P 14528002
.ERR     MNOTE '**DATA NOT ALLIGNED ON HEX OR BYTE BOUNDARY**' @YM2954P 14548002
         MEXIT                                                 @YM2954P 14568002
.ALIGN   ANOP                                                  @YM2954P 14588002
&CODE    SETA  1                                               @YM2954P 14608002
         AIF   (&SUB EQ 1).SUBA                                @YM2954P 14628002
&K       SETA  K'&SYSLIST(3)                                   @YM2954P 14648002
         AGO   .SUBB                                           @YM2954P 14668002
.SUBA    ANOP                                                  @YM2954P 14688002
&K       SETA  K'&SYSLIST(&OP,3)                               @YM2954P 14708002
.SUBB    ANOP                                                  @YM2954P 14728002
         AIF   (&K EQ 1).ERR3                                  @YM2954P 14748002
         AIF   (&IBITS-(&IBITS/4)*4 EQ 0).OK                   @YM2954P 14768002
         MNOTE '**BIT LENGTH SPECIFICATION INCONSISTENT WITH CONTROL**' 14788002
         MEXIT                                                 @YM2954P 14808002
.OK      AIF   (&SUB EQ 1).SUBC                                @YM2954P 14828002
         AIF   ('&SYSLIST(3)' EQ '').USEDEFN                   @YM2954P 14848002
&LETTER  SETC  '&SYSLIST(3)'(&K,1)                             @YM2954P 14868002
&S       SETC  '&SYSLIST(3)'(1,&K-1)                           @YM2954P 14888002
         AGO   .SUBD                                           @YM2954P 14908002
.SUBC    ANOP                                                  @YM2954P 14928002
         AIF   ('&SYSLIST(&OP,3)' EQ '').USEDEFN               @YM2954P 14948002
&LETTER  SETC  '&SYSLIST(&OP,3)'(&K,1)                         @YM2954P 14968002
&S       SETC  '&SYSLIST(&OP,3)'(1,&K-1)                       @YM2954P 14988002
.SUBD    ANOP                                                  @YM2954P 15008002
&BYTESP  SETA  &S                                              @YM2954P 15028002
         AIF   ('&LETTER' EQ 'B').BOTH                         @YM2954P 15048002
         AIF   ('&LETTER' EQ 'L').LEFT                         @YM2954P 15068002
         AIF   ('&LETTER' EQ 'R').RIGHT                        @YM2954P 15088002
.ERR3    MNOTE '**PARAMETER 3 INCOMPLETE-DSGEN DEFINITION USED**'       15108002
         AGO   .USEDEFN                                        @YM2954P 15128002
.RIGHT   ANOP                                                  @YM2954P 15148002
&CODE    SETA  2                                               @YM2954P 15168002
         AGO   .L                                              @YM2954P 15188002
.LEFT    ANOP                                                  @YM2954P 15208002
&CODE    SETA  1                                               @YM2954P 15228002
         AGO   .L                                              @YM2954P 15248002
.USEDEFN ANOP                                                  @YM2954P 15268002
&BYTESP  SETA  &IBITS/8                                        @YM2954P 15288002
.BOTH    AIF   (&ISHIFT NE 0).ERR                              @YM2954P 15308002
&CODE    SETA  3                                               @YM2954P 15328002
&T       SETA  &BYTESP*2-1                                     @YM2954P 15348002
&S       SETC  '&T'                                            @YM2954P 15368002
.OK2     AIF   (&ISHIFT EQ 0).L                                @YM2954P 15388002
&CODE    SETA  2                                               @YM2954P 15408002
.L       ANOP                                                  @YM2954P 15428002
&B       SETB  1                                               @YM2954P 15448002
         AIF   (&SUB EQ 1).SUBE                                @YM2954P 15468002
         AIF   ('&SYSLIST(2)' EQ '').BACKUP                    @YM2954P 15488002
&K       SETA  K'&SYSLIST(2)                                   @YM2954P 15508002

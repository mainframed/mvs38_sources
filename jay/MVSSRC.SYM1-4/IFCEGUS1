*********************************************************************** 06600042
* HEX MACRO  HEX MACRO  HEX MACRO  HEX MACRO  HEX MACRO  HEX MACRO      06650042
         MACRO                                                          06700042
         HEX   ,                                                        06750042
.*                            THIS MACRO FACILITATES GENERATION OF THE  06800042
.*                                 TABLE ENTRIES SPECIFYING HEX TYPE    06850042
.*                                 CONVERSIONS.                         06900042
.*                            &SYSLIST(&OP,1) - NAME OF DATA ITEM TO BE 06950042
.*                                 CONVERTED                            07000042
.*                            &SYSLIST(&OP,2) - # OF SPACES TO SKIP ON  07050042
.*                                 MULTIPLE PRINT OR NBR OF SPACES TO   07100042
.*                                 BACK UP FROM COLUMN FOR START OF     07150042
.*                                 PRINTING. THE LETTER B PRECEEDS THE  07200042
.*                                 NUMBER (B8) IF THE LATTER OPTION IS  07250042
.*                                 DESIRED. OMISSION OF THIS OPERAND    07300042
.*                                 FORCES OPTION 2 WITH BACKUP SPACING  07350042
.*                                 EQUAL TO THE NUMBER OF BYTES SPECI-  07400042
.*                                 FIED BY THE NEXT OPERAND (TWICE THIS 07450042
.*                                 NUMBER IF SUFFIX IS EXPLICITLY OR    07500042
.*                                 IMPLICITLY B).                       07550042
.*                            &SYSLIST(&OP,3) - NUMBER OF BYTES TO      07600042
.*                                 PROCESS. THIS NUMBER IS SUFFIXED     07650042
.*                                 WITH AN L,R, OR B TO STIPULATE       07700042
.*                                 PRINTING OF THE LEFT HALF, RIGHT     07750042
.*                                 HALF, OR BOTH HALVES OF THE BYTE     07800042
.*                                 RESPECTIVELY. OMISSION OF THIS       07850042
.*                                 OPERAND DEFAULTS TO B WITH THE NUM-  07900042
.*                                 BER OF BYTES EQUAL TO THE ITEM LNGTH 07950042
.*                            A NEW ENTRY IS GENERATED ONLY WHEN NECES- 08000042
.*                                 SARY. THIS MACRO MUST BE THE FIRST   08050042
.*                                 MACRO INSTRUCTION ISSUED FOR THE     08100042
.*                                 SUBJECT LINE(S).                     08150042
         GBLA  &BITS(2600),&DB,&DT,&INITIAL                             08200042
         GBLA  &ITEMITR,&ITEMFST(20),&ITEMNBR,&ITEMMDX                  08250042
         GBLA  &ITEMNDX(2600),&ITEMXPT                                  08300042
         GBLA  &LABELNR,&LASTCC,&LB,&LBLCTR(100),&LPTR,&LT              08350042
         GBLA  &NRENTS,&SHIFT(2600)                                     08400042
         GBLB  &DIAGITF,&DIAGLIN,&DIAGSET,&DIAGTST,&FSW,&TEST           08450042
         GBLC  &CARRCON,&CSECT,&DSECT,&ITEM(2600)                       08500042
         GBLC  &LABELS(2600),&LBL(100),&NRENTSX                         08550042
         LCLB  &B,&SUB                                                  08600042
         LCLA  &IBITS,&ISHIFT,&K,&T,&CODE,&BYTESP,&OP                   08650042
         LCLC  &S,&LETTER,&ARG                                          08700042
       ACTR  500000                                                     08750042
         AIF   (&DIAGTST EQ 0).NOMSG                                    08800042
         AIF   (&INITIAL EQ 0).NOMSG                                    08850042
         AIF   (&DIAGLIN EQ 1).NOMSG                                    08900042
         MNOTE '**NO LINE ASSOCIATED WITH THIS DATA**'                  08950042
*****THE FOLLOWING GENERATED FOR TEST PURPOSES*****                     09000042
*        LINE                                                           09050042
         LINE                                                           09100042
.NOMSG   ANOP                                                           09150042
         AIF   (K'&SYSLIST(1) EQ K'&SYSLIST(1,1)).LOOP                  09200042
&SUB     SETB  1                                                        09250042
.LOOP    ANOP                                                           09300042
&OP      SETA  &OP+1                                                    09350042
&ARG     SETC  '&SYSLIST(&OP,1)'                                        09400042
         ITEMFIND  &ARG                                                 09450042
         AIF   (&ITEMXPT EQ 0).MEXIT                                    09500042
.FOUND   ANOP                                                           09550042
&ISHIFT  SETA  &SHIFT(&ITEMXPT)                                         09600042
&IBITS   SETA  &BITS(&ITEMXPT)                                          09650042
         AIF   (&ISHIFT EQ 0 OR &ISHIFT EQ 4).ALIGN                     09700042
.ERR     MNOTE '**DATA NOT ALLIGNED ON HEX OR BYTE BOUNDARY**'          09750042
.MEXIT   MEXIT                                                          09800042
.ALIGN   ANOP                                                           09850042
&CODE    SETA  1                                                        09900042
         AIF   (&SUB EQ 1).SUBA                                         09950042
&K       SETA  K'&SYSLIST(3)                                            10000042
         AGO   .SUBB                                                    10050042
.SUBA    ANOP                                                           10100042
&K       SETA  K'&SYSLIST(&OP,3)                                        10150042
.SUBB    ANOP                                                           10200042
         AIF   (&K EQ 1).ERR3                                           10250042
         AIF   (&IBITS-(&IBITS/4)*4 EQ 0).OK                            10300042
         MNOTE '**BIT LENGTH SPECIFICATION INCONSISTENT WITH CONTROL**' 10350042
         MEXIT                                                          10400042
.OK      AIF   (&SUB EQ 1).SUBC                                         10450042
         AIF   ('&SYSLIST(3)' EQ '').USEDEFN                            10500042
&LETTER  SETC  '&SYSLIST(3)'(&K,1)                                      10550042
&S       SETC  '&SYSLIST(3)'(1,&K-1)                                    10600042
         AGO   .SUBD                                                    10650042
.SUBC    ANOP                                                           10700042
         AIF   ('&SYSLIST(&OP,3)' EQ '').USEDEFN                        10750042
&LETTER  SETC  '&SYSLIST(&OP,3)'(&K,1)                                  10800042
&S       SETC  '&SYSLIST(&OP,3)'(1,&K-1)                                10850042
.SUBD    ANOP                                                           10900042
&BYTESP  SETA  &S                                                       10950042
         AIF   ('&LETTER' EQ 'B').BOTH                                  11000042
         AIF   ('&LETTER' EQ 'L').LEFT                                  11050042
         AIF   ('&LETTER' EQ 'R').RIGHT                                 11100042
.ERR3    MNOTE '**PARAMETER 3 INCOMPLETE-DSGEN DEFINITION USED**'       11150042
         AGO   .USEDEFN                                                 11200042
.RIGHT   ANOP                                                           11250042
&CODE    SETA  2                                                        11300042
         AGO   .L                                                       11350042
.LEFT    ANOP                                                           11400042
&CODE    SETA  1                                                        11450042
         AGO   .L                                                       11500042
.USEDEFN ANOP                                                           11550042
&BYTESP  SETA  &IBITS/8                                                 11600042
.BOTH    AIF   (&ISHIFT NE 0).ERR                                       11650042
&CODE    SETA  3                                                        11700042
&T       SETA  &BYTESP*2-1                                              11750042
&S       SETC  '&T'                                                     11800042
.OK2     AIF   (&ISHIFT EQ 0).L                                         11850042
&CODE    SETA  2                                                        11900042
.L       ANOP                                                           11950042
&B       SETB  1                                                        12000042
         AIF   (&SUB EQ 1).SUBE                                         12050042
         AIF   ('&SYSLIST(2)' EQ '').BACKUP                             12100042
&K       SETA  K'&SYSLIST(2)                                            12150042
&B       SETB  ('&SYSLIST(2)'(1,1) EQ 'B')                              12200042
&S       SETC  '&SYSLIST(2)'(&B+1,&K-&B)                                12250042
         AGO   .SUBF                                                    12300042
.SUBE    ANOP                                                           12350042
         AIF   ('&SYSLIST(&OP,2)' EQ '').BACKUP                         12400042
&K       SETA  K'&SYSLIST(&OP,2)                                        12450042
&B       SETB  ('&SYSLIST(&OP,2)'(1,1) EQ 'B')                          12500042
&S       SETC  '&SYSLIST(&OP,2)'(&B+1,&K-&B)                            12550042
.SUBF    ANOP                                                           12600042
.BACKUP  ANOP                                                           12650042
&NRENTS  SETA  &NRENTS+1                                                12700042
         AIF   (&BYTESP GT 0 AND &BYTESP LT 16).SUB0                    12750042
         MNOTE '**NO. OF BYTES OUTSIDE RANGE OF 1-15**'                 12800042
         MEXIT                                                          12850042
.SUB0    AIF   (&SUB EQ 1).SUBG                                         12900042
         DC    AL.12(&SYSLIST(1)-&DSECT),XL.4'A'                        12950042
         AGO   .SUBH                                                    13000042
.SUBG    ANOP                                                           13050042
         DC    AL.12(&SYSLIST(&OP,1)-&DSECT),XL.4'A'                    13100042
.SUBH    ANOP                                                           13150042
         DC    BL.1'&B',FL.7'&S',AL.4(&BYTESP),FL.4'&CODE'              13200042
.OUT     ANOP                                                           13250042
&ITEMXPT     SETA  0                                                    13300042
         AIF   (&SUB EQ 0).OUTA                                         13350042
         AIF   (&OP LT N'&SYSLIST).LOOP                                 13400042
.OUTA    ANOP                                                           13450042
         MEND                                                           13500042

*********************************************************************** 13600042
* BIN MACRO  BIN MACRO  BIN MACRO  BIN MACRO  BIN MACRO  BIN MACRO      13650042
         MACRO                                                          13700042
         BIN   ,                                                        13750042
.*                                 &SYSLIST(&OP,1) - NAME OF LOG ITEM   13800042
.*                                      RECORD DSECT                    13850042
.*                                 $SYSLIST(&OP,2) - NBR OF SPACES      13900042
.*                                      TO SKIP ON MULTIPLE             13950042
.*                                      PRINT OR NBR OF SPACES          14000042
.*                                      TO BACK UP FROM COLUMN FOR      14050042
.*                                      START OF PRINTING. THE LETTER   14100042
.*                                      B PRECEEDS THE NUMBER (B5) IF   14150042
.*                                      THE LATTER IS DESIRED. OMISSION 14200042
.*                                      OF THIS OPERAND FORCES OPTION 2 14250042
.*                                      WITH BACKUP SPACING EQUAL TO    14300042
.*                                 THE NUMBER OF BITS IN THE ITEM       14350042
.*                                 DEFINITION (VIA MACRO 'DSGEN').      14400042
.*                            A LIMIT OF 15 BITS IS IMPOSED FOR THE     14450042
.*                                 TOTAL NUMBER OF BITS TO PRINT.       14500042
         GBLA  &BITS(2600),&DB,&DT,&INITIAL                             14550042
         GBLA  &ITEMITR,&ITEMFST(20),&ITEMNBR,&ITEMMDX                  14600042
         GBLA  &ITEMNDX(2600),&ITEMXPT                                  14650042
         GBLA  &LABELNR,&LASTCC,&LB,&LBLCTR(100),&LPTR,&LT              14700042
         GBLA  &NRENTS,&SHIFT(2600)                                     14750042
         GBLB  &DIAGITF,&DIAGLIN,&DIAGSET,&DIAGTST,&FSW,&TEST           14800042
         GBLC  &CARRCON,&CSECT,&DSECT,&ITEM(2600)                       14850042
         GBLC  &LABELS(2600),&LBL(100),&NRENTSX                         14900042
         LCLB  &B,&SUB                                                  14950042
         LCLA  &IBITS,&ISHIFT,&K                                        15000042
         LCLA  &TEMP,&T,&OP                                             15050042
         LCLC  &S,&ARG                                                  15100042
          ACTR  500000                                                  15150042
         AIF   (&DIAGTST EQ 0).NOMSG                                    15200042
         AIF   (&INITIAL EQ 0).NOMSG                                    15250042
         AIF   (&DIAGLIN EQ 1).NOMSG                                    15300042
         MNOTE '**NO LINE ASSOCIATED WITH THIS DATA**'                  15350042
*****THE FOLLOWING GENERATED FOR TEST PURPOSES*****                     15400042
*        LINE                                                           15450042
         LINE                                                           15500042
.NOMSG   ANOP                                                           15550042
         AIF   (K'&SYSLIST(1) EQ K'&SYSLIST(1,1)).LOOP                  15600042
&SUB     SETB  1                                                        15650042
.LOOP    ANOP                                                           15700042
&OP      SETA  &OP+1                                                    15750042
&ARG     SETC  '&SYSLIST(&OP,1)'                                        15800042
         ITEMFIND &ARG                                                  15850042
         AIF   (&ITEMXPT EQ 0).MEXIT                                    15900042
&ISHIFT  SETA  &SHIFT(&ITEMXPT)                                         15950042
&IBITS   SETA  &BITS(&ITEMXPT)                                          16000042
         AIF   (&SUB EQ 1).SUB00                                        16050042
         AIF   ('&SYSLIST(3)' EQ '').SETT                               16100042
&IBITS   SETA  &SYSLIST(3)                                              16150042
         AGO   .SETT                                                    16200042
.SUB00   AIF   ('&SYSLIST(&OP,3)' EQ '').SETT                           16250042
&IBITS   SETA  &SYSLIST(&OP,3)                                          16300042
.SETT    ANOP                                                           16350042
&T       SETA  &IBITS-1                                                 16400042
&B       SETB  0                                                        16450042
         AIF   (&IBITS GT 0 AND &IBITS LT 16).SUB0                      16500042
         MNOTE '**NO. OF BITS OUTSIDE RANGE OF 1-15**'                  16550042
.MEXIT   MEXIT                                                          16600042
.SUB0    AIF   (&SUB EQ 1).SUBA                                         16650042
         AIF   ('&SYSLIST(2)' EQ '').BACKUP                             16700042
&K       SETA  K'&SYSLIST(2)                                            16750042
&B       SETB  ('&SYSLIST(2)'(1,1) EQ 'B')                              16800042
&S       SETC  '&SYSLIST(2)'(&B+1,&K-&B)                                16850042
         AGO   .SUBB                                                    16900042
.SUBA    ANOP                                                           16950042
         AIF   ('&SYSLIST(&OP,2)' EQ '').BACKUP                         17000042
&K       SETA  K'&SYSLIST(&OP,2)                                        17050042
&B       SETB  ('&SYSLIST(&OP,2)'(1,1) EQ 'B')                          17100042
&S       SETC  '&SYSLIST(&OP,2)'(&B+1,&K-&B)                            17150042
.SUBB    ANOP                                                           17200042
&T       SETA  &S                                                       17250042
.BACKUP  ANOP                                                           17300042
&NRENTS  SETA  &NRENTS+1                                                17350042
&TEMP    SETA  &ISHIFT                                                  17400042
         AIF   (&IBITS NE 1 OR &B NE 0).BIN2                            17450042
         DC    AL.12(&SYSLIST(&OP,1)-&DSECT),XL.4'&TEMP'                17500042
         AGO   .OUT                                                     17550042
         AIF   (&SUB EQ 1).BIN2                                         17600042
         AIF   ('&SYSLIST(2)' NE '').BIN3                               17650042
         AGO   .BIN2A                                                   17700042
.BIN2    AIF   ('&SYSLIST(&OP,2)' NE '').BIN3                           17750042
.BIN2A   ANOP                                                           17800042
&B       SETB  1                                                        17850042
.BIN3    ANOP                                                           17900042
         DC    AL.12(&SYSLIST(&OP,1)-&DSECT),XL.4'C'                    17950042
         DC    BL.1'&B',AL.7(&T),AL.4(&IBITS),FL.4'&ISHIFT'             18000042
.OUT     ANOP                                                           18050042
&ITEMXPT     SETA  0                                                    18100042
         AIF   (&SUB EQ 0).OUTA                                         18150042
         AIF   (&OP LT N'&SYSLIST).LOOP                                 18200042
.OUTA    ANOP                                                           18250042
         MEND                                                           18300042

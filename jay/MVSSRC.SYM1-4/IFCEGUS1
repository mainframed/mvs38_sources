* ITEMFIND MACRO  ITEMFIND MACRO  ITEMFIND MACRO  ITEMFIND MACRO        30400042
         MACRO                                                          30450042
         ITEMFIND &ARG,&ITFDIAG=                                        30500042
         GBLA  &BITS(2600),&DB,&DT,&INITIAL                             30550042
         GBLA  &ITEMITR,&ITEMFST(20),&ITEMNBR,&ITEMMDX                  30600042
         GBLA  &ITEMNDX(2600),&ITEMXPT                                  30650042
         GBLA  &LABELNR,&LASTCC,&LB,&LBLCTR(100),&LPTR,&LT              30700042
         GBLA  &NRENTS,&SHIFT(2600)                                     30750042
         GBLB  &DIAGITF,&DIAGLIN,&DIAGSET,&DIAGTST,&FSW,&TEST           30800042
         GBLC  &CARRCON,&CSECT,&DSECT,&ITEM(2600)                       30850042
         GBLC  &LABELS(2600),&LBL(100),&NRENTSX                         30900042
         LCLA  &A,&B,&X,&I                                              30950042
         LCLC  &C1,&C2,&S                                               31000042
         ACTR  500000                                                   31050042
&ITEMXPT SETA  0                  INDICATE ITEM NOT FOUND.              31100042
         AIF   ('&ARG' NE '').NORM                                      31150042
         AIF   ('&ITFDIAG' EQ 'OFF'  OR  '&ITFDIAG' EQ 'ON').DIAGSET    31200042
  MNOTE 0,'ITEMFIND - OPERAND (&ITFDIAG) INVALID FOR ITFDIAG KEYWORD.'  31250042
         MEXIT                                                          31300042
.DIAGSET ANOP                                                           31350042
&DIAGITF SETB  ('&ITFDIAG' EQ 'ON')                                     31400042
         MEXIT                                                          31450042
.NORM    AIF   (&ITEMITR GT 0).SORTED                                   31500042
         ITEMSORT                                                       31550042
.SORTED  ANOP                                                           31600042
&I       SETA  &ITEMITR           COMPUTED ITTERATION COUNT.            31650042
&X       SETA  &ITEMMDX           COMPUTED MAXIMUM INDEX.               31700042
&C1      SETC  '&ARG'             ISOLATE SEARCH ARGUMENT.              31750042
&C1      SETC  '&C1'.'       '(1,8-K'&C1) PAD TO FULL 8 CHARACTERS.     31800042
.SRCHUP  ANOP                                                           31850042
&A       SETA  &A+&X              CURRENT TABLE SUBSCRIPT + INDEX.      31900042
&S       SETC  '+'                                                      31950042
         AIF   (&A  GT &ITEMNBR  AND  NOT &DIAGITF).SRCHDN1             32000042
         AIF   (&A  LE &ITEMNBR).SRCH00                                 32050042
&C2      SETC  '*-OVER-*'                                               32100042
&I       SETA  &I-1               DECREMENT ITTERATION COUNTER.         32150042
&X       SETA  &X/2               HALVE INDEX VALUE.                    32200042
 MNOTE *,'&ARG - &C2   ITRCTR = &I   PTR = &A&S &X'                     32250042
         AIF   (&I GE 0).SRCHDWN  IF ITTERATION COUNT NOT EXPIRED.      32300042
.NOFIND  MNOTE  20,' ITEMFIND FAILED FOR NAME  &C1  '                   32350042
         MEXIT                                                          32400042
.SRCH00  ANOP                                                           32450042
&I       SETA  &I-1               DECREMENT ITTERATION COUNTER.         32500042
         AIF   (&I LT 0).NOFIND   IF COUNT HAS GONE TO ZERO.            32550042
&X       SETA  &X/2               HALVE INDEX VALUE.                    32600042
&B       SETA  &ITEMNDX(&A)       GET SUBSCRIPT FROM INDEX TABLE.       32650042
&C2      SETC  '&ITEM(&B)'.'       '(1,8-K'&ITEM(&B))  PAD COMPARAND.   32700042
         AIF   (NOT &DIAGITF).COMPARE                                   32750042
 MNOTE *,'&ARG - &C2   ITRCTR = &I   PTR = &A&S &X   POS = &B'          32800042
.COMPARE AIF   ('&C1' LT '&C2').SRCHDWN   ARGUMENT LOWER, MOVE DOWN.    32850042
         AIF   ('&C1' GT '&C2').SRCHUP    ARGUMENT HIGHER, MOVE UP.     32900042
&ITEMXPT SETA  &B                 FOUND. SET INDEX POINTER FOR RETURN.  32950042
&ITEMFST(&I+1)  SETA  &ITEMFST(&I+1)+1                                  33000042
         MEXIT                                                          33050042
.SRCHDWN ANOP                                                           33100042
&A       SETA  &A-&X              CURRENT POINTER MINUS INDEX VALUE.    33150042
&S       SETC  '-'                                                      33200042
         AIF   (&A LE &ITEMNBR).SRCH00    IF WITHIN BOUNDS OF TABLE.    33250042
.SRCHDN1 ANOP                                                           33300042
&I       SETA  &I-1               DECREMENT ITTERATION COUNTER.         33350042
&X       SETA  &X/2               HALVE INDEX VALUE.                    33400042
         AIF   (&I GE 0).SRCHDWN  IF ITTERATION COUNT NOT EXPIRED.      33450042

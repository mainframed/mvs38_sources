         LCLB  &SW(3000)                                                34350042
         LCLC  &C1,&C2                                                  34400042
         ACTR  500000                                                   34450042
&A1      SETA  0                                                        34500042
         AIF   (&ITEMITR EQ 0).LOOP0   TEST FOR RE-ENTRY.               34550042
 MNOTE   0,'ITEMSORT MACRO OCCURRS MORE THAN ONCE. TABLE NOT RESORTED.' 34600042
         MEXIT                                                          34650042
.LOOP0   ANOP                                                           34700042
&A1      SETA  &A1+1                                                    34750042
&ITEMNDX(&A1) SETA &A1                                                  34800042
         AIF   (&A1 LT &ITEMNBR).LOOP0                                  34850042
         AIF   (&ITEMNBR LT 2).MEXIT                                    34900042
&A1      SETA  &ITEMNBR/2         HALF OF TOTAL ENTRIES.                34950042
&A2      SETA  0                                                        35000042
.LOOP1   ANOP                                                           35050042
&A2      SETA  &A2+1                                                    35100042
&SW(&A2) SETB  1                  SET FIRST HALF OF SWITCHES TO 1.      35150042
         AIF   (&A2 LT &A1).LOOP1                                       35200042
.LOOP2   ANOP                                                           35250042
&A2      SETA  &A2+1                                                    35300042
&SW(&A2) SETB  0                  SET SECOND HALF TO ZERO.              35350042
         AIF   (&A2 LT &ITEMNBR).LOOP2                                  35400042
&R       SETA  0                  SET R TO FIRST TRIAL ITEM.            35450042
&ZI      SETA  &ITEMNBR-1         SET ZI TO LAST ITEM.                  35500042
&I       SETA  &ZI-1                                                    35550042
.T3      ANOP                                                           35600042
&SR      SETB  (&SW(&R+1))        GET FLAG, WHICH WAY TO GO?            35650042
&R       SETA  (&R*2)+1+&SR       R=2R+(1 OR 2)                         35700042
         AIF   (&R LE &I).T3                                            35750042
.T4      ANOP                                                           35800042
&R       SETA  (&R-1)/2           R=(R-1)/2                             35850042
&SR      SETB  (&SW(&R+1))        GET SUBTREE SELECT FLAG.              35900042
         AIF   (&SR EQ 0).T7                                            35950042
         AIF   (&GOTO EQ 0).T5                                          36000042
.T7      ANOP                                                           36050042
&A1      SETA  &ITEMNDX(&R+1)                                           36100042
&A2      SETA  &ITEMNDX(&ZI+1)                                          36150042
&C1      SETC  '&ITEM(&A1)'.'       '(1,8-K'&ITEM(&A1))                 36200042
&C2      SETC  '&ITEM(&A2)'.'       '(1,8-K'&ITEM(&A2))                 36250042
         AIF   ('&C1' LT '&C2').T8                                      36300042
.T9      ANOP                                                           36350042
&TEMP    SETA  &ITEMNDX(&R+1)     Z(R) TO TEMP.                         36400042
&TB      SETB  (&SW(&R+1))                                              36450042
&ITEMNDX(&R+1) SETA &ITEMNDX(&ZI+1)    Z(I) TO Z(R)                     36500042
&SW(&R+1) SETB (&SW(&ZI+1))                                             36550042
&ITEMNDX(&ZI+1) SETA &TEMP        TEMP TO Z(I)                          36600042
&SW(&ZI+1) SETB (&TB)                                                   36650042
&SR      SETB  (&SR EQ 0)                                               36700042
&SW(&R+1) SETB (&SR)                                                    36750042
.T8      AIF   (&R GT 0).T4                                             36800042
&GOTO    SETB  1                  SET 'GO TO' T7                        36850042
&ZI      SETA  &ZI-1                                                    36900042
&I       SETA  &I-1                                                     36950042
         AIF   (&I GE 0).T3                                             37000042
.MEXIT   ANOP                                                           37050042
&ITEMITR SETA 1                                                         37100042
&ITEMMDX SETA 1                                                         37150042
.ITRLOOP ANOP                                                           37200042
&ITEMITR SETA &ITEMITR+1                                                37250042
&ITEMMDX SETA &ITEMMDX+&ITEMMDX                                         37300042
         AIF  (&ITEMMDX LT &ITEMNBR).ITRLOOP                            37350042
&ITEMITR SETA &ITEMITR-1                                                37400042
&ITEMMDX SETA &ITEMMDX/2                                                37450042
&A1      SETA &ITEMNBR                                                  37500042
&A2      SETA &ITEMMDX                                                  37550042
&TEMP    SETA &ITEMITR                                                  37600042
 MNOTE *,' ITEMS = &A1, MAX INDEX = &A2, ITTERATION COUNT = &TEMP'      37650042
         MEXIT                                                          37700042
.T5      ANOP                                                           37750042
&TEMP    SETA  &ITEMNDX(&R+1)     Z(R) TO TEMP.                         37800042
&TB      SETB  (&SW(&R+1))                                              37850042
&ITEMNDX(&R+1) SETA &ITEMNDX(&ZI+1)    Z(I) TO Z(R)                     37900042
&SW(&R+1) SETB (&SW(&ZI+1))                                             37950042
&ITEMNDX(&ZI+1) SETA &TEMP        TEMP TO Z(I)                          38000042
&SW(&ZI+1) SETB (&TB)                                                   38050042
&SR      SETB  (&SR EQ 0)                                               38100042
&SW(&R+1) SETB (&SR)                                                    38150042
         AGO   .T3                                                      38200042
         MEND                                                           38250042

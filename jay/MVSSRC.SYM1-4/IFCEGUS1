BRCNT    STM   R5,R8,XSAVE                                              49900042
         IC    R7,L0(R4)                GET 1ST BYTE                    49950042
         SLL   R7,L8                    MAKE ROOM                       50000042
         IC    R7,L1(R4)                GET 2ND BYTE                    50050042
         SLL   R7,L8                    MAKE ROOM                       50100042
         IC    R7,L2(R4)                GET 3RD BYTE                    50150042
         SLL   R7,L8                    MAKE ROOM                       50200042
         IC    R7,L3(R4)                GET 4TH BYTE                    50250042
         LA    R8,CHARS                 GET ADDR. OF CHAR. TABLE        50300042
         SR    R6,R6                    CLEAR REG                       50350042
         SLDL  R6,L4                    GET 4 BITS                      50400042
         AR    R6,R8                    COMPUTE OFFSET INTO CHAR. TABLE 50450042
         MVC   L0(L1,R3),L0(R6)         MOVE CHAR INTO LINE             50500042
         SR    R6,R6                    CLEAR REG                       50550042
         SLDL  R6,L4                    GET 4 BITS                      50600042
         AR    R6,R8                    COMPUTE OFFSET INTO CHAR. TABLE 50650042
         MVC   L1(L1,R3),L0(R6)         MOVE CHAR INTO LINE             50700042
         SR    R6,R6                    CLEAR REG                       50750042
         SLDL  R6,L4                    GET 4 BITS                      50800042
         AR    R6,R8                    COMPUTE OFFSET INTO CHAR. TABLE 50850042
         MVC   L2(L1,R3),L0(R6)         MOVE CHAR INTO LINE             50900042
         SR    R6,R6                    CLEAR REG                       50950042
         SLDL  R6,L4                    GET 4 BITS                      51000042
         AR    R6,R8                    COMPUTE OFFSET INTO CHAR. TABLE 51050042
         MVC   L3(L1,R3),L0(R6)         MOVE CHAR INTO LINE             51100042
         SR    R6,R6                    CLEAR REG                       51150042
         SLDL  R6,L4                    GET 4 BITS                      51200042
         AR    R6,R8                    COMPUTE OFFSET INTO CHAR. TABLE 51250042
         MVC   L4(L1,R3),L0(R6)         MOVE CHAR INTO LINE             51300042
         SR    R6,R6                    CLEAR REG                       51350042
         SLDL  R6,L4                    GET 4 BITS                      51400042
         AR    R6,R8                    COMPUTE OFFSET INTO CHAR. TABLE 51450042
         MVC   L5(L1,R3),L0(R6)         MOVE CHAR INTO LINE             51500042
         SR    R6,R6                    CLEAR REG                       51550042
         SLDL  R6,L4                    GET 4 BITS                      51600042
         AR    R6,R8                    COMPUTE OFFSET INTO CHAR. TABLE 51650042
         MVC   L6(L1,R3),L0(R6)         MOVE CHAR INTO LINE             51700042
         SR    R6,R6                    CLEAR REG                       51750042
         SLDL  R6,L4                    GET 4 BITS                      51800042
         AR    R6,R8                    COMPUTE OFFSET INTO CHAR. TABLE 51850042
         MVC   L7(L1,R3),L0(R6)         MOVE CHAR INTO LINE             51900042
         B     RETURN                                                   51950042
*********************************************************************** 52000042
*              CHECK IF BYTES 14-19 SHOULD BE PRINTED                   52050042
*********************************************************************** 52100042
CONDSNS  STM   R5,R8,XSAVE                                              52150042
         TM    L0(R4),HEXFF             Q/ ANY BITS ON IN BYTE 8        52200042
         BNZ   NXTEST                   YES, SKIP OVERLAY               52250042
         LR    R5,R3                    GET PTR TO LINE POS.            52300042
         LA    R6,L96                   AMOUNT TO BACKSPACE             52350042
         SR    R5,R6                    BACKSPACE                       52400042
         MVC   L0(L4,R5),XXXX           MOVE IN 'XXXX'                  52450042
NXTEST   TM    XB12BIT0,HEXFF           Q/ ANY BITS ON IN BYTE 12       52500042
         BNZ   NXTEST1                  YES, SKIP OVERLAY               52550042
         TM    XB13BIT0,HEXFF           Q/ ANY BITS ON IN BYTE 13       52600042
         BNZ   NXTEST1                  YES, SKIP OVERLAY               52650042
         LR    R5,R3                    GET PTR TO PRINT POS.           52700042
         LA    R6,L64                   AMOUNT TO BACKSPACE PTR         52750042
         SR    R5,R6                    BACKSPACE                       52800042
         MVC   L0(L4,R5),XXXX           MOVE IN 'XXXX'                  52850042
NXTEST1  TM    XB10BIT0,HEXFF           Q/ ANY BITS ON IN BYTE 10       52900042
         BNZ   RETURN                   YES, SKIP OVERLAY               52950042
         LR    R5,R3                    GET PTR TO PRINT POS.           53000042
         LA    R6,L32                   AMOUNT TO BACKSPACE PTR         53050042
         SR    R5,R6                    BACKSPACE                       53100042
         MVC   L0(L4,R5),XXXX           MOVE IN 'XXXX'                  53150042
         B     RETURN                                                   53200042
*********************************************************************** 53250042
*              CHECK IF BYTES 20-27 SHOULD BE PRINTED                   53300042
*********************************************************************** 53350042
CONDSNS1 STM   R5,R8,XSAVE              SAVE REGS                       53400042
         TM    L0(R4),HEX30             Q/ IS BIT 2 OR 3 ON IN BYTE 12  53450042
         BC    5,RETURN                 YES, 1 OR BOTH IS ON            53500042
         LR    R5,R3                    GET PTR TO PRINT POS.           53550042
         LA    R6,L31                   AMOUNT TO BACKSPACE PTR         53600042
         SR    R5,R6                    BACKSPACE PTR                   53650042
         MVC   L0(L30,R5),OVRLAYS       MOVE IN OVERLAY                 53700042
         B     RETURN                                                   53750042
*********************************************************************** 53800042
*              CONVERT RELEASE                                          53850042
*********************************************************************** 53900042
CONVREL  STM   R5,R8,XSAVE                                              53950042
         SR    R6,R6                    CLEAR REG                       54000042
         IC    R6,L0(R4)                GET SYSTEM AND RELEASE VALUES   54050042
         SRDL  R6,L5                    SAVE REL. NO. IN REG 7          54100042
         SLL   R6,L2                    MULT. SYSTEM VALUE BY 4         54150042
         L     R8,XLABELP               GET LABEL PTR                   54200042
         LA    R6,RELTAB(R6)            INDEX INTO TABLE OF SYSTEMS     54250042
         MVC   L0(L4,R8),L0(R6)         MOVE SYSTEM NAME INTO LABEL     54300042
         SR    R6,R6                    CLEAR REG                       54350042
         SLDL  R6,L5                    RETRIEVE RELEASE NO.            54400042
         LTR   R6,R6                    Q/ IS REL. NO. ZERO             54450042
         BZ    NOREL                    YES                             54500042
         CVD   R6,WORK                  CONVERT REL. NO. TO DECIMAL     54550042
         UNPK  L0(L2,R3),WORK+L6(L2)    UNPACK IT INTO PRINT BUFFER     54600042
         MVZ   L1(L1,R3),L0(R3)         MOVE IN PROPER ZONE             54650042
         B     RETURN                                                   54700042
NOREL    MVC   L0(L4,R3),NONE           MOVE IN 'NONE' FOR RELEASE      54750042
         B     RETURN                                                   54800042
*********************************************************************** 54850042
*              EXPAND RECORD                                            54900042
*********************************************************************** 54950042
EXPNDRTN STM   R5,R8,XSAVE                                              55000042
         ST    R2,SAVE2                 SAVE R2 FOR LATER               55050042
         LA    R5,OBRAREA               PICK UP ADDR OF EXPANDED AREA   55100042
         USING EXPNOBR,R5                                               55150042
         USING NORMOBR,R4                                               55200042
         XC    EXPNOBR(L106),EXPNOBR    CLEAR AREA                      55250042
         TM    SW2B0,HEX20              Q/ IS THIS A SHORT OBR RECORD   55300042
         BO    MOVESHRT                 YES                             55350042
MOVEALL  MVC   EXPNOBR(L106),NORMOBR    MOVE ENTIRE RECORD              55400042
EXPNDRET LR    R2,R5                    SWAP PTRS                       55450042
         B     RETURN                                                   55500042
         USING SHRTOBR,R4                                               55550042
MOVESHRT MVC   EXPNHDRA(L24),SHRTHDRA   MOVE HEADER                     55600042
         MVC   EXPNSDRA(L10),SHRTSDRA   MOVE STATISTICAL AREA           55700042
         B     EXPNDRET                                                 55750042
         DROP  R4,R5                                                    55800042
*********************************************************************** 55850042
*              CLEAR HEX JOBNAME IF SHORT OBR RECORD                    55900042
*********************************************************************** 55950042
NONAME   TM    L0(R4),HEX20             Q/ IS THIS A SHORT OBR RECORD   56000042
         BCR   14,R14                   NO, SO LET HEX JOB NAME PRINT   56050042
         ST    R5,XSAVE                 SAVE WORK REG                   56100042
         LA    R5,L44                   AMOUNT TO BACKSPACE PTR         56150042
         SR    R3,R5                    BACKUP PTR                      56200042
         MVC   L0(L17,R3),BLANKS        CLEAR OUT HEX JOBNAME           56250042
         AR    R3,R5                    RESTORE PTR                     56300042
         L     R5,XSAVE                 RESTORE REG                     56350042
         BR    R14                                                      56400042
*********************************************************************** 56450042
*              DETERMINE IF LONG OR SHORT OBR RECORD                    56500042
*********************************************************************** 56550042
RECTYPE  STM   R5,R8,XSAVE                                              56600042
         LR    R5,R3                    GET PTR TO CUR. DATA PRINT POS. 56650042
         LA    R6,L31                   AMOUNT TO BACKSPACE PTR         56700042
         SR    R5,R6                    BACKSPACE PTR                   56750042
         TM    L0(R4),HEX20             Q/ IS THIS A SHORT OBR RECORD   56800042
         BO    RECSHORT                 YES                             56850042
         MVC   L0(L10,R5),UNITCK        NO, INSERT 'UNIT CHECK' LABEL   56900042
         B     RETURN                                                   56950042
RECSHORT MVC   L0(L10,R5),OVFL          INSERT 'OVERFLOW'               57000042
         B     RETURN                                                   57050042
*********************************************************************** 57100042
*              RESET REG. 2 TO START OF INPUT RECORD                    57150042
*********************************************************************** 57200042
RESETR2  L     R2,SAVE2                 RESTORE REG                     57250042
         BR    R14                                                      57300042
*********************************************************************** 57350042
*              COMMON REGISTER RESTORE AND RETURN                       57400042
*********************************************************************** 57450042
RETURN   LM    R5,R8,XSAVE                                              57500042
         BR    R14                                                      57550042
*********************************************************************** 57600042
*              TEST FOR CONVERTED RECORD - KEY 32                       57650042
*********************************************************************** 57700042
SETCONV  TM    L0(R4),HEX02             Q/ IS CONVERT SWITCH ON         57750042
         BCR   1,R14                    YES                             57800042
         SR    R15,R15                  ZERO IS TO STOP PRINT           57850042
         BR    R14                                                      57900042
*********************************************************************** 57950042
*              SETCPU NO. AND SERIAL NO.                                58000042
*********************************************************************** 58050042
SETCPUNO STM   R5,R8,XSAVE              SAVE REGS                       58100042
         SR    R5,R5                    CLEAR REG                       58150042
         IC    R5,L0(R4)                GET 1ST BYTE OF CPU MODEL NO.   58200042
         SLL   R5,L4                    MAKE ROOM FOR SECOND BYTE       58250042
         IC    R5,L1(R4)                GET 2ND BYTE                    58300042
         LTR   R5,R5                    Q/ IS MODEL NO. ZERO            58350042
         LR    R5,R3                    GET PTR TO CURRENT PRNT POS.    58400042
         BNZ   SETSER                   MODEL NO. IS NOT ZERO           58450042
         LA    R6,L46                   AMOUNT TO BACKSPACE PTR         58500042
         SR    R5,R6                    BACKSPACE PTR                   58550042
         MVC   L0(L29,R5),UNIVERS       INSERT LABEL FOR NONE           58600042
         B     RETURN                                                   58650042
SETSER   LA    R6,L36                   AMOUNT TO BACKSPACE             58700042
         SR    R5,R6                    BACKSPACE PTR                   58750042
         MVC   L0(L10,R5),SERNO         INSERT 'SERIAL NO.'             58800042
         B     RETURN                                                   58850042
*********************************************************************** 58900042
*              SET DATE                                                 58950042
*********************************************************************** 59000042
SETDATE  CLC   L1(L3,R4),ZERO           Q/ IS DATE ZERO                 59050042
         BE    SETDTE                   YES                             59100042
         UNPK  XSAVE(L8),L1(L3,R4)      UNPACK DATE                     59150042
         MVC   L5(L2,R3),XSAVE+L3       MOVE YEAR                       59200042
         MVC   L0(L3,R3),XSAVE+L5       MOVE JULIAN DAY                 59250042
         BR    R14                                                      59300042
SETDTE   MVC   L0(L4,R3),NONE           SET DATE TO NONE                59350042
         BR    R14                                                      59400042
*********************************************************************** 59450042
*              HEX (CONVERT 1 HEX BYTE TO 2 EBCDIC CHARACTER)           59500042
*********************************************************************** 59550042
SETHEX1  MVI   TFLG,X'01'               INDICATE BACKSPACE REQUIRED     59600042
SETHEX   STM   R5,R8,XSAVE              SAVE REGS                       59650042
         LA    R8,CHARS                 GET ADDR OF EBCDIC CHAR. TABLE  59700042
         IC    R7,L0(R4)                PICK UP BYTE TO CONVERT         59750042
         SLL   R7,L24                   MOVE IT TO HI PART OF R7        59800042
         SR    R6,R6                    CLEAR REG                       59850042
         SLDL  R6,L4                    MOVE 4 BITS TO R6               59900042
         AR    R6,R8                    COMPUTE OFFSET INTO CHAR. TABLE 59950042
         TM    TFLG,X'01'               Q/ BACKSPACE REQUIRED           60000042
         BO    PERFCNT                  YES                             60050042
         MVC   L0(L1,R3),L0(R6)         MOVE CHAR TO LINE               60100042
SETHEXX  SR    R6,R6                    CLEAR REG                       60150042
         SLDL  R6,L4                    MOVE LAST 4 BITS TO R6          60200042
         AR    R6,R8                    COMPUTE OFFSET                  60250042
         TM    TFLG,X'01'               Q/ BACKSPACE REQUIRED           60300042
         BO    PERFCNT1                 YES                             60350042
         MVC   L1(L1,R3),L0(R6)         MOVE CHAR TO LINE + 1           60400042
         B     RETURN                                                   60450042
PERFCNT  LR    R5,R3                    GET PTR TO PRINT POS.           60500042
         LA    R8,L29                   SET UP FOR BACKSPACE            60550042
         SR    R5,R8                    BACKSPACE                       60600042
         MVC   L0(L1,R5),L0(R6)         MOVE CHAR INTO LINE             60650042
         LA    R8,CHARS                 RESTORE REG 8                   60700042
         B     SETHEXX                  RETURN TO MAINLINE              60750042
PERFCNT1 MVC   L1(L1,R5),L0(R6)         MOVE CHAR INTO LINE             60800042
         MVI   TFLG,X'00'               CLEAR FLAG                      60850042
         B     RETURN                                                   60900042
*********************************************************************** 60950042
*              HEX (CONVERT 2 HEX BYTES TO 4 EBCDIC CHARACTERS)         61000042
*********************************************************************** 61050042
SETHEX2  STM   R5,R8,XSAVE              SAVE REGS                       61100042
         LA    R8,CHARS                 GET ADDR OF EBCDIC CHAR. TABLE  61150042
         IC    R7,L0(R4)                PICK UP BYTE TO CONVERT         61200042
         SLL   R7,L24                   MOVE IT TO HI PART OF R7        61250042
         SR    R6,R6                    CLEAR REG                       61300042
         SLDL  R6,L4                    MOVE 4 BITS TO R6               61350042
         AR    R6,R8                    COMPUTE OFFSET INTO CHAR. TABLE 61400042
         MVC   L0(L1,R3),L0(R6)         MOVE CHAR TO LINE               61450042
         SR    R6,R6                    CLEAR                           61500042
         SLDL  R6,L4                    MOVE LAST 4 BITS TO R6          61550042
         AR    R6,R8                    COMPUTE OFFSET                  61600042
         MVC   L1(L1,R3),L0(R6)         MOVE CHAR TO LINE + 1           61650042
         IC    R7,L1(R4)                PICK UP 2ND INPUT BYTE          61700042
         SLL   R7,L24                   MOVE IT TO HI PART OF R7        61750042
         SR    R6,R6                    CLEAR REG                       61800042
         SLDL  R6,L4                    MOVE 4 BITS TO R6               61850042
         AR    R6,R8                    COMPUTE OFFSET INTO CHAR. TABLE 61900042
         MVC   L2(L1,R3),L0(R6)         MOVE CHAR TO LINE + 2           61950042
         SR    R6,R6                    CLEAR                           62000042
         SLDL  R6,L4                    MOVE LAST 4 BITS TO R6          62050042
         AR    R6,R8                    COMPUTE OFFSET                  62100042
         MVC   L3(L1,R3),L0(R6)         MOVE CHAR TO LINE + 3           62150042
         B     RETURN                                                   62200042
*********************************************************************** 62250042
*              SET PROGRAM NAME                                         62300042
*********************************************************************** 62350042
SETPGMN  DS    0H                                                       62400042
         TM    SW2B0,HEX20              Q/ IS THIS A SHORT OBR RECORD   62450042
         BCR   1,R14                    YES, THUS NO JOB ID             62500042
         MVC   L0(L12,R3),JOBID         MOVE IN 'JOB IDENTITY'          62550042
         MVC   L13(L8,R3),XPGN1         MOVE IN JOB NAME                62600042
         BR    R14                                                      62650042
*********************************************************************** 62700042
*              ADVANCE DATA PTR TO NEXT POSITION                        62750042
*********************************************************************** 62800042
SKIPPER  BR    R14                      RETURN                          62850042
*********************************************************************** 62900042
*              DETERMINE SENSE DATA TYPE                                62950042
*********************************************************************** 63000042
SNSTYPE  TM    XB3SNSTY,HEX01           Q/ IS THIS HARDWARE VERIF. SNS  63050042
         BO    HARDWARE                 YES                             63100042
         TM    XB3SNSTY,HEX02           Q/ IS THIS DATA-RELATED SENSE   63150042
         BO    DATAREL                  YES                             63200042
SNSRET   BR    R14                      MUST BE EQUIPMENT-RELATED SNS   63250042
HARDWARE STM   R5,R8,XSAVE                                              63300042
         LA    R5,HWSENSE-4             GET ADR OF 1ST HW LINE          63350042
         ST    R5,XR9PTR                SAVE PTR                        63400042
         L     R5,HWLABAD               GET ADR OF 1ST HW LABELS        63450042
         ST    R5,XLABELP               SAVE PTR                        63500042
         SR    R15,R15                  DONT PRINT THIS LINE            63550042
         B     RETURN                                                   63600042
DATAREL  STM   R5,R8,XSAVE                                              63650042
         LA    R5,DATSENSE-4            GET ADR OF 1ST DAT-REL LINE     63700042
         ST    R5,XR9PTR                SAVE PTR                        63750042
         L     R5,DATLABAD              GET ADR OF 1ST DAT-REL LABEL    63800042
         ST    R5,XLABELP               SAVE PTR                        63850042
         SR    R15,R15                  DONT PRINT THIS LINE            63900042
         B     RETURN                                                   63950042
*********************************************************************** 64000042
*              ADVANCE PRINT POSITION PTR 1 SPACES                      64050042
*********************************************************************** 64100042
SPACER1  LA    R3,L1(R3)                ADVANCE PRINT POS. PTR 1 SPACE  64150042
         BR    R14                                                      64200042
*********************************************************************** 64250042
*              ADVANCE PRINT POSITION PTR 16 SPACES                     64300042
*********************************************************************** 64350042
SPACER16 LA    R3,L16(R3)               UP PTR BY 16                    64400042
         BR    R14                                                      64450042
*********************************************************************** 64500042
*              MP TEST                                                  64550042
*********************************************************************** 64600042
TESTMP   TM    L0(R4),HEX10             Q/ IS THIS A MP RECORD          64650042
         BZ    NOMP                     NO                              64700042
         MVC   L0(L20,R3),MPCPU         MOVE IN MP LABEL                64750042
         TM    L0(R4),HEX08             Q/ IS IT CPUA OR CPUB           64800042
         BO    MPB                      IT IS CPUB                      64850042
         MVI   L21(R3),A                MOVE 'A' IN PRINT LINE          64900042
NOMP     BR    R14                                                      64950042
MPB      MVI   L21(R3),B                MOVE 'B' IN PRINT LINE          65000042
         BR    R14                                                      65050042
*********************************************************************** 65100042
*              IF SHORT RECORD THEN SKIP TO STAT. EDIT SETS             65150042
*********************************************************************** 65200042
TESTSHRT TM    SW2B0,HEX20              Q/ IS THIS A SHORT OBR RECORD   65250042
         BZ    SKIPPER                  NO                              65300042
         BCTR  R3,R0                    BACK UP POS. PTR 1 SPACE        65350042
         MVC   L0(L6,R3),BLANKS         CLEAR HEX SECONDARY CUA         65400042
         MVC   XR9PTR(L8),SHRTADCN      POINT TO STAT. EDIT SETS        65450042
         BR    R14                                                      65500042
*********************************************************************** 65550042
*              LINE/LABEL SETS                                          65600042
*********************************************************************** 65650042
ENTRIES  DS    0F                                                       65700042
         DC    XL4'00'             FORMAT 1                             65750042
         DC    XL4'00'             FORMAT 2                             65800042
         DC    A(LABELS)           LABEL PTR                            65850042
         DC    H'0'                DISP                                 65900042
*********************************************************************** 65950042

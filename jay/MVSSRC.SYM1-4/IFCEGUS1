*********************************************************************** 00150042
*  DSGEN MACRO DSGEN MACRO DSGEN MACRO DSGEN MACRO DSGEN MACRO          00200042
         MACRO                                                          00250042
&NAME    DSGEN ,                                                        00300042
.*                            THIS MACRO GENERATES ONE DSECT WORD       00350042
.*                                 FORMATTED FOR THE SUBJECT LOG RECORD 00400042
.*                                 EACH OPERAND HAS TWO SUB LISTS. THE  00450042
.*                                 FIRST IS THE NAME OF THE LOG ITEM    00500042
.*                                 WHILE THE SECOND IS THE # OF BITS TO 00550042
.*                                 ALLOCATE FOR THIS ITEM. ITEMS MUST   00600042
.*                                 BE DEFINED CONTIGUOUSLY WITHOUT GAPS 00650042
.*                                 UNUSED BIT GROUPS MUST BE GIVEN      00700042
.*                                 DUMMY NAMES. THE NAMES PROVIDED FOR  00750042
.*                                 THE LOG ITEMS MAY THEN BE USED IN    00800042
.*                                 THE CONVERSION MACRO INSTRUCTIONS.   00850042
         GBLA  &BITS(2600),&DB,&DT,&INITIAL                             00900042
         GBLA  &ITEMITR,&ITEMFST(20),&ITEMNBR,&ITEMMDX                  00950042
         GBLA  &ITEMNDX(2600),&ITEMXPT                                  01000042
         GBLA  &LABELNR,&LASTCC,&LB,&LBLCTR(100),&LPTR,&LT              01050042
         GBLA  &NRENTS,&SHIFT(2600)                                     01100042
         GBLB  &DIAGITF,&DIAGLIN,&DIAGSET,&DIAGTST,&FSW,&TEST           01150042
         GBLC  &CARRCON,&CSECT,&DSECT,&ITEM(2600)                       01200042
         GBLC  &LABELS(2600),&LBL(100),&NRENTSX                         01250042
         LCLB  &ONLY1,&CTIND                                            01300042
         LCLA  &START1,&START2,&END1,&END2                              01350042
         LCLC  &SYMBOL,&EQUSYM                                          01400042
         LCLA  &OP,&COUNT,&BYTE,&EQUCNT                                 01450042
         ACTR  500000                                                   01500042
         AIF   (&ITEMITR EQ 0).START                                    01550042
 MNOTE   20,'DSGEN MACRO OCCURRS AFTER ITEMSORT MACRO HAS BEEN ISSUED.' 01600042
         MEXIT                                                          01650042
.START   AIF   (N'&SYSLIST(&OP+1) LT 3).LOOP                            01700042
         AIF   ('&SYSLIST(&OP+1,3)' EQ 'EQU').LOOP     TEST FOR EQU     01750042
  AIF   ('&SYSLIST(&OP+1,3)' EQ '' OR '&SYSLIST(&OP+1,4)' EQ '').ERR1   01800042
         AIF   ('&SYSLIST(&OP+1,3)' GT '&SYSLIST(&OP+1,4)').ERR3        01850042
&START1  SETA  &SYSLIST(&OP+1,3)        GET START COUNT OF 1            01900042
&START2  SETA  &SYSLIST(&OP+1,4)        GET FIRST END COUNT             01950042
         AGO   .SET2A                   BRANCH AROUND                   02000042
         AIF   ('&SYSLIST(&OP+1,5)' GT '&SYSLIST(&OP+1,6)').ERR3        02050042
.ERR1    MNOTE '**FIRST SYMBOL CONCATENATION INCOMPLETE**'              02100042
         MEXIT                                                          02150042
.SET2A   AIF   ('&SYSLIST(&OP+1,5)' EQ ''AND '&SYSLIST(&OP+1,6)' EQ '' X02200042
               ).SET2                   TEST FOR NO SECOND              02250042
  AIF   ('&SYSLIST(&OP+1,5)' EQ '' OR '&SYSLIST(&OP+1,6)' EQ '').ERR2   02300042
&END1    SETA  &SYSLIST(&OP+1,5)        GET START COUNT OF 2            02350042
&END2    SETA  &SYSLIST(&OP+1,6)        GET END COUNT OF 2              02400042
         AGO   .LOOP                                                    02450042
.SET2    ANOP                                                           02500042
&ONLY1   SETB  1                        INDICATE ONLY ONE CONCATENATION 02550042
         AGO   .LOOP                                                    02600042
.ERR2    MNOTE '**SECOND SYMBOL CONCATENATION INCOMPLETE**'             02650042
         MEXIT                                                          02700042
.ERR3    MNOTE '**START GREATER THAN END CONCATENATION**'               02750042
         MEXIT                                                          02800042
.LOOP    ANOP                                                           02850042
&OP      SETA  &OP+1                                                    02900042
         AIF   (&OP GT N'&SYSLIST).DONE      TEST IF ALL DONE           02950042
&ITEMNBR SETA  &ITEMNBR+1                                               03000042
         AIF   ('&SYSLIST(&OP,2)' NE '').OK1                            03050042
         MNOTE '**NULL BIT COUNT FIELD**'                               03100042
         MEXIT                                                          03150042
.OK1     ANOP                                                           03200042
&SYMBOL  SETC  '&SYSLIST(&OP,1)'                                        03250042
         AIF   ('&SYMBOL' NE '').OK                                     03300042
*              *****     LOG ITEM SYMBOL NOT PROVIDED     *****         03350042
         AIF   ('&SYSLIST(&OP,3)' NE 'EQU').CNT                         03400042
         AGO   .START                   GET NEXT                        03450042
.OK      AIF   (N'&SYSLIST(&OP) LT 3).NO3    TEST FOR OLD FORMAT        03500042
         AIF   ('&SYSLIST(&OP,3)' EQ 'EQU').EQU   TEST FOR EQU          03550042
         AIF   (&ONLY1).OK2             TEST FOR ONLY ONE CONCAT        03600042
&SYMBOL  SETC  '&SYMBOL.&START1.&END1'       GET 1 AND 2 CONCAT         03650042
         AGO   .NO3                     BRANCH AROUND                   03700042
.OK2     ANOP                                                           03750042
&SYMBOL  SETC  '&SYMBOL.&START1'        GET 1 CONCATENATED              03800042
.NO3     ANOP                                                           03850042
&SYMBOL  EQU   *+&BYTE                                                  03900042
&ITEM(&ITEMNBR) SETC '&SYMBOL'          GET LABEL IN GLOBAL             03950042
&BITS(&ITEMNBR) SETA &SYSLIST(&OP,2)                                    04000042
&SHIFT(&ITEMNBR) SETA &COUNT-&BYTE*8                                    04050042
.CNT     ANOP                                                           04100042
&CTIND   SETB  1                        SET COUNT INDICATOR             04150042
&COUNT   SETA  &COUNT+&SYSLIST(&OP,2)                                   04200042
&BYTE    SETA  &COUNT/8                                                 04250042
         AIF   (N'&SYSLIST(&OP) LT 3).START  TEST FOR OLD FORMAT        04300042
         AIF   (&ONLY1).TSTST           TEST FOR ONLY ONE CONCAT        04350042
         AIF   (&END1 EQ &END2).TSTST   TEST FOR SECOND END             04400042
&END1    SETA  &END1+1                  INCREMENT COUNT                 04450042
&OP      SETA  &OP-1                    DECREMENT INCREMENT             04500042
         AGO   .LOOP                    GENERATE NEXT                   04550042
.TSTST   ANOP                                                           04600042
         AIF   (&START1 EQ &START2).LPDONE   TEST FOR END               04650042
         AIF   (&ONLY1).NSET1           TEST FOR ONLY ONE CONCAT        04700042
&END1    SETA  &SYSLIST(&OP,5)          RE-INIT SECOND START COUNT      04750042
.NSET1   ANOP                                                           04800042
&OP      SETA  &OP-1                    DECREMENT INDEX                 04850042
&START1  SETA  &START1+1                INCREMENT FIRST COUNT           04900042
         AGO   .LOOP                    GO DO NEXT                      04950042
.LPDONE  ANOP                                                           05000042
&ONLY1   SETB  0                        CLEAR INDICATOR                 05050042
         AGO   .START                   GO TEST FOR MORE SUB-LISTS      05100042
.DONE    AIF   (&COUNT-&COUNT/8*8 NE 0).ERR  TEST FOR MULTIPLE OF EIGHT 05150042
         AIF   (&CTIND EQ 0).END        NO DS IF NO COUNT               05200042
&COUNT   SETA  &COUNT/8                 GET NUMBER OF BYTES             05250042
&NAME    DS    &COUNT.C                                                 05300042
.END     MEXIT                                                          05350042
.ERR     MNOTE '**BIT COUNT FLDS DO NOT TOTAL MULTIPLE OF 8**'          05400042
         MEXIT                                                          05450042
.EQU     AIF   ('&SYSLIST(&OP,4)' NE '').EQU1     TEST FOR NULL LABEL   05500042
.ERR4    MNOTE '**EQU OPERAND OMITTED OR NOT FOUND**'                   05550042
         MEXIT                                                          05600042
.EQU1    AIF   (&SYSLIST(&OP,2) LT 16).EQU2  COUNT OVER FIFTEEN         05650042
*****               THE FOLLOWING STATEMENT                *****        05700042
*****     NUMBER OF BITS EXCEED 15 - DO NOT USE AS BIN     *****        05750042
.EQU2    ANOP                                                           05800042
&EQUCNT  SETA  0                        CLEAR COUNTER                   05850042
&EQUSYM  SETC  '&SYSLIST(&OP,4)'        GET EQU OPERAND                 05900042
.EQULOP  ANOP                                                           05950042
&EQUCNT  SETA  &EQUCNT+1                INCREMENT INDEX                 06000042
         AIF   ('&ITEM(&EQUCNT)' EQ '&EQUSYM').EQUOUT  LABEL FOUND?     06050042
         AIF   (&EQUCNT EQ &ITEMNBR).ERR4    ALL TESTED?                06100042
         AGO   .EQULOP                  NO-GET NEXT                     06150042
.EQUOUT  ANOP                                                           06200042
&ITEM(&ITEMNBR) SETC '&SYMBOL'          GET LABEL NAME                  06250042
&BITS(&ITEMNBR) SETA &SYSLIST(&OP,2)    GET NUMBER OF BITS              06300042
&SHIFT(&ITEMNBR) SETA &SHIFT(&EQUCNT)   GET SHIFT COUNT                 06350042
&SYMBOL  EQU   &EQUSYM                  THIS SYMBOL EQUATED             06400042
         AGO   .START                   RETURN FOR NEXT                 06450042
         MEND                                                           06500042

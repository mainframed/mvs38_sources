*********************************************************************** 18400042
* LINE MACRO  LINE MACRO  LINE MACRO  LINE MACRO  LINE MACRO LINE MACRO 18450042
         MACRO                                                          18500042
&NAME    LINE  &DATA,&LABEL,&SKIP=                                      18550042
.*                            THIS MACRO ESTABLISHES THE MARGINS OR     18600042
.*                                 COLUMN SETTINGS FOR EACH LINE. IT    18650042
.*                                 GENERATES NECESSARY CARRIAGE CONTROL 18700042
.*                                 ENTRIES. LASTLY IT DETERMINES THE    18750042
.*                                 NUMBER OF ENTRIES ASSOCIATED WITH    18800042
.*                                 THE PREVIOUS LINE AND STORES THAT #  18850042
.*                                 INTO THE APPROPRIATE BYTE.           18900042
.*                            &DATA(1) - CHARACTER POSITION TO BEGIN    18950042
.*                                 DATA.                                19000042
.*                            &DATA(2) - # OF SPACES BETWEEN SUCCESSIVE 19050042
.*                                 DATA COLUMN STARTING CHARACTERS.     19100042
.*                            &LABEL(1) - CHAR. POS. TO BEGIN LABELS.   19150042
.*                            &LABEL(2) - # SPACES BETWEEN LINE'S LABEL 19200042
.*                                 STARTING CHARACTERS.                 19250042
.*                            &SKIP= - # OF LINES TO ADVANCE CARRIAGE   19300042
.*                                 TO BEGIN NEXT PRINTED LINE.          19350042
.*                            NEW ENTRIES ARE GENERATED ONLY WHEN       19400042
.*                                 NECESSARY. OMISSION OF ITEMS MEANS   19450042
.*                                 THEY HAVE NOT CHANGED SINCE PREVIOUS 19500042
.*                                 SETTINGS. A LINE MACRO IS REQUIRED   19550042
.*                                 FOR EACH PRINTED LINE. IF NO SETTING 19600042
.*                                 CHANGES ARE REQUIRED ALL OPERANDS    19650042
.*                                 ARE OMITTED IN WHICH CASE ONLY THE   19700042
.*                                 # OF ENTRIES FOR THE PREVIOUS LINE   19750042
.*                                 IS COMPUTED. THIS MACRO MUST BE THE  19800042
.*                                 FIRST MACRO INSTRUCTION ISSUED FOR   19850042
.*                                 THE SUBJECT LINE                     19900042
         GBLA  &BITS(2600),&DB,&DT,&INITIAL                             19950042
         GBLA  &ITEMITR,&ITEMFST(20),&ITEMNBR,&ITEMMDX                  20000042
         GBLA  &ITEMNDX(2600),&ITEMXPT                                  20050042
         GBLA  &LABELNR,&LASTCC,&LB,&LBLCTR(100),&LPTR,&LT              20100042
         GBLA  &NRENTS,&SHIFT(2600)                                     20150042
         GBLB  &DIAGITF,&DIAGLIN,&DIAGSET,&DIAGTST,&FSW,&TEST           20200042
         GBLC  &CARRCON,&CSECT,&DSECT,&ITEM(2600)                       20250042
         GBLC  &LABELS(2600),&LBL(100),&NRENTSX                         20300042
         LCLA  &S                                                       20350042
         AIF   (&INITIAL EQ 0 OR &DIAGTST EQ 0).DIAGSET                 20400042
         AIF   (&DIAGSET EQ 0).DIAGSET                                  20450042
         MNOTE '**NO LABEL ASSOCIATED WITH LAST SET**'                  20500042
***** THE FOLLOWING GENERATED FOR TEST PURPOSES *****                   20550042
*        LABEL '%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%'       20600042
               LABEL '%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%' 20650042
.DIAGSET ANOP                                                           20700042
&DIAGSET SETB  1                                                        20750042
&DIAGLIN SETB  1                                                        20800042
&NAME    DS    0C                                                       20850042
         AIF   (&INITIAL EQ 0).FIRSTLN                                  20900042
         LINEND                                                         20950042
         AGO   .NTFIRST                                                 21000042
.FIRSTLN ANOP                                                           21050042
&NRENTSX SETC  'N&SYSNDX'                                               21100042
&INITIAL SETA  1                                                        21150042
N&SYSNDX EQU   *                                                        21200042
.NTFIRST ANOP                                                           21250042
         DC    AL1(0)                                                   21300042
         AIF   ('&DATA(1)' EQ '').CKDTABS                               21350042
         AIF   (&DATA(1) EQ &DB).CKDTABS                                21400042
&DB      SETA  &DATA(1)                                                 21450042
         AGO   .AA                                                      21500042
.CKDTABS AIF   ('&DATA(2)' EQ '').NCDATA                                21550042
         AIF   (&DATA(2) EQ &DT).NCDATA                                 21600042
         AGO   .CC                                                      21650042
.AA      AIF   ('&DATA(2)' EQ '').BB                                    21700042
         AIF   (&DATA(2) EQ &DT).BB                                     21750042
.CC      ANOP                                                           21800042
&DT      SETA  &DATA(2)                                                 21850042
.BB      DC    XL2'FFFE'                                                21900042
         DC    FL1'&DB'                                                 21950042
         DC    FL1'&DT'                                                 22000042
&NRENTS  SETA  &NRENTS+1                                                22050042
.NCDATA  ANOP                                                           22100042
         AIF   ('&LABEL(1)' EQ '').CKLTABS                              22150042
         AIF   (&LABEL(1) EQ &LB).CKLTABS                               22200042
&LB      SETA  &LABEL(1)                                                22250042
         AGO   .AAA                                                     22300042
.CKLTABS AIF   ('&LABEL(2)' EQ '').NCLABEL                              22350042
         AIF   (&LABEL(2) EQ &LT).NCLABEL                               22400042
         AGO   .CCC                                                     22450042
.AAA     AIF   ('&LABEL(2)' EQ '').BBB                                  22500042
         AIF   (&LABEL(2) EQ &LT).BBB                                   22550042
.CCC     ANOP                                                           22600042
&LT      SETA  &LABEL(2)                                                22650042
.BBB     DC    XL2'FFFF'                                                22700042
         DC    FL1'&LB'                                                 22750042
         DC    FL1'&LT'                                                 22800042
&NRENTS  SETA  &NRENTS+1                                                22850042
.NCLABEL ANOP                                                           22900042
         AIF   ('&SKIP' NE '').CONT                                     22950042
         AIF   (&TEST EQ 0).NOCHANG                                     23000042
         AGO   .DEFALTA                                                 23050042
.CONT    ANOP                                                           23100042
         AIF   (&TEST EQ 0).AB                                          23150042
         AIF   ('&SKIP' EQ 'E').AB                                      23200042
&S       SETA  &SKIP                                                    23250042
.DEFAULT ANOP                                                           23300042
         AIF   (&SKIP EQ &LASTCC).DEFALTA                               23350042
         ORG   &CARRCON+3                                               23400042
         DC    AL.3(0),AL.2(&S),AL.3(1)                                 23450042
         ORG                                                            23500042
&LASTCC  SETA  &S                                                       23550042
.DEFALTA ANOP                                                           23600042
&TEST    SETB  0                                                        23650042
         AGO   .DD                                                      23700042
.AB      AIF   ('&SKIP' NE 'E').ABB                                     23750042
A&SYSNDX DC    X'FFFB8B00'              EJECT                           23800042
         AGO   .ABC                                                     23850042
.ABB     AIF   (&SKIP EQ &LASTCC).NOCHANG                               23900042
A&SYSNDX DC    XL2'FFFB'                                                23950042
         DC    AL.3(0),AL.2(&SKIP),AL.3(1)                              24000042
         DC    AL.3(0),AL.2(&SKIP),AL.3(1)                              24050042
&LASTCC  SETA  &SKIP                                                    24100042
&TEST    SETB  1                                                        24150042
.ABC     ANOP                                                           24200042
&CARRCON SETC  'A&SYSNDX'                                               24250042
&NRENTS  SETA  &NRENTS+1                                                24300042
.DD      ANOP                                                           24350042
.NOCHANG ANOP                                                           24400042
         MEND                                                           24450042

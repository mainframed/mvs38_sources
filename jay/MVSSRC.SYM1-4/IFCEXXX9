         DC    AL1(0)                                                   03920002
         AIF   ('&DATA(1)' EQ '').CKDTABS                               03990002
         AIF   ('&DATA'(1,1) NE '(').SLABEL                             04060002
         AIF   (&DATA(1) EQ &DB).CKDTABS                                04130002
&DB      SETA  &DATA(1)                                                 04200002
         AGO   .AA                                                      04270002
.CKDTABS AIF   ('&DATA(2)' EQ '').NCDATA                                04340002

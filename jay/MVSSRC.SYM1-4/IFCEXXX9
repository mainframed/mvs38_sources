*  BIN MACRO     BIN MACRO     BIN MACRO     BIN MACRO     BIN MACRO  * 20300002
         SPACE 2                                                        20370002
         MACRO                                                          20440002
         BIN   &LOGITEM,&SKIP$OR                                        20510002
.*                                 &LOGITEM - NAME OF ITEM IN LOG       20580002
.*                                      RECORD DSECT                    20650002
.*                                 &SKIP$OR - NBR OF SPACES TO SKIP ON  20720002
.*                                 MULTIPLE PRINT OR NBR OF SPACES      20790002
.*                                      TO BACK UP FROM COLUMN FOR      20860002
.*                                      START OF PRINTING. THE LETTER   20930002
.*                                      B PRECEEDS THE NUMBER (B5) IF   21000002
.*                                      THE LATTER IS DESIRED. OMISSION 21070002
.*                                      OF THIS OPERAND FORCES OPTION 2 21140002
.*                                      WITH BACKUP SPACING EQUAL TO    21210002
.*                                 THE NUMBER OF BITS IN THE ITEM       21280002
.*                                 DEFINITION (VIA MACRO 'DSGEN').      21350002
.*                            A LIMIT OF 15 BITS IS IMPOSED FOR THE     21420002
.*                                 TOTAL NUMBER OF BITS TO PRINT.       21490002
         GBLC &ITEM(1000)                                               21560002
         GBLA &ITEMNBR,&BITS(1000),&SHIFT(1000),&NRENTS                 21630002
         GBLC  &DSECT                                                   21700002
         LCLB  &B                                                       21770002
         LCLA  &IBITS,&NDX,&ISHIFT,&K                                   21840002
         LCLA  &TEMP,&T                                                 21910002
         LCLC  &S                                                       21980002
.LOOP    ANOP                                                           22050002
&NDX     SETA  &NDX+1                                                   22120002
         AIF   ('&ITEM(&NDX)' EQ '&LOGITEM').FOUND                      22190002
         AIF   (&NDX LT &ITEMNBR).LOOP                                  22260002
         MNOTE *,'YOU BLEW IT'                                          22330002
         ROUTINE DUM,SKIPPER                                            22400002
         MEXIT                                                          22470002
.FOUND   ANOP                                                           22540002
&ISHIFT  SETA  &SHIFT(&NDX)                                             22610002
&IBITS   SETA  &BITS(&NDX)                                              22680002
&T       SETA  &IBITS-1                                                 22750002
&B       SETB  0                                                        22820002
         AIF   ('&SKIP$OR' EQ '').BACKUP                                22890002
&K       SETA  K'&SKIP$OR                                               22960002
&B       SETB  ('&SKIP$OR'(1,1) EQ 'B')                                 23030002
&S       SETC  '&SKIP$OR'(&B+1,&K-&B)                                   23100002
&T       SETA  &S                                                       23170002
.BACKUP  ANOP                                                           23240002
&NRENTS  SETA  &NRENTS+1                                                23310002
&TEMP    SETA  &ISHIFT                                                  23380002
A&SYSNDX DC    AL.12(&LOGITEM-&DSECT),XL.4'&TEMP'                       23450002
         AIF (&IBITS EQ 1 AND &B EQ 0).OUT                              23520002
         ORG   A&SYSNDX                                                 23590002
&B       SETB  1                                                        23660002
         DC    AL.12(&LOGITEM-&DSECT),XL.4'C'                           23730002
         DC    BL.1'&B',AL.7(&T),AL.4(&IBITS),FL.4'&ISHIFT'             23800002
.OUT     ANOP                                                           23870002
         MEND                                                           23940002
         EJECT                                                          24010002
*  HEX MACRO     HEX MACRO     HEX MACRO    HEXN MACRO     HEX MACRO  * 24080002
         SPACE 2                                                        24150002
         MACRO                                                          24220002
         HEX   &LOGITEM,&SKIP$OR,&BYTES                                 24290002
.*                            THIS MACRO FACILITATES GENERATION OF THE  24360002
.*                                 TABLE ENTRIES SPECIFYING HEX TYPE    24430002
.*                                 CONVERSIONS.                         24500002
.*                            &LOGITEM - NAME OF DATA ITEM TO BE        24570002
.*                                 CONVERTED                            24640002
.*                            &SKIP$OR - NUMBER OF SPACES TO SKIP ON    24710002
.*                                 MULTIPLE PRINT OR NBR OF SPACES TO   24780002
.*                                 BACK UP FROM COLUMN FOR START OF     24850002
.*                                 PRINTING. THE LETTER B PRECEEDS THE  24920002
.*                                 NUMBER (B8) IF THE LATTER OPTION IS  24990002
.*                                 DESIRED. OMISSION OF THIS OPERAND    25060002
.*                                 FORCES OPTION 2 WITH BACKUP SPACING  25130002
.*                                 EQUAL TO THE NUMBER OF BYTES SPECI-  25200002
.*                                 FIED BY THE NEXT OPERAND (TWICE THIS 25270002
.*                                 NUMBER IF SUFFIX IS EXPLICITLY OR    25340002
.*                                 IMPLICITLY B).                       25410002
.*                            &BYTES - NUMBER OF TOTAL INPUT BYTES TO   25480002
.*                                 PROCESS. THIS NUMBER IS SUFFIXED     25550002
.*                                 WITH AN L,R, OR B TO STIPULATE       25620002
.*                                 PRINTING OF THE LEFT HALF, RIGHT     25690002
.*                                 HALF, OR BOTH HALVES OF THE BYTE     25760002
.*                                 RESPECTIVELY. OMISSION OF THIS       25830002
.*                                 OPERAND DEFAULTS TO B WITH THE NUM-  25900002
.*                                 BER OF BYTES EQUAL TO THE ITEM LNGTH 25970002
.*                            A NEW ENTRY IS GENERATED ONLY WHEN NECES- 26040002
.*                                 SARY. THIS MACRO MUST BE THE FIRST   26110002
.*                                 MACRO INSTRUCTION ISSUED FOR THE     26180002
.*                                 SUBJECT LINE(S).                     26250002
         GBLC &ITEM(1000),&DSECT                                        26320002
         GBLA &ITEMNBR,&BITS(1000),&SHIFT(1000),&NRENTS                 26390002
         LCLB  &B                                                       26460002
         LCLA  &IBITS,&NDX,&ISHIFT,&K,&T,&CODE,&BYTESP                  26530002
         LCLC  &S,&LETTER                                               26600002
.LOOP    ANOP                                                           26670002
&NDX     SETA  &NDX+1                                                   26740002
         AIF   ('&ITEM(&NDX)' EQ '&LOGITEM').FOUND                      26810002
         AIF   (&NDX LT &ITEMNBR).LOOP                                  26880002
         MNOTE *,'LOG ITEM NOT DEFINED'                                 26950002
         MEXIT                                                          27020002
.FOUND   ANOP                                                           27090002
&ISHIFT  SETA  &SHIFT(&NDX)                                             27160002
&IBITS   SETA  &BITS(&NDX)                                              27230002
&CODE    SETA  1                                                        27300002
&K       SETA  K'&BYTES                                                 27370002
         AIF   (&IBITS-(&IBITS/4)*4 EQ 0).OK                            27440002
         MNOTE *,'BIT LENGTH SPECIFICATION INCONSISTENT WITH CONTROL'   27510002
         MEXIT                                                          27580002
.OK      AIF   ('&BYTES' EQ '').USEDEFN                                 27650002
&LETTER  SETC  '&BYTES'(&K,1)                                           27720002
&S       SETC  '&BYTES'(1,&K-1)                                         27790002
&BYTESP  SETA  &S                                                       27860002
         AIF   ('&LETTER' EQ 'B').BOTH                                  27930002
         AIF   (&ISHIFT EQ 0 OR &ISHIFT EQ 4).OK2                       28000002
.ERR     MNOTE *,'DATA NOT ALLIGNED ON HEX OR BYTE BOUNDARY'            28070002
         MEXIT                                                          28140002
.USEDEFN ANOP                                                           28210002
&BYTESP  SETA  &IBITS/8                                                 28280002
.BOTH    AIF   (&ISHIFT NE 0).ERR                                       28350002
&CODE    SETA  3                                                        28420002
&T       SETA  &BYTESP*2-1                                              28490002
&S       SETC  '&T'                                                     28560002
.OK2     AIF   (&ISHIFT EQ 0).L                                         28630002
&CODE    SETA  2                                                        28700002
.L       ANOP                                                           28770002
&B       SETB  1                                                        28840002
         AIF   ('&SKIP$OR' EQ '').BACKUP                                28910002
&K       SETA  K'&SKIP$OR                                               28980002
&B       SETB  ('&SKIP$OR'(1,1) EQ 'B')                                 29050002
&S       SETC  '&SKIP$OR'(&B+1,&K-&B)                                   29120002
.BACKUP  ANOP                                                           29190002
&NRENTS  SETA  &NRENTS+1                                                29260002
         DC    AL.12(&LOGITEM-&DSECT),XL.4'A'                           29330002
         DC    BL.1'&B',FL.7'&S',FL.4'&BYTESP',FL.4'&CODE'              29400002
.OUT     ANOP                                                           29470002
         MEND                                                           29540002
         MACRO                                                          29610002
         SPECIAL  &ROUTINE,&PARAM,&SPACE=                               29680002
.*                            THIS MACRO GENERATES A SPECIAL ROUTINE    29750002
.*                                 ENTRY TO BE PROCESSED BY THE SUM. M. 29820002
.*                            &ROUTINE - SYMBOL ON SPECIAL ROUTINE EWT. 29890002
.*                            &PARAM - AN 8 BIT BINARY REPRESENTATION   29960002
.*                                 TO OCCUPY THE 'MASK' FIELD BUT TO BE 30030002
.*                                 INTERPRETED AS AN ARGUMENT OF THE    30100002
.*                                 SPECIAL ROUTINE.                     30170002
.*                            &SPACE - THE NUMBER OF 2BYTE COUNTERS TO  30240002
.*                                 ALLOCATE FOR REQUIRED PROCESSING.    30310002
         GBLC  &CSECT                                                   30380002
         GBLA  &COUNTR                                                  30450002
         LCLA  &ADD                                                     30520002
         DC    AL1(128)                                                 30590002
         DC    BL.8'&PARAM'                                             30660002
         DC    AL2(&ROUTINE-&CSECT)                                     30730002
         DC    AL2(&COUNTR)                                             30800002
&ADD     SETA  1                                                        30870002
         AIF   ('&SPACE' EQ '').NORM                                    30940002
&ADD     SETA  &SPACE                                                   31010002
.NORM    ANOP                                                           31080002
&COUNTR  SETA  &COUNTR+&ADD                                             31150002
         MEND                                                           31220002
         MACRO                                                          31290002
         SUM   &LOGITEM,&MASK                                           31360002
.*                            THIS MACRO GENERATES A NORMAL EDIT ENTRY  31430002
.*                                 FOR PROCESSING BY THE SUMMARY MODULE 31500002
.*                            &LOGITEM - NAME OF DATA ITEM TO BE SUMMED 31570002
.*                            &MASK - BYTE MASK FOR BITS TO BE TESTED.  31640002
.*                                 THIS OPERAND SHOULD BE OMITTED FOR   31710002
.*                                 SINGLE BIT ITEMS IN WHICH CASE THE   31780002
.*                                 MASK WILL BE SET ACCORDING TO THE    31850002
.*                                 OFF BYTE BIT DISPLACEMENT OF THE     31920002
.*                                ITEM AS DEFINED BY THE MACRO 'DSGEN'  31990002
         GBLA  &COUNTR,&ITEMNBR,&BITS(1000),&SHIFT(1000)                32060002
         GBLC  &ITEM(1000),&DSECT                                       32130002
         LCLA  &IBITS,&NDX,&ISHIFT,&M,&BITNBR                           32200002
.LOOP    ANOP                                                           32270002
&NDX     SETA  &NDX+1                                                   32340002
         AIF   ('&ITEM(&NDX)' EQ '&LOGITEM').FOUND                      32410002
         AIF   (&NDX LT &ITEMNBR).LOOP                                  32480002
         MNOTE *,'LOGITEM NOT FOUND'                                    32550002
         MEXIT                                                          32620002
.FOUND   ANOP                                                           32690002
         DC    AL1(0)                                                   32760002
&ISHIFT  SETA  &SHIFT(&NDX)                                             32830002
&IBITS   SETA  &BITS(&NDX)                                              32900002
         AIF   ('&MASK' EQ '').FORM                                     32970002
         DC    B'&MASK'                                                 33040002
         AGO   .ITEM                                                    33110002
.FORM    ANOP                                                           33180002
&BITNBR  SETA  &ISHIFT-7                                                33250002
&BITNBR  SETA  0-&BITNBR                                                33320002
&M       SETA  1                                                        33390002
.LOOP2   ANOP                                                           33460002
         AIF   (&BITNBR EQ 0).GOTIT                                     33530002
&M       SETA  &M*2                                                     33600002
&BITNBR  SETA  &BITNBR-1                                                33670002
         AGO   .LOOP2                                                   33740002
.GOTIT   ANOP                                                           33810002
         DC    AL1(&M)                                                  33880002
.ITEM    DC    AL2(&LOGITEM-&DSECT)                                     33950002
         DC    AL2(&COUNTR)                                             34020002
&COUNTR  SETA  &COUNTR+1                                                34090002
         MEND                                                           34160002
         GBLC  &DSECT,&CSECT                                            34230002

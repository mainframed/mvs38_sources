         TITLE 'IFCE0135 MODEL  135 MACHINE CHECK EDIT'                 00080002
         MACRO                                                          00160002
&NAME    DSGEN                                                          00240002
.*                            THIS MACRO GENERATES ONE DSECT WORD       00320002
.*                                 FORMATTED FOR THE SUBJECT LOG RECORD 00400002
.*                                 EACH OPERAND HAS TWO SUB LISTS. THE  00480002
.*                                 FIRST IS THE NAME OF THE LOG ITEM    00560002
.*                                 WHILE THE SECOND IS THE # OF BITS TO 00640002
.*                                 ALLOCATE FOR THIS ITEM. ITEMS MUST   00720002
.*                                 BE DEFINED CONTIGUOUSLY WITHOUT GAPS 00800002
.*                                 UNUSED BIT GROUPS MUST BE GIVEN      00880002
.*                                 DUMMY NAMES. THE NAMES PROVIDED FOR  00960002
.*                                 THE LOG ITEMS MAY THEN BE USED IN    01040002
.*                                 THE CONVERSION MACRO INSTRUCTIONS.   01120002
         GBLC &ITEM(1000)                                               01200002
         GBLA &ITEMNBR,&BITS(1000),&SHIFT(1000)                         01280002
         LCLB  &ONLY1,&CTIND                                            01360002
         LCLA  &START1,&START2,&END1,&END2                              01440002
         LCLC  &SYMBOL,&EQUSYM                                          01520002
         LCLA  &OP,&COUNT,&BYTE,&EQUCNT                                 01600002
         ACTR  100000                                                   01680002
.START   AIF   (N'&SYSLIST(&OP+1) LT 3).LOOP                            01760002
         AIF   ('&SYSLIST(&OP+1,3)' EQ 'EQU').LOOP     TEST FOR EQU     01840002
  AIF   ('&SYSLIST(&OP+1,3)' EQ '' AND '&SYSLIST(&OP+1,4)' EQ '').ERR1  01920002
  AIF   ('&SYSLIST(&OP+1,3)' EQ '' OR '&SYSLIST(&OP+1,4)' EQ '').ERR1   02000002
         AIF   ('&SYSLIST(&OP+1,3)' GT '&SYSLIST(&OP+1,4)').ERR3        02080002
&START1  SETA  &SYSLIST(&OP+1,3)        GET START COUNT OF 1            02160002
&START2  SETA  &SYSLIST(&OP+1,4)        GET FIRST END COUNT             02240002
         AGO   .SET2A                   BRANCH AROUND                   02320002
.ERR1    MNOTE '**FIRST SYMBOL CONCATENATION INCOMPLETE**'              02400002
         MEXIT                                                          02480002
.SET2A   AIF   ('&SYSLIST(&OP+1,5)' EQ ''AND '&SYSLIST(&OP+1,6)' EQ '' X02560002
               ).SET2                   TEST FOR NO SECOND              02640002
  AIF   ('&SYSLIST(&OP+1,5)' EQ '' OR '&SYSLIST(&OP+1,6)' EQ '').ERR2   02720002
         AIF   ('&SYSLIST(&OP+1,5)' GT '&SYSLIST(&OP+1,6)').ERR3        02800002
&END1    SETA  &SYSLIST(&OP+1,5)        GET START COUNT OF 2            02880002
&END2    SETA  &SYSLIST(&OP+1,6)        GET END COUNT OF 2              02960002
         AGO   .LOOP                                                    03040002
.SET2    ANOP                                                           03120002
&ONLY1   SETB  1                        INDICATE ONLY ONE CONCATENATION 03200002
         AGO   .LOOP                                                    03280002
.ERR2    MNOTE '**SECOND SYMBOL CONCATENATION INCOMPLETE**'             03360002
         MEXIT                                                          03440002
.ERR3    MNOTE '**START GREATER THAN END CONCATENATION**'               03520002
         MEXIT                                                          03600002
.LOOP    ANOP                                                           03680002
&OP      SETA  &OP+1                                                    03760002
         AIF   (&OP GT N'&SYSLIST).DONE      TEST IF ALL DONE           03840002
&ITEMNBR SETA  &ITEMNBR+1                                               03920002
         AIF   ('&SYSLIST(&OP,2)' NE '').OK1                            04000002
         MNOTE '**NULL BIT COUNT FIELD**'                               04080002
         MEXIT                                                          04160002
.OK1     ANOP                                                           04240002
&SYMBOL  SETC  '&SYSLIST(&OP,1)'                                        04320002
         AIF   ('&SYMBOL' NE '').OK                                     04400002
*              *****     LOG ITEM SYMBOL NOT PROVIDED     *****         04480002
         AIF   ('&SYSLIST(&OP,3)' NE 'EQU').CNT                         04560002
         AGO   .START                   GET NEXT                        04640002
.OK      AIF   (N'&SYSLIST(&OP) LT 3).NO3    TEST FOR OLD FORMAT        04720002
         AIF   ('&SYSLIST(&OP,3)' EQ 'EQU').EQU   TEST FOR EQU          04800002
         AIF   (&ONLY1).OK2             TEST FOR ONLY ONE CONCAT        04880002
&SYMBOL  SETC  '&SYMBOL.&START1.&END1'       GET 1 AND 2 CONCAT         04960002
         AGO   .NO3                     BRANCH AROUND                   05040002
.OK2     ANOP                                                           05120002
&SYMBOL  SETC  '&SYMBOL.&START1'        GET 1 CONCATENATED              05200002
.NO3     ANOP                                                           05280002
&SYMBOL  EQU   *+&BYTE                                                  05360002
&ITEM(&ITEMNBR) SETC '&SYMBOL'          GET LABEL IN GLOBAL             05440002
&BITS(&ITEMNBR) SETA &SYSLIST(&OP,2)                                    05520002
&SHIFT(&ITEMNBR) SETA &COUNT-&BYTE*8                                    05600002
.CNT     ANOP                                                           05680002
&CTIND   SETB  1                        SET COUNT INDICATOR             05760002
&COUNT   SETA  &COUNT+&SYSLIST(&OP,2)                                   05840002
&BYTE    SETA  &COUNT/8                                                 05920002
         AIF   (N'&SYSLIST(&OP) LT 3).START  TEST FOR OLD FORMAT        06000002
         AIF   (&ONLY1).TSTST           TEST FOR ONLY ONE CONCAT        06080002
         AIF   (&END1 EQ &END2).TSTST   TEST FOR SECOND END             06160002
&END1    SETA  &END1+1                  INCREMENT COUNT                 06240002
&OP      SETA  &OP-1                    DECREMENT INCREMENT             06320002
         AGO   .LOOP                    GENERATE NEXT                   06400002
.TSTST   ANOP                                                           06480002
         AIF   (&START1 EQ &START2).LPDONE   TEST FOR END               06560002
         AIF   (&ONLY1).NSET1           TEST FOR ONLY ONE CONCAT        06640002
&END1    SETA  &SYSLIST(&OP,5)          RE-INIT SECOND START COUNT      06720002
.NSET1   ANOP                                                           06800002
&OP      SETA  &OP-1                    DECREMENT INDEX                 06880002
&START1  SETA  &START1+1                INCREMENT FIRST COUNT           06960002
         AGO   .LOOP                    GO DO NEXT                      07040002
.LPDONE  ANOP                                                           07120002
&ONLY1   SETB  0                        CLEAR INDICATOR                 07200002
         AGO   .START                   GO TEST FOR MORE SUB-LISTS      07280002
.DONE    AIF   (&COUNT-&COUNT/8*8 NE 0).ERR  TEST FOR MULTIPLE OF EIGHT 07360002
         AIF   (&CTIND EQ 0).END        NO DS IF NO COUNT               07440002
&COUNT   SETA  &COUNT/8                 GET NUMBER OF BYTES             07520002
&NAME    DS    &COUNT.C                                                 07600002
.END     MEXIT                                                          07680002
.ERR     MNOTE '**BIT COUNT FLDS DO NOT TOTAL MULTIPLE OF 8**'          07760002
         MEXIT                                                          07840002
.EQU     AIF   ('&SYSLIST(&OP,4)' NE '').EQU1     TEST FOR NULL LABEL   07920002
.ERR4    MNOTE '**EQU OPERAND OMITTED OR NOT FOUND**'                   08000002
         MEXIT                                                          08080002
.EQU1    AIF   (&SYSLIST(&OP,2) LT 16).EQU2  COUNT OVER FIFTEEN         08160002
*****               THE FOLLOWING STATEMENT                *****        08240002
*****     NUMBER OF BITS EXCEED 15 - DO NOT USE AS BIN     *****        08320002
.EQU2    ANOP                                                           08400002
&EQUCNT  SETA  0                        CLEAR COUNTER                   08480002
&EQUSYM  SETC  '&SYSLIST(&OP,4)'        GET EQU OPERAND                 08560002
.EQULOP  ANOP                                                           08640002
&EQUCNT  SETA  &EQUCNT+1                INCREMENT INDEX                 08720002
         AIF   ('&ITEM(&EQUCNT)' EQ '&EQUSYM').EQUOUT  LABEL FOUND?     08800002
         AIF   (&EQUCNT EQ &ITEMNBR).ERR4    ALL TESTED?                08880002
         AGO   .EQULOP                  NO-GET NEXT                     08960002
.EQUOUT  ANOP                                                           09040002
&ITEM(&ITEMNBR) SETC '&SYMBOL'          GET LABEL NAME                  09120002
&BITS(&ITEMNBR) SETA &SYSLIST(&OP,2)    GET NUMBER OF BITS              09200002
&SHIFT(&ITEMNBR) SETA &SHIFT(&EQUCNT)   GET SHIFT COUNT                 09280002
&SYMBOL  EQU   &EQUSYM                  THIS SYMBOL EQUATED             09360002
         AGO   .START                   RETURN FOR NEXT                 09440002
         MEND                                                           09520002
         EJECT                                                          09600002
*  LINE MACRO     LINE MACRO     LINE MACRO     LINE MACRO            * 09680002
         SPACE 2                                                        09760002
         MACRO                                                          09840002
&NAME    LINE  &DATA,&LABEL,&SKIP=                                      09920002
.*                            THIS MACRO ESTABLISHES THE MARGINS OR     10000002
.*                                 COLUMN SETTINGS FOR EACH LINE. IT    10080002
.*                                 GENERATES NECESSARY CARRIAGE CONTROL 10160002
.*                                 ENTRIES. LASTLY IT DETERMINES THE    10240002
.*                                 NUMBER OF ENTRIES ASSOCIATED WITH    10320002
.*                                 THE PREVIOUS LINE AND STORES THAT #  10400002
.*                                 INTO THE APPROPRIATE BYTE.           10480002
.*                            &DATA(1) - CHARACTER POSITION TO BEGIN    10560002
.*                                 DATA.                                10640002
.*                            &DATA(2) - # OF SPACES BETWEEN SUCCESSIVE 10720002
.*                                 DATA COLUMN STARTING CHARACTERS.     10800002
.*                            &LABEL(1) - CHAR. POS. TO BEGIN LABELS.   10880002
.*                            &LABEL(2) - # SPACES BETWEEN LINE'S LABEL 10960002
.*                                 STARTING CHARACTERS.                 11040002
.*                            &SKIP= - # OF LINES TO ADVANCE CARRIAGE   11120002
.*                                 TO BEGIN NEXT PRINTED LINE.          11200002
.*                            NEW ENTRIES ARE GENERATED ONLY WHEN       11280002
.*                                 NECESSARY. OMISSION OF ITEMS MEANS   11360002
.*                                 THEY HAVE NOT CHANGED SINCE PREVIOUS 11440002
.*                                 SETTINGS. A LINE MACRO IS REQUIRED   11520002
.*                                 FOR EACH PRINTED LINE. IF NO SETTING 11600002
.*                                 CHANGES ARE REQUIRED ALL OPERANDS    11680002
.*                                 ARE OMITTED IN WHICH CASE ONLY THE   11760002
.*                                 # OF ENTRIES FOR THE PREVIOUS LINE   11840002
.*                                 IS COMPUTED. THIS MACRO MUST BE THE  11920002
.*                                 FIRST MACRO INSTRUCTION ISSUED FOR   12000002
.*                                 THE SUBJECT LINE                     12080002
         GBLA  &DB,&DT,&LB,&LT,&NRENTS                                  12160002
         GBLA  &LASTCC,&INITIAL                                         12240002
         GBLC  &NRENTSX                                                 12320002
         GBLC  &CARRCON                                                 12400002
         GBLB  &TEST                                                    12480002
         LCLA  &S                                                       12560002
&NAME    DS    0C                                                       12640002
         AIF   (&INITIAL EQ 0).FIRSTLN                                  12720002
         LINEND                                                         12800002
         AGO   .NTFIRST                                                 12880002
.FIRSTLN ANOP                                                           12960002
&NRENTSX SETC  'N&SYSNDX'                                               13040002
&INITIAL SETA  1                                                        13120002
N&SYSNDX EQU   *                                                        13200002
.NTFIRST ANOP                                                           13280002
         DC    AL1(0)                                                   13360002
         AIF   ('&DATA(1)' EQ '').CKDTABS                               13440002
         AIF   (&DATA(1) EQ &DB).CKDTABS                                13520002
&DB      SETA  &DATA(1)                                                 13600002
         AGO   .AA                                                      13680002
.CKDTABS AIF   ('&DATA(2)' EQ '').NCDATA                                13760002
         AIF   (&DATA(2) EQ &DT).NCDATA                                 13840002
         AGO   .CC                                                      13920002
.AA      AIF   ('&DATA(2)' EQ '').BB                                    14000002
         AIF   (&DATA(2) EQ &DT).BB                                     14080002
.CC      ANOP                                                           14160002
&DT      SETA  &DATA(2)                                                 14240002
.BB      DC    XL2'FFFE'                                                14320002
         DC    FL1'&DB'                                                 14400002
         DC    FL1'&DT'                                                 14480002
&NRENTS  SETA  &NRENTS+1                                                14560002
.NCDATA  ANOP                                                           14640002
         AIF   ('&LABEL(1)' EQ '').CKLTABS                              14720002
         AIF   (&LABEL(1) EQ &LB).CKLTABS                               14800002
&LB      SETA  &LABEL(1)                                                14880002
         AGO   .AAA                                                     14960002
.CKLTABS AIF   ('&LABEL(2)' EQ '').NCLABEL                              15040002
         AIF   (&LABEL(2) EQ &LT).NCLABEL                               15120002
         AGO   .CCC                                                     15200002
.AAA     AIF   ('&LABEL(2)' EQ '').BBB                                  15280002
         AIF   (&LABEL(2) EQ &LT).BBB                                   15360002
.CCC     ANOP                                                           15440002
&LT      SETA  &LABEL(2)                                                15520002
.BBB     DC    XL2'FFFF'                                                15600002
         DC    FL1'&LB'                                                 15680002
         DC    FL1'&LT'                                                 15760002
&NRENTS  SETA  &NRENTS+1                                                15840002
.NCLABEL ANOP                                                           15920002
         AIF   ('&SKIP' NE '').CONT                                     16000002
         AIF   (&TEST EQ 0).NOCHANG                                     16080002
&S       SETA  &LASTCC                                                  16160002
         AGO   .DEFAULT                                                 16240002
.CONT    ANOP                                                           16320002
         AIF   (&TEST EQ 0).AB                                          16400002
&S       SETA  &SKIP                                                    16480002
.DEFAULT ANOP                                                           16560002
         ORG   &CARRCON+3                                               16640002
         DC    AL.3(0),AL.2(&S),AL.3(1)                                 16720002
         ORG                                                            16800002
&LASTCC  SETA  &S                                                       16880002
&TEST    SETB  0                                                        16960002
         AGO   .DD                                                      17040002
.AB      AIF   (&SKIP EQ &LASTCC).NOCHANG                               17120002
A&SYSNDX DC    XL2'FFFB'                                                17200002
         DC    AL.3(0),AL.2(&SKIP),AL.3(1)                              17280002
         DC    FL1'9'                                                   17360002
&LASTCC  SETA  &SKIP                                                    17440002
&TEST    SETB  1                                                        17520002
&CARRCON SETC  'A&SYSNDX'                                               17600002
&NRENTS  SETA  &NRENTS+1                                                17680002
.DD      ANOP                                                           17760002
.NOCHANG ANOP                                                           17840002
         MEND                                                           17920002
         EJECT                                                          18000002
         MACRO                                                          18080002
         LINEND                                                         18160002
.*                            THIS MACRO IS USED ONLY BY THE LINE       18240002
.*                                 MACRO TO SET THE NUMBER OF ENTRIES   18320002
.*                                 ASSOCIATED WITH EACH LINE.           18400002
         GBLA  &NRENTS                                                  18480002
         GBLC &NRENTSX                                                  18560002
         AIF   (&NRENTS EQ 0).NORENTS   TEST FOR NO COUNT               18640002
         ORG   &NRENTSX                                                 18720002
         DC    FL1'&NRENTS'                                             18800002
         ORG                                                            18880002
&NRENTS  SETA  0                                                        18960002
.NORENTS ANOP                                                           19040002
B&SYSNDX EQU   *                                                        19120002
&NRENTSX SETC  'B&SYSNDX'                                               19200002
         MEND                                                           19280002
         MACRO                                                          19360002
         BIN   &LOGITEM,&SKIP$OR                                        19440002
.*                                 &LOGITEM - NAME OF ITEM IN LOG       19520002
.*                                      RECORD DSECT                    19600002
.*                                 &SKIP$OR - NBR OF SPACES TO SKIP ON  19680002
.*                                 MULTIPLE PRINT OR NBR OF SPACES      19760002
.*                                      TO BACK UP FROM COLUMN FOR      19840002
.*                                      START OF PRINTING. THE LETTER   19920002
.*                                      B PRECEEDS THE NUMBER (B5) IF   20000002
.*                                      THE LATTER IS DESIRED. OMISSION 20080002
.*                                      OF THIS OPERAND FORCES OPTION 2 20160002
.*                                      WITH BACKUP SPACING EQUAL TO    20240002
.*                                 THE NUMBER OF BITS IN THE ITEM       20320002
.*                                 DEFINITION (VIA MACRO 'DSGEN').      20400002
.*                            A LIMIT OF 15 BITS IS IMPOSED FOR THE     20480002
.*                                 TOTAL NUMBER OF BITS TO PRINT.       20560002
         GBLC &ITEM(1000)                                               20640002
         GBLA &ITEMNBR,&BITS(1000),&SHIFT(1000),&NRENTS                 20720002
         GBLC  &DSECT                                                   20800002
         LCLB  &B                                                       20880002
         LCLA  &IBITS,&NDX,&ISHIFT,&K                                   20960002
         LCLA  &TEMP,&T                                                 21040002
         LCLC  &S                                                       21120002
.LOOP    ANOP                                                           21200002
&NDX     SETA  &NDX+1                                                   21280002
         AIF   ('&ITEM(&NDX)' EQ '&LOGITEM').FOUND                      21360002
         AIF   (&NDX LT &ITEMNBR).LOOP                                  21440002
         MNOTE *,'YOU BLEW IT'                                          21520002
         ROUTINE DUM,SKIPPER                                            21600002
         MEXIT                                                          21680002
.FOUND   ANOP                                                           21760002
&ISHIFT  SETA  &SHIFT(&NDX)                                             21840002
&IBITS   SETA  &BITS(&NDX)                                              21920002
&T       SETA  &IBITS-1                                                 22000002
&B       SETB  0                                                        22080002
         AIF   ('&SKIP$OR' EQ '').BACKUP                                22160002
&K       SETA  K'&SKIP$OR                                               22240002
&B       SETB  ('&SKIP$OR'(1,1) EQ 'B')                                 22320002
&S       SETC  '&SKIP$OR'(&B+1,&K-&B)                                   22400002
&T       SETA  &S                                                       22480002
.BACKUP  ANOP                                                           22560002
&NRENTS  SETA  &NRENTS+1                                                22640002
&TEMP    SETA  &ISHIFT                                                  22720002
         AIF   (&IBITS NE 1 OR &B NE 0).BIN2                            22800002
         DC    AL.12(&LOGITEM-&DSECT),XL.4'&TEMP'                       22880002
         MEXIT                                                          22960002
.BIN2    AIF   ('&SKIP$OR' NE '').BIN3                                  23040002
&B       SETB  1                                                        23120002
.BIN3    ANOP                                                           23200002
         DC    AL.12(&LOGITEM-&DSECT),XL.4'C'                           23280002
         DC    BL.1'&B',AL.7(&T),AL.4(&IBITS),FL.4'&ISHIFT'             23360002
.OUT     ANOP                                                           23440002
         MEND                                                           23520002
         MACRO                                                          23600002
         HEX   &LOGITEM,&SKIP$OR,&BYTES                                 23680002
.*                            THIS MACRO FACILITATES GENERATION OF THE  23760002
.*                                 TABLE ENTRIES SPECIFYING HEX TYPE    23840002
.*                                 CONVERSIONS.                         23920002
.*                            &LOGITEM - NAME OF DATA ITEM TO BE        24000002
.*                                 CONVERTED                            24080002
.*                            &SKIP$OR - NUMBER OF SPACES TO SKIP ON    24160002
.*                                 MULTIPLE PRINT OR NBR OF SPACES TO   24240002
.*                                 BACK UP FROM COLUMN FOR START OF     24320002
.*                                 PRINTING. THE LETTER B PRECEEDS THE  24400002
.*                                 NUMBER (B8) IF THE LATTER OPTION IS  24480002
.*                                 DESIRED. OMISSION OF THIS OPERAND    24560002
.*                                 FORCES OPTION 2 WITH BACKUP SPACING  24640002
.*                                 EQUAL TO THE NUMBER OF BYTES SPECI-  24720002
.*                                 FIED BY THE NEXT OPERAND (TWICE THIS 24800002
.*                                 NUMBER IF SUFFIX IS EXPLICITLY OR    24880002
.*                                 IMPLICITLY B).                       24960002
.*                            &BYTES - NUMBER OF TOTAL INPUT BYTES TO   25040002
.*                                 PROCESS. THIS NUMBER IS SUFFIXED     25120002
.*                                 WITH AN L,R, OR B TO STIPULATE       25200002
.*                                 PRINTING OF THE LEFT HALF, RIGHT     25280002
.*                                 HALF, OR BOTH HALVES OF THE BYTE     25360002
.*                                 RESPECTIVELY. OMISSION OF THIS       25440002
.*                                 OPERAND DEFAULTS TO B WITH THE NUM-  25520002
.*                                 BER OF BYTES EQUAL TO THE ITEM LNGTH 25600002
.*                            A NEW ENTRY IS GENERATED ONLY WHEN NECES- 25680002
.*                                 SARY. THIS MACRO MUST BE THE FIRST   25760002
.*                                 MACRO INSTRUCTION ISSUED FOR THE     25840002
.*                                 SUBJECT LINE(S).                     25920002
         GBLC &ITEM(1000),&DSECT                                        26000002
         GBLA &ITEMNBR,&BITS(1000),&SHIFT(1000),&NRENTS                 26080002
         LCLB  &B                                                       26160002
         LCLA  &IBITS,&NDX,&ISHIFT,&K,&T,&CODE,&BYTESP                  26240002
         LCLC  &S,&LETTER                                               26320002
.LOOP    ANOP                                                           26400002
&NDX     SETA  &NDX+1                                                   26480002
         AIF   ('&ITEM(&NDX)' EQ '&LOGITEM').FOUND                      26560002
         AIF   (&NDX LT &ITEMNBR).LOOP                                  26640002
         MNOTE *,'LOG ITEM NOT DEFINED'                                 26720002
         MEXIT                                                          26800002
.FOUND   ANOP                                                           26880002
&ISHIFT  SETA  &SHIFT(&NDX)                                             26960002
&IBITS   SETA  &BITS(&NDX)                                              27040002
&CODE    SETA  1                                                        27120002
&K       SETA  K'&BYTES                                                 27200002
         AIF   (&IBITS-(&IBITS/4)*4 EQ 0).OK                            27280002
         MNOTE *,'BIT LENGTH SPECIFICATION INCONSISTENT WITH CONTROL'   27360002
         MEXIT                                                          27440002
.OK      AIF   ('&BYTES' EQ '').USEDEFN                                 27520002
&LETTER  SETC  '&BYTES'(&K,1)                                           27600002
&S       SETC  '&BYTES'(1,&K-1)                                         27680002
&BYTESP  SETA  &S                                                       27760002
         AIF   ('&LETTER' EQ 'B').BOTH                                  27840002
         AIF   ('&LETTER' EQ 'L').LEFT                                  27920002
&CODE    SETA  2                                                        28000002
         AGO   .L                                                       28080002
.LEFT    ANOP                                                           28160002
&CODE    SETA  1                                                        28240002
         AGO   .L                                                       28320002
         AIF   (&ISHIFT EQ 0 OR &ISHIFT EQ 4).OK2                       28400002
.ERR     MNOTE *,'DATA NOT ALLIGNED ON HEX OR BYTE BOUNDARY'            28480002
         MEXIT                                                          28560002
.USEDEFN ANOP                                                           28640002
&BYTESP  SETA  &IBITS/8                                                 28720002
.BOTH    AIF   (&ISHIFT NE 0).ERR                                       28800002
&CODE    SETA  3                                                        28880002
&T       SETA  &BYTESP*2-1                                              28960002
&S       SETC  '&T'                                                     29040002
.OK2     AIF   (&ISHIFT EQ 0).L                                         29120002
&CODE    SETA  2                                                        29200002
.L       ANOP                                                           29280002
&B       SETB  1                                                        29360002
         AIF   ('&SKIP$OR' EQ '').BACKUP                                29440002
&K       SETA  K'&SKIP$OR                                               29520002
&B       SETB  ('&SKIP$OR'(1,1) EQ 'B')                                 29600002
&S       SETC  '&SKIP$OR'(&B+1,&K-&B)                                   29680002
.BACKUP  ANOP                                                           29760002
&NRENTS  SETA  &NRENTS+1                                                29840002
         DC    AL.12(&LOGITEM-&DSECT),XL.4'A'                           29920002
         DC    BL.1'&B',FL.7'&S',AL.4(&BYTESP),FL.4'&CODE'              30000002
.OUT     ANOP                                                           30080002
         MEND                                                           30160002
         SPACE 2                                                        30240002
*  ROUTINE MACRO     ROUTINE MACRO     ROUTINE MACRO     ROUTINE MACRO* 30320002
         SPACE 2                                                        30400002
         MACRO                                                          30480002
&NAME    ROUTINE &LOGITEM,&RNAME                                        30560002
         GBLC  &DSECT,&CSECT                                            30640002
         GBLC  &ITEM(1000)                                              30720002
         GBLA  &SHIFT(1000),&ITEMNBR                                    30800002
         GBLA  &NRENTS                                                  30880002
         LCLA  &ISHIFT,&NDX                                             30960002
.LOOP    ANOP                                                           31040002
&NDX     SETA  &NDX+1                                                   31120002
         AIF   ('&ITEM(&NDX)' EQ '&LOGITEM').FOUND                      31200002
         AIF   (&NDX LT &ITEMNBR).LOOP                                  31280002
         MNOTE *,'LOG ITEM NOT DEFINED'                                 31360002
         MEXIT                                                          31440002
.FOUND   ANOP                                                           31520002
&ISHIFT  SETA  &SHIFT(&NDX)                                             31600002
&NAME    DC    AL.12(&LOGITEM-&DSECT),XL.4'D'                           31680002
         DC    AL.4(&ISHIFT),AL.12(&RNAME-&CSECT)                       31760002
&NRENTS  SETA  &NRENTS+1                                                31840002

         B     NOPRINT                  DO NOT PRINT                    66560002
*                                                                       66640002
LONGTST  TM    SW2B0,HEX20              TEST FOR SHORT RECORD           66720002
         BCR   ONE,R14                  YES-RETURN TO PRINT             66800002
         B     NOPRINT                  LONG-DO NOT PRINT               66880002
*                                                                       66960002
SKIPTST1 TM    SW2B0,HEX20              TEST FOR SHORT RECORD           67040002
         BCR   EIGHT,R14                NO-RETURN                       67120002
         MVC   NINE(L8),SADCON          BUMP POINTERS                   67200002
         B     NOPRINT                  DO NOT PRINT                    67280002
*                                                                       67360002
SKIPTST2 TM    SW2B0,HEX20              TEST FOR SHORT RECORD           67440002
         BCR   EIGHT,R14                NO-RETURN                       67520002
         MVC   NINE(L8),SADCON1         BUMP POINTERS                   67600002
         B     NOPRINT                  RETURN-DO NOT PRINT             67680002
*                                                                       67760002
RESETR2  L     R2,SAVE2                 RESTORE R2 POINTER              67840002
         B     NOPRINT                  DO NOT PRINT                    67920002
*                                                                       68000002
SAVE2    DC    F'0'                                                     68080002
*                                                                       68160002
SADCON   DC    A(SHORT1-L4)                                             68240002
         DC    A(SHRTL1-L1)                                             68320002
SADCON1  DC    A(SHORT2-L4)                                             68400002
         DC    A(SHRTL2-L1)                                             68480002
*                                                                     * 68560002
* THIS CODE WILL MOVE THE JOB NAME AND VOLID FROM THE RECORD TO THE   * 68640002
* BUFFER                                                              * 68720002
*                                                                     * 68800002
JOBNAME  MVC   L0(L8,R3),L0(R4)   MOVE THE JOBID TO PRINT LINE          68880002
         BR    R14                RETURN                                68960002
VOLNAME  MVC   L0(L6,R3),L0(R4)   MOVE THE VOLID TO PRINT LINE          69040002
         BR    R14                RETURN                                69120002
FORMID   MVC   L0(L4,R3),L0(R4)         GET FORMAT ID                   69200002
         BR    R14                                                      69280002
*                                                                     * 69360002
SETHEX   STM   R5,R8,XSAVE        SAVE REGS                             69440002
         LA    R8,CHARS           PTR TO CHARS                          69520002
         IC    R7,L0(R4)          PICK CHAR                             69600002
         SLL   R7,L24             MOVE IT TO HI OF R7                   69680002
         SR    R6,R6              CLEAR REG                             69760002
         SLDL  R6,L4              MOVE 4 BITS TO R6                     69840002
         AR    R6,R8              COMPUTE OFFSET                        69920002
         MVC   L0(L1,R3),L0(R6)   MOVE CHAF TO LINE                     70000002
         SR    R6,R6              CLEAR                                 70080002
         SLDL  R6,L4              MOVE LAST 4 BITS TO R6                70160002
         AR    R6,R8              COMPUTE PFFSET                        70240002
         MVC   L1(L1,R3),L0(R6)   MOVE CHAR TO LINE + 1                 70320002
         B     RETURN          RETURN                                   70400002
SPACER16 LA    R3,L16(R3)                                               70480002
         BR    R14                                                      70560002
SPACER1  LA    R3,L1(R3)                                                70640002
         BR    R14                                                      70720002
*                                                                     * 70800002
* THIS CODE WILL TAKE THE DATE FROM THE RECORD UNPACK IT AND PUT IT   * 70880002
* IN THE PRINT BUFFER.                                                * 70960002
*                                                                     * 71040002
SETDATE  CLC   L1(L3,R4),ZERO          IS DATE ZERO                     71120002
         BE    SETDTE                  BR- YES                          71200002
         UNPK  XSAVE(L8),L1(L3,R4)     UNPACK DATE                      71280002
         MVC   L5(L2,R3),XSAVE+L3      MOVE YEAR                        71360002
         MVC   L0(L3,R3),XSAVE+L5                                       71440002
         BR    R14                                                      71520002
SETDTE   MVC   L0(L4,R3),NONE     SET DATE TO NONE                      71600002
         BR    R14                     RETURN                           71680002
SETPGMN  TM    SW2B0,HEX20              TEST FOR SHORT RECORD           71760002
         BCR   ONE,R14                  YES-NO JOB ID                   71840002
         MVC   0(L12,R3),JOBID          MOVE IN LABEL                   71920002
         MVC   L13(L8,R3),XPGN1         MOVE IN NAME                    72000002
         BR    R14                                                      72080002
SETCONV  TM    L0(R4),HEX02             HAS RECORD BEEN CONVERTED KEY32 72160002
         BCR   ON,R14                   YES RETURN                      72240002
         SR    R15,R15                  NO CLEAR R15 FOR NO PRINT OF    72320002
         BR    R14                      LINE BR-14                      72400002
ON       EQU   1                                                        72480002
HEX02    EQU   X'02'                                                    72560002
*                                       SET DEVICE TYPE FIELD           72640002
*                                                                       72720002
*                                                                       72800002
*          ROUTINE TO CONVERT 4 BIT COUNTER                             72880002
*                                                                     * 72960002
CONVLFT  STM   R5,R8,XSAVE              SAVE REGS                       73040002
         SR    R6,R6                   CLEAR REGISTERS                  73120002
         IC    R6,L0(R4)               PICK UP COUNTER IN QUEST         73200002
         SRL   R6,L4                   DROP OFF 4 LOW BITS              73280002
         B     SDRCVT                  BRANCH TO CONVERT IT             73360002
CONVRHT  STM   R5,R8,XSAVE             SAVE REGS                        73440002
         IC    R6,L0(R4)               PICK CHAR                        73520002
         SLL   R6,L28                                                   73600002
         SRL   R6,L28                  DROP OFF HI ORDER BITS           73680002
SDRCVT   CVD   R6,WORK                 CONVERT IT                       73760002
         UNPK  L0(L3,R3),WORK+L5(L3)     TO DECIMAL                     73840002
         MVZ   L2(L1,R3),L0(R3)        PUT ON ZONE                      73920002
         B     RETURN                  TERMINATE                        74000002
ZERO     DC    XL4'00'                                                  74080002
*                                                                     * 74160002
RETURN   LM    R5,R8,XSAVE              SAVE REGISTERS                  74240002
         BR    R14                                                      74320002
*                                                                     * 74400002
CONVREL  STM   R5,R8,XSAVE              SAVE WORK REGS                  74480002
         SR    R6,R6                    CLEAR R6                        74560002
         IC    R6,L0(R4)                PICK UP REL & SYS VALUES        74640002
         SRDL  R6,L5                    SAVE REL NO IN R7               74720002
         SLL   R6,L2                    MULTIPLY SYS VALUE BY 4         74800002
         L     R8,XLABELP               PICK UP LABEL POINTER           74880002
         LA    R6,RELTAB(R6)            INDEX INTO TABLE OF SYSTEMS     74960002
         MVC   L0(L4,R8),L0(R6)         MOVE SYSTEM NAME INTO LABEL     75040002
         SR    R6,R6                    CLEAR R6                        75120002
         SLDL  R6,L5                    RETREIVE REL NO.                75200002
         LTR   R6,R6                    IS IT ZERO                      75280002
         BZ    NOREL                    BR-YES                          75360002
         CVD   R6,WORK                  CONVERT IT                      75440002
         UNPK  L0(L2,R3),WORK+L6(L2)   UNPACK IT INTO PRINT BUFF        75520002
         MVZ   L1(L1,R3),L0(R3)         MOVE ON PROPER ZONE             75600002
         B     RETURN                   EXIT                            75680002
NOREL    MVC   L0(L4,R3),NONE           MOVE IN NONE FOR REL            75760002
         B     RETURN                                                   75840002

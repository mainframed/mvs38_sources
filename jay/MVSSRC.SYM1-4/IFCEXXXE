&NAME    DSGEN                                                          44400002
.*                            THIS MACRO GENERATES ONE DSECT WORD       44480002
.*                                 FORMATTED FOR THE SUBJECT LOG RECORD 44560002
.*                                 EACH OPERAND HAS TWO SUB LISTS. THE  44640002
.*                                 FIRST IS THE NAME OF THE LOG ITEM    44720002
.*                                 WHILE THE SECOND IS THE # OF BITS TO 44800002
.*                                 ALLOCATE FOR THIS ITEM. ITEMS MUST   44880002
.*                                 BE DEFINED CONTIGUOUSLY WITHOUT GAPS 44960002
.*                                 UNUSED BIT GROUPS MUST BE GIVEN      45040002
.*                                 DUMMY NAMES. THE NAMES PROVIDED FOR  45120002
.*                                 THE LOG ITEMS MAY THEN BE USED IN    45200002
.*                                 THE CONVERSION MACRO INSTRUCTIONS.   45280002
         GBLC &ITEM(1000)                                               45360002
         GBLA &ITEMNBR,&BITS(1000),&SHIFT(1000)                         45440002
         LCLB  &ONLY1,&CTIND                                            45520002
         LCLA  &START1,&START2,&END1,&END2                              45600002
         LCLC  &SYMBOL,&EQUSYM                                          45680002
         LCLA  &OP,&COUNT,&BYTE,&EQUCNT                                 45760002
         ACTR  100000                                                   45840002
.START   AIF   (N'&SYSLIST(&OP+1) LT 3).LOOP                            45920002
         AIF   ('&SYSLIST(&OP+1,3)' EQ 'EQU').LOOP     TEST FOR EQU     46000002
  AIF   ('&SYSLIST(&OP+1,3)' EQ '' OR '&SYSLIST(&OP+1,4)' EQ '').ERR1   46080002
         AIF   ('&SYSLIST(&OP+1,3)' GT '&SYSLIST(&OP+1,4)').ERR3        46160002
&START1  SETA  &SYSLIST(&OP+1,3)        GET START COUNT OF 1            46240002
&START2  SETA  &SYSLIST(&OP+1,4)        GET FIRST END COUNT             46320002
         AGO   .SET2A                   BRANCH AROUND                   46400002
.ERR1    MNOTE '**FIRST SYMBOL CONCATENATION INCOMPLETE**'              46480002
         MEXIT                                                          46560002
.SET2A   AIF   ('&SYSLIST(&OP+1,5)' EQ ''AND '&SYSLIST(&OP+1,6)' EQ '' X46640002
               ).SET2                   TEST FOR NO SECOND              46720002
  AIF   ('&SYSLIST(&OP+1,5)' EQ '' OR '&SYSLIST(&OP+1,6)' EQ '').ERR2   46800002
         AIF   ('&SYSLIST(&OP+1,5)' GT '&SYSLIST(&OP+1,6)').ERR3        46880002
&END1    SETA  &SYSLIST(&OP+1,5)        GET START COUNT OF 2            46960002
&END2    SETA  &SYSLIST(&OP+1,6)        GET END COUNT OF 2              47040002
         AGO   .LOOP                                                    47120002
.SET2    ANOP                                                           47200002
&ONLY1   SETB  1                        INDICATE ONLY ONE CONCATENATION 47280002

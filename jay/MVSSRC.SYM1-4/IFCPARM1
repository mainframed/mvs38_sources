         LA    @08,HEXTAB-1(J)                                     0228 00464000
         MVC   0(1,@07),0(@08)                                     0228 00465000
*          END;                                            /*@Z427VLS*/ 00466000
*      END;                                                /*@Z427VLS*/ 00467000
@RF00225 BCTR  J,0                                                 0230 00468000
         LTR   J,J                                                 0230 00469000
         BP    @DL00224                                            0230 00470000
@DC00224 DS    0H                                                  0231 00471000
*      IF VALID = OFF THEN                                         0231 00472000
         TM    VALID,B'10000000'                                   0231 00473000

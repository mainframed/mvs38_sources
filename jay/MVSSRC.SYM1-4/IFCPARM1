         AL    @08,@CF00055                                        0333 00919000
         LR    INDEX1,@08                                          0333 00920000
*    END;                                                  /*@Z427VLS*/ 00921000
*      /*                                                    @Z427VLS*/ 00922000
*      /* INCREMENT THE INDEX TO THE TABLE OR PRINT AN ERROR MSG.    */ 00923000
*      /*                                                    @Z427VLS*/ 00924000
*  IF INDEX2 < DIM(CPUCUAV) THEN                                   0335 00925000
@RC00318 L     @08,INDEX2                                          0335 00926000

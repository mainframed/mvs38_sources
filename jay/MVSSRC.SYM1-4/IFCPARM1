*/********************************************************************/ 00686000
*/*                                                          @Z427VLS*/ 00687000
*/*  GENERATE A JULIAN YY/DDD FIELD FROM THE INPUT STREAM AND PLACE  */ 00688000
*/*  IT IN THE VARIABLE YYDDD.                               @Z427VLS*/ 00689000
*/*                                                          @Z427VLS*/ 00690000
*/********************************************************************/ 00691000
*YEARDAY: PROC OPTIONS(SAVE(1,14));                        /*@Z427VLS*/ 00692000
YEARDAY  ST    @14,@SA00010                                        0275 00693000
         ST    @01,@SA00010+4                                      0275 00694000
*    /*                                                      @Z427VLS*/ 00695000
*    /* THE FIELD IS FIRST CHECKED FOR VALID NUMERICS.  THE FIRST    */ 00696000
*    /* FOUR HEX VALUES ARE THEN PACKED INTO THE FIELD HOLD, AND THEN*/ 00697000
*    /* PLACED IN THE YEAR/DAY HOLD AREA (YYDDD).  THE REMAINING HEX */ 00698000
*    /* VALUE IS SHIFTED AND ORED WITH A SIGN FLAG.  THE FIELD IS    */ 00699000
*    /* THEN CHECKED AGAINST DAY 366.  IF GREATER, AN ERROR MESSAGE  */ 00700000
*    /* IS OUTPUT.                                           @Z427VLS*/ 00701000
*    /*                                                      @Z427VLS*/ 00702000
*    CALL VALIDNUM(5);                                     /*@Z427VLS*/ 00703000
         LA    @01,@AL00276                                        0276 00704000
         BAL   @14,VALIDNUM                                        0276 00705000
*    CALL HEXPACK(4);                                      /*@Z427VLS*/ 00706000
         LA    @01,@AL00277                                        0277 00707000
         BAL   @14,HEXPACK                                         0277 00708000
*    YY = HOLD(1);                                         /*@Z427VLS*/ 00709000
         MVC   YY(1),HOLD                                          0278 00710000
*    DD = HOLD(2);                                         /*@Z427VLS*/ 00711000
         MVC   DD(1),HOLD+1                                        0279 00712000
*    SLL(PARMFLD(INDEX1),FOUR);                            /*@Z427VLS*/ 00713000
         SLR   @08,@08                                             0280 00714000
         IC    @08,PARMFLD-1(INDEX1)                               0280 00715000
         SLL   @08,4                                               0280 00716000
         STC   @08,PARMFLD-1(INDEX1)                               0280 00717000
*    DS = PARMFLD(INDEX1) | SIGNFLAG;                      /*@Z427VLS*/ 00718000
         LA    @08,PARMFLD-1(INDEX1)                               0281 00719000
         MVC   DS(1),0(@08)                                        0281 00720000
         OI    DS,X'0F'                                            0281 00721000
*    IF DDDS > '366F'X | DDDS = '000F'X THEN                       0282 00722000
         CLC   DDDS(2),@CB00737                                    0282 00723000
         BH    @RT00282                                            0282 00724000
         CLC   DDDS(2),@CB00738                                    0282 00725000
         BNE   @RF00282                                            0282 00726000
@RT00282 DS    0H                                                  0283 00727000
*      DO PLACE = 2;                                       /*@Z427VLS*/ 00728000
         LA    @08,2                                               0283 00729000
         ST    @08,PLACE                                           0283 00730000
*        CALL OUTMSG(SYNTAX);                              /*@Z427VLS*/ 00731000
         LA    @01,@AL00284                                        0284 00732000
         BAL   @14,OUTMSG                                          0284 00733000
*      END;                                                /*@Z427VLS*/ 00734000
*    INDEX1 = INDEX1 + 1;                                  /*@Z427VLS*/ 00735000
@RF00282 AL    INDEX1,@CF00055                                     0286 00736000
*END;                                                      /*@Z427VLS*/ 00737000
@EL00010 DS    0H                                                  0287 00738000
@EF00010 DS    0H                                                  0287 00739000
@ER00010 L     @14,@SA00010                                        0287 00740000
         L     @01,@SA00010+4                                      0287 00741000
         BR    @14                                                 0287 00742000
*/********************************************************************/ 00743000
*/*                                                          @Z427VLS*/ 00744000
*/*  PACK A FIELD OF A GIVEN LENGTH INTO THE VARIABLE HOLD.  THE     */ 00745000
*/*  INPUT IS ASSUMED TO BE HEX CHARACTERS, WITH A LEADING ZERO FOR  */ 00746000
*/*  EACH TWO DIGITS.                                        @Z427VLS*/ 00747000
*/********************************************************************/ 00748000
*HEXPACK: PROC (HEXLEN) OPTIONS(SAVE(1,14));               /*@Z427VLS*/ 00749000
HEXPACK  ST    @14,12(,@13)                                        0288 00750000
         ST    @01,24(,@13)                                        0288 00751000
         MVC   @PC00011(4),0(@01)                                  0288 00752000
*   DCL HEXLEN FIXED(31);                                  /*@Z427VLS*/ 00753000
*   INDEX3 = HEXLEN / 2;                                   /*@Z427VLS*/ 00754000
         L     @08,@PC00011                                        0290 00755000
         L     @00,HEXLEN(,@08)                                    0290 00756000
         SRDA  @00,32                                              0290 00757000
         D     @00,@CF00074                                        0290 00758000
         ST    @01,INDEX3                                          0290 00759000
*   DO I = 1 TO INDEX3 BY 1;                               /*@Z427VLS*/ 00760000
         LA    I,1                                                 0291 00761000
         B     @DE00291                                            0291 00762000
@DL00291 DS    0H                                                  0292 00763000
*     /*                                                     @Z427VLS*/ 00764000
*     /* THE DATA IN PARMFLD IS ACCESSED TWO CHARACTERS AT A TIME.   */ 00765000
*     /* THE FIRST CHARACTER IS SHIFTED LEFT FOUR BITS AND THEN ORED */ 00766000
*     /* WITH THE SECOND CHARACTER.  THE SECOND CHARACTER IS ASSUMED */ 00767000
*     /* TO HAVE THE FIRST FOUR BITS AS ZEROES.              @Z427VLS*/ 00768000
*     /*                                                     @Z427VLS*/ 00769000
*     HOLD(I) = PARMFLD(INDEX1);                           /*@Z427VLS*/ 00770000
         LA    @08,HOLD-1(I)                                       0292 00771000
         LA    @07,PARMFLD-1(INDEX1)                               0292 00772000
         MVC   0(1,@08),0(@07)                                     0292 00773000
*     SLL (HOLD(I),FOUR);                                  /*@Z427VLS*/ 00774000
         SLR   @08,@08                                             0293 00775000
         IC    @08,HOLD-1(I)                                       0293 00776000
         SLL   @08,4                                               0293 00777000
         STC   @08,HOLD-1(I)                                       0293 00778000
*     HOLD(I) = HOLD(I) | PARMFLD(INDEX1 + 1);             /*@Z427VLS*/ 00779000
         LA    @08,HOLD-1(I)                                       0294 00780000
         LA    @07,PARMFLD(INDEX1)                                 0294 00781000
         OC    0(1,@08),0(@07)                                     0294 00782000
*     INDEX1 = INDEX1 + 2;                                 /*@Z427VLS*/ 00783000

*          CALL OUTMSG(SYNTAX);                            /*@Z427VLS*/ 00479000
         LA    @01,@AL00233                                        0233 00480000
         BAL   @14,OUTMSG                                          0233 00481000
*        END;                                              /*@Z427VLS*/ 00482000
*    END;                                                  /*@Z427VLS*/ 00483000
@RF00231 LA    @08,1                                               0235 00484000
         AL    @08,INDEX3                                          0235 00485000
         ST    @08,INDEX3                                          0235 00486000
@DE00222 CR    @08,I                                               0235 00487000
         BNH   @DL00222                                            0235 00488000
* END;                                                     /*@Z427VLS*/ 00489000
@EL00005 DS    0H                                                  0236 00490000

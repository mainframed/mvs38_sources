         MVC   CUAHOLD(3),0(@08)                                   0320 00883000
*      IF CUAHOLD(3) = 'X' THEN                                    0321 00884000
         CLI   CUAHOLD+2,C'X'                                      0321 00885000
         BNE   @RF00321                                            0321 00886000
*        PARMFLD(INDEX1+9) = '0';         /* CHANGE X TO ZERO        */ 00887000
         LA    @08,PARMFLD+8(INDEX1)                               0322 00888000
         MVI   0(@08),C'0'                                         0322 00889000
*      CALL VALIDNUM(6);                                   /*@Z427VLS*/ 00890000
@RF00321 LA    @01,@AL00323                                        0323 00891000
         BAL   @14,VALIDNUM                                        0323 00892000
*      INDEX1 = INDEX1 + 6;                                /*@Z427VLS*/ 00893000
         AL    INDEX1,@CF00081                                     0324 00894000
*      PARMFLD(INDEX1) = '0';                              /*@Z427VLS*/ 00895000
         LA    @08,PARMFLD-1(INDEX1)                               0325 00896000
         MVI   0(@08),C'0'                                         0325 00897000
*      CALL VALIDHEX(4);                                   /*@Z427VLS*/ 00898000
         LA    @01,@AL00326                                        0326 00899000

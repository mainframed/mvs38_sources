         BAL   @14,HEXPACK                                         0328 00905000
*      INDEX1 = INDEX1 + 1;                 /* INCR OVER DELIMITER   */ 00906000
         AL    INDEX1,@CF00055                                     0329 00907000
*    END;                                                  /*@Z427VLS*/ 00908000
*  ELSE                                                            0331 00909000
*    DO;                                    /* BAD INPUT LENGTH      */ 00910000
         B     @RC00318                                            0331 00911000
@RF00318 DS    0H                                                  0332 00912000
*      CALL OUTMSG(SYNTAX);                                /*@Z427VLS*/ 00913000

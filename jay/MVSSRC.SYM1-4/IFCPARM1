         BL    @DL00194                                            0213 00401000
*      END;                                                /*@Z427VLS*/ 00402000
*    IF FOUND = OFF THEN                                           0215 00403000
@RC00126 TM    FOUND,B'00010000'                                   0215 00404000
         BNZ   @RF00215                                            0215 00405000
*      CALL OUTMSG(SYNTAX);               /* DEVICE NOT VALID.       */ 00406000
         LA    @01,@AL00216                                        0216 00407000
         BAL   @14,OUTMSG                                          0216 00408000
*    INDEX1 = INDEX1 + 5;                 /* INCR TO NEXT OPERAND    */ 00409000
@RF00215 AL    INDEX1,@CF00079                                     0217 00410000
* END;                                                     /*@Z427VLS*/ 00411000
@EL00004 DS    0H                                                  0218 00412000
@EF00004 DS    0H                                                  0218 00413000
@ER00004 L     @14,@SA00004                                        0218 00414000
         L     @01,@SA00004+4                                      0218 00415000
         BR    @14                                                 0218 00416000
*/********************************************************************/ 00417000
*/*                                                          @Z427VLS*/ 00418000
*/*  VALID THE FIELD STARTING AT PARMFLD(INDEX1) FOR A LENGTH AS     */ 00419000
*/*  GIVEN AS THE INPUT PARAMETER.  IF A CHARACTER IS NOT FOUND TO   */ 00420000
*/*  BE A VALID HEX, THE FIELD IS INDICATED AS HAVING A SYNTAX       */ 00421000
*/*  ERROR.  THE CONTENTS OF THE ORIGINAL FIELD ARE CHANGED TO THE   */ 00422000
*/*  HEXADECIMAL EQUIVALENT FOR THE CHARACTER.  THOSE VALUES CAN THEN*/ 00423000
*/*  BE EASILY CONVERTED FOR THE NECESSARY FIELD.            @Z427VLS*/ 00424000
*/*                                                          @Z427VLS*/ 00425000
*/********************************************************************/ 00426000
* VALIDHEX: PROC (LENGTHH) ;                               /*@Z427VLS*/ 00427000
VALIDHEX STM   @14,@12,@SA00005                                    0219 00428000
         MVC   @PC00005(4),0(@01)                                  0219 00429000
*    DCL LENGTHH FIXED(31);               /* LENGTH OF THE INPUT FLD */ 00430000
*    I = INDEX1 + LENGTHH - 1;            /* LAST CHARACTER POSITION */ 00431000
         L     @08,@PC00005                                        0221 00432000
         LR    I,INDEX1                                            0221 00433000
         AL    I,LENGTHH(,@08)                                     0221 00434000
         BCTR  I,0                                                 0221 00435000
*    DO INDEX3 = INDEX1 TO I BY 1;        /* STEP THROUGH FIELD      */ 00436000
         LR    @08,INDEX1                                          0222 00437000
         ST    @08,INDEX3                                          0222 00438000
         B     @DE00222                                            0222 00439000
@DL00222 DS    0H                                                  0223 00440000
*      VALID = OFF;                                        /*@Z427VLS*/ 00441000
         NI    VALID,B'01111111'                                   0223 00442000
*      /*                                                    @Z427VLS*/ 00443000
*      /* CHECK TO SEE IF THE FIELD IS A VALID HEXADECIMAL NUMBER.   */ 00444000
*      /* IF YES, TERMINATE THE SEARCH, AND PLACE THE HEX EQUIVALENT */ 00445000
*      /* OF THE CHARACTER IN THE PARAMETER FIELD. IF NO, PRINT      */ 00446000
*      /* AN ERROR MESSAGE FOR THAT OPERAND.                 @Z427VLS*/ 00447000
*      /*                                                    @Z427VLS*/ 00448000
*      DO J = 16 TO 1 BY -1 WHILE (VALID = OFF);           /*@Z427VLS*/ 00449000
         LA    J,16                                                0224 00450000
@DL00224 TM    VALID,B'10000000'                                   0224 00451000
         BNZ   @DC00224                                            0224 00452000
*        IF PARMFLD(INDEX3) = NUMTAB(J) THEN                       0225 00453000
         L     @08,INDEX3                                          0225 00454000

         MACRO                                                          44100002
         SUM   &LOGITEM,&MASK                                           44150002
.*                            THIS MACRO GENERATES A NORMAL EDIT ENTRY  44200002
.*                                 FOR PROCESSING BY THE SUMMARY MODULE 44250002
.*                            &LOGITEM - NAME OF DATA ITEM TO BE SUMMED 44300002
.*                            &MASK - BYTE MASK FOR BITS TO BE TESTED.  44350002
.*                                 THIS OPERAND SHOULD BE OMITTED FOR   44400002
.*                                 SINGLE BIT ITEMS IN WHICH CASE THE   44450002
.*                                 MASK WILL BE SET ACCORDING TO THE    44500002
.*                                 OFF BYTE BIT DISPLACEMENT OF THE     44550002
.*                                 ITEM AS DEFINED BY THE MACRO 'DSGEN' 44600002
         GBLA  &COUNTR,&ITEMNBR,&BITS(3000),&SHIFT(3000)                44650002
         GBLC  &ITEM(3000),&DSECT                                       44700002
         LCLA  &IBITS,&NDX,&ISHIFT,&M,&BITNBR                           44750002
.LOOP    ANOP                                                           44800002
&NDX     SETA  &NDX+1                                                   44850002
         AIF   ('&ITEM(&NDX)' EQ '&LOGITEM').FOUND                      44900002
         AIF   (&NDX LT &ITEMNBR).LOOP                                  44950002
         MNOTE *,'LOGITEM NOT FOUND'                                    45000002
         MEXIT                                                          45050002
.FOUND   ANOP                                                           45100002
&ISHIFT  SETA  &SHIFT(&NDX)                                             45150002
&IBITS   SETA  &BITS(&NDX)                                              45200002
         AIF   (&IBITS EQ 1).FORM1      TEST FOR OLD FORM               45250002
         AIF   ('&MASK' NE '').FORM1    TEST FOR MASK                   45300002
         AIF   (&IBITS+&ISHIFT GT 8).MSG     TEST FOR MORE THAN EIGHT   45350002
         DC    AL.1(0),AL.3(&ISHIFT),AL.4(&IBITS)                       45400002
         DC    AL1(0)                                                   45450002
         AGO   .ITEM                                                    45500002
.MSG     MNOTE '**BITS GREATER THAN 8**'                                45550002
         MEXIT                                                          45600002
.FORM1   ANOP                                                           45650002
         DC    AL1(0)                                                   45700002
         AIF   ('&MASK' EQ '').FORM                                     45750002
         DC    B'&MASK'                                                 45800002
         AGO   .ITEM                                                    45850002
.FORM    ANOP                                                           45900002
&BITNBR  SETA  &ISHIFT-7                                                45950002
&BITNBR  SETA  0-&BITNBR                                                46000002
&M       SETA  1                                                        46050002
.LOOP2   ANOP                                                           46100002
         AIF   (&BITNBR EQ 0).GOTIT                                     46150002

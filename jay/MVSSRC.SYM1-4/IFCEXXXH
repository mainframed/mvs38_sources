         SR    R6,R6                    CLEAR R6                        58000002
         IC    R6,L0(R4)                PICK UP REL & SYS VALUES        58050002
         SRDL  R6,L5                    SAVE REL NO IN R7               58100002
        SLL   R6,L2                    MULTIPLY SYS VALUE BY 4          58150002
         L     R8,XLABELP               PICK UP LABEL POINTER           58200002
         LA    R6,RELTAB(R6)            INDEX INTO TABLE OF SYSTEMS     58250002
         MVC   L0(L4,R8),L0(R6)         MOVE SYSTEM NAME INTO LABEL     58300002
         SR    R6,R6                    CLEAR R6                        58350002
         SLDL  R6,L5                    RETREIVE REL NO.                58400002
         LTR   R6,R6                    IS IT ZERO                      58450002
         BZ    NOREL                    BR-YES                          58500002
         CVD   R6,WORK                  CONVERT IT                      58550002
         UNPK  L0(L2,R3),WORK+L6(L2)    UNPACK IT INTO PRINT BUFF       58600002
         MVZ   L1(L1,R3),L0(R3)         MOVE ON PROPER ZONE             58650002
         B     RETURN                   EXIT                            58700002
NOREL    MVC   L0(L4,R3),NONE           MOVE IN NONE FOR REL            58750002
         B     RETURN                                                   58800002

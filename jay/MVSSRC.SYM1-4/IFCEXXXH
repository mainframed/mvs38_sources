         TITLE 'IFCEXXXH - 3895 EDIT TABLE - ALIAS IFCE3895'            00050002
* ** THIS MODULE IS ENTIRELY IN SUPPORT OF CODE G30HIMD  ****           00100002
**** IT IS APPLICABLE FOR VS1 W/ SU01, MVS W/ SU27 AND AS SVS ICR ****  00150002
         SPACE 2                                                        00200002
         MACRO                                                          00250002
         BIN   ,                                                        00300002
.*                                 &SYSLIST(&OP,1) - NAME OF LOG ITEM   00350002
.*                                      RECORD DSECT                    00400002
.*                                 $SYSLIST(&OP,2) - NBR OF SPACES      00450002
.*                                      TO SKIP ON MULTIPLE             00500002
.*                                      PRINT OR NBR OF SPACES          00550002
.*                                      TO BACK UP FROM COLUMN FOR      00600002
.*                                      START OF PRINTING. THE LETTER   00650002
.*                                      B PRECEEDS THE NUMBER (B5) IF   00700002
.*                                      THE LATTER IS DESIRED. OMISSION 00750002
.*                                      OF THIS OPERAND FORCES OPTION 2 00800002
.*                                      WITH BACKUP SPACING EQUAL TO    00850002
.*                                 THE NUMBER OF BITS IN THE ITEM       00900002
.*                                 DEFINITION (VIA MACRO 'DSGEN').      00950002
.*                            A LIMIT OF 15 BITS IS IMPOSED FOR THE     01000002
.*                                 TOTAL NUMBER OF BITS TO PRINT.       01050002
         GBLA  &BITS(3000),&DB,&DT,&INITIAL                             01100002
         GBLA  &ITEMITR,&ITEMFST(20),&ITEMNBR,&ITEMMDX                  01150002
         GBLA  &ITEMNDX(3000),&ITEMXPT                                  01200002
         GBLA  &LABELNR,&LASTCC,&LB,&LBLCTR(100),&LPTR,&LT              01250002
         GBLA  &NRENTS,&SHIFT(3000)                                     01300002
         GBLB  &DIAGITF,&DIAGLIN,&DIAGSET,&DIAGTST,&FSW,&TEST           01350002
         GBLC  &CARRCON,&CSECT,&DSECT,&ITEM(3000)                       01400002
         GBLC  &LABELS(3000),&LBL(100),&NRENTSX                         01450002
         LCLB  &B,&SUB                                                  01500002
         LCLA  &IBITS,&ISHIFT,&K                                        01550002
         LCLA  &TEMP,&T,&OP                                             01600002
         LCLC  &S,&ARG                                                  01650002
          ACTR  500000                                                  01700002
         AIF   (&DIAGTST EQ 0).NOMSG                                    01750002
         AIF   (&INITIAL EQ 0).NOMSG                                    01800002
         AIF   (&DIAGLIN EQ 1).NOMSG                                    01850002
         MNOTE '**NO LINE ASSOCIATED WITH THIS DATA**'                  01900002
*****THE FOLLOWING GENERATED FOR TEST PURPOSES*****                     01950002
*        LINE                                                           02000002
         LINE                                                           02050002
.NOMSG   ANOP                                                           02100002
         AIF   (K'&SYSLIST(1) EQ K'&SYSLIST(1,1)).LOOP                  02150002
&SUB     SETB  1                                                        02200002
.LOOP    ANOP                                                           02250002
&OP      SETA  &OP+1                                                    02300002
&ARG     SETC  '&SYSLIST(&OP,1)'                                        02350002
         ITEMFIND &ARG                                                  02400002
         AIF   (&ITEMXPT EQ 0).MEXIT                                    02450002
&ISHIFT  SETA  &SHIFT(&ITEMXPT)                                         02500002
&IBITS   SETA  &BITS(&ITEMXPT)                                          02550002
         AIF   (&SUB EQ 1).SUB00                                        02600002
         AIF   ('&SYSLIST(3)' EQ '').SETT                               02650002
&IBITS   SETA  &SYSLIST(3)                                              02700002
         AGO   .SETT                                                    02750002
.SUB00   AIF   ('&SYSLIST(&OP,3)' EQ '').SETT                           02800002
&IBITS   SETA  &SYSLIST(&OP,3)                                          02850002
.SETT    ANOP                                                           02900002
&T       SETA  &IBITS-1                                                 02950002
&B       SETB  0                                                        03000002
         AIF   (&IBITS GT 0 AND &IBITS LT 16).SUB0                      03050002
         MNOTE '**NO. OF BITS OUTSIDE RANGE OF 1-15**'                  03100002
.MEXIT   MEXIT                                                          03150002
.SUB0    AIF   (&SUB EQ 1).SUBA                                         03200002
         AIF   ('&SYSLIST(2)' EQ '').BACKUP                             03250002
&K       SETA  K'&SYSLIST(2)                                            03300002
&B       SETB  ('&SYSLIST(2)'(1,1) EQ 'B')                              03350002
&S       SETC  '&SYSLIST(2)'(&B+1,&K-&B)                                03400002
         AGO   .SUBB                                                    03450002
.SUBA    ANOP                                                           03500002
         AIF   ('&SYSLIST(&OP,2)' EQ '').BACKUP                         03550002
&K       SETA  K'&SYSLIST(&OP,2)                                        03600002
&B       SETB  ('&SYSLIST(&OP,2)'(1,1) EQ 'B')                          03650002
&S       SETC  '&SYSLIST(&OP,2)'(&B+1,&K-&B)                            03700002
.SUBB    ANOP                                                           03750002
&T       SETA  &S                                                       03800002
.BACKUP  ANOP                                                           03850002
&NRENTS  SETA  &NRENTS+1                                                03900002
         AIF   (&IBITS NE 1 OR &B NE 0).BIN2                            03950002
&TEMP    SETA  &ISHIFT                                                  04000002
         DC    AL.12(&SYSLIST(&OP,1)-&DSECT),XL.4'&TEMP'                04050002
         AGO   .OUT                                                     04100002
         AIF   (&SUB EQ 1).BIN2                                         04150002
         AIF   ('&SYSLIST(2)' NE '').BIN3                               04200002
         AGO   .BIN2A                                                   04250002
.BIN2    AIF   ('&SYSLIST(&OP,2)' NE '').BIN3                           04300002
.BIN2A   ANOP                                                           04350002
&B       SETB  1                                                        04400002
.BIN3    ANOP                                                           04450002
         DC    AL.12(&SYSLIST(&OP,1)-&DSECT),XL.4'C'                    04500002
         DC    BL.1'&B',AL.7(&T),AL.4(&IBITS),FL.4'&ISHIFT'             04550002
.OUT     ANOP                                                           04600002
&ITEMXPT     SETA  0                                                    04650002
         AIF   (&SUB EQ 0).OUTA                                         04700002
         AIF   (&OP LT N'&SYSLIST).LOOP                                 04750002
.OUTA    ANOP                                                           04800002
         MEND                                                           04850002
         MACRO                                                          04900002
&NAME    DSGEN ,                                                        04950002
.*                            THIS MACRO GENERATES ONE DSECT WORD       05000002
.*                                 FORMATTED FOR THE SUBJECT LOG RECORD 05050002
.*                                 EACH OPERAND HAS TWO SUB LISTS. THE  05100002
.*                                 FIRST IS THE NAME OF THE LOG ITEM    05150002
.*                                 WHILE THE SECOND IS THE # OF BITS TO 05200002
.*                                 ALLOCATE FOR THIS ITEM. ITEMS MUST   05250002
.*                                 BE DEFINED CONTIGUOUSLY WITHOUT GAPS 05300002
.*                                 UNUSED BIT GROUPS MUST BE GIVEN      05350002
.*                                 DUMMY NAMES. THE NAMES PROVIDED FOR  05400002
.*                                 THE LOG ITEMS MAY THEN BE USED IN    05450002
.*                                 THE CONVERSION MACRO INSTRUCTIONS.   05500002
         GBLA  &BITS(3000),&DB,&DT,&INITIAL                             05550002
         GBLA  &ITEMITR,&ITEMFST(20),&ITEMNBR,&ITEMMDX                  05600002
         GBLA  &ITEMNDX(3000),&ITEMXPT                                  05650002
         GBLA  &LABELNR,&LASTCC,&LB,&LBLCTR(100),&LPTR,&LT              05700002
         GBLA  &NRENTS,&SHIFT(3000)                                     05750002
         GBLB  &DIAGITF,&DIAGLIN,&DIAGSET,&DIAGTST,&FSW,&TEST           05800002
         GBLC  &CARRCON,&CSECT,&DSECT,&ITEM(3000)                       05850002
         GBLC  &LABELS(3000),&LBL(100),&NRENTSX                         05900002
         LCLB  &ONLY1,&CTIND                                            05950002
         LCLA  &START1,&START2,&END1,&END2                              06000002
         LCLC  &SYMBOL,&EQUSYM                                          06050002
         LCLA  &OP,&COUNT,&BYTE,&EQUCNT                                 06100002
         ACTR  100000                                                   06150002
         AIF   (&ITEMITR EQ 0).START                                    06200002
 MNOTE   20,'DSGEN MACRO OCCURRS AFTER ITEMSORT MACRO HAS BEEN ISSUED.' 06250002
         MEXIT                                                          06300002
.START   AIF   (N'&SYSLIST(&OP+1) LT 3).LOOP                            06350002
         AIF   ('&SYSLIST(&OP+1,3)' EQ 'EQU').LOOP     TEST FOR EQU     06400002
  AIF   ('&SYSLIST(&OP+1,3)' EQ '' OR '&SYSLIST(&OP+1,4)' EQ '').ERR1   06450002
         AIF   ('&SYSLIST(&OP+1,3)' GT '&SYSLIST(&OP+1,4)').ERR3        06500002
&START1  SETA  &SYSLIST(&OP+1,3)        GET START COUNT OF 1            06550002
&START2  SETA  &SYSLIST(&OP+1,4)        GET FIRST END COUNT             06600002
         AGO   .SET2A                   BRANCH AROUND                   06650002
         AIF   ('&SYSLIST(&OP+1,5)' GT '&SYSLIST(&OP+1,6)').ERR3        06700002
.ERR1    MNOTE '**FIRST SYMBOL CONCATENATION INCOMPLETE**'              06750002
         MEXIT                                                          06800002
.SET2A   AIF   ('&SYSLIST(&OP+1,5)' EQ ''AND '&SYSLIST(&OP+1,6)' EQ '' X06850002
               ).SET2                   TEST FOR NO SECOND              06900002
  AIF   ('&SYSLIST(&OP+1,5)' EQ '' OR '&SYSLIST(&OP+1,6)' EQ '').ERR2   06950002
&END1    SETA  &SYSLIST(&OP+1,5)        GET START COUNT OF 2            07000002
&END2    SETA  &SYSLIST(&OP+1,6)        GET END COUNT OF 2              07050002
         AGO   .LOOP                                                    07100002
.SET2    ANOP                                                           07150002
&ONLY1   SETB  1                        INDICATE ONLY ONE CONCATENATION 07200002
         AGO   .LOOP                                                    07250002
.ERR2    MNOTE '**SECOND SYMBOL CONCATENATION INCOMPLETE**'             07300002
         MEXIT                                                          07350002
.ERR3    MNOTE '**START GREATER THAN END CONCATENATION**'               07400002
         MEXIT                                                          07450002
.LOOP    ANOP                                                           07500002
&OP      SETA  &OP+1                                                    07550002
         AIF   (&OP GT N'&SYSLIST).DONE      TEST IF ALL DONE           07600002
&ITEMNBR SETA  &ITEMNBR+1                                               07650002
         AIF   ('&SYSLIST(&OP,2)' NE '').OK1                            07700002
         MNOTE '**NULL BIT COUNT FIELD**'                               07750002
         MEXIT                                                          07800002
.OK1     ANOP                                                           07850002
&SYMBOL  SETC  '&SYSLIST(&OP,1)'                                        07900002
         AIF   ('&SYMBOL' NE '').OK                                     07950002
*              *****     LOG ITEM SYMBOL NOT PROVIDED     *****         08000002
         AIF   ('&SYSLIST(&OP,3)' NE 'EQU').CNT                         08050002
         AGO   .START                   GET NEXT                        08100002
.OK      AIF   (N'&SYSLIST(&OP) LT 3).NO3    TEST FOR OLD FORMAT        08150002
         AIF   ('&SYSLIST(&OP,3)' EQ 'EQU').EQU   TEST FOR EQU          08200002
         AIF   (&ONLY1).OK2             TEST FOR ONLY ONE CONCAT        08250002
&SYMBOL  SETC  '&SYMBOL.&START1.&END1'       GET 1 AND 2 CONCAT         08300002
         AGO   .NO3                     BRANCH AROUND                   08350002
.OK2     ANOP                                                           08400002
&SYMBOL  SETC  '&SYMBOL.&START1'        GET 1 CONCATENATED              08450002
.NO3     ANOP                                                           08500002
&SYMBOL  EQU   *+&BYTE                                                  08550002
&ITEM(&ITEMNBR) SETC '&SYMBOL'          GET LABEL IN GLOBAL             08600002
&BITS(&ITEMNBR) SETA &SYSLIST(&OP,2)                                    08650002
&SHIFT(&ITEMNBR) SETA &COUNT-&BYTE*8                                    08700002
.CNT     ANOP                                                           08750002
&CTIND   SETB  1                        SET COUNT INDICATOR             08800002
&COUNT   SETA  &COUNT+&SYSLIST(&OP,2)                                   08850002
&BYTE    SETA  &COUNT/8                                                 08900002
         AIF   (N'&SYSLIST(&OP) LT 3).START  TEST FOR OLD FORMAT        08950002
         AIF   (&ONLY1).TSTST           TEST FOR ONLY ONE CONCAT        09000002
         AIF   (&END1 EQ &END2).TSTST   TEST FOR SECOND END             09050002
&END1    SETA  &END1+1                  INCREMENT COUNT                 09100002
&OP      SETA  &OP-1                    DECREMENT INCREMENT             09150002
         AGO   .LOOP                    GENERATE NEXT                   09200002
.TSTST   ANOP                                                           09250002
         AIF   (&START1 EQ &START2).LPDONE   TEST FOR END               09300002
         AIF   (&ONLY1).NSET1           TEST FOR ONLY ONE CONCAT        09350002
&END1    SETA  &SYSLIST(&OP,5)          RE-INIT SECOND START COUNT      09400002
.NSET1   ANOP                                                           09450002
&OP      SETA  &OP-1                    DECREMENT INDEX                 09500002
&START1  SETA  &START1+1                INCREMENT FIRST COUNT           09550002
         AGO   .LOOP                    GO DO NEXT                      09600002
.LPDONE  ANOP                                                           09650002
&ONLY1   SETB  0                        CLEAR INDICATOR                 09700002
         AGO   .START                   GO TEST FOR MORE SUB-LISTS      09750002
.DONE    AIF   (&COUNT-&COUNT/8*8 NE 0).ERR  TEST FOR MULTIPLE OF EIGHT 09800002
         AIF   (&CTIND EQ 0).END        NO DS IF NO COUNT               09850002
&COUNT   SETA  &COUNT/8                 GET NUMBER OF BYTES             09900002
&NAME    DS    &COUNT.C                                                 09950002
.END     MEXIT                                                          10000002
.ERR     MNOTE '**BIT COUNT FLDS DO NOT TOTAL MULTIPLE OF 8**'          10050002
         MEXIT                                                          10100002
.EQU     AIF   ('&SYSLIST(&OP,4)' NE '').EQU1     TEST FOR NULL LABEL   10150002
.ERR4    MNOTE '**EQU OPERAND OMITTED OR NOT FOUND**'                   10200002
         MEXIT                                                          10250002
.EQU1    AIF   (&SYSLIST(&OP,2) LT 16).EQU2  COUNT OVER FIFTEEN         10300002
*****               THE FOLLOWING STATEMENT                *****        10350002
*****     NUMBER OF BITS EXCEED 15 - DO NOT USE AS BIN     *****        10400002
.EQU2    ANOP                                                           10450002
&EQUCNT  SETA  0                        CLEAR COUNTER                   10500002
&EQUSYM  SETC  '&SYSLIST(&OP,4)'        GET EQU OPERAND                 10550002
.EQULOP  ANOP                                                           10600002
&EQUCNT  SETA  &EQUCNT+1                INCREMENT INDEX                 10650002
         AIF   ('&ITEM(&EQUCNT)' EQ '&EQUSYM').EQUOUT  LABEL FOUND?     10700002
         AIF   (&EQUCNT EQ &ITEMNBR).ERR4    ALL TESTED?                10750002
         AGO   .EQULOP                  NO-GET NEXT                     10800002
.EQUOUT  ANOP                                                           10850002
&ITEM(&ITEMNBR) SETC '&SYMBOL'          GET LABEL NAME                  10900002
&BITS(&ITEMNBR) SETA &SYSLIST(&OP,2)    GET NUMBER OF BITS              10950002
&SHIFT(&ITEMNBR) SETA &SHIFT(&EQUCNT)   GET SHIFT COUNT                 11000002
&SYMBOL  EQU   &EQUSYM                  THIS SYMBOL EQUATED             11050002
         AGO   .START                   RETURN FOR NEXT                 11100002
         MEND                                                           11150002
         MACRO                                                          11200002
         CONVT ,                                                        11250002
         GBLA  &BITS(3000),&DB,&DT,&INITIAL                             11300002
         GBLA  &ITEMITR,&ITEMFST(20),&ITEMNBR,&ITEMMDX                  11350002
         GBLA  &ITEMNDX(3000),&ITEMXPT                                  11400002
         GBLA  &LABELNR,&LASTCC,&LB,&LBLCTR(100),&LPTR,&LT              11450002
         GBLA  &NRENTS,&SHIFT(3000)                                     11500002
         GBLB  &DIAGITF,&DIAGLIN,&DIAGSET,&DIAGTST,&FSW,&TEST           11550002
         GBLC  &CARRCON,&CSECT,&DSECT,&ITEM(3000)                       11600002
         GBLC  &LABELS(3000),&LBL(100),&NRENTSX                         11650002
         LCLB  &SUB                                                     11700002
         LCLA  &OP                                                      11750002
         LCLA  &OFFSET,&OUT,&IN,&IND,&IBITS,&ISHIFT                     11800002
.*                                                                      11850002
.*       CONVERT BINARY TO DECIMAL                                      11900002
.*             &NAME IS THE REFERENCE INTO THE DSECT RECORD             11950002
.*             &OUT    IS THE NUMBER OF CHARS TO BE PUT ONTO PRINT LINE 12000002
.*                                      DEFAULT IS FOUR MAXIMUM IS 8    12050002
.*             &IN       IS THE NUMBER OF BYTES AS INPUT                12100002
.*                                      ERROR DEFAULT IS 2 MAXIMUM IS 4 12150002
.*                                                                      12200002
         AIF   (&DIAGTST EQ 0).NOMSG                                    12250002
         AIF   (&INITIAL EQ 0).NOMSG                                    12300002
         AIF   (&DIAGLIN EQ 1).NOMSG                                    12350002
         MNOTE '**NO LINE ASSOCIATED WITH THIS DATA**'                  12400002
*****THE FOLLOWING GENERATED FOR TEST PURPOSES*****                     12450002
*        LINE                                                           12500002
         LINE                                                           12550002
.NOMSG   ANOP                                                           12600002
         AIF   (K'&SYSLIST(1) EQ K'&SYSLIST(1,1)).LLOP                  12650002
&SUB     SETB  1                                                        12700002
.LLOP    ANOP                                                           12750002
&OP      SETA  &OP+1                                                    12800002
&OUT     SETA  4                        .BR NOT NULL ELSE SET TO 4      12850002
         AIF   (&SUB EQ 1).SUBA                                         12900002
         AIF   ('&SYSLIST(3)' EQ '').B  TEST FOR NULL INPUT             12950002
&OUT     SETA  &SYSLIST(3)             SET &OUT FROM INPUT              13000002
         AGO   .A                                                       13050002
.SUBA    AIF   ('&SYSLIST(&OP,3)' EQ '').B                              13100002
&OUT     SETA  &SYSLIST(&OP,3)          SET &OUT FROM INPUT             13150002
.A       AIF   (&OUT GT 0 AND &OUT LT 9).B   TEST RANGE                 13200002
.AERR    MNOTE '*** BYTES OUT INCORRECTLY SPECIFIED DEFAULT USED ***'   13250002
&OUT     SETA  4                                                        13300002
.B       AIF   (&SUB EQ 1).SUBB                                         13350002
         AIF   ('&SYSLIST(2)' EQ '').LOOP    BRANCH IF NULL             13400002
&IN      SETA  &SYSLIST(2)              .SET &IN FROM INPUT             13450002
         AGO   .BB                                                      13500002
.SUBB    AIF   ('&SYSLIST(&OP,2)' EQ '').LOOP                           13550002
&IN      SETA  &SYSLIST(&OP,2)          .SET &IN FROM INPUT             13600002
.BB      AIF   (&IN GT 0 AND &IN LT 5).D     TEST RANGE                 13650002
.BERR    MNOTE '*** BYTES IN INCORRECTLY SPECIFIED DEFAULT USED ***'    13700002
&IN      SETA  2                                                        13750002
         AGO   .D                                                       13800002
.LOOP    ANOP                           .LOOP TO FIND &NAME IN DSECT    13850002
&IND     SETA  &IND+1                   .SET UP LOOP COUNTER            13900002
         AIF   ('&ITEM(&IND)' EQ '&SYSLIST(&OP,1)').FOUND               13950002
         AIF   (&IND LT &ITEMNBR).LOOP  .TEST FOR END OF DSECT          14000002
         MNOTE '*** LOG ITEM NOT DEFINED ***'                           14050002
         MEXIT                                                          14100002
.FOUND   ANOP                                                           14150002
&IBITS   SETA  &BITS(&IND)              .SET UP NUMBER OF BITS SPEC     14200002
         AIF   (&IBITS-((&IBITS/8)*8) EQ 0).C  .TEST FOR MULTIPLE OF 8  14250002
         MNOTE '*** DATA NOT DEFINED A MULTIPLE OF 8 BITS ***'          14300002
         MEXIT                                                          14350002
.C       ANOP                                                           14400002
&IN      SETA  &IBITS/8                 .COMPUTE NUMBER OF BYTES        14450002
         AIF   (&IN LT 1 OR &IN GT 4).BERR  .TEST RANGE B IF IN ERROR   14500002
&ISHIFT  SETA  &SHIFT(&IND)             .SET UP FOR TEST OF BYTE BNDR   14550002
         AIF   (&ISHIFT EQ 0).D         .TEST FOR BYTE BNDRY            14600002
         MNOTE '*** DATA NOT ALIGNED ON A BYTE BOUNDARY ***'            14650002
         MEXIT                                                          14700002
.*                                                                      14750002
.D       ANOP                                                           14800002
&NRENTS  SETA  &NRENTS+1                                                14850002
         DC    AL.12(&SYSLIST(&OP,1)-&DSECT),AL.4(9)                    14900002
         DC    X'&IN',X'&OUT'                                           14950002
&IND     SETA  0                                                        15000002
         AIF   (&SUB EQ 0).OUTA                                         15050002
         AIF   (&OP LT N'&SYSLIST).LLOP                                 15100002
.OUTA    ANOP                                                           15150002
         MEND                                                           15200002
         MACRO                                                          15250002
         ITEMFIND &ARG,&ITFDIAG=                                        15300002
         GBLA  &BITS(3000),&DB,&DT,&INITIAL                             15350002
         GBLA  &ITEMITR,&ITEMFST(20),&ITEMNBR,&ITEMMDX                  15400002
         GBLA  &ITEMNDX(3000),&ITEMXPT                                  15450002
         GBLA  &LABELNR,&LASTCC,&LB,&LBLCTR(100),&LPTR,&LT              15500002
         GBLA  &NRENTS,&SHIFT(3000)                                     15550002
         GBLB  &DIAGITF,&DIAGLIN,&DIAGSET,&DIAGTST,&FSW,&TEST           15600002
         GBLC  &CARRCON,&CSECT,&DSECT,&ITEM(3000)                       15650002
         GBLC  &LABELS(3000),&LBL(100),&NRENTSX                         15700002
         LCLA  &A,&B,&X,&I                                              15750002
         LCLC  &C1,&C2,&S                                               15800002
         ACTR  100000                                                   15850002
&ITEMXPT SETA  0                  INDICATE ITEM NOT FOUND.              15900002
         AIF   ('&ARG' NE '').NORM                                      15950002
         AIF   ('&ITFDIAG' EQ 'OFF'  OR  '&ITFDIAG' EQ 'ON').DIAGSET    16000002
  MNOTE 0,'ITEMFIND - OPERAND (&ITFDIAG) INVALID FOR ITFDIAG KEYWORD.'  16050002
         MEXIT                                                          16100002
.DIAGSET ANOP                                                           16150002
&DIAGITF SETB  ('&ITFDIAG' EQ 'ON')                                     16200002
         MEXIT                                                          16250002
.NORM    AIF   (&ITEMITR GT 0).SORTED                                   16300002
         ITEMSORT                                                       16350002
.SORTED  ANOP                                                           16400002
&I       SETA  &ITEMITR           COMPUTED ITTERATION COUNT.            16450002
&X       SETA  &ITEMMDX           COMPUTED MAXIMUM INDEX.               16500002
&C1      SETC  '&ARG'             ISOLATE SEARCH ARGUMENT.              16550002
&C1      SETC  '&C1'.'       '(1,8-K'&C1) PAD TO FULL 8 CHARACTERS.     16600002
.SRCHUP  ANOP                                                           16650002
&A       SETA  &A+&X              CURRENT TABLE SUBSCRIPT + INDEX.      16700002
&S       SETC  '+'                                                      16750002
         AIF   (&A  GT &ITEMNBR  AND  NOT &DIAGITF).SRCHDN1             16800002
         AIF   (&A  LE &ITEMNBR).SRCH00                                 16850002
&C2      SETC  '*-OVER-*'                                               16900002
&I       SETA  &I-1               DECREMENT ITTERATION COUNTER.         16950002
&X       SETA  &X/2               HALVE INDEX VALUE.                    17000002
 MNOTE *,'&ARG - &C2   ITRCTR = &I   PTR = &A&S &X'                     17050002
         AIF   (&I GE 0).SRCHDWN  IF ITTERATION COUNT NOT EXPIRED.      17100002
.NOFIND  MNOTE  20,' ITEMFIND FAILED FOR NAME  &C1  '                   17150002
         MEXIT                                                          17200002
.SRCH00  ANOP                                                           17250002
&I       SETA  &I-1               DECREMENT ITTERATION COUNTER.         17300002
         AIF   (&I LT 0).NOFIND   IF COUNT HAS GONE TO ZERO.            17350002
&X       SETA  &X/2               HALVE INDEX VALUE.                    17400002
&B       SETA  &ITEMNDX(&A)       GET SUBSCRIPT FROM INDEX TABLE.       17450002
&C2      SETC  '&ITEM(&B)'.'       '(1,8-K'&ITEM(&B))  PAD COMPARAND.   17500002
         AIF   (NOT &DIAGITF).COMPARE                                   17550002
 MNOTE *,'&ARG - &C2   ITRCTR = &I   PTR = &A&S &X   POS = &B'          17600002
.COMPARE AIF   ('&C1' LT '&C2').SRCHDWN   ARGUMENT LOWER, MOVE DOWN.    17650002
         AIF   ('&C1' GT '&C2').SRCHUP    ARGUMENT HIGHER, MOVE UP.     17700002
&ITEMXPT SETA  &B                 FOUND. SET INDEX POINTER FOR RETURN.  17750002
&ITEMFST(&I+1)  SETA  &ITEMFST(&I+1)+1                                  17800002
         MEXIT                                                          17850002
.SRCHDWN ANOP                                                           17900002
&A       SETA  &A-&X              CURRENT POINTER MINUS INDEX VALUE.    17950002
&S       SETC  '-'                                                      18000002
         AIF   (&A LE &ITEMNBR).SRCH00    IF WITHIN BOUNDS OF TABLE.    18050002
.SRCHDN1 ANOP                                                           18100002
&I       SETA  &I-1               DECREMENT ITTERATION COUNTER.         18150002
&X       SETA  &X/2               HALVE INDEX VALUE.                    18200002
         AIF   (&I GE 0).SRCHDWN  IF ITTERATION COUNT NOT EXPIRED.      18250002
         MNOTE  20,' ITEMFIND FAILED FOR NAME  &C1  '                   18300002
         MEND                                                           18350002
         MACRO                                                          18400002
         ITEMSORT  &DUMMY                                               18450002
         GBLA  &BITS(3000),&DB,&DT,&INITIAL                             18500002
         GBLA  &ITEMITR,&ITEMFST(20),&ITEMNBR,&ITEMMDX                  18550002
         GBLA  &ITEMNDX(3000),&ITEMXPT                                  18600002
         GBLA  &LABELNR,&LASTCC,&LB,&LBLCTR(100),&LPTR,&LT              18650002

         STM   R5,R8,REGSAVE            SAVE REGS                       57400002
         CLC   DATE(L4),X00             IS DATE 0 ?                     57450002
         BE    RETURN                   YES, BR                         57500002
         CLC   L0(L4,R3),X00            IS LO DATE / ?                  57550002
         BNE   XDATE2                   NO, BR                          57600002
         MVC   L0(L4,R3),DATE           MOVE DATE IN                    57650002
XDATE2   EQU   *                                                        57700002
         CLC   DATE(L4),L0(R3)          CHECK FOR LO DATE               57750002
         BL    SAVELOW                  IT IS LOWER, BRANCH             57800002
         CLC   DATE(L4),L4(R3)          CHECK FOR HI DATE               57850002
         BNH   RETURN                   IT IS IN BETWEEN, BR            57900002
         MVC   L4(L4,R3),DATE           IT IS HIGHER, SAVE IT           57950002
         B     RETURN                   RETURN                          58000002
MDRTYPE  STM   R5,R8,REGSAVE                                            58050002
         LR    R5,R3               SAVE COUNTER POINTER                 58100002
         TM    RECTYP,X40          COUNTER OVERFLOW?                    58150002
         BO    OVFLW               BRANCH IF YES                        58200002
         LA    R5,L2(R5)           STEP COUNTER FOR UNKNOWN RECS        58250002
         BAL   R8,ADDUP            ADD TO COUNTER              @XA19168 58260001
         B     MDRERR              GO CHECK FOR ERRORS         @XA19168 58270001
OVFLW    BAL   R8,ADDUP            GO ADD TO COUNTER                    58300002
         LA    R5,L2(R5)           STEP COUNTER FOR MDR ERRORS @XA19168 58310001
MDRERR   TM    RECTYP,X80          ERROR READING PRED SENSE    @XA19168 58320001
         BZ    RETURN              NO, RETURN                  @XA19168 58330001
         BAL   R8,ADDUP            YES, ADD TO ERROR COUNTER   @XA19168 58340001
         B     RETURN                                                   58350002
SAVELOW  EQU   *                                                        58400002
         MVC   L0(L4,R3),DATE           IT IS LO, SAVE IT               58450002
         B     RETURN                   RETURN                          58500002
ENTRY    DS    0F                                                       58550002
         SPECIAL XDATE1,0,SPACE=4                                       58600002
         SPECIAL MDRCK,0,SPACE=2                                        58650002
         SPECIAL OBRCNT,0,SPACE=4                              @XA19168 58700001
         SPECIAL OBRSENSE,0,SPACE=112                          @XA20885 58750001
         SPECIAL OBRSIO,0,SPACE=2                                       58800002
         DC    XL1'FF'                  END OF SUM SECTION              58850002
MDRCT    EQU   *                                                        58900002
         SPECIAL MDRTYPE,0,SPACE=3                             @XA19168 58950001
         SPECIAL MDRCHK,0,SPACE=346                            @XA20885 59000001
         DC    XL1'FF'             MDR END                              59050002
PEDIT    DS    0F                                                       59100002
&DSECT   SETC  'HAWKCTR'                                                59150002
&CSECT   SETC  'PEDIT'                                                  59200002
         USING *,R9                                                     59250002
         USING HAWKCTR,R2                                               59300002
         DC    A(PENTRY)                                                59350002
         DC    A(IFCSXXXH)                                              59400002
R9PTR    DC    F'0'                                                     59450002
LABELPTR DC    F'0'                                                     59500002
*                                                                       59550002
*        COUNTER DSECT DEFINITION,MUST BE IN ACCESS SEQUENCE            59600002

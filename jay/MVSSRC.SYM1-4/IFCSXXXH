*                                                                       62150002
         DS    0F                                                       62200002
RETURN1  LM    R5,R8,REGSAVE1           RESTORE REGS                    62250002
RETURN2  BR    R14                      RETURN TO CALLER                62300002
SKIP     BR    R14                 RETURN                               62350002
SKIPPER  BR    R14                 RETURN                               62400002
SETCPUNO EQU   *                                                        62450002
         STM   R5,R8,REGSAVE1           SAVE REGS                       62500002
         SR    R5,R5                    ZERO R5                         62550002
         IC    R5,L0(R4)                PICK UP CPU TYPE                62600002
         SLL   R5,L8                      V                             62650002
         IC    R5,L1(R4)                  V                             62700002
         LTR   R5,R5                    IS IT ZERO ?                    62750002
         BNZ   RETURN1                  NO, BRANCH                      62800002
         LA    R6,L46                   BACK UP POINTER 46 BYTES        62850002
         SR    R3,R6                     TO PUT IN NEW INFO             62900002
         MVC   L0(L9,R3),UNIV           SAY MODEL = UNIVERSAL           62950002
         MVC   L23(L6,R3),NONE          SAY SERIAL = NONE               63000002
         B     RETURN1                  RETURN                          63050002
SETDATE  EQU   *                                                        63100002
         UNPK  WORK(L8),L0(L4,R4)       UNPACK A DATE                   63150002
         MVZ   WORK+L7(L1),WORK         MOVE IN ZONE BITS               63200002

         AR    R6,R8                   COMPUTE OFFSET                   57600002
         MVC   L1(L1,R3),L0(R6)        MOVE LAST CHARACTER              57700002
         BCT   R5,AGAIN                                                 57800002
         B     RETURN                                                   57900002

         TITLE 'IFCET002 - EDIT TABLE - LINKNAME=IFCET002, ALIASES=IFCEC00100002
               T012'                                                    00200002
*  DSGEN MACRO     DSGEN MACRO     DSGEN MACRO     DSGEN MACRO        * 00300002
         SPACE 2                                                        00400002
         MACRO                                                          00500002
&NAME    DSGEN &X1,&X2,&X3,&X4,&X5,&X6,&X7,&X8,&X9,&X10,&X11,&X12,&X13,X00600002
               &X14,&X15,&X16,&X17,&X18,&X19,&X20,&X21,&X22,&X23,&X24, X00700002
               &X25,&X26,&X27,&X28,&X29,&X30,&X31,&X32                  00800002
.*                            THIS MACRO GENERATES ONE DSECT WORD       00900002
.*                                 FORMATTED FOR THE SUBJECT LOG RECORD 01000002
.*                                 EACH OPERAND HAS TWO SUB LISTS. THE  01100002
.*                                 FIRST IS THE NAME OF THE LOG ITEM    01200002
.*                                 WHILE THE SECOND IS THE # OF BITS TO 01300002
.*                                 ALLOCATE FOR THIS ITEM. ITEMS MUST   01400002
.*                                 BE DEFINED CONTIGUOUSLY WITHOUT GAPS 01500002
.*                                 UNUSED BIT GROUPS MUST BE GIVEN      01600002
.*                                 DUMMY NAMES. THE NAMES PROVIDED FOR  01700002
.*                                 THE LOG ITEMS MAY THEN BE USED IN    01800002
.*                                 THE CONVERSION MACRO INSTRUCTIONS.   01900002
         GBLC &ITEM(1000)                                               02000002
         GBLA &ITEMNBR,&BITS(1000),&SHIFT(1000)                         02100002
         LCLC  &SYMBOL                                                  02200002
         LCLA  &OP,&COUNT,&BYTE                                         02300002
.LOOP    ANOP                                                           02400002
&OP      SETA  &OP+1                                                    02500002
&ITEMNBR SETA  &ITEMNBR+1                                               02600002
&SYMBOL  SETC  '&SYSLIST(&OP,1)'                                        02700002
         AIF   ('&SYSLIST(&OP,2)' NE '').OK1                            02800002
         MNOTE *,'NULL BIT COUNT FIELD'                                 02900002
         MEXIT                                                          03000002
.OK1     ANOP                                                           03100002
         AIF   ('&SYMBOL' NE '').OK                                     03200002
         MNOTE 'LOG ITEM SYMBOL NOT PROVIDED'                           03300002
         AGO   .CNT                                                     03400002
.OK      ANOP                                                           03500002
         AIF   ('&SYMBOL'(1,1) EQ '''').CNT                *TEMP*       03600002
&SYMBOL  EQU   *+&BYTE                                                  03700002
&ITEM(&ITEMNBR)  SETC '&SYSLIST(&OP,1)'                                 03800002
&BITS(&ITEMNBR) SETA &SYSLIST(&OP,2)                                    03900002
&SHIFT(&ITEMNBR) SETA &COUNT-&BYTE*8                                    04000002
.CNT     ANOP                                                           04100002
&COUNT   SETA  &COUNT+&SYSLIST(&OP,2)                                   04200002
&BYTE    SETA  &COUNT/8                                                 04300002
         AIF (&OP LT N'&SYSLIST).LOOP                                   04400002
         AIF   (&COUNT NE 32).NOT32                                     04500002
&NAME    DS    4C                                                       04600002
         MEXIT                                                          04700002
.NOT32   AIF   (&COUNT NE 64).ERR                                       04800002
&NAME    DS    8C                                                       04900002
         MEXIT                                                          05000002
.ERR     MNOTE '**BIT COUNT FIELDS DO NOT TOTAL 32 OR 64**'             05100002
         MEND                                                           05200002
         EJECT                                                          05300002
*  LINE MACRO     LINE MACRO     LINE MACRO     LINE MACRO            * 05400002
         SPACE 2                                                        05500002
         MACRO                                                          05600002
&NAME    LINE  &DATA,&LABEL,&SKIP=                                      05700002
.*                            THIS MACRO ESTABLISHES THE MARGINS OR     05800002
.*                                 COLUMN SETTINGS FOR EACH LINE. IT    05900002
.*                                 GENERATES NECESSARY CARRIAGE CONTROL 06000002
.*                                 ENTRIES. LASTLY IT DETERMINES THE    06100002
.*                                 NUMBER OF ENTRIES ASSOCIATED WITH    06200002
.*                                 THE PREVIOUS LINE AND STORES THAT #  06300002
.*                                 INTO THE APPROPRIATE BYTE.           06400002
.*                            &DATA(1) - CHARACTER POSITION TO BEGIN    06500002
.*                                 DATA.                                06600002
.*                            &DATA(2) - # OF SPACES BETWEEN SUCCESSIVE 06700002
.*                                 DATA COLUMN STARTING CHARACTERS.     06800002
.*                            &LABEL(1) - CHAR. POS. TO BEGIN LABELS.   06900002
.*                            &LABEL(2) - # SPACES BETWEEN LINE'S LABEL 07000002
.*                                 STARTING CHARACTERS.                 07100002
.*                            &SKIP= - # OF LINES TO ADVANCE CARRIAGE   07200002
.*                                 TO BEGIN NEXT PRINTED LINE.          07300002
.*                            NEW ENTRIES ARE GENERATED ONLY WHEN       07400002
.*                                 NECESSARY. OMISSION OF ITEMS MEANS   07500002
.*                                 THEY HAVE NOT CHANGED SINCE PREVIOUS 07600002
.*                                 SETTINGS. A LINE MACRO IS REQUIRED   07700002
.*                                 FOR EACH PRINTED LINE. IF NO SETTING 07800002
.*                                 CHANGES ARE REQUIRED ALL OPERANDS    07900002
.*                                 ARE OMITTED IN WHICH CASE ONLY THE   08000002
.*                                 # OF ENTRIES FOR THE PREVIOUS LINE   08100002
.*                                 IS COMPUTED. THIS MACRO MUST BE THE  08200002
.*                                 FIRST MACRO INSTRUCTION ISSUED FOR   08300002
.*                                 THE SUBJECT LINE                     08400002
         GBLA  &DB,&DT,&LB,&LT,&NRENTS                                  08500002
         GBLA  &LASTCC,&INITIAL                                         08600002
         GBLC  &NRENTSX                                                 08700002
         GBLC  &CARRCON                                                 08800002
         GBLB  &TEST                                                    08900002
         LCLA  &S                                                       09000002
&NAME    DS    0C                                                       09100002
         AIF   (&INITIAL EQ 0).FIRSTLN                                  09200002
         LINEND                                                         09300002
         AGO   .NTFIRST                                                 09400002
.FIRSTLN ANOP                                                           09500002
&NRENTSX SETC  'N&SYSNDX'                                               09600002
&INITIAL SETA  1                                                        09700002
N&SYSNDX EQU   *                                                        09800002
.NTFIRST ANOP                                                           09900002
         DC    AL1(0)                                                   10000002
         AIF   ('&DATA(1)' EQ '').CKDTABS                               10100002
         AIF   (&DATA(1) EQ &DB).CKDTABS                                10200002
&DB      SETA  &DATA(1)                                                 10300002
         AGO   .AA                                                      10400002
.CKDTABS AIF   ('&DATA(2)' EQ '').NCDATA                                10500002
         AIF   (&DATA(2) EQ &DT).NCDATA                                 10600002
         AGO   .CC                                                      10700002
.AA      AIF   ('&DATA(2)' EQ '').BB                                    10800002
         AIF   (&DATA(2) EQ &DT).BB                                     10900002
.CC      ANOP                                                           11000002
&DT      SETA  &DATA(2)                                                 11100002
.BB      DC    XL2'FFFE'                                                11200002
         DC    FL1'&DB'                                                 11300002
         DC    FL1'&DT'                                                 11400002
&NRENTS  SETA  &NRENTS+1                                                11500002
.NCDATA  ANOP                                                           11600002
         AIF   ('&LABEL(1)' EQ '').CKLTABS                              11700002
         AIF   (&LABEL(1) EQ &LB).CKLTABS                               11800002
&LB      SETA  &LABEL(1)                                                11900002
         AGO   .AAA                                                     12000002
.CKLTABS AIF   ('&LABEL(2)' EQ '').NCLABEL                              12100002
         AIF   (&LABEL(2) EQ &LT).NCLABEL                               12200002
         AGO   .CCC                                                     12300002
.AAA     AIF   ('&LABEL(2)' EQ '').BBB                                  12400002
         AIF   (&LABEL(2) EQ &LT).BBB                                   12500002
.CCC     ANOP                                                           12600002
&LT      SETA  &LABEL(2)                                                12700002
.BBB     DC    XL2'FFFF'                                                12800002
         DC    FL1'&LB'                                                 12900002
         DC    FL1'&LT'                                                 13000002
&NRENTS  SETA  &NRENTS+1                                                13100002
.NCLABEL ANOP                                                           13200002
         AIF   ('&SKIP' NE '').CONT                                     13300002
         AIF   (&TEST EQ 0).NOCHANG                                     13400002
&S       SETA  &LASTCC                                                  13500002
         AGO   .DEFAULT                                                 13600002
.CONT    ANOP                                                           13700002
         AIF   (&TEST EQ 0).AB                                          13800002
&S       SETA  &SKIP                                                    13900002
.DEFAULT ANOP                                                           14000002
         ORG   &CARRCON+3                                               14100002
         DC    AL.3(0),AL.2(&S),AL.3(1)                                 14200002
         ORG                                                            14300002
&LASTCC  SETA  &S                                                       14400002
&TEST    SETB  0                                                        14500002
         AGO   .DD                                                      14600002
.AB      AIF   (&SKIP EQ &LASTCC).NOCHANG                               14700002
A&SYSNDX DC    XL2'FFFB'                                                14800002
         DC    AL.3(0),AL.2(&SKIP),AL.3(1)                              14900002
         DC    FL1'9'                                                   15000002
&LASTCC  SETA  &SKIP                                                    15100002
&TEST    SETB  1                                                        15200002
&CARRCON SETC  'A&SYSNDX'                                               15300002
&NRENTS  SETA  &NRENTS+1                                                15400002
.DD      ANOP                                                           15500002
.NOCHANG ANOP                                                           15600002
         MEND                                                           15700002
         EJECT                                                          15800002
*  BIN MACRO     BIN MACRO     BIN MACRO     BIN MACRO     BIN MACRO  * 15900002
         SPACE 2                                                        16000002
         MACRO                                                          16100002
         BIN   &LOGITEM,&SKIP$OR                                        16200002
.*                                 &LOGITEM - NAME OF ITEM IN LOG       16300002
.*                                      RECORD DSECT                    16400002
.*                                 &SKIP$OR - NBR OF SPACES TO SKIP ON  16500002
.*                                 MULTIPLE PRINT OR NBR OF SPACES      16600002
.*                                      TO BACK UP FROM COLUMN FOR      16700002
.*                                      START OF PRINTING. THE LETTER   16800002
.*                                      B PRECEEDS THE NUMBER (B5) IF   16900002
.*                                      THE LATTER IS DESIRED. OMISSION 17000002
.*                                      OF THIS OPERAND FORCES OPTION 2 17100002
.*                                      WITH BACKUP SPACING EQUAL TO    17200002
.*                                 THE NUMBER OF BITS IN THE ITEM       17300002
.*                                 DEFINITION (VIA MACRO 'DSGEN').      17400002
.*                            A LIMIT OF 15 BITS IS IMPOSED FOR THE     17500002
.*                                 TOTAL NUMBER OF BITS TO PRINT.       17600002
         GBLC &ITEM(1000)                                               17700002
         GBLA &ITEMNBR,&BITS(1000),&SHIFT(1000),&NRENTS                 17800002
         GBLC  &DSECT                                                   17900002
         LCLB  &B                                                       18000002
         LCLA  &IBITS,&NDX,&ISHIFT,&K                                   18100002
         LCLA  &TEMP,&T                                                 18200002
         LCLC  &S                                                       18300002
.LOOP    ANOP                                                           18400002
&NDX     SETA  &NDX+1                                                   18500002
         AIF   ('&ITEM(&NDX)' EQ '&LOGITEM').FOUND                      18600002
         AIF   (&NDX LT &ITEMNBR).LOOP                                  18700002
         MNOTE *,'YOU BLEW IT'                                          18800002
         ROUTINE DUM,SKIPPER                                            18900002
         MEXIT                                                          19000002
.FOUND   ANOP                                                           19100002
&ISHIFT  SETA  &SHIFT(&NDX)                                             19200002
&IBITS   SETA  &BITS(&NDX)                                              19300002
&T       SETA  &IBITS-1                                                 19400002
&B       SETB  0                                                        19500002
         AIF   ('&SKIP$OR' EQ '').BACKUP                                19600002
&K       SETA  K'&SKIP$OR                                               19700002
&B       SETB  ('&SKIP$OR'(1,1) EQ 'B')                                 19800002
&S       SETC  '&SKIP$OR'(&B+1,&K-&B)                                   19900002
&T       SETA  &S                                                       20000002
.BACKUP  ANOP                                                           20100002
&NRENTS  SETA  &NRENTS+1                                                20200002
&TEMP    SETA  &ISHIFT                                                  20300002
A&SYSNDX DC    AL.12(&LOGITEM-&DSECT),XL.4'&TEMP'                       20400002
         AIF (&IBITS EQ 1 AND &B EQ 0).OUT                              20500002
         ORG   A&SYSNDX                                                 20600002
&B       SETB  1                                                        20700002
         DC    AL.12(&LOGITEM-&DSECT),XL.4'C'                           20800002
         DC    BL.1'&B',AL.7(&T),AL.4(&IBITS),FL.4'&ISHIFT'             20900002
.OUT     ANOP                                                           21000002
         MEND                                                           21100002
         EJECT                                                          21200002
*  HEX MACRO     HEX MACRO     HEX MACRO    HEXN MACRO     HEX MACRO  * 21300002
         SPACE 2                                                        21400002
         MACRO                                                          21500002
         HEX   &LOGITEM,&SKIP$OR,&BYTES                                 21600002
.*                            THIS MACRO FACILITATES GENERATION OF THE  21700002
.*                                 TABLE ENTRIES SPECIFYING HEX TYPE    21800002
.*                                 CONVERSIONS.                         21900002
.*                            &LOGITEM - NAME OF DATA ITEM TO BE        22000002
.*                                 CONVERTED                            22100002
.*                            &SKIP$OR - NUMBER OF SPACES TO SKIP ON    22200002
.*                                 MULTIPLE PRINT OR NBR OF SPACES TO   22300002
.*                                 BACK UP FROM COLUMN FOR START OF     22400002
.*                                 PRINTING. THE LETTER B PRECEEDS THE  22500002
.*                                 NUMBER (B8) IF THE LATTER OPTION IS  22600002
.*                                 DESIRED. OMISSION OF THIS OPERAND    22700002
.*                                 FORCES OPTION 2 WITH BACKUP SPACING  22800002
.*                                 EQUAL TO THE NUMBER OF BYTES SPECI-  22900002
.*                                 FIED BY THE NEXT OPERAND (TWICE THIS 23000002
.*                                 NUMBER IF SUFFIX IS EXPLICITLY OR    23100002
.*                                 IMPLICITLY B).                       23200002
.*                            &BYTES - NUMBER OF TOTAL INPUT BYTES TO   23300002
.*                                 PROCESS. THIS NUMBER IS SUFFIXED     23400002
.*                                 WITH AN L,R, OR B TO STIPULATE       23500002
.*                                 PRINTING OF THE LEFT HALF, RIGHT     23600002
.*                                 HALF, OR BOTH HALVES OF THE BYTE     23700002
.*                                 RESPECTIVELY. OMISSION OF THIS       23800002
.*                                 OPERAND DEFAULTS TO B WITH THE NUM-  23900002
.*                                 BER OF BYTES EQUAL TO THE ITEM LNGTH 24000002
.*                            A NEW ENTRY IS GENERATED ONLY WHEN NECES- 24100002
.*                                 SARY. THIS MACRO MUST BE THE FIRST   24200002
.*                                 MACRO INSTRUCTION ISSUED FOR THE     24300002
.*                                 SUBJECT LINE(S).                     24400002
         GBLC &ITEM(1000),&DSECT                                        24500002
         GBLA &ITEMNBR,&BITS(1000),&SHIFT(1000),&NRENTS                 24600002
         LCLB  &B                                                       24700002
         LCLA  &IBITS,&NDX,&ISHIFT,&K,&T,&CODE,&BYTESP                  24800002
         LCLC  &S,&LETTER                                               24900002
.LOOP    ANOP                                                           25000002
&NDX     SETA  &NDX+1                                                   25100002
         AIF   ('&ITEM(&NDX)' EQ '&LOGITEM').FOUND                      25200002
         AIF   (&NDX LT &ITEMNBR).LOOP                                  25300002
         MNOTE *,'LOG ITEM NOT DEFINED'                                 25400002
         MEXIT                                                          25500002
.FOUND   ANOP                                                           25600002
&ISHIFT  SETA  &SHIFT(&NDX)                                             25700002
&IBITS   SETA  &BITS(&NDX)                                              25800002
&CODE    SETA  1                                                        25900002
&K       SETA  K'&BYTES                                                 26000002
         AIF   (&IBITS-(&IBITS/4)*4 EQ 0).OK                            26100002
         MNOTE *,'BIT LENGTH SPECIFICATION INCONSISTENT WITH CONTROL'   26200002
         MEXIT                                                          26300002
.OK      AIF   ('&BYTES' EQ '').USEDEFN                                 26400002
&LETTER  SETC  '&BYTES'(&K,1)                                           26500002
&S       SETC  '&BYTES'(1,&K-1)                                         26600002
&BYTESP  SETA  &S                                                       26700002
         AIF   ('&LETTER' EQ 'B').BOTH                                  26800002
         AIF   (&ISHIFT EQ 0 OR &ISHIFT EQ 4).OK2                       26900002
.ERR     MNOTE *,'DATA NOT ALLIGNED ON HEX OR BYTE BOUNDARY'            27000002
         MEXIT                                                          27100002
.USEDEFN ANOP                                                           27200002
&BYTESP  SETA  &IBITS/8                                                 27300002
.BOTH    AIF   (&ISHIFT NE 0).ERR                                       27400002
&CODE    SETA  3                                                        27500002
&T       SETA  &BYTESP*2-1                                              27600002
&S       SETC  '&T'                                                     27700002
.OK2     AIF   (&ISHIFT EQ 0).L                                         27800002
&CODE    SETA  2                                                        27900002
.L       ANOP                                                           28000002
&B       SETB  1                                                        28100002
         AIF   ('&SKIP$OR' EQ '').BACKUP                                28200002
&K       SETA  K'&SKIP$OR                                               28300002
&B       SETB  ('&SKIP$OR'(1,1) EQ 'B')                                 28400002
&S       SETC  '&SKIP$OR'(&B+1,&K-&B)                                   28500002
.BACKUP  ANOP                                                           28600002
&NRENTS  SETA  &NRENTS+1                                                28700002
         DC    AL.12(&LOGITEM-&DSECT),XL.4'A'                           28800002
         DC    BL.1'&B',FL.7'&S',FL.4'&BYTESP',FL.4'&CODE'              28900002
.OUT     ANOP                                                           29000002
         MEND                                                           29100002
         SPACE 2                                                        29200002
*  ROUTINE MACRO     ROUTINE MACRO     ROUTINE MACRO     ROUTINE MACRO* 29300002
         SPACE 2                                                        29400002
         MACRO                                                          29500002
&NAME    ROUTINE &LOGITEM,&RNAME                                        29600002
         GBLC  &DSECT,&CSECT                                            29700002
         GBLC  &ITEM(1000)                                              29800002
         GBLA  &SHIFT(1000),&ITEMNBR                                    29900002
         GBLA  &NRENTS                                                  30000002
         LCLA  &ISHIFT,&NDX                                             30100002
.LOOP    ANOP                                                           30200002
&NDX     SETA  &NDX+1                                                   30300002
         AIF   ('&ITEM(&NDX)' EQ '&LOGITEM').FOUND                      30400002
         AIF   (&NDX LT &ITEMNBR).LOOP                                  30500002
         MNOTE *,'LOG ITEM NOT DEFINED'                                 30600002
         MEXIT                                                          30700002
.FOUND   ANOP                                                           30800002
&ISHIFT  SETA  &SHIFT(&NDX)                                             30900002
&NAME    DC    AL.12(&LOGITEM-&DSECT),XL.4'D'                           31000002
         DC    AL.4(&ISHIFT),AL.12(&RNAME-&CSECT)                       31100002
&NRENTS  SETA  &NRENTS+1                                                31200002
         MEND                                                           31300002
         EJECT                                                          31400002
*  LABEL MACRO     LABEL MACRO     LABEL MACRO     LABEL MACRO        * 31500002
         SPACE 2                                                        31600002
         MACRO                                                          31700002
&NAME    LABEL &LABEL1,&LABEL2,&LABEL3,&LABEL4,&LABEL5,&LABEL6,        C31800002
               &LABEL7,&LABEL8                                          31900002
.*                            THIS MACRO ALLOWS UP TO ACHT LABELS TO    32000002
.*                                 BE SPECIFIED FOR A GIVEN LINE. THE   32100002
.*                                 LABELS SHOULD BE ENCLOSED IN QUOTA-  32200002
.*                                 TION MARKS. THE COLUMN AND LINE END  32300002
.*                                 CONTROLS WILL BE INSERTED BY THE     32400002
.*                                 MACRO. OMITTED OPERANDS MEAN NO      32500002
.*                                 LABEL REQUIRED FOR RESPECTIVE        32600002
.*                                 COLUMN. THIS MACRO WILL ALSO ALERT   32700002
.*                                 THE PROGRAMMER TO EXACTLY WHERE THE  32800002
.*                                 TABLE MUST BE BROKEN IF NECESSARY    32900002
.*                                 TO AVOID EXCEEDING THE MAXIMUM LIMIT 33000002
         GBLC &LABELS(2000)                                             33100002
         GBLC  &LBL(100)                                                33200002
         GBLA  &LBLCTR(100)                                             33300002
         GBLA  &LABELNR                                                 33400002
         GBLA  &NRENTS,&LPTR                                            33500002
         LCLC  &MARGIN,&TACK                                            33600002
         LCLC  &TEXT                                                    33700002
         LCLA  &OP,&K,&START,&NRCHAR,&REMAIN                            33800002
&MARGIN  SETC  '*'                                                      33900002
         AIF   ('&NAME' EQ '').LOOPOUT                                  34000002
&LPTR    SETA  &LPTR+1                                                  34100002
&LBL(&LPTR) SETC '&NAME'                                                34200002
&LBLCTR(&LPTR) SETA &LABELNR+1                                          34300002
.LOOPOUT ANOP                                                           34400002
&OP      SETA  &OP+1                                                    34500002
&K       SETA  K'&SYSLIST(&OP)                                          34600002
         AIF (&K GE 2).NORM                                             34700002
&K       SETA  2                                                        34800002
.NORM    ANOP                                                           34900002
&START   SETA  2                                                        35000002
         AIF (&OP LT N'&SYSLIST).NOTLAST                                35100002
&MARGIN  SETC  '$'                                                      35200002
.NOTLAST ANOP                                                           35300002
&NRCHAR  SETA  8                                                        35400002
&TACK    SETC  ''                                                       35500002
.LOOPIN  ANOP                                                           35600002
&REMAIN  SETA  &K-&START                                                35700002
         AIF (&REMAIN GE 8).MORE                                        35800002
&TACK    SETC  '&MARGIN'                                                35900002
&NRCHAR  SETA  &REMAIN                                                  36000002
.MORE    ANOP                                                           36100002
&TEXT    SETC  ''                                                       36200002
         AIF (&REMAIN EQ 0).ENDTEXT                                     36300002
&TEXT    SETC  '&SYSLIST(&OP)'(&START,&NRCHAR)                          36400002
.ENDTEXT ANOP                                                           36500002
&LABELNR SETA  &LABELNR+1                                               36600002
&LABELS(&LABELNR) SETC '&TEXT'.'&TACK'                                  36700002
&START   SETA  &START+8                                                 36800002
         AIF ('&TACK' EQ '').LOOPIN                                     36900002
         AIF ('&TACK' EQ '*').LOOPOUT                                   37000002
         MEND                                                           37100002
         SPACE 2                                                        37200002
         MACRO                                                          37300002
         LSTART                                                         37400002
.*                            THIS MACRO IS ISSUED WHEN THE LABEL AREA  37500002
.*                                 OF THE TABLE IS TO BEGIN.            37600002
         GBLC &LABELS(2000)                                             37700002
         GBLA  &LABELNR                                                 37800002
         GBLC  &LBL(100)                                                37900002
         GBLA  &LBLCTR(100)                                             38000002
         LCLA  &NDX,&CTR                                                38100002
         LINEND                                                         38200002
&CTR     SETA  1                                                        38300002
.LOOP    ANOP                                                           38400002
&NDX     SETA  &NDX+1                                                   38500002
         AIF   (&LBLCTR(&CTR) NE &NDX).NOLBL                            38600002
&LBL(&CTR) DC  C'&LABELS(&NDX)'                                         38700002
&CTR     SETA  &CTR+1                                                   38800002
         AGO   .LOOPCTL                                                 38900002
.NOLBL   ANOP                                                           39000002
         DC    C'&LABELS(&NDX)'                                         39100002
.LOOPCTL AIF   (&NDX LT &LABELNR).LOOP                                  39200002
         MEND                                                           39300002
         SPACE 2                                                        39400002
         MACRO                                                          39500002
         LINEND                                                         39600002
.*                            THIS MACRO IS USED ONLY BY THE LINE       39700002
.*                                 MACRO TO SET THE NUMBER OF ENTRIES   39800002
.*                                 ASSOCIATED WITH EACH LINE.           39900002
         GBLA  &NRENTS                                                  40000002
         GBLC &NRENTSX                                                  40100002
         ORG   &NRENTSX                                                 40200002
         DC    FL1'&NRENTS'                                             40300002
         ORG                                                            40400002
&NRENTS  SETA  0                                                        40500002
B&SYSNDX EQU   *                                                        40600002
&NRENTSX SETC  'B&SYSNDX'                                               40700002
         MEND                                                           40800002
         GBLC  &DSECT,&CSECT                                            40900002
&CSECT   SETC  'IFCET002'                                               41000002
&DSECT   SETC  'M2305'                                                  41100002

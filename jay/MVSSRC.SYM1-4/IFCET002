CONVREL  STM   R5,R8,XSAVE              SAVE WORK REGS                  64900002
         SR    R6,R6                    CLEAR R6                        65000002
         IC    R6,L0(R4)                PICK UP REL & SYS VALUES        65100002
         SRDL  R6,L5                    SAVE REL NO IN R7               65200002
        SLL   R6,L2                    MULTIPLY SYS VALUE BY 4          65300002
         L     R8,XLABELP               PICK UP LABEL POINTER           65400002
         LA    R6,RELTAB(R6)            INDEX INTO TABLE OF SYSTEMS     65500002
         MVC   L0(L4,R8),L0(R6)         MOVE SYSTEM NAME INTO LABEL     65600002
         SR    R6,R6                    CLEAR R6                        65700002
         SLDL  R6,L5                    RETREIVE REL NO.                65800002
         LTR   R6,R6                    IS IT ZERO                      65900002
         BZ    NOREL                    BR-YES                          66000002
         CVD   R6,WORK                  CONVERT IT                      66100002
         UNPK  L0(L2,R3),WORK+L6(L2)    UNPACK IT INTO PRINT BUFF       66200002
         MVZ   L1(L1,R3),L0(R3)         MOVE ON PROPER ZONE             66300002
         B     RETURN                   EXIT                            66400002

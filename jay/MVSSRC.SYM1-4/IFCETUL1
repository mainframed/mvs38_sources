         MACRO                                                          13520002
         HEX   &LOGITEM,&SKIP$OR,&BYTES                                 13560002
.*                                                                      13600002
.*       THIS  MACRO               FACILITATES GENERATION               13640002
.*       OF    THE                 TABLE ENTRIES SPECIFYING HEX         13680002
.*       TYPE  CONVERSIONS.                                             13720002
.*                                                                      13760002
.*       &LOGITEM -                NAME OF DATA ITEM TO BE              13800002
.*       CONVERTED.                                                     13840002
.*                                                                      13880002
.*       &SKIP$OR -                NUMBER OF SPACES TO SKIP             13920002
.*       ON    MULTIPLE            PRINT OF NUMBER OF                   13960002
.*       SPACES TO                 BACK UP FROM COLUMN FOR              14000002
.*       START OF                  PRINTING. THE LETTER B               14040002
.*       PRECEEDS THE              NUMBER (B8) IF THE                   14080002
.*       LATTER OPTION             IS DESIRED. OMISSION                 14120002
.*       OF    THIS                OPERAND FORCES OPTION 2 WITH         14160002
.*       BACKUP SPACING            EQUAL TO THE NUMBER                  14200002
.*       OF    BYTES               SPECIFIED BY THE NEXT                14240002
.*       OPERAND (TWICE            THIS NUMBER IF SUFFIX                14280002
.*       IS    EXPLICITLY          OR IMPLICITLY B).                    14320002
.*                                                                      14360002
.*       &BYTES -                  NUMBER OF TOTAL INPUT BYTES          14400002
.*       TO    PROCESS.            THIS NUMBER IS SUFFIXED              14440002
.*       WITH  A                   L,R, OR B TO STIPULATE               14480002
.*       PRINTING OF               THE LEFT HALF, RIGHT                 14520002
.*       HALF, OR                  BOTH HALVES OF THE BYTE              14560002
.*       RESPECTIVELY. OMISSION    OF THIS                              14600002
.*       OPERAND DEFAULTS          TO B WITH THE                        14640002
.*       NUMBER OF                 BYTES EQUAL TO THE ITEM              14680002
.*       LENGTH.                                                        14720002
.*                                                                      14760002
.*       A     NEW                 ENTRY IS GENERATED ONLY WHEN         14800002
.*       NECESSARY. THIS           MACRO MUST BE THE                    14840002
.*       FIRST MACRO               INSTRUCTION ISSUED FOR               14880002
.*       THE   SUBJECT             LINE(S)                              14920002
         GBLC  &ITEM(2000),&DSECT                                       14960002
         GBLA  &ITEMNBR,&BITS(2000),&SHIFT(2000),&NRENTS                15000002
         LCLB  &B                                                       15040002
         LCLA  &IBITS,&NDX,&ISHIFT,&K,&T,&CODE,&BYTESP                  15080002
         LCLC  &S,&LETTER                                               15120002
.LOOP    ANOP                                                           15160002
&NDX     SETA  &NDX+1                                                   15200002
         AIF   ('&ITEM(&NDX)' EQ '&LOGITEM').FOUND                      15240002
         AIF   (&NDX LT &ITEMNBR).LOOP                                  15280002
         MNOTE '**LOG ITEM NOT FOUND**'                                 15320002
         MEXIT                                                          15360002
.FOUND   ANOP                                                           15400002
&ISHIFT  SETA  &SHIFT(&NDX)                                             15440002
&IBITS   SETA  &BITS(&NDX)                                              15480002
&CODE    SETA  1                                                        15520002
&K       SETA  K'&BYTES                                                 15560002
         AIF   (&IBITS-(&IBITS/4)*4 EQ 0).OK                            15600002
         MNOTE 'BIT LENGTH SPECIFICATION INCONSISTENT WITH CONTROL'     15640002
         MEXIT                                                          15680002
.OK      AIF   ('&BYTES' EQ '').USEDEFN                                 15720002
&LETTER  SETC  '&BYTES'(&K,1)                                           15760002
&S       SETC  '&BYTES'(1,&K-1)                                         15800002
&BYTESP  SETA  &S                                                       15840002
         AIF   ('&LETTER' EQ 'B').BOTH                                  15880002
         AIF   ('&LETTER' EQ 'L').LEFT                                  15920002
&CODE    SETA  2                                                        15960002
         AGO   .L                                                       16000002
.LEFT    ANOP                                                           16040002
&CODE    SETA  1                                                        16080002
         AGO   .L                                                       16120002
         AIF   (&ISHIFT EQ 0 OR &ISHIFT EQ 4).OK2                       16160002
.ERR     MNOTE 'DATA NOT ALIGNED ON HEX OR BYTE BOUNDARY'               16200002
         MEXIT                                                          16240002
.USEDEFN ANOP                                                           16280002
&BYTESP  SETA  &IBITS/8                                                 16320002
.BOTH    AIF   (&ISHIFT NE 0).ERR                                       16360002
&CODE    SETA  3                                                        16400002
&T       SETA  &BYTESP*2-1                                              16440002
&S       SETC  '&T'                                                     16480002
.OK2     AIF   (&ISHIFT EQ 0).L                                         16520002
&CODE    SETA  2                                                        16560002
.L       ANOP                                                           16600002
&B       SETB  1                                                        16640002
         AIF   ('&SKIP$OR' EQ '').BACKUP                                16680002
&K       SETA  K'&SKIP$OR                                               16720002
&B       SETB  ('&SKIP$OR'(1,1) EQ 'B')                                 16760002
&S       SETC  '&SKIP$OR'(&B+1,&K-&B)                                   16800002
.BACKUP  ANOP                                                           16840002

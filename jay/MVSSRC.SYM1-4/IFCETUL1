         TITLE 'IFCETUL1 MODEL  145 MACHINE CHECK EDIT'                 00040002
*                                  DSGEN MACRO     DSGEN MACRO          00080002
*                                  DSGEN MACRO     DSGEN MACRO          00120002
         SPACE 2                                                        00160002
         MACRO                                                          00200002
&NAME    DSGEN                                                          00240002
.*       THIS  MACRO               GENERATES ONE DSECT WORD             00280002
.*       FORMATTED FOR             THE SUBJECT LOG RECORD               00320002
.*       EACH  OPERAND             HAS TWO SUB LISTS. THE               00360002
.*       FIRST IS                  THE NAME OF THE LOG ITEM             00400002
.*       WHILE THE                 SECOND IS THE # OF BITS TO           00440002
.*       ALLOCATE FOR              THIS ITEM. ITEMS MUST                00480002
.*       BE    DEFINED             CONTIGUOUSLY WITHOUT GAPS            00520002
.*       UNUSED BIT                GROUPS MUST BE GIVEN                 00560002
.*       DUMMY NAMES.              THE NAMES PROVIDED FOR               00600002
.*       THE   LOG                 ITEMS MAY THEN BE USED IN            00640002
.*       THE   CONVERSION          MACRO INSTRUCTIONS.                  00680002
         GBLC  &ITEM(2000)                                              00720002
         GBLA  &ITEMNBR,&BITS(2000),&SHIFT(2000)                        00760002
         LCLB  &ONLY1,&CTIND                                            00800002
         LCLA  &START1,&START2,&END1,&END2                              00840002
         LCLC  &SYMBOL,&EQUSYM                                          00880002
         LCLA  &OP,&COUNT,&BYTE,&EQUCNT                                 00920002
         ACTR  100000                                                   00960002
.START   AIF   (N'&SYSLIST(&OP+1) LT 3).LOOP                            01000002
         AIF   ('&SYSLIST(&OP+1,3)' EQ 'EQU').LOOP     TEST FOR EQU     01040002
  AIF   ('&SYSLIST(&OP+1,3)' EQ '' AND '&SYSLIST(&OP+1,4)' EQ '').ERR1  01080002
  AIF   ('&SYSLIST(&OP+1,3)' EQ '' OR '&SYSLIST(&OP+1,4)' EQ '').ERR1   01120002
         AIF   ('&SYSLIST(&OP+1,3)' GT '&SYSLIST(&OP+1,4)').ERR3        01160002
&START1  SETA  &SYSLIST(&OP+1,3)   GET START COUNT OF 1                 01200002
&START2  SETA  &SYSLIST(&OP+1,4)   GET FIRST END COUNT                  01240002
         AGO   .SET2A              BRANCH AROUND                        01280002
.ERR1    MNOTE '**FIRST SYMBOL CONCATENATION INCOMPLETE**'              01320002
         MEXIT                                                          01360002
.SET2A   AIF   ('&SYSLIST(&OP+1,5)' EQ ''AND '&SYSLIST(&OP+1,6)' EQ '' X01400002
               ).SET2                   TEST FOR NO SECOND              01440002
  AIF   ('&SYSLIST(&OP+1,5)' EQ '' OR '&SYSLIST(&OP+1,6)' EQ '').ERR2   01480002
         AIF   ('&SYSLIST(&OP+1,5)' GT '&SYSLIST(&OP+1,6)').ERR3        01520002
&END1    SETA  &SYSLIST(&OP+1,5)   GET START COUNT OF 2                 01560002
&END2    SETA  &SYSLIST(&OP+1,6)   GET END COUNT OF 2                   01600002
         AGO   .LOOP                                                    01640002
.SET2    ANOP                                                           01680002
&ONLY1   SETB  1                   INDICATE ONLY ONE CONCATENATION      01720002
         AGO   .LOOP                                                    01760002
.ERR2    MNOTE '**SECOND SYMBOL CONCATENATION INCOMPLETE**'             01800002
         MEXIT                                                          01840002
.ERR3    MNOTE '**START GREATER THAN END CONCATENATION**'               01880002
         MEXIT                                                          01920002
.LOOP    ANOP                                                           01960002
&OP      SETA  &OP+1                                                    02000002
         AIF   (&OP GT N'&SYSLIST).DONE      TEST IF ALL DONE           02040002
&ITEMNBR SETA  &ITEMNBR+1                                               02080002
         AIF   ('&SYSLIST(&OP,2)'  NE '').OK1                           02120002
         MNOTE '**NULL BIT COUNT FIELD**'                               02160002
         MEXIT                                                          02200002
.OK1     ANOP                                                           02240002
&SYMBOL  SETC  '&SYSLIST(&OP,1)'                                        02280002
         AIF   ('&SYMBOL'          NE '').OK                            02320002
*              *****     LOG ITEM SYMBOL NOT PROVIDED     *****         02360002
         AIF   ('&SYSLIST(&OP,3)'  NE 'EQU').CNT                        02400002
         AGO   .START              GET NEXT                             02440002
.OK      AIF   (N'&SYSLIST(&OP) LT 3).NO3    TEST FOR OLD FORMAT        02480002
         AIF   ('&SYSLIST(&OP,3)'  EQ 'EQU').EQU   TEST FOR EQU         02520002
         AIF   (&ONLY1).OK2        TEST FOR ONLY ONE CONCAT             02560002
&SYMBOL  SETC  '&SYMBOL.&START1.&END1' GET 1 AND 2 CONCAT               02600002
         AGO   .NO3                BRANCH AROUND                        02640002
.OK2     ANOP                                                           02680002
&SYMBOL  SETC  '&SYMBOL.&START1'   GET 1 CONCATENATED                   02720002
.NO3     ANOP                                                           02760002
&SYMBOL  EQU   *+&BYTE                                                  02800002
&ITEM(&ITEMNBR) SETC '&SYMBOL'     GET LABEL IN GLOBAL                  02840002
&BITS(&ITEMNBR) SETA &SYSLIST(&OP,2)                                    02880002
&SHIFT(&ITEMNBR) SETA &COUNT-&BYTE*8                                    02920002
.CNT     ANOP                                                           02960002
&CTIND   SETB  1                   SET COUNT INDICATOR                  03000002
&COUNT   SETA  &COUNT+&SYSLIST(&OP,2)                                   03040002
&BYTE    SETA  &COUNT/8                                                 03080002
         AIF   (N'&SYSLIST(&OP) LT 3).START  TEST FOR OLD FORMAT        03120002
         AIF   (&ONLY1).TSTST      TEST FOR ONLY ONE CONCAT             03160002
         AIF   (&END1 EQ &END2).TSTST   TEST FOR SECOND END             03200002
&END1    SETA  &END1+1             INCREMENT COUNT                      03240002
&OP      SETA  &OP-1               DECREMENT INCREMENT                  03280002
         AGO   .LOOP               GENERATE NEXT                        03320002
.TSTST   ANOP                                                           03360002
         AIF   (&START1            EQ &START2).LPDONE   TEST FOR END    03400002
         AIF   (&ONLY1).NSET1      TEST FOR ONLY ONE CONCAT             03440002
&END1    SETA  &SYSLIST(&OP,5)     RE-INIT SECOND START COUNT           03480002
.NSET1   ANOP                                                           03520002
&OP      SETA  &OP-1               DECREMENT INDEX                      03560002
&START1  SETA  &START1+1           INCREMENT FIRST COUNT                03600002
         AGO   .LOOP               GO DO NEXT                           03640002
.LPDONE  ANOP                                                           03680002
&ONLY1   SETB  0                   CLEAR INDICATOR                      03720002
         AGO   .START              GO TEST FOR MORE SUB-LISTS           03760002
.DONE    AIF   (&COUNT-&COUNT/8*8  NE 0).ERR  TEST FOR MULTIPLE OF 8    03800002
         AIF   (&CTIND             EQ 0).END        NO DS IF NO COUNT   03840002
&COUNT   SETA  &COUNT/8            GET NUMBER OF BYTES                  03880002
&NAME    DS    &COUNT.C                                                 03920002
.END     MEXIT                                                          03960002
.ERR     MNOTE '**BIT COUNT FLDS DO NOT TOTAL MULTIPLE OF 8**'          04000002
         MEXIT                                                          04040002
.EQU     AIF   ('&SYSLIST(&OP,4)'  NE '').EQU1     TEST FOR NULL LABEL  04080002
.ERR4    MNOTE '**EQU OPERAND OMITTED OR NOT FOUND**'                   04120002
         MEXIT                                                          04160002
.EQU1    AIF   (&SYSLIST(&OP,2)    LT 16).EQU2  COUNT OVER FIFTEEN      04200002
*****               THE FOLLOWING STATEMENT                *****        04240002
*****     NUMBER OF BITS EXCEED 15 - DO NOT USE AS BIN     *****        04280002
.EQU2    ANOP                                                           04320002
&EQUCNT  SETA  0                   CLEAR COUNTER                        04360002
&EQUSYM  SETC  '&SYSLIST(&OP,4)'   GET EQU OPERAND                      04400002
.EQULOP  ANOP                                                           04440002
&EQUCNT  SETA  &EQUCNT+1           INCREMENT INDEX                      04480002
         AIF   ('&ITEM(&EQUCNT)'   EQ '&EQUSYM').EQUOUT  LABEL FOUND?   04520002
         AIF   (&EQUCNT            EQ &ITEMNBR).ERR4    ALL TESTED?     04560002
         AGO   .EQULOP             NO-GET NEXT                          04600002
.EQUOUT  ANOP                                                           04640002
&ITEM(&ITEMNBR) SETC '&SYMBOL'     GET LABEL NAME                       04680002
&BITS(&ITEMNBR) SETA &SYSLIST(&OP,2) GET NUMBER OF BITS                 04720002
&SHIFT(&ITEMNBR) SETA &SHIFT(&EQUCNT) GET SHIFT COUNT                   04760002
&SYMBOL  EQU   &EQUSYM             THIS SYMBOL EQUATED                  04800002
         AGO   .START              RETURN FOR NEXT                      04840002
         MEND                                                           04880002
*                                                                       04920002
*                                                                       04960002
         MACRO                                                          05000002
&NAME    LINE  &DATA,&LABEL,&SKIP=                                      05040002
.*       THIS  MACRO               ESTABLISHES THE MARGINS OR           05080002
.*       COLUMN SETTINGS           FOR EACH LINE. IT                    05120002
.*       GENERATES NECESSARY       CARRIAGE CONTROL                     05160002
.*       ENTRIES. LASTLY           IT DETERMINES THE                    05200002
.*       NUMBER OF                 ENTRIES ASSOCIATED WITH              05240002
.*       THE   PREVIOUS            LINE AND STORES THAT #               05280002
.*       INTO  THE                 APPROPRIATE BYTE.                    05320002
.*                                                                      05360002
.*       &DATA(1) -                CHARACTER POSITION TO                05400002
.*       BEGIN DATA.                                                    05440002
.*                                                                      05480002
.*       &DATA(2) -                # OF SPACES BETWEEN                  05520002
.*       SUCCESSIVE DATA           COLUMN STARTING                      05560002
.*       CHARACTERS.                                                    05600002
.*                                                                      05640002
.*       &LABEL(1) -               CHARACTER POSITION                   05680002
.*       TO    BEGIN               LABELS.                              05720002
.*                                                                      05760002
.*       &LABEL(2) -               # OF SPACES BETWEEN                  05800002
.*       LINE'S LABEL              STARTING CHARACTERS.                 05840002
.*                                                                      05880002
.*       &SKIP= -                  # OF LINES TO ADVANCE                05920002
.*       CARRIAGE TO               BEGIN NEXT PRINTED LINE.             05960002
.*                                                                      06000002
.*       NEW   ENTRIES             ARE GENERATED ONLY WHEN              06040002
.*       NECESSARY. OMISSION       OF ITEMS MEANS                       06080002
.*       THEY  HAVE                NOT CHANGED SINCE PREVIOUS           06120002
.*       SETTINGS. A               LINE MACRO IS REQUIRED               06160002
.*       FOR   EACH                PRINTED LINE. IF NO SETTING          06200002
.*       CHANGES ARE               REQUIRED ALL OPERANDS                06240002
.*       ARE   OMITTED             IN WHICH CASE ONLY THE               06280002
.*       #     OF                  ENTRIES FOR THE PREVIOUS LINE        06320002
.*       IS    COMPUTED.           THIS MACRO MUST BE THE               06360002
.*       FIRST MACRO               INSTRUCTION ISSUED FOR               06400002
.*       THE   SUBJECT             LINE.                                06440002
         GBLA  &DB,&DT,&LB,&LT,&NRENTS                                  06480002
         GBLA  &LASTCC,&INITIAL                                         06520002
         GBLC  &NRENTSX                                                 06560002
         GBLC  &CARRCON                                                 06600002
         GBLB  &TEST                                                    06640002
         LCLA  &S                                                       06680002
&NAME    DS    0C                                                       06720002
         AIF   (&INITIAL EQ 0).FIRSTLN                                  06760002
         LINEND                                                         06800002
         AGO   .NTFIRST                                                 06840002
.FIRSTLN ANOP                                                           06880002
&NRENTSX SETC  'N&SYSNDX'                                               06920002
&INITIAL SETA  1                                                        06960002
N&SYSNDX EQU   *                                                        07000002
.NTFIRST ANOP                                                           07040002
         DC    AL1(0)                                                   07080002
         AIF   ('&DATA(1)' EQ '').CKDTABS                               07120002
         AIF   (&DATA(1) EQ &DB).CKDTABS                                07160002
&DB      SETA  &DATA(1)                                                 07200002
         AGO   .AA                                                      07240002
.CKDTABS AIF   ('&DATA(2)' EQ '').NCDATA                                07280002
         AIF   (&DATA(2) EQ &DT).NCDATA                                 07320002
         AGO   .CC                                                      07360002
.AA      AIF   ('&DATA(2)' EQ '').BB                                    07400002
         AIF   (&DATA(2) EQ &DT).BB                                     07440002
.CC      ANOP                                                           07480002
&DT      SETA  &DATA(2)                                                 07520002
.BB      DC    XL2'FFFE'                                                07560002
         DC    FL1'&DB'                                                 07600002
         DC    FL1'&DT'                                                 07640002
&NRENTS  SETA  &NRENTS+1                                                07680002
.NCDATA  ANOP                                                           07720002
         AIF   ('&LABEL(1)' EQ '').CKLTABS                              07760002
         AIF   (&LABEL(1) EQ &LB).CKLTABS                               07800002
&LB      SETA  &LABEL(1)                                                07840002
         AGO   .AAA                                                     07880002
.CKLTABS AIF   ('&LABEL(2)' EQ '').NCLABEL                              07920002
         AIF   (&LABEL(2) EQ &LT).NCLABEL                               07960002
         AGO   .CCC                                                     08000002
.AAA     AIF   ('&LABEL(2)' EQ '').BBB                                  08040002
         AIF   (&LABEL(2) EQ &LT).BBB                                   08080002
.CCC     ANOP                                                           08120002
&LT      SETA  &LABEL(2)                                                08160002
.BBB     DC    XL2'FFFF'                                                08200002
         DC    FL1'&LB'                                                 08240002
         DC    FL1'&LT'                                                 08280002
&NRENTS  SETA  &NRENTS+1                                                08320002
.NCLABEL ANOP                                                           08360002
         AIF   ('&SKIP' NE '').CONT                                     08400002
         AIF   (&TEST EQ 0).NOCHANG                                     08440002
&S       SETA  &LASTCC                                                  08480002
         AGO   .DEFAULT                                                 08520002
.CONT    ANOP                                                           08560002
         AIF   (&TEST EQ 0).AB                                          08600002
&S       SETA  &SKIP                                                    08640002
.DEFAULT ANOP                                                           08680002
         ORG   &CARRCON+3                                               08720002
         DC    AL.3(0),AL.2(&S),AL.3(1)                                 08760002
         ORG                                                            08800002
&LASTCC  SETA  &S                                                       08840002
&TEST    SETB  0                                                        08880002
         AGO   .DD                                                      08920002
.AB      AIF   (&SKIP EQ &LASTCC).NOCHANG                               08960002
A&SYSNDX DC    XL2'FFFB'                                                09000002
         DC    AL.3(0),AL.2(&SKIP),AL.3(1)                              09040002
         DC    FL1'9'                                                   09080002
&LASTCC  SETA  &SKIP                                                    09120002
&TEST    SETB  1                                                        09160002
&CARRCON SETC  'A&SYSNDX'                                               09200002
&NRENTS  SETA  &NRENTS+1                                                09240002
.DD      ANOP                                                           09280002
.NOCHANG ANOP                                                           09320002
         MEND                                                           09360002

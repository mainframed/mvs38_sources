         MACRO                                                          09600002
         BIN   ,                                                        09640002
.*       &SYSLIST(&OP,1) -         NAME OF LOG ITEM                     09680002
.*       RECORD DSECT                                                   09720002
.*       $SYSLIST(&OP,2) -         NBR OF SPACES                        09760002
.*       TO    SKIP                ON MULTIPLE                          09800002
.*       PRINT OR                  NBR OF SPACES                        09840002
.*       TO    BACK                UP FROM COLUMN FOR                   09880002
.*       START OF                  PRINTING. THE LETTER                 09920002
.*       B     PRECEEDS            THE NUMBER (B5) IF                   09960002
.*       THE   LATTER              IS DESIRED. OMISSION                 10000002
.*       OF    THIS                OPERAND FORCES OPTION 2              10040002
.*       WITH  BACKUP              SPACING EQUAL TO                     10080002
.*       THE   NUMBER              OF BITS IN THE ITEM                  10120002
.*       DEFINITION (VIA           MACRO 'DSGEN').                      10160002
.*       A     LIMIT               OF 15 BITS IS IMPOSED FOR THE        10200002
.*       TOTAL NUMBER              OF BITS TO PRINT.                    10240002
         GBLA  &INITIAL                                                 10280002
         GBLB  &DIAGTST,&DIAGSET,&DIAGLIN                               10320002
         GBLC  &ITEM(2000),&DSECT                                       10360002
         GBLA  &BITS(2000),&ITEMNBR,&SHIFT(2000),&NRENTS                10400002
         LCLB  &B,&SUB                                                  10440002
         LCLA  &IBITS,&NDX,&ISHIFT,&K                                   10480002
         LCLA  &TEMP,&T,&OP                                             10520002
         LCLC  &S                                                       10560002
         ACTR  500000                                                   10600002
         AIF   (&DIAGTST EQ 0).NOMSG                                    10640002
         AIF   (&INITIAL EQ 0).NOMSG                                    10680002
         AIF   (&DIAGLIN EQ 1).NOMSG                                    10720002
         MNOTE '**NO LINE ASSOCIATED WITH THIS DATA**'                  10760002
*****THE FOLLOWING GENERATED FOR TEST PURPOSES*****                     10800002
*        LINE                                                           10840002
         LINE                                                           10880002
.NOMSG   ANOP                                                           10920002
         AIF   (K'&SYSLIST(1) EQ K'&SYSLIST(1,1)).LOOP                  10960002
&SUB     SETB  1                                                        11000002
.LOOP    ANOP                                                           11040002
&OP      SETA  &OP+1                                                    11080002
.LOOP1   ANOP                                                           11120002
&NDX     SETA  &NDX+1                                                   11160002
         AIF   ('&ITEM(&NDX)' EQ '&SYSLIST(&OP,1)').FOUND               11200002
         AIF   (&NDX LT &ITEMNBR).LOOP1                                 11240002
         MNOTE '**LOG ITEM NOT DEFINED**'                               11280002
         MEXIT                                                          11320002
.FOUND   ANOP                                                           11360002
&ISHIFT  SETA  &SHIFT(&NDX)                                             11400002
&IBITS   SETA  &BITS(&NDX)                                              11440002
         AIF   (&SUB EQ 1).SUB00                                        11480002
         AIF   ('&SYSLIST(3)' EQ '').SETT                               11520002
&IBITS   SETA  &SYSLIST(3)                                              11560002
         AGO   .SETT                                                    11600002
.SUB00   AIF   ('&SYSLIST(&OP,3)'  EQ '').SETT                          11640002
&IBITS   SETA  &SYSLIST(&OP,3)                                          11680002
.SETT    ANOP                                                           11720002
&T       SETA  &IBITS-1                                                 11760002
&B       SETB  0                                                        11800002
         AIF   (&IBITS GT 0 AND &IBITS LT 16).SUB0                      11840002
         MNOTE '**NO. OF BITS OUTSIDE RANGE OF 1-15**'                  11880002
         MEXIT                                                          11920002
.SUB0    AIF   (&SUB EQ 1).SUBA                                         11960002
         AIF   ('&SYSLIST(2)' EQ '').BACKUP                             12000002
&K       SETA  K'&SYSLIST(2)                                            12040002
&B       SETB  ('&SYSLIST(2)'(1,1) EQ 'B')                              12080002
&S       SETC  '&SYSLIST(2)'(&B+1,&K-&B)                                12120002
         AGO   .SUBB                                                    12160002
.SUBA    ANOP                                                           12200002
         AIF   ('&SYSLIST(&OP,2)'  EQ '').BACKUP                        12240002
&K       SETA  K'&SYSLIST(&OP,2)                                        12280002
&B       SETB  ('&SYSLIST(&OP,2)'(1,1) EQ 'B')                          12320002
&S       SETC  '&SYSLIST(&OP,2)'(&B+1,&K-&B)                            12360002
.SUBB    ANOP                                                           12400002
&T       SETA  &S                                                       12440002
.BACKUP  ANOP                                                           12480002
&NRENTS  SETA  &NRENTS+1                                                12520002
&TEMP    SETA  &ISHIFT                                                  12560002
         AIF   (&IBITS NE 1 OR &B NE 0).BIN2                            12600002
         DC    AL.12(&SYSLIST(&OP,1)-&DSECT),XL.4'&TEMP'                12640002
         AGO   .OUT                                                     12680002
.BIN2    AIF   (&SUB EQ 1).BIN2A                                        12720002
         AIF   ('&SYSLIST(2)' NE '').BIN3                               12760002
         AGO   .BIN2B                                                   12800002
.BIN2A   AIF   ('&SYSLIST(&OP,2)'  NE '').BIN3                          12840002
.BIN2B   ANOP                                                           12880002
&B       SETB  1                                                        12920002
.BIN3    ANOP                                                           12960002
         DC    AL.12(&SYSLIST(&OP,1)-&DSECT),XL.4'C'                    13000002
         DC    BL.1'&B',AL.7(&T),AL.4(&IBITS),FL.4'&ISHIFT'             13040002
.OUT     ANOP                                                           13080002
&NDX     SETA  0                                                        13120002
         AIF   (&SUB EQ 0).OUTA                                         13160002
         AIF   (&OP LT N'&SYSLIST).LOOP                                 13200002
.OUTA    ANOP                                                           13240002
         MEND                                                           13280002

CONVREL  STM   R5,R8,XSAVE         SAVE WORK REGS                       33640002
         SR    R6,R6               CLEAR R6                             33680002
         IC    R6,L0(R4)           PICK UP REL & SYS VALUES             33720002
         SRDL  R6,L5               SAVE REL NO IN R7                    33760002
         SLL   R6,L2               MULTIPLY SYS VALUE BY 4              33800002
         L     R8,XLABELP          PICK UP LABEL POINTER                33840002
         LA    R6,RELTAB(R6)       INDEX INTO TABLE OF SYSTEMS          33880002
         MVC   L0(L4,R8),L0(R6)    MOVE SYSTEM NAME INTO LABEL          33920002
         SR    R6,R6               CLEAR R6                             33960002
         SLDL  R6,L5               RETREIVE REL NO.                     34000002
         LTR   R6,R6               IS IT ZERO                           34040002
         BZ    NOREL               BR-YES                               34080002
         CVD   R6,WORK             CONVERT IT                           34120002
         UNPK  L0(L2,R3),WORK+L6(L2) UNPACK IT INTO PRINT BUFF          34160002
         MVZ   L1(L1,R3),L0(R3)    MOVE ON PROPER ZONE                  34200002
         B     RETURN              EXIT                                 34240002
NOREL    MVC   L0(L4,R3),NONE      MOVE IN NONE FOR REL                 34280002
         B     RETURN                                                   34320002

         L     @06,NOEDIT                                          0303 00368000
         LTR   @06,@06                                             0303 00369000
         BZ    @RF00303                                            0303 00370000
*   DO;                                                    /*@Z427VLS*/ 00371000
*    BINWORD = ENUMFAIL;                /*SET NUMBER TO CONVERT      */ 00372000

         LTR   @14,@14                                             0192 00030000
         BZ    @RF00192                                            0192 00031000
*  DO;                                                     /*@G51DGVS*/ 00032000
*   GEN(MVC  PRTLINE(26),BUG48); /*MOVE DEBUG48 INTO PRT LINE*/    0194 00033000
         MVC  PRTLINE(26),BUG48                                         00034000
*   CALL PRINTIT;  /*GO PRINT THE DISCRIPTION OF DUMP        @G51DGVS*/ 00035000
         BAL   @14,PRINTIT                                         0195 00036000
*   SAVE7 = R7;/*SAVE THE ADDRESS OF THE RECORD              @G51DGVS*/ 00037000
         ST    R7,SAVE7                                            0196 00038000
*   SAVERLN = RECLNGTH; /*SAVE THE RECORD LENGTH FOR RETURN  @G51DGVS*/ 00039000

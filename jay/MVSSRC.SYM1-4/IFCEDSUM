*   DO;                                                    /*@G51DGVS*/ 00174000
*   INDEX1 = A;                /*SAVE FOR INDEXING LATTER WHILE CORREC* 00175000
         ST    @14,INDEX1                                          0247 00176000
*   FOUND = YES;                  /*FOUND MATCH SO SET TO EXIT*/   0248 00177000
         OI    FOUND,B'00001000'                                   0248 00178000
*   END;                                                   /*@G51DGVS*/ 00179000
* END;                                                     /*@G51DGVS*/ 00180000
@RF00245 LA    @14,1                                               0250 00181000
         AL    @14,A                                               0250 00182000
         ST    @14,A                                               0250 00183000

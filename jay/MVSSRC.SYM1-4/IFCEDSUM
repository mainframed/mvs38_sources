*  END;                                                    /*@G51DGVS*/ 00055000
* IF CORE ^= YES THEN            /*HAVE WE DONE THE GETMAIN FOR FRMS*/  00056000
@RF00192 TM    CORE,B'01000000'                                    0205 00057000
         BO    @RF00205                                            0205 00058000
*  DO;                                                     /*@G51DGVS*/ 00059000
*   RESPECIFY (R0,R15) RESTRICTED;                         /*@G51DGVS*/ 00060000
*   R0 = 2000;   /*THIS IS MIN FOR SUBPOOL  GETMAIN ANWAY    @G51DGVS*/ 00061000
         LA    R0,2000                                             0208 00062000
*   GEN(GETMAIN EC,LV=(0),A=NEWREC,SP=20);                 /*@G51DGVS*/ 00063000
         GETMAIN EC,LV=(0),A=NEWREC,SP=20                               00064000
*   IF R15 ^= ZERO THEN          /*CONTINUE IF WE GOT THE CORE*/   0210 00065000
         LTR   R15,R15                                             0210 00066000

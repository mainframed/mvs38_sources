*  IF R15 = ZERO THEN             /*DID WE GET A GOOD READ DIRECT*/     00271000
         LTR   R15,R15                                             0270 00272000
         BNZ   @RF00270                                            0270 00273000
*   DO;                           /*THEN CONTINUE PROCESSING @G51DGVS*/ 00274000
*    IF LRTKEY1 = 'A0'X THEN      /*IS IT AN MCH FRAME             D*/  00275000
         CLI   LRTKEY1(R7),X'A0'                                   0272 00276000

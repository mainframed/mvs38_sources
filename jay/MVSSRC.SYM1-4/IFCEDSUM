*  IF  ERROR = ZERO THEN         /*DONT DO IF FAILED ABOVE   @G51DGVS*/ 00086000
@RF00205 LH    @14,ERROR                                           0218 00087000
         LTR   @14,@14                                             0218 00088000
         BNZ   @RF00218                                            0218 00089000
*   DO;                                                    /*@G51DGVS*/ 00090000
*    RESPECIFY (R0,R8,R9) RESTRICTED;                      /*@G51DGVS*/ 00091000
*    R1 = RECLNGTH;              /*SET  UP TO MOVE THE RECORD TO*/ 0221 00092000
         LH    R1,RECLNGTH(,R2)                                    0221 00093000
*    R9 = R1;                    /*THE NEW AREA SO WE CAN READ IN THE*/ 00094000
         LR    R9,R1                                               0222 00095000
*    R0 = NEWREC;                /*FRAME RECORD IF ONE EXIST @G51DGVS*/ 00096000

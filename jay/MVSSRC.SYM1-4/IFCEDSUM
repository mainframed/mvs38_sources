*   RECLNGTH = 384;  /*SET UP THE LENGTH OF THE TABLE        @G51DGVS*/ 00044000
         MVC   RECLNGTH(2,R2),@CH01253                             0199 00045000
*   CALL HEXDUMP;   /*GO HEX DUMP THE FRAME TABLE            @G51DGVS*/ 00046000
         BAL   @14,HEXDUMP                                         0200 00047000
*   FIRSTHEX = ON;  /*SET THE SWITCH SO WE ONLY DO IT ONCE   @G51DGVS*/ 00048000
         OI    FIRSTHEX,B'00010000'                                0201 00049000
*   R7 = SAVE7;     /*RESTORE THE REG TO THE ORGINAL VALUE   @G51DGVS*/ 00050000
         L     R7,SAVE7                                            0202 00051000

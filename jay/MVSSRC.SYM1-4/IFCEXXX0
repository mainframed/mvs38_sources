*  BIN MACRO     BIN MACRO     BIN MACRO     BIN MACRO     BIN MACRO  * 23200002
         SPACE 2                                                        23280002
         MACRO                                                          23360002
         BIN   &LOGITEM,&SKIP$OR                                        23440002
.*                                 &LOGITEM - NAME OF ITEM IN LOG       23520002
.*                                      RECORD DSECT                    23600002
.*                                 &SKIP$OR - NBR OF SPACES TO SKIP ON  23680002
.*                                 MULTIPLE PRINT OR NBR OF SPACES      23760002
.*                                      TO BACK UP FROM COLUMN FOR      23840002
.*                                      START OF PRINTING. THE LETTER   23920002
.*                                      B PRECEEDS THE NUMBER (B5) IF   24000002
.*                                      THE LATTER IS DESIRED. OMISSION 24080002
.*                                      OF THIS OPERAND FORCES OPTION 2 24160002
.*                                      WITH BACKUP SPACING EQUAL TO    24240002
.*                                 THE NUMBER OF BITS IN THE ITEM       24320002
.*                                 DEFINITION (VIA MACRO 'DSGEN').      24400002
.*                            A LIMIT OF 15 BITS IS IMPOSED FOR THE     24480002
.*                                 TOTAL NUMBER OF BITS TO PRINT.       24560002
         GBLC &ITEM(1000)                                               24640002
         GBLA &ITEMNBR,&BITS(1000),&SHIFT(1000),&NRENTS                 24720002
         GBLC  &DSECT                                                   24800002
         LCLB  &B                                                       24880002
         LCLA  &IBITS,&NDX,&ISHIFT,&K                                   24960002
         LCLA  &TEMP,&T                                                 25040002
         LCLC  &S                                                       25120002
.LOOP    ANOP                                                           25200002
&NDX     SETA  &NDX+1                                                   25280002
         AIF   ('&ITEM(&NDX)' EQ '&LOGITEM').FOUND                      25360002
         AIF   (&NDX LT &ITEMNBR).LOOP                                  25440002
         MNOTE *,'YOU BLEW IT'                                          25520002
         ROUTINE DUM,SKIPPER                                            25600002
         MEXIT                                                          25680002
.FOUND   ANOP                                                           25760002
&ISHIFT  SETA  &SHIFT(&NDX)                                             25840002
&IBITS   SETA  &BITS(&NDX)                                              25920002
&T       SETA  &IBITS-1                                                 26000002
&B       SETB  0                                                        26080002
         AIF   ('&SKIP$OR' EQ '').BACKUP                                26160002
&K       SETA  K'&SKIP$OR                                               26240002
&B       SETB  ('&SKIP$OR'(1,1) EQ 'B')                                 26320002
&S       SETC  '&SKIP$OR'(&B+1,&K-&B)                                   26400002
&T       SETA  &S                                                       26480002
.BACKUP  ANOP                                                           26560002
&NRENTS  SETA  &NRENTS+1                                                26640002
&TEMP    SETA  &ISHIFT                                                  26720002
A&SYSNDX DC    AL.12(&LOGITEM-&DSECT),XL.4'&TEMP'                       26800002
         AIF (&IBITS EQ 1 AND &B EQ 0).OUT                              26880002
         ORG   A&SYSNDX                                                 26960002
&B       SETB  1                                                        27040002
         DC    AL.12(&LOGITEM-&DSECT),XL.4'C'                           27120002
         DC    BL.1'&B',AL.7(&T),AL.4(&IBITS),FL.4'&ISHIFT'             27200002
.OUT     ANOP                                                           27280002
         MEND                                                           27360002
         EJECT                                                          27440002
*  HEX MACRO     HEX MACRO     HEX MACRO    HEXN MACRO     HEX MACRO  * 27520002
         SPACE 2                                                        27600002
         MACRO                                                          27680002
         HEX   &LOGITEM,&SKIP$OR,&BYTES                                 27760002
.*                            THIS MACRO FACILITATES GENERATION OF THE  27840002
.*                                 TABLE ENTRIES SPECIFYING HEX TYPE    27920002
.*                                 CONVERSIONS.                         28000002
.*                            &LOGITEM - NAME OF DATA ITEM TO BE        28080002
.*                                 CONVERTED                            28160002
.*                            &SKIP$OR - NUMBER OF SPACES TO SKIP ON    28240002
.*                                 MULTIPLE PRINT OR NBR OF SPACES TO   28320002
.*                                 BACK UP FROM COLUMN FOR START OF     28400002
.*                                 PRINTING. THE LETTER B PRECEEDS THE  28480002
.*                                 NUMBER (B8) IF THE LATTER OPTION IS  28560002
.*                                 DESIRED. OMISSION OF THIS OPERAND    28640002
.*                                 FORCES OPTION 2 WITH BACKUP SPACING  28720002
.*                                 EQUAL TO THE NUMBER OF BYTES SPECI-  28800002
.*                                 FIED BY THE NEXT OPERAND (TWICE THIS 28880002
.*                                 NUMBER IF SUFFIX IS EXPLICITLY OR    28960002
.*                                 IMPLICITLY B).                       29040002
.*                            &BYTES - NUMBER OF TOTAL INPUT BYTES TO   29120002
.*                                 PROCESS. THIS NUMBER IS SUFFIXED     29200002
.*                                 WITH AN L,R, OR B TO STIPULATE       29280002
.*                                 PRINTING OF THE LEFT HALF, RIGHT     29360002
.*                                 HALF, OR BOTH HALVES OF THE BYTE     29440002
.*                                 RESPECTIVELY. OMISSION OF THIS       29520002
.*                                 OPERAND DEFAULTS TO B WITH THE NUM-  29600002
.*                                 BER OF BYTES EQUAL TO THE ITEM LNGTH 29680002
.*                            A NEW ENTRY IS GENERATED ONLY WHEN NECES- 29760002
.*                                 SARY. THIS MACRO MUST BE THE FIRST   29840002
.*                                 MACRO INSTRUCTION ISSUED FOR THE     29920002
.*                                 SUBJECT LINE(S).                     30000002
         GBLC &ITEM(1000),&DSECT                                        30080002
         GBLA &ITEMNBR,&BITS(1000),&SHIFT(1000),&NRENTS                 30160002
         LCLB  &B                                                       30240002
         LCLA  &IBITS,&NDX,&ISHIFT,&K,&T,&CODE,&BYTESP                  30320002
         LCLC  &S,&LETTER                                               30400002
.LOOP    ANOP                                                           30480002
&NDX     SETA  &NDX+1                                                   30560002
         AIF   ('&ITEM(&NDX)' EQ '&LOGITEM').FOUND                      30640002
         AIF   (&NDX LT &ITEMNBR).LOOP                                  30720002
         MNOTE *,'LOG ITEM NOT DEFINED'                                 30800002
         MEXIT                                                          30880002
.FOUND   ANOP                                                           30960002
&ISHIFT  SETA  &SHIFT(&NDX)                                             31040002
&IBITS   SETA  &BITS(&NDX)                                              31120002
&CODE    SETA  1                                                        31200002
&K       SETA  K'&BYTES                                                 31280002
         AIF   (&IBITS-(&IBITS/4)*4 EQ 0).OK                            31360002
         MNOTE *,'BIT LENGTH SPECIFICATION INCONSISTENT WITH CONTROL'   31440002
         MEXIT                                                          31520002
.OK      AIF   ('&BYTES' EQ '').USEDEFN                                 31600002
&LETTER  SETC  '&BYTES'(&K,1)                                           31680002
&S       SETC  '&BYTES'(1,&K-1)                                         31760002
&BYTESP  SETA  &S                                                       31840002
         AIF   ('&LETTER' EQ 'B').BOTH                                  31920002
         AIF   (&ISHIFT EQ 0 OR &ISHIFT EQ 4).OK2                       32000002
.ERR     MNOTE *,'DATA NOT ALLIGNED ON HEX OR BYTE BOUNDARY'            32080002
         MEXIT                                                          32160002
.USEDEFN ANOP                                                           32240002
&BYTESP  SETA  &IBITS/8                                                 32320002
.BOTH    AIF   (&ISHIFT NE 0).ERR                                       32400002
&CODE    SETA  3                                                        32480002
&T       SETA  &BYTESP*2-1                                              32560002
&S       SETC  '&T'                                                     32640002
.OK2     AIF   (&ISHIFT EQ 0).L                                         32720002
&CODE    SETA  2                                                        32800002
.L       ANOP                                                           32880002
&B       SETB  1                                                        32960002
         AIF   ('&SKIP$OR' EQ '').BACKUP                                33040002
&K       SETA  K'&SKIP$OR                                               33120002
&B       SETB  ('&SKIP$OR'(1,1) EQ 'B')                                 33200002
&S       SETC  '&SKIP$OR'(&B+1,&K-&B)                                   33280002
.BACKUP  ANOP                                                           33360002
&NRENTS  SETA  &NRENTS+1                                                33440002
         DC    AL.12(&LOGITEM-&DSECT),XL.4'A'                           33520002
         DC    BL.1'&B',FL.7'&S',FL.4'&BYTESP',FL.4'&CODE'              33600002
.OUT     ANOP                                                           33680002
         MEND                                                           33760002
         MACRO                                                          33840002
         SPECIAL  &ROUTINE,&PARAM,&SPACE=                               33920002
.*                            THIS MACRO GENERATES A SPECIAL ROUTINE    34000002
.*                                 ENTRY TO BE PROCESSED BY THE SUM. M. 34080002
.*                            &ROUTINE - SYMBOL ON SPECIAL ROUTINE EWT. 34160002
.*                            &PARAM - AN 8 BIT BINARY REPRESENTATION   34240002
.*                                 TO OCCUPY THE 'MASK' FIELD BUT TO BE 34320002
.*                                 INTERPRETED AS AN ARGUMENT OF THE    34400002
.*                                 SPECIAL ROUTINE.                     34480002
.*                            &SPACE - THE NUMBER OF 2BYTE COUNTERS TO  34560002
.*                                 ALLOCATE FOR REQUIRED PROCESSING.    34640002
         GBLC  &CSECT                                                   34720002
         GBLA  &COUNTR                                                  34800002
         LCLA  &ADD                                                     34880002
         DC    AL1(128)                                                 34960002
         DC    BL.8'&PARAM'                                             35040002
         DC    AL2(&ROUTINE-&CSECT)                                     35120002
         DC    AL2(&COUNTR)                                             35200002
&ADD     SETA  1                                                        35280002
         AIF   ('&SPACE' EQ '').NORM                                    35360002
&ADD     SETA  &SPACE                                                   35440002
.NORM    ANOP                                                           35520002
&COUNTR  SETA  &COUNTR+&ADD                                             35600002
         MEND                                                           35680002
         MACRO                                                          35760002
         SUM   &LOGITEM,&MASK                                           35840002
.*                            THIS MACRO GENERATES A NORMAL EDIT ENTRY  35920002
.*                                 FOR PROCESSING BY THE SUMMARY MODULE 36000002
.*                            &LOGITEM - NAME OF DATA ITEM TO BE SUMMED 36080002
.*                            &MASK - BYTE MASK FOR BITS TO BE TESTED.  36160002
.*                                 THIS OPERAND SHOULD BE OMITTED FOR   36240002
.*                                 SINGLE BIT ITEMS IN WHICH CASE THE   36320002
.*                                 MASK WILL BE SET ACCORDING TO THE    36400002
.*                                 OFF BYTE BIT DISPLACEMENT OF THE     36480002
.*                                ITEM AS DEFINED BY THE MACRO 'DSGEN'  36560002
         GBLA  &COUNTR,&ITEMNBR,&BITS(1000),&SHIFT(1000)                36640002
         GBLC  &ITEM(1000),&DSECT                                       36720002
         LCLA  &IBITS,&NDX,&ISHIFT,&M,&BITNBR                           36800002
.LOOP    ANOP                                                           36880002
&NDX     SETA  &NDX+1                                                   36960002
         AIF   ('&ITEM(&NDX)' EQ '&LOGITEM').FOUND                      37040002
         AIF   (&NDX LT &ITEMNBR).LOOP                                  37120002
         MNOTE *,'LOGITEM NOT FOUND'                                    37200002
         MEXIT                                                          37280002
.FOUND   ANOP                                                           37360002
         DC    AL1(0)                                                   37440002
&ISHIFT  SETA  &SHIFT(&NDX)                                             37520002
&IBITS   SETA  &BITS(&NDX)                                              37600002
         AIF   ('&MASK' EQ '').FORM                                     37680002
         DC    B'&MASK'                                                 37760002
         AGO   .ITEM                                                    37840002
.FORM    ANOP                                                           37920002
&BITNBR  SETA  &ISHIFT-7                                                38000002
&BITNBR  SETA  0-&BITNBR                                                38080002
&M       SETA  1                                                        38160002
.LOOP2   ANOP                                                           38240002
         AIF   (&BITNBR EQ 0).GOTIT                                     38320002
&M       SETA  &M*2                                                     38400002
&BITNBR  SETA  &BITNBR-1                                                38480002
         AGO   .LOOP2                                                   38560002
.GOTIT   ANOP                                                           38640002
         DC    AL1(&M)                                                  38720002
.ITEM    DC    AL2(&LOGITEM-&DSECT)                                     38800002
         DC    AL2(&COUNTR)                                             38880002
&COUNTR  SETA  &COUNTR+1                                                38960002
         MEND                                                           39040002
         GBLC  &DSECT,&CSECT                                            39120002

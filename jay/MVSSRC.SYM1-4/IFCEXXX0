&SYMBOL  SETC  '&SYSLIST(&OP,1)'                                        13440002
         AIF   ('&SYSLIST(&OP,2)' NE '').OK1                            13520002
         MNOTE *,'NULL BIT COUNT FIELD'                                 13600002
         MEXIT                                                          13680002
.OK1     ANOP                                                           13760002
         AIF   ('&SYMBOL' NE '').OK                                     13840002
         MNOTE 'LOG ITEM SYMBOL NOT PROVIDED'                           13920002
         AGO   .CNT                                                     14000002
.OK      ANOP                                                           14080002
         AIF   ('&SYMBOL'(1,1) EQ '''').CNT                *TEMP*       14160002
&SYMBOL  EQU   *+&BYTE                                                  14240002
&ITEM(&ITEMNBR)  SETC '&SYSLIST(&OP,1)'                                 14320002
&BITS(&ITEMNBR) SETA &SYSLIST(&OP,2)                                    14400002
&SHIFT(&ITEMNBR) SETA &COUNT-&BYTE*8                                    14480002
.CNT     ANOP                                                           14560002
&COUNT   SETA  &COUNT+&SYSLIST(&OP,2)                                   14640002
&BYTE    SETA  &COUNT/8                                                 14720002
         AIF (&OP LT N'&SYSLIST).LOOP                                   14800002
         AIF   (&COUNT NE 32).NOT32                                     14880002
&NAME    DS    4C                                                       14960002
         MEXIT                                                          15040002
.NOT32   AIF   (&COUNT NE 64).ERR                                       15120002
&NAME    DS    8C                                                       15200002
         MEXIT                                                          15280002
.ERR     MNOTE '**BIT COUNT FIELDS DO NOT TOTAL 32 OR 64**'             15360002
         MEND                                                           15440002
         MACRO                                                          15520002
         LINEND                                                         15600002
.*                            THIS MACRO IS USED ONLY BY THE LINE       15680002

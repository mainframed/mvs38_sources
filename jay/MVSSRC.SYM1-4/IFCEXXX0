*  LINE MACRO     LINE MACRO     LINE MACRO     LINE MACRO            * 00320002
         MACRO                                                          00400002
&NAME    LINE  &DATA,&LABEL,&SKIP=                                      00480002
.*                            THIS MACRO ESTABLISHES THE MARGINS OR     00560002
.*                                 COLUMN SETTINGS FOR EACH LINE. IT    00640002
.*                                 GENERATES NECESSARY CARRIAGE CONTROL 00720002
.*                                 ENTRIES. LASTLY IT DETERMINES THE    00800002
.*                                 NUMBER OF ENTRIES ASSOCIATED WITH    00880002
.*                                 THE PREVIOUS LINE AND STORES THAT #  00960002
.*                                 INTO THE APPROPRIATE BYTE.           01040002
.*                                 &DATA(1) - CHARACTER POSITION TO     01120002
.*                                 BEGIN DATA.                          01200002
.*                                                                      01280002
.*                                 &DATA(2) - # OF SPACES BETWEEN       01360002
.*                                 SUCCESSIVE DATA COLUMN STARTING      01440002
.*                                 CHARACTERS.                          01520002
.*                                                                      01600002
.*                                 &LABEL(1) - CHARACTER POSITION       01680002
.*                                 TO BEGIN LABELS.                     01760002
.*                                                                      01840002
.*                                 &LABEL(2) - # OF SPACES BETWEEN      01920002
.*                                 LINE'S LABEL STARTING CHARACTERS.    02000002
.*                                                                      02080002
.*                                 &SKIP= - # OF LINES TO ADVANCE       02160002
.*                                 CARRIAGE TO BEGIN NEXT PRINTED LINE. 02240002
.*                                                                      02320002
.*                                 NEW ENTRIES ARE GENERATED ONLY WHEN  02400002
.*                                 NECESSARY. OMISSION OF ITEMS MEANS   02480002
.*                                 THEY HAVE NOT CHANGED SINCE PREVIOUS 02560002
.*                                 SETTINGS. A LINE MACRO IS REQUIRED   02640002
.*                                 FOR EACH PRINTED LINE. IF NO SETTING 02720002
.*                                 CHANGES ARE REQUIRED ALL OPERANDS    02800002
.*                                 ARE OMITTED IN WHICH CASE ONLY THE   02880002
.*                                 # OF ENTRIES FOR THE PREVIOUS LINE   02960002
.*                                 FIRST MACRO INSTRUCTION ISSUED FOR   03040002
.*                                 THE SUBJECT LINE.                    03120002
         GBLA  &DB,&DT,&LB,&LT,&NRENTS                                  03200002
         GBLA  &LASTCC,&INITIAL                                         03280002
         GBLC  &NRENTSX                                                 03360002
         GBLC  &CARRCON                                                 03440002
         GBLB  &TEST                                                    03520002
         LCLA  &S                                                       03600002
&NAME    DS    0C                                                       03680002
         AIF   (&INITIAL EQ 0).FIRSTLN                                  03760002
         LINEND                                                         03840002
         AGO   .NTFIRST                                                 03920002
.FIRSTLN ANOP                                                           04000002
&NRENTSX SETC  'N&SYSNDX'                                               04080002
&INITIAL SETA  1                                                        04160002
&LASTCC  SETA  1                                                        04240002
N&SYSNDX EQU   *                                                        04320002
.NTFIRST ANOP                                                           04400002
         DC    AL1(0)                                                   04480002
         AIF   ('&DATA(1)' EQ '').CKDTABS                               04560002
         AIF   ('&DATA'(1,1) NE '(').SLABEL                             04640002
         AIF   (&DATA(1) EQ &DB).CKDTABS                                04720002
&DB      SETA  &DATA(1)                                                 04800002
         AGO   .AA                                                      04880002
.CKDTABS AIF   ('&DATA(2)' EQ '').NCDATA                                04960002
         AIF   (&DATA(2) EQ &DT).NCDATA                                 05040002
         AGO   .CC                                                      05120002
.AA      AIF   ('&DATA(2)' EQ '').BB                                    05200002
         AIF   (&DATA(2) EQ &DT).BB                                     05280002
.CC      ANOP                                                           05360002
&DT      SETA  &DATA(2)                                                 05440002
.BB      DC    XL2'FFFE'                                                05520002
         DC    FL1'&DB'                                                 05600002
         DC    FL1'&DT'                                                 05680002
&NRENTS  SETA  &NRENTS+1                                                05760002
.NCDATA  ANOP                                                           05840002
         AIF   ('&LABEL(1)' EQ '').CKLTABS                              05920002
         AIF   ('&LABEL'(1,1) NE '(').SLABEL                            06000002
         AIF   (&LABEL(1) EQ &LB).CKLTABS                               06080002
&LB      SETA  &LABEL(1)                                                06160002
         AGO   .AAA                                                     06240002
.CKLTABS AIF   ('&LABEL(2)' EQ '').NCLABEL                              06320002
         AIF   (&LABEL(2) EQ &LT).NCLABEL                               06400002
         AGO   .CCC                                                     06480002
.AAA     AIF   ('&LABEL(2)' EQ '').BBB                                  06560002
         AIF   (&LABEL(2) EQ &LT).BBB                                   06640002
.CCC     ANOP                                                           06720002
&LT      SETA  &LABEL(2)                                                06800002
.BBB     DC    XL2'FFFF'                                                06880002
         DC    FL1'&LB'                                                 06960002
         DC    FL1'&LT'                                                 07040002
&NRENTS  SETA  &NRENTS+1                                                07120002
.NCLABEL ANOP                                                           07200002
         AIF   ('&SKIP' EQ 'E').EJECT                                   07280002
         AIF   ('&SKIP' EQ '').TESTOFF                                  07360002
.CONT    ANOP                                                           07440002
         AIF   (&TEST EQ 0).AB                                          07520002
         AIF   (&SKIP EQ &LASTCC).TESTOFF                               07600002
&S       SETA  &SKIP                                                    07680002
         ORG   &CARRCON+3                                               07760002
         DC    AL.3(0),AL.2(&S),AL.3(1)                                 07840002
         ORG                                                            07920002
&LASTCC  SETA  &S                                                       08000002
.SLABEL  ANOP                                                           08080002
.TESTOFF ANOP                                                           08160002
&TEST    SETB  0                                                        08240002
         AGO   .DD                                                      08320002
.EJECT   DC    XL2'FFFB'                                                08400002
&NRENTS  SETA  &NRENTS+1                                                08480002
         DC    X'8B'              SKIP TO CHANNEL 1                     08560002
         DC    AL.3(0),AL.2(&LASTCC),AL.3(1)                            08640002
&TEST    SETB  1                                                        08720002
         AGO   .DD                                                      08800002
.AB      AIF   (&SKIP EQ &LASTCC).NOCHANG                               08880002
A&SYSNDX DC    XL2'FFFB'                                                08960002
         DC    AL.3(0),AL.2(&SKIP),AL.3(1)                              09040002
         DC    AL.3(0),AL.2(&SKIP),AL.3(1)                              09120002
&LASTCC  SETA  &SKIP                                                    09200002
&TEST    SETB  1                                                        09280002

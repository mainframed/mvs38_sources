*  LINE MACRO     LINE MACRO     LINE MACRO     LINE MACRO            * 00375002
         MACRO                                                          00450002
&NAME    LINE  &DATA,&LABEL,&SKIP=                                      00525002
.*                            THIS MACRO ESTABLISHES THE MARGINS OR     00600002
.*                                 COLUMN SETTINGS FOR EACH LINE. IT    00675002
.*                                 GENERATES NECESSARY CARRIAGE CONTROL 00750002
.*                                 ENTRIES. LASTLY IT DETERMINES THE    00825002
.*                                 NUMBER OF ENTRIES ASSOCIATED WITH    00900002
.*                                 THE PREVIOUS LINE AND STORES THAT #  00975002
.*                                 INTO THE APPROPRIATE BYTE.           01050002
.*                                 &DATA(1) - CHARACTER POSITION TO     01125002
.*                                 BEGIN DATA.                          01200002
.*                                                                      01275002
.*                                 &DATA(2) - # OF SPACES BETWEEN       01350002
.*                                 SUCCESSIVE DATA COLUMN STARTING      01425002
.*                                 CHARACTERS.                          01500002
.*                                                                      01575002
.*                                 &LABEL(1) - CHARACTER POSITION       01650002
.*                                 TO BEGIN LABELS.                     01725002
.*                                                                      01800002
.*                                 &LABEL(2) - # OF SPACES BETWEEN      01875002
.*                                 LINE'S LABEL STARTING CHARACTERS.    01950002
.*                                                                      02025002
.*                                 &SKIP= - # OF LINES TO ADVANCE       02100002
.*                                 CARRIAGE TO BEGIN NEXT PRINTED LINE. 02175002
.*                                                                      02250002
.*                                 NEW ENTRIES ARE GENERATED ONLY WHEN  02325002
.*                                 NECESSARY. OMISSION OF ITEMS MEANS   02400002
.*                                 THEY HAVE NOT CHANGED SINCE PREVIOUS 02475002
.*                                 SETTINGS. A LINE MACRO IS REQUIRED   02550002
.*                                 FOR EACH PRINTED LINE. IF NO SETTING 02625002
.*                                 CHANGES ARE REQUIRED ALL OPERANDS    02700002
.*                                 ARE OMITTED IN WHICH CASE ONLY THE   02775002
.*                                 # OF ENTRIES FOR THE PREVIOUS LINE   02850002
.*                                 FIRST MACRO INSTRUCTION ISSUED FOR   02925002
.*                                 THE SUBJECT LINE.                    03000002
         GBLA  &DB,&DT,&LB,&LT,&NRENTS                                  03075002
         GBLA  &LASTCC,&INITIAL                                         03150002
         GBLC  &NRENTSX                                                 03225002
         GBLC  &CARRCON                                                 03300002
         GBLB  &TEST                                                    03375002
         LCLA  &S                                                       03450002
&NAME    DS    0C                                                       03525002
         AIF   (&INITIAL EQ 0).FIRSTLN                                  03600002
         LINEND                                                         03675002
         AGO   .NTFIRST                                                 03750002
.FIRSTLN ANOP                                                           03825002
&NRENTSX SETC  'N&SYSNDX'                                               03900002
&INITIAL SETA  1                                                        03975002
&LASTCC  SETA  1                                                        04050002
N&SYSNDX EQU   *                                                        04125002
.NTFIRST ANOP                                                           04200002
         DC    AL1(0)                                                   04275002
         AIF   ('&DATA(1)' EQ '').CKDTABS                               04350002
         AIF   ('&DATA'(1,1) NE '(').SLABEL                             04425002
         AIF   (&DATA(1) EQ &DB).CKDTABS                                04500002
&DB      SETA  &DATA(1)                                                 04575002
         AGO   .AA                                                      04650002
.CKDTABS AIF   ('&DATA(2)' EQ '').NCDATA                                04725002
         AIF   (&DATA(2) EQ &DT).NCDATA                                 04800002
         AGO   .CC                                                      04875002
.AA      AIF   ('&DATA(2)' EQ '').BB                                    04950002
         AIF   (&DATA(2) EQ &DT).BB                                     05025002
.CC      ANOP                                                           05100002
&DT      SETA  &DATA(2)                                                 05175002
.BB      DC    XL2'FFFE'                                                05250002
         DC    FL1'&DB'                                                 05325002
         DC    FL1'&DT'                                                 05400002
&NRENTS  SETA  &NRENTS+1                                                05475002
.NCDATA  ANOP                                                           05550002
         AIF   ('&LABEL(1)' EQ '').CKLTABS                              05625002
         AIF   ('&LABEL'(1,1) NE '(').SLABEL                            05700002
         AIF   (&LABEL(1) EQ &LB).CKLTABS                               05775002
&LB      SETA  &LABEL(1)                                                05850002
         AGO   .AAA                                                     05925002
.CKLTABS AIF   ('&LABEL(2)' EQ '').NCLABEL                              06000002
         AIF   (&LABEL(2) EQ &LT).NCLABEL                               06075002
         AGO   .CCC                                                     06150002
.AAA     AIF   ('&LABEL(2)' EQ '').BBB                                  06225002
         AIF   (&LABEL(2) EQ &LT).BBB                                   06300002
.CCC     ANOP                                                           06375002
&LT      SETA  &LABEL(2)                                                06450002
.BBB     DC    XL2'FFFF'                                                06525002
         DC    FL1'&LB'                                                 06600002
         DC    FL1'&LT'                                                 06675002
&NRENTS  SETA  &NRENTS+1                                                06750002
.NCLABEL ANOP                                                           06825002
         AIF   ('&SKIP' EQ 'E').EJECT                                   06900002
         AIF   ('&SKIP' EQ '').TESTOFF                                  06975002
.CONT    ANOP                                                           07050002
         AIF   (&TEST EQ 0).AB                                          07125002
         AIF   (&SKIP EQ &LASTCC).TESTOFF                               07200002
&S       SETA  &SKIP                                                    07275002
         ORG   &CARRCON+3                                               07350002
         DC    AL.3(0),AL.2(&S),AL.3(1)                                 07425002
         ORG                                                            07500002
&LASTCC  SETA  &S                                                       07575002
.SLABEL  ANOP                                                           07650002
.TESTOFF ANOP                                                           07725002
&TEST    SETB  0                                                        07800002
         AGO   .DD                                                      07875002
.EJECT   DC    XL2'FFFB'                                                07950002
&NRENTS  SETA  &NRENTS+1                                                08025002
         DC    X'8B'              SKIP TO CHANNEL 1                     08100002
         DC    AL.3(0),AL.2(&LASTCC),AL.3(1)                            08175002
&TEST    SETB  1                                                        08250002
         AGO   .DD                                                      08325002
.AB      AIF   (&SKIP EQ &LASTCC).NOCHANG                               08400002
A&SYSNDX DC    XL2'FFFB'                                                08475002

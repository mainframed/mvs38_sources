*  BIN MACRO     BIN MACRO     BIN MACRO     BIN MACRO     BIN MACRO  * 21825002
         SPACE 2                                                        21900002
         MACRO                                                          21975002
         BIN   &LOGITEM,&SKIP$OR                                        22050002
.*                                 &LOGITEM - NAME OF ITEM IN LOG       22125002
.*                                      RECORD DSECT                    22200002
.*                                 &SKIP$OR - NBR OF SPACES TO SKIP ON  22275002
.*                                 MULTIPLE PRINT OR NBR OF SPACES      22350002
.*                                      TO BACK UP FROM COLUMN FOR      22425002
.*                                      START OF PRINTING. THE LETTER   22500002
.*                                      B PRECEEDS THE NUMBER (B5) IF   22575002
.*                                      THE LATTER IS DESIRED. OMISSION 22650002
.*                                      OF THIS OPERAND FORCES OPTION 2 22725002
.*                                      WITH BACKUP SPACING EQUAL TO    22800002
.*                                 THE NUMBER OF BITS IN THE ITEM       22875002
.*                                 DEFINITION (VIA MACRO 'DSGEN').      22950002
.*                            A LIMIT OF 15 BITS IS IMPOSED FOR THE     23025002
.*                                 TOTAL NUMBER OF BITS TO PRINT.       23100002
         GBLC &ITEM(1000)                                               23175002
         GBLA &ITEMNBR,&BITS(1000),&SHIFT(1000),&NRENTS                 23250002
         GBLC  &DSECT                                                   23325002
         LCLB  &B                                                       23400002
         LCLA  &IBITS,&NDX,&ISHIFT,&K                                   23475002
         LCLA  &TEMP,&T                                                 23550002
         LCLC  &S                                                       23625002
.LOOP    ANOP                                                           23700002
&NDX     SETA  &NDX+1                                                   23775002
         AIF   ('&ITEM(&NDX)' EQ '&LOGITEM').FOUND                      23850002
         AIF   (&NDX LT &ITEMNBR).LOOP                                  23925002
         MNOTE *,'YOU BLEW IT'                                          24000002
         ROUTINE DUM,SKIPPER                                            24075002
         MEXIT                                                          24150002
.FOUND   ANOP                                                           24225002
&ISHIFT  SETA  &SHIFT(&NDX)                                             24300002
&IBITS   SETA  &BITS(&NDX)                                              24375002
&T       SETA  &IBITS-1                                                 24450002
&B       SETB  0                                                        24525002
         AIF   ('&SKIP$OR' EQ '').BACKUP                                24600002
&K       SETA  K'&SKIP$OR                                               24675002
&B       SETB  ('&SKIP$OR'(1,1) EQ 'B')                                 24750002
&S       SETC  '&SKIP$OR'(&B+1,&K-&B)                                   24825002
&T       SETA  &S                                                       24900002
.BACKUP  ANOP                                                           24975002
&NRENTS  SETA  &NRENTS+1                                                25050002
&TEMP    SETA  &ISHIFT                                                  25125002
A&SYSNDX DC    AL.12(&LOGITEM-&DSECT),XL.4'&TEMP'                       25200002
         AIF (&IBITS EQ 1 AND &B EQ 0).OUT                              25275002
         ORG   A&SYSNDX                                                 25350002
&B       SETB  1                                                        25425002
         DC    AL.12(&LOGITEM-&DSECT),XL.4'C'                           25500002
         DC    BL.1'&B',AL.7(&T),AL.4(&IBITS),FL.4'&ISHIFT'             25575002
.OUT     ANOP                                                           25650002
         MEND                                                           25725002
         EJECT                                                          25800002
*  HEX MACRO     HEX MACRO     HEX MACRO    HEXN MACRO     HEX MACRO  * 25875002
         SPACE 2                                                        25950002
         MACRO                                                          26025002
         HEX   &LOGITEM,&SKIP$OR,&BYTES                                 26100002
.*                            THIS MACRO FACILITATES GENERATION OF THE  26175002
.*                                 TABLE ENTRIES SPECIFYING HEX TYPE    26250002
.*                                 CONVERSIONS.                         26325002
.*                            &LOGITEM - NAME OF DATA ITEM TO BE        26400002
.*                                 CONVERTED                            26475002
.*                            &SKIP$OR - NUMBER OF SPACES TO SKIP ON    26550002
.*                                 MULTIPLE PRINT OR NBR OF SPACES TO   26625002
.*                                 BACK UP FROM COLUMN FOR START OF     26700002
.*                                 PRINTING. THE LETTER B PRECEEDS THE  26775002
.*                                 NUMBER (B8) IF THE LATTER OPTION IS  26850002
.*                                 DESIRED. OMISSION OF THIS OPERAND    26925002
.*                                 FORCES OPTION 2 WITH BACKUP SPACING  27000002
.*                                 EQUAL TO THE NUMBER OF BYTES SPECI-  27075002
.*                                 FIED BY THE NEXT OPERAND (TWICE THIS 27150002
.*                                 NUMBER IF SUFFIX IS EXPLICITLY OR    27225002
.*                                 IMPLICITLY B).                       27300002
.*                            &BYTES - NUMBER OF TOTAL INPUT BYTES TO   27375002
.*                                 PROCESS. THIS NUMBER IS SUFFIXED     27450002
.*                                 WITH AN L,R, OR B TO STIPULATE       27525002
.*                                 PRINTING OF THE LEFT HALF, RIGHT     27600002
.*                                 HALF, OR BOTH HALVES OF THE BYTE     27675002
.*                                 RESPECTIVELY. OMISSION OF THIS       27750002
.*                                 OPERAND DEFAULTS TO B WITH THE NUM-  27825002
.*                                 BER OF BYTES EQUAL TO THE ITEM LNGTH 27900002
.*                            A NEW ENTRY IS GENERATED ONLY WHEN NECES- 27975002
.*                                 SARY. THIS MACRO MUST BE THE FIRST   28050002
.*                                 MACRO INSTRUCTION ISSUED FOR THE     28125002
.*                                 SUBJECT LINE(S).                     28200002
         GBLC &ITEM(1000),&DSECT                                        28275002
         GBLA &ITEMNBR,&BITS(1000),&SHIFT(1000),&NRENTS                 28350002
         LCLB  &B                                                       28425002
         LCLA  &IBITS,&NDX,&ISHIFT,&K,&T,&CODE,&BYTESP                  28500002
         LCLC  &S,&LETTER                                               28575002
.LOOP    ANOP                                                           28650002
&NDX     SETA  &NDX+1                                                   28725002
         AIF   ('&ITEM(&NDX)' EQ '&LOGITEM').FOUND                      28800002
         AIF   (&NDX LT &ITEMNBR).LOOP                                  28875002
         MNOTE *,'LOG ITEM NOT DEFINED'                                 28950002
         MEXIT                                                          29025002
.FOUND   ANOP                                                           29100002
&ISHIFT  SETA  &SHIFT(&NDX)                                             29175002
&IBITS   SETA  &BITS(&NDX)                                              29250002
&CODE    SETA  1                                                        29325002
&K       SETA  K'&BYTES                                                 29400002
         AIF   (&IBITS-(&IBITS/4)*4 EQ 0).OK                            29475002
         MNOTE *,'BIT LENGTH SPECIFICATION INCONSISTENT WITH CONTROL'   29550002
         MEXIT                                                          29625002
.OK      AIF   ('&BYTES' EQ '').USEDEFN                                 29700002
&LETTER  SETC  '&BYTES'(&K,1)                                           29775002
&S       SETC  '&BYTES'(1,&K-1)                                         29850002
&BYTESP  SETA  &S                                                       29925002
         AIF   ('&LETTER' EQ 'B').BOTH                                  30000002
         AIF   (&ISHIFT EQ 0 OR &ISHIFT EQ 4).OK2                       30075002
.ERR     MNOTE *,'DATA NOT ALLIGNED ON HEX OR BYTE BOUNDARY'            30150002
         MEXIT                                                          30225002
.USEDEFN ANOP                                                           30300002
&BYTESP  SETA  &IBITS/8                                                 30375002
.BOTH    AIF   (&ISHIFT NE 0).ERR                                       30450002
&CODE    SETA  3                                                        30525002
&T       SETA  &BYTESP*2-1                                              30600002
&S       SETC  '&T'                                                     30675002
.OK2     AIF   (&ISHIFT EQ 0).L                                         30750002
&CODE    SETA  2                                                        30825002
.L       ANOP                                                           30900002
&B       SETB  1                                                        30975002
         AIF   ('&SKIP$OR' EQ '').BACKUP                                31050002
&K       SETA  K'&SKIP$OR                                               31125002
&B       SETB  ('&SKIP$OR'(1,1) EQ 'B')                                 31200002
&S       SETC  '&SKIP$OR'(&B+1,&K-&B)                                   31275002
.BACKUP  ANOP                                                           31350002
&NRENTS  SETA  &NRENTS+1                                                31425002
         DC    AL.12(&LOGITEM-&DSECT),XL.4'A'                           31500002
         DC    BL.1'&B',FL.7'&S',FL.4'&BYTESP',FL.4'&CODE'              31575002
.OUT     ANOP                                                           31650002
         MEND                                                           31725002
         MACRO                                                          31800002
         SPECIAL  &ROUTINE,&PARAM,&SPACE=                               31875002
.*                            THIS MACRO GENERATES A SPECIAL ROUTINE    31950002
.*                                 ENTRY TO BE PROCESSED BY THE SUM. M. 32025002
.*                            &ROUTINE - SYMBOL ON SPECIAL ROUTINE EWT. 32100002
.*                            &PARAM - AN 8 BIT BINARY REPRESENTATION   32175002
.*                                 TO OCCUPY THE 'MASK' FIELD BUT TO BE 32250002
.*                                 INTERPRETED AS AN ARGUMENT OF THE    32325002
.*                                 SPECIAL ROUTINE.                     32400002
.*                            &SPACE - THE NUMBER OF 2BYTE COUNTERS TO  32475002
.*                                 ALLOCATE FOR REQUIRED PROCESSING.    32550002
         GBLC  &CSECT                                                   32625002
         GBLA  &COUNTR                                                  32700002
         LCLA  &ADD                                                     32775002
         DC    AL1(128)                                                 32850002
         DC    BL.8'&PARAM'                                             32925002
         DC    AL2(&ROUTINE-&CSECT)                                     33000002
         DC    AL2(&COUNTR)                                             33075002
&ADD     SETA  1                                                        33150002
         AIF   ('&SPACE' EQ '').NORM                                    33225002
&ADD     SETA  &SPACE                                                   33300002
.NORM    ANOP                                                           33375002
&COUNTR  SETA  &COUNTR+&ADD                                             33450002
         MEND                                                           33525002
         MACRO                                                          33600002
         SUM   &LOGITEM,&MASK                                           33675002
.*                            THIS MACRO GENERATES A NORMAL EDIT ENTRY  33750002
.*                                 FOR PROCESSING BY THE SUMMARY MODULE 33825002
.*                            &LOGITEM - NAME OF DATA ITEM TO BE SUMMED 33900002
.*                            &MASK - BYTE MASK FOR BITS TO BE TESTED.  33975002
.*                                 THIS OPERAND SHOULD BE OMITTED FOR   34050002
.*                                 SINGLE BIT ITEMS IN WHICH CASE THE   34125002
.*                                 MASK WILL BE SET ACCORDING TO THE    34200002
.*                                 OFF BYTE BIT DISPLACEMENT OF THE     34275002
.*                                ITEM AS DEFINED BY THE MACRO 'DSGEN'  34350002
         GBLA  &COUNTR,&ITEMNBR,&BITS(1000),&SHIFT(1000)                34425002
         GBLC  &ITEM(1000),&DSECT                                       34500002
         LCLA  &IBITS,&NDX,&ISHIFT,&M,&BITNBR                           34575002
.LOOP    ANOP                                                           34650002
&NDX     SETA  &NDX+1                                                   34725002
         AIF   ('&ITEM(&NDX)' EQ '&LOGITEM').FOUND                      34800002
         AIF   (&NDX LT &ITEMNBR).LOOP                                  34875002
         MNOTE *,'LOGITEM NOT FOUND'                                    34950002
         MEXIT                                                          35025002
.FOUND   ANOP                                                           35100002
         DC    AL1(0)                                                   35175002
&ISHIFT  SETA  &SHIFT(&NDX)                                             35250002
&IBITS   SETA  &BITS(&NDX)                                              35325002
         AIF   ('&MASK' EQ '').FORM                                     35400002
         DC    B'&MASK'                                                 35475002
         AGO   .ITEM                                                    35550002
.FORM    ANOP                                                           35625002
&BITNBR  SETA  &ISHIFT-7                                                35700002
&BITNBR  SETA  0-&BITNBR                                                35775002
&M       SETA  1                                                        35850002
.LOOP2   ANOP                                                           35925002
         AIF   (&BITNBR EQ 0).GOTIT                                     36000002
&M       SETA  &M*2                                                     36075002
&BITNBR  SETA  &BITNBR-1                                                36150002
         AGO   .LOOP2                                                   36225002
.GOTIT   ANOP                                                           36300002
         DC    AL1(&M)                                                  36375002
.ITEM    DC    AL2(&LOGITEM-&DSECT)                                     36450002
         DC    AL2(&COUNTR)                                             36525002
&COUNTR  SETA  &COUNTR+1                                                36600002
         MEND                                                           36675002
         GBLC  &DSECT,&CSECT                                            36750002

&K       SETA  2                                                        18300002
.NORM    ANOP                                                           18375002
&START   SETA  2                                                        18450002
         AIF (&OP LT N'&SYSLIST).NOTLAST                                18525002
&MARGIN  SETC  '$'                                                      18600002
.NOTLAST ANOP                                                           18675002
&NRCHAR  SETA  8                                                        18750002
&TACK    SETC  ''                                                       18825002
.LOOPIN  ANOP                                                           18900002
&REMAIN  SETA  &K-&START                                                18975002
         AIF (&REMAIN GE 8).MORE                                        19050002
&TACK    SETC  '&MARGIN'                                                19125002
&NRCHAR  SETA  &REMAIN                                                  19200002
.MORE    ANOP                                                           19275002
&TEXT    SETC  ''                                                       19350002
         AIF (&REMAIN EQ 0).ENDTEXT                                     19425002
&TEXT    SETC  '&SYSLIST(&OP)'(&START,&NRCHAR)                          19500002
.ENDTEXT ANOP                                                           19575002
&LABELNR SETA  &LABELNR+1                                               19650002
&LABELS(&LABELNR) SETC '&TEXT'.'&TACK'                                  19725002
&START   SETA  &START+8                                                 19800002
         AIF ('&TACK' EQ '').LOOPIN                                     19875002
         AIF ('&TACK' EQ '*').LOOPOUT                                   19950002
         MEND                                                           20025002
         SPACE 2                                                        20100002
         MACRO                                                          20175002
         LSTART                                                         20250002
.*                            THIS MACRO IS ISSUED WHEN THE LABEL AREA  20325002
.*                                 OF THE TABLE IS TO BEGIN.            20400002
         GBLC &LABELS(2000)                                             20475002

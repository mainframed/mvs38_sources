               &X25,&X26,&X27,&X28,&X29,&X30,&X31,&X32                  11250002
.*                            THIS MACRO GENERATES ONE DSECT WORD       11325002
.*                                 FORMATTED FOR THE SUBJECT LOG RECORD 11400002
.*                                 EACH OPERAND HAS TWO SUB LISTS. THE  11475002
.*                                 FIRST IS THE NAME OF THE LOG ITEM    11550002
.*                                 WHILE THE SECOND IS THE # OF BITS TO 11625002
.*                                 ALLOCATE FOR THIS ITEM. ITEMS MUST   11700002
.*                                 BE DEFINED CONTIGUOUSLY WITHOUT GAPS 11775002
.*                                 UNUSED BIT GROUPS MUST BE GIVEN      11850002
.*                                 DUMMY NAMES. THE NAMES PROVIDED FOR  11925002
.*                                 THE LOG ITEMS MAY THEN BE USED IN    12000002
.*                                 THE CONVERSION MACRO INSTRUCTIONS.   12075002
         GBLC &ITEM(1000)                                               12150002
         GBLA &ITEMNBR,&BITS(1000),&SHIFT(1000)                         12225002
         LCLC  &SYMBOL                                                  12300002
         LCLA  &OP,&COUNT,&BYTE                                         12375002
.LOOP    ANOP                                                           12450002
&OP      SETA  &OP+1                                                    12525002
&ITEMNBR SETA  &ITEMNBR+1                                               12600002
&SYMBOL  SETC  '&SYSLIST(&OP,1)'                                        12675002
         AIF   ('&SYSLIST(&OP,2)' NE '').OK1                            12750002
         MNOTE *,'NULL BIT COUNT FIELD'                                 12825002
         MEXIT                                                          12900002
.OK1     ANOP                                                           12975002
         AIF   ('&SYMBOL' NE '').OK                                     13050002
         MNOTE 'LOG ITEM SYMBOL NOT PROVIDED'                           13125002
         AGO   .CNT                                                     13200002
.OK      ANOP                                                           13275002
         AIF   ('&SYMBOL'(1,1) EQ '''').CNT                *TEMP*       13350002
&SYMBOL  EQU   *+&BYTE                                                  13425002
&ITEM(&ITEMNBR)  SETC '&SYSLIST(&OP,1)'                                 13500002
&BITS(&ITEMNBR) SETA &SYSLIST(&OP,2)                                    13575002
&SHIFT(&ITEMNBR) SETA &COUNT-&BYTE*8                                    13650002
.CNT     ANOP                                                           13725002
&COUNT   SETA  &COUNT+&SYSLIST(&OP,2)                                   13800002
&BYTE    SETA  &COUNT/8                                                 13875002
         AIF (&OP LT N'&SYSLIST).LOOP                                   13950002
         AIF   (&COUNT NE 32).NOT32                                     14025002
&NAME    DS    4C                                                       14100002
         MEXIT                                                          14175002
.NOT32   AIF   (&COUNT NE 64).ERR                                       14250002
&NAME    DS    8C                                                       14325002
         MEXIT                                                          14400002
.ERR     MNOTE '**BIT COUNT FIELDS DO NOT TOTAL 32 OR 64**'             14475002
         MEND                                                           14550002
         MACRO                                                          14625002
         LINEND                                                         14700002
.*                            THIS MACRO IS USED ONLY BY THE LINE       14775002
.*                                 MACRO TO SET THE NUMBER OF ENTRIES   14850002
.*                                 ASSOCIATED WITH EACH LINE.           14925002
         GBLA  &NRENTS                                                  15000002

*    DO INDEX3 = INDEX1 TO I BY 1;                         /*@Z427VLS*/ 00438000
         LR    INDEX3,INDEX1                                       0214 00439000
         B     @DE00214                                            0214 00440000
@DL00214 DS    0H                                                  0215 00441000
*      VALID = OFF;                                        /*@Z427VLS*/ 00442000
         NI    VALID,B'01111111'                                   0215 00443000
*      /*                                                    @Z427VLS*/ 00444000
*      /* CHECK TO SEE IF THE FIELD IS A VALID DECIMAL NUMBER.  IF   */ 00445000
*      /* YES, TERMINATE THE SEARCH AND REPLACE THE CHARACTER WITH   */ 00446000
*      /* HEXADECIMAL EQUIVALENT.  IF NO, PRINT AN ERROR MESSAGE FOR */ 00447000
*      /* THIS OPERAND.                                      @Z427VLS*/ 00448000
*      /*                                                    @Z427VLS*/ 00449000
*      DO J = 10 TO 1 BY -1 WHILE (VALID = OFF);           /*@Z427VLS*/ 00450000
         LA    J,10                                                0216 00451000
@DL00216 TM    VALID,B'10000000'                                   0216 00452000
         BNZ   @DC00216                                            0216 00453000
*        IF PARMFLD(INDEX3) = NUMTAB(J) THEN                       0217 00454000

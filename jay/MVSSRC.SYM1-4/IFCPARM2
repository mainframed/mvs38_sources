*/********************************************************************/ 00677000
*CHARPACK: PROC OPTIONS(SAVE(1,14));                       /*@Z427VLS*/ 00678000
CHARPACK ST    @14,12(,@13)                                        0271 00679000
         ST    @01,24(,@13)                                        0271 00680000
*  SLL (HOLDAREA(1:2),FOUR);                               /*@Z427VLS*/ 00681000
         SLR   @08,@08                                             0272 00682000
         ICM   @08,3,HOLDAREA                                      0272 00683000
         SLL   @08,4                                               0272 00684000
         STCM  @08,3,HOLDAREA                                      0272 00685000
*  SRL (HOLDAREA(2:2),FOUR);                               /*@Z427VLS*/ 00686000
         SLR   @08,@08                                             0273 00687000
         IC    @08,HOLDAREA+1                                      0273 00688000
         SRL   @08,4                                               0273 00689000
         STC   @08,HOLDAREA+1                                      0273 00690000
*  INDEX2 = HOLDAREA(1) + 1;                               /*@Z427VLS*/ 00691000
         LA    @08,1                                               0274 00692000
         SLR   @07,@07                                             0274 00693000
         IC    @07,HOLDAREA                                        0274 00694000
         ALR   @07,@08                                             0274 00695000
         ST    @07,INDEX2                                          0274 00696000
*  KEYWORD(2*J-1) = NUMTAB(INDEX2);                        /*@Z427VLS*/ 00697000
         LR    @03,J                                               0275 00698000
         ALR   @03,@03                                             0275 00699000
         LA    @01,KEYWORD-2(@03)                                  0275 00700000
         LA    @15,NUMTAB-1(@07)                                   0275 00701000
         MVC   0(1,@01),0(@15)                                     0275 00702000
*  INDEX2 = HOLDAREA(2) + 1;                               /*@Z427VLS*/ 00703000
         SLR   @07,@07                                             0276 00704000
         IC    @07,HOLDAREA+1                                      0276 00705000
         ALR   @08,@07                                             0276 00706000
         ST    @08,INDEX2                                          0276 00707000
*  KEYWORD(2*J) = NUMTAB(INDEX2);                          /*@Z427VLS*/ 00708000
         LA    @07,KEYWORD-1(@03)                                  0277 00709000
         LA    @03,NUMTAB-1(@08)                                   0277 00710000
         MVC   0(1,@07),0(@03)                                     0277 00711000
*END;                                                      /*@Z427VLS*/ 00712000
@EL00012 DS    0H                                                  0278 00713000
@EF00012 DS    0H                                                  0278 00714000
@ER00012 L     @14,12(,@13)                                        0278 00715000
         L     @01,24(,@13)                                        0278 00716000
         BR    @14                                                 0278 00717000
*/********************************************************************/ 00718000
*/*                                                          @Z427VLS*/ 00719000
*/*  CONVERT THE HEXADECIMAL CHARACTERS TO PRINT ABLE  CHARACTERS.   */ 00720000
*/*  THE VALUE TO CONVERT IS PLACED IN HOLDAREA(2).  THE ROUTINE     */ 00721000
*/*  CHARPACK IS CALLED TO CONVERT IT, FOLLOWED BY WORDPAK1 TO       */ 00722000
*/*  OUTPUT TO THE PRINT LINE WITH NO LEADING COMMA.         @Z427VLS*/ 00723000
*/*                                                          @Z427VLS*/ 00724000
*/********************************************************************/ 00725000
*WORDCON1: PROC (WORDLEN2,WORDSTR2) OPTIONS(SAVE(1,14));   /*@Z427VLS*/ 00726000
WORDCON1 ST    @14,@SA00013                                        0279 00727000
         ST    @01,@SA00013+4                                      0279 00728000
         MVC   @PC00013(8),0(@01)                                  0279 00729000
*    DCL WORDLEN2 FIXED(31);                               /*@Z427VLS*/ 00730000
*    DCL WORDSTR2 CHAR(*);                                 /*@Z427VLS*/ 00731000
*    DO I = 1 TO WORDLEN2 BY 1;                            /*@Z427VLS*/ 00732000
         LA    @08,1                                               0282 00733000
         ST    @08,I                                               0282 00734000
         B     @DE00282                                            0282 00735000
@DL00282 DS    0H                                                  0283 00736000
*      HOLDAREA(1) = HEXZERO;                              /*@Z427VLS*/ 00737000
         MVI   HOLDAREA,X'00'                                      0283 00738000
*      HOLDAREA(2) = WORDSTR2(I);                          /*@Z427VLS*/ 00739000
         L     @07,@PC00013+4                                      0284 00740000
         ALR   @07,@08                                             0284 00741000
         BCTR  @07,0                                               0284 00742000
         MVC   HOLDAREA+1(1),WORDSTR2(@07)                         0284 00743000
*      SLL (HOLDAREA(1:2),FOUR);                           /*@Z427VLS*/ 00744000
         SLR   @07,@07                                             0285 00745000
         ICM   @07,3,HOLDAREA                                      0285 00746000
         SLL   @07,4                                               0285 00747000
         STCM  @07,3,HOLDAREA                                      0285 00748000
*      SRL (HOLDAREA(2:2),FOUR);                           /*@Z427VLS*/ 00749000
         SLR   @07,@07                                             0286 00750000
         IC    @07,HOLDAREA+1                                      0286 00751000
         SRL   @07,4                                               0286 00752000
         STC   @07,HOLDAREA+1                                      0286 00753000
*      INDEX2 = HOLDAREA(1) + 1;                           /*@Z427VLS*/ 00754000
         LA    @07,1                                               0287 00755000
         SLR   @03,@03                                             0287 00756000
         IC    @03,HOLDAREA                                        0287 00757000
         ALR   @03,@07                                             0287 00758000
         ST    @03,INDEX2                                          0287 00759000
*      KEYWORD(2*I-1) = NUMTAB(INDEX2);                    /*@Z427VLS*/ 00760000
         LR    @15,@08                                             0288 00761000
         ALR   @15,@15                                             0288 00762000

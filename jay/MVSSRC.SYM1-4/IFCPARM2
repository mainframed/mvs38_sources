         ALR   @07,@08                                             0300 00818000
         BCTR  @07,0                                               0300 00819000
         MVC   HOLDAREA+1(1),WORDSTR3(@07)                         0300 00820000
*      SLL (HOLDAREA(1:2),FOUR);                           /*@Z427VLS*/ 00821000
         SLR   @07,@07                                             0301 00822000

*/********************************************************************/ 00493000
*/*                                                          @Z427VLS*/ 00494000
*/*  PACK A FIELD OF A GIVEN LENGTH INTO THE VARIABLE HOLD.  THE     */ 00495000
*/*  INPUT IS ASSUMED TO BE HEX CHARACTERS, WITH A LEADING ZERO FOR  */ 00496000
*/*  EACH TWO DIGITS.                                        @Z427VLS*/ 00497000
*/********************************************************************/ 00498000
*HEXPACK: PROC (HEXLEN) OPTIONS(SAVE(1,14));               /*@Z427VLS*/ 00499000
HEXPACK  ST    @14,12(,@13)                                        0229 00500000
         ST    @01,24(,@13)                                        0229 00501000
         MVC   @PC00008(4),0(@01)                                  0229 00502000
*   DCL HEXLEN FIXED(31);                                  /*@Z427VLS*/ 00503000
*   INDEX3 = HEXLEN / 2;                                   /*@Z427VLS*/ 00504000
         L     @08,@PC00008                                        0231 00505000
         L     @00,HEXLEN(,@08)                                    0231 00506000
         SRDA  @00,32                                              0231 00507000
         D     @00,@CF00076                                        0231 00508000
         LR    INDEX3,@01                                          0231 00509000
*   DO I = 1 TO INDEX3 BY 1;                               /*@Z427VLS*/ 00510000
         LA    @08,1                                               0232 00511000
         ST    @08,I                                               0232 00512000
         B     @DE00232                                            0232 00513000
@DL00232 DS    0H                                                  0233 00514000
*     /*                                                     @Z427VLS*/ 00515000
*     /* THE DATA IN PARMFLD IS ACCESSED TWO CHARACTERS AT A TIME.   */ 00516000
*     /* THE FIRST CHARACTER IS SHIFTED LEFT FOUR BITS AND THEN ORED */ 00517000
*     /* WITH THE SECOND CHARACTER.  THE SECOND CHARACTER IS ASSUMED */ 00518000
*     /* TO HAVE THE FIRST FOUR BITS AS ZEROES.              @Z427VLS*/ 00519000
*     /*                                                     @Z427VLS*/ 00520000
*     HOLD(I) = PARMFLD(INDEX1);                           /*@Z427VLS*/ 00521000
         LA    @07,HOLD-1(@08)                                     0233 00522000
         LA    @04,PARMFLD-1(INDEX1)                               0233 00523000
         MVC   0(1,@07),0(@04)                                     0233 00524000
*     SLL (HOLD(I),FOUR);                                  /*@Z427VLS*/ 00525000
         SLR   @07,@07                                             0234 00526000
         IC    @07,HOLD-1(@08)                                     0234 00527000
         SLL   @07,4                                               0234 00528000
         STC   @07,HOLD-1(@08)                                     0234 00529000
*     HOLD(I) = HOLD(I) | PARMFLD(INDEX1 + 1);             /*@Z427VLS*/ 00530000
         LA    @07,HOLD-1(@08)                                     0235 00531000
         LA    @04,PARMFLD(INDEX1)                                 0235 00532000

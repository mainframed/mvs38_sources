*WORDCON2: PROC (WORDLEN3,WORDSTR3) OPTIONS(SAVE(1,14));   /*@Z427VLS*/ 00803000
WORDCON2 ST    @14,@SA00014                                        0295 00804000
         ST    @01,@SA00014+4                                      0295 00805000
         MVC   @PC00014(8),0(@01)                                  0295 00806000
*    DCL WORDLEN3 FIXED(31);                               /*@Z427VLS*/ 00807000
*    DCL WORDSTR3 CHAR(*);                                 /*@Z427VLS*/ 00808000
*    DO I = 1 TO WORDLEN3 BY 1;                            /*@Z427VLS*/ 00809000
         LA    @08,1                                               0298 00810000
         ST    @08,I                                               0298 00811000
         B     @DE00298                                            0298 00812000
@DL00298 DS    0H                                                  0299 00813000
*      HOLDAREA(1) = HEXZERO;                              /*@Z427VLS*/ 00814000
         MVI   HOLDAREA,X'00'                                      0299 00815000
*      HOLDAREA(2) = WORDSTR3(I);                          /*@Z427VLS*/ 00816000

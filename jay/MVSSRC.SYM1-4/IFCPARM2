         ST    @08,I                                               0237 00538000
@DE00232 CR    @08,INDEX3                                          0237 00539000
         BNH   @DL00232                                            0237 00540000
*END;                                                      /*@Z427VLS*/ 00541000
@EL00008 DS    0H                                                  0238 00542000
@EF00008 DS    0H                                                  0238 00543000
@ER00008 L     @14,12(,@13)                                        0238 00544000
         L     @01,24(,@13)                                        0238 00545000
         BR    @14                                                 0238 00546000
*/********************************************************************/ 00547000
*/*                                                          @Z427VLS*/ 00548000

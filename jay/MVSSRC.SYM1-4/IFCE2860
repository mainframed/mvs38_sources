         AGO   .SUBF                                           @YL02021 15810002
.SUBE    ANOP                                                  @YL02021 15840002
         AIF   ('&SYSLIST(&OP,2)' EQ '').BACKUP                @YL02021 15870002
&K       SETA  K'&SYSLIST(&OP,2)                               @YL02021 15900002
&B       SETB  ('&SYSLIST(&OP,2)'(1,1) EQ 'B')                 @YL02021 15930002
&S       SETC  '&SYSLIST(&OP,2)'(&B+1,&K-&B)                   @YL02021 15960002
.SUBF    ANOP                                                  @YL02021 15990002
.BACKUP  ANOP                                                  @YL02021 16020002
&NRENTS  SETA  &NRENTS+1                                       @YL02021 16050002
         AIF   (&BYTESP GT 0 AND &BYTESP LT 16).SUB0           @YL02021 16080002
         MNOTE '**NO. OF BYTES OUTSIDE RANGE OF 1-15**'        @YL02021 16110002

         TITLE 'IFCE2860 - EDIT TABLE - LINKNAME=IFCE2860'              00120020
         MACRO                                                          00180002
&NAME    DSGEN ,                                                        00200002
.*                            THIS MACRO GENERATES ONE DSECT WORD       00220002
.*                                 FORMATTED FOR THE SUBJECT LOG RECORD 00240002
.*                                 EACH OPERAND HAS TWO SUB LISTS. THE  00260002
.*                                 FIRST IS THE NAME OF THE LOG ITEM    00280002
.*                                 WHILE THE SECOND IS THE # OF BITS TO 00300002
.*                                 ALLOCATE FOR THIS ITEM. ITEMS MUST   00320002
.*                                 BE DEFINED CONTIGUOUSLY WITHOUT GAPS 00340002
.*                                 UNUSED BIT GROUPS MUST BE GIVEN      00360002
.*                                 DUMMY NAMES. THE NAMES PROVIDED FOR  00380002
.*                                 THE LOG ITEMS MAY THEN BE USED IN    00400002
.*                                 THE CONVERSION MACRO INSTRUCTIONS.   00420002
         GBLC &ITEM(1500)                                      @YM2954P 00440002
         GBLA &ITEMNBR,&BITS(1500),&SHIFT(1500)                @YM2954P 00460002
         LCLB  &ONLY1,&CTIND                                   @YM2954P 00480002
         LCLA  &START1,&START2,&END1,&END2                     @YM2954P 00500002
         LCLC  &SYMBOL,&EQUSYM                                 @YM2954P 00520002
         LCLA  &OP,&COUNT,&BYTE,&EQUCNT                        @YM2954P 00540002
         ACTR  100000                                          @YM2954P 00560002
.START   AIF   (N'&SYSLIST(&OP+1) LT 3).LOOP                   @YM2954P 00580002
         AIF   ('&SYSLIST(&OP+1,3)' EQ 'EQU').LOOP     TEST FOR EQU     00600002
  AIF   ('&SYSLIST(&OP+1,3)' EQ '' OR '&SYSLIST(&OP+1,4)' EQ '').ERR1   00620002
         AIF   ('&SYSLIST(&OP+1,3)' GT '&SYSLIST(&OP+1,4)').ERR3        00640002
&START1  SETA  &SYSLIST(&OP+1,3)        GET START COUNT OF 1   @YM2954P 00660002
&START2  SETA  &SYSLIST(&OP+1,4)        GET FIRST END COUNT    @YM2954P 00680002
         AGO   .SET2A                   BRANCH AROUND          @YM2954P 00700002
.ERR1    MNOTE '**FIRST SYMBOL CONCATENATION INCOMPLETE**'     @YM2954P 00720002
         MEXIT                                                 @YM2954P 00740002
.SET2A   AIF   ('&SYSLIST(&OP+1,5)' EQ ''AND '&SYSLIST(&OP+1,6)' EQ '' X00760002
               ).SET2                   TEST FOR NO SECOND     @YM2954P 00780002
  AIF   ('&SYSLIST(&OP+1,5)' EQ '' OR '&SYSLIST(&OP+1,6)' EQ '').ERR2   00800002
         AIF   ('&SYSLIST(&OP+1,5)' GT '&SYSLIST(&OP+1,6)').ERR3        00820002
&END1    SETA  &SYSLIST(&OP+1,5)        GET START COUNT OF 2   @YM2954P 00840002
&END2    SETA  &SYSLIST(&OP+1,6)        GET END COUNT OF 2     @YM2954P 00860002
         AGO   .LOOP                                           @YM2954P 00880002
.SET2    ANOP                                                  @YM2954P 00900002
&ONLY1   SETB  1                        INDICATE ONLY ONE CONCATENATION 00920002
         AGO   .LOOP                                           @YM2954P 00940002
.ERR2    MNOTE '**SECOND SYMBOL CONCATENATION INCOMPLETE**'    @YM2954P 00960002
         MEXIT                                                 @YM2954P 00980002
.ERR3    MNOTE '**START GREATER THAN END CONCATENATION**'      @YM2954P 01000002
         MEXIT                                                 @YM2954P 01020002
.LOOP    ANOP                                                  @YM2954P 01040002
&OP      SETA  &OP+1                                           @YM2954P 01060002
         AIF   (&OP GT N'&SYSLIST).DONE      TEST IF ALL DONE  @YM2954P 01080002
&ITEMNBR SETA  &ITEMNBR+1                                      @YM2954P 01100002
         AIF   ('&SYSLIST(&OP,2)' NE '').OK1                   @YM2954P 01120002
         MNOTE '**NULL BIT COUNT FIELD**'                      @YM2954P 01140002
         MEXIT                                                 @YM2954P 01160002
.OK1     ANOP                                                  @YM2954P 01180002
&SYMBOL  SETC  '&SYSLIST(&OP,1)'                               @YM2954P 01200002
         AIF   ('&SYMBOL' NE '').OK                            @YM2954P 01220002
*              *****     LOG ITEM SYMBOL NOT PROVIDED     *****         01240002
         AIF   ('&SYSLIST(&OP,3)' NE 'EQU').CNT                @YM2954P 01260002
         AGO   .START                   GET NEXT               @YM2954P 01280002
.OK      AIF   (N'&SYSLIST(&OP) LT 3).NO3    TEST FOR OLD FORMAT        01300002
         AIF   ('&SYSLIST(&OP,3)' EQ 'EQU').EQU   TEST FOR EQU @YM2954P 01320002
         AIF   (&ONLY1).OK2             TEST FOR ONLY ONE CONCAT        01340002
&SYMBOL  SETC  '&SYMBOL.&START1.&END1'       GET 1 AND 2 CONCAT         01360002
         AGO   .NO3                     BRANCH AROUND          @YM2954P 01380002
.OK2     ANOP                                                  @YM2954P 01400002
&SYMBOL  SETC  '&SYMBOL.&START1'        GET 1 CONCATENATED     @YM2954P 01420002
.NO3     ANOP                                                  @YM2954P 01440002
&SYMBOL  EQU   *+&BYTE                                         @YM2954P 01460002
&ITEM(&ITEMNBR) SETC '&SYMBOL'          GET LABEL IN GLOBAL    @YM2954P 01480002
&BITS(&ITEMNBR) SETA &SYSLIST(&OP,2)                           @YM2954P 01500002
&SHIFT(&ITEMNBR) SETA &COUNT-&BYTE*8                           @YM2954P 01520002
.CNT     ANOP                                                  @YM2954P 01540002
&CTIND   SETB  1                        SET COUNT INDICATOR    @YM2954P 01560002
&COUNT   SETA  &COUNT+&SYSLIST(&OP,2)                          @YM2954P 01580002
&BYTE    SETA  &COUNT/8                                        @YM2954P 01600002
         AIF   (N'&SYSLIST(&OP) LT 3).START  TEST FOR OLD FORMAT        01620002
         AIF   (&ONLY1).TSTST           TEST FOR ONLY ONE CONCAT        01640002
         AIF   (&END1 EQ &END2).TSTST   TEST FOR SECOND END    @YM2954P 01660002
&END1    SETA  &END1+1                  INCREMENT COUNT        @YM2954P 01680002
&OP      SETA  &OP-1                    DECREMENT INCREMENT    @YM2954P 01700002
         AGO   .LOOP                    GENERATE NEXT          @YM2954P 01720002
.TSTST   ANOP                                                  @YM2954P 01740002
         AIF   (&START1 EQ &START2).LPDONE   TEST FOR END      @YM2954P 01760002
         AIF   (&ONLY1).NSET1           TEST FOR ONLY ONE CONCAT        01780002
&END1    SETA  &SYSLIST(&OP,5)          RE-INIT SECOND START COUNT      01800002
.NSET1   ANOP                                                  @YM2954P 01820002
&OP      SETA  &OP-1                    DECREMENT INDEX        @YM2954P 01840002
&START1  SETA  &START1+1                INCREMENT FIRST COUNT  @YM2954P 01860002
         AGO   .LOOP                    GO DO NEXT             @YM2954P 01880002
.LPDONE  ANOP                                                  @YM2954P 01900002
&ONLY1   SETB  0                        CLEAR INDICATOR        @YM2954P 01920002
         AGO   .START                   GO TEST FOR MORE SUB-LISTS      01940002
.DONE    AIF   (&COUNT-&COUNT/8*8 NE 0).ERR  TEST FOR MULTIPLE OF EIGHT 01960002
         AIF   (&CTIND EQ 0).END        NO DS IF NO COUNT      @YM2954P 01980002
&COUNT   SETA  &COUNT/8                 GET NUMBER OF BYTES    @YM2954P 02000002
&NAME    DS    &COUNT.C                                        @YM2954P 02020002
.END     MEXIT                                                 @YM2954P 02040002
.ERR     MNOTE '**BIT COUNT FLDS DO NOT TOTAL MULTIPLE OF 8**' @YM2954P 02060002
         MEXIT                                                 @YM2954P 02080002
.EQU     AIF   ('&SYSLIST(&OP,4)' NE '').EQU1     TEST FOR NULL LABEL   02100002
.ERR4    MNOTE '**EQU OPERAND OMITTED OR NOT FOUND**'          @YM2954P 02120002
         MEXIT                                                 @YM2954P 02140002
.EQU1    AIF   (&SYSLIST(&OP,2) LT 16).EQU2  COUNT OVER FIFTEEN         02160002
*****               THE FOLLOWING STATEMENT                *****        02180002
*****     NUMBER OF BITS EXCEED 15 - DO NOT USE AS BIN     *****        02200002
.EQU2    ANOP                                                  @YM2954P 02220002
&EQUCNT  SETA  0                        CLEAR COUNTER          @YM2954P 02240002
&EQUSYM  SETC  '&SYSLIST(&OP,4)'        GET EQU OPERAND        @YM2954P 02260002
.EQULOP  ANOP                                                  @YM2954P 02280002
&EQUCNT  SETA  &EQUCNT+1                INCREMENT INDEX        @YM2954P 02300002
         AIF   ('&ITEM(&EQUCNT)' EQ '&EQUSYM').EQUOUT  LABEL FOUND?     02320002
         AIF   (&EQUCNT EQ &ITEMNBR).ERR4    ALL TESTED?       @YM2954P 02340002
         AGO   .EQULOP                  NO-GET NEXT            @YM2954P 02360002
.EQUOUT  ANOP                                                  @YM2954P 02380002
&ITEM(&ITEMNBR) SETC '&SYMBOL'          GET LABEL NAME         @YM2954P 02400002
&BITS(&ITEMNBR) SETA &SYSLIST(&OP,2)    GET NUMBER OF BITS     @YM2954P 02420002
&SHIFT(&ITEMNBR) SETA &SHIFT(&EQUCNT)   GET SHIFT COUNT        @YM2954P 02440002
&SYMBOL  EQU   &EQUSYM                  THIS SYMBOL EQUATED    @YM2954P 02460002
         AGO   .START                   RETURN FOR NEXT        @YM2954P 02480002
         MEND                                                  @YM2954P 02500002
         EJECT                                                          03180020
         MACRO                                                          03240020
&NAME    LINE  &DATA,&LABEL,&SKIP=                                      03300020
.*                            THIS MACRO ESTABLISHES THE MARGINS OR     03360020
.*                                 COLUMN SETTINGS FOR EACH LINE. IT    03420020
.*                                 GENERATES NECESSARY CARRIAGE CONTROL 03480020
.*                                 ENTRIES. LASTLY IT DETERMINES THE    03540020
.*                                 NUMBER OF ENTRIES ASSOCIATED WITH    03600020
.*                                 THE PREVIOUS LINE AND STORES THAT #  03660020
.*                                 INTO THE APPROPRIATE BYTE.           03720020
.*                            &DATA(1) - CHARACTER POSITION TO BEGIN    03780020
.*                                 DATA.                                03840020
.*                            &DATA(2) - # OF SPACES BETWEEN SUCCESSIVE 03900020
.*                                 DATA COLUMN STARTING CHARACTERS.     03960020
.*                            &LABEL(1) - CHAR. POS. TO BEGIN LABELS.   04020020
.*                            &LABEL(2) - # SPACES BETWEEN LINE'S LABEL 04080020
.*                                 STARTING CHARACTERS.                 04140020
.*                            &SKIP= - # OF LINES TO ADVANCE CARRIAGE   04200020
.*                                 TO BEGIN NEXT PRINTED LINE.          04260020
.*                            NEW ENTRIES ARE GENERATED ONLY WHEN       04320020
.*                                 NECESSARY. OMISSION OF ITEMS MEANS   04380020
.*                                 THEY HAVE NOT CHANGED SINCE PREVIOUS 04440020
.*                                 SETTINGS. A LINE MACRO IS REQUIRED   04500020
.*                                 FOR EACH PRINTED LINE. IF NO SETTING 04560020
.*                                 CHANGES ARE REQUIRED ALL OPERANDS    04620020
.*                                 ARE OMITTED IN WHICH CASE ONLY THE   04680020
.*                                 # OF ENTRIES FOR THE PREVIOUS LINE   04740020
.*                                 IS COMPUTED. THIS MACRO MUST BE THE  04800020
.*                                 FIRST MACRO INSTRUCTION ISSUED FOR   04860020
.*                                 THE SUBJECT LINE                     04920020
         GBLA  &DB,&DT,&LB,&LT,&NRENTS                                  04980020
         GBLA  &LASTCC,&INITIAL                                         05040020
         GBLC  &NRENTSX                                                 05100020
         GBLC  &CARRCON                                                 05160020
         GBLB  &TEST                                                    05220020
         LCLA  &S                                                       05280020
&NAME    DS    0C                                                       05340020
         AIF   (&INITIAL EQ 0).FIRSTLN                                  05400020
         LINEND                                                         05460020
         AGO   .NTFIRST                                                 05520020
.FIRSTLN ANOP                                                           05580020
&NRENTSX SETC  'N&SYSNDX'                                               05640020
&INITIAL SETA  1                                                        05700020
N&SYSNDX EQU   *                                                        05760020
.NTFIRST ANOP                                                           05820020
         DC    AL1(0)                                                   05880020
         AIF   ('&DATA(1)' EQ '').CKDTABS                               05940020
         AIF   (&DATA(1) EQ &DB).CKDTABS                                06000020
&DB      SETA  &DATA(1)                                                 06060020
         AGO   .AA                                                      06120020
.CKDTABS AIF   ('&DATA(2)' EQ '').NCDATA                                06180020
         AIF   (&DATA(2) EQ &DT).NCDATA                                 06240020
         AGO   .CC                                                      06300020
.AA      AIF   ('&DATA(2)' EQ '').BB                                    06360020
         AIF   (&DATA(2) EQ &DT).BB                                     06420020
.CC      ANOP                                                           06480020
&DT      SETA  &DATA(2)                                                 06540020
.*                                 CONVERSIONS.                         06600020
.*                            &LOGITEM - NAME OF DATA ITEM TO BE        06660020
.BB      DC    XL2'FFFE'                                                06720020
         DC    FL1'&DB'                                                 06780020
         DC    FL1'&DT'                                                 06840020
&NRENTS  SETA  &NRENTS+1                                                06900020
.NCDATA  ANOP                                                           06960020
         AIF   ('&LABEL(1)' EQ '').CKLTABS                              07020020
         AIF   (&LABEL(1) EQ &LB).CKLTABS                               07080020
&LB      SETA  &LABEL(1)                                                07140020
         AGO   .AAA                                                     07200020
.CKLTABS AIF   ('&LABEL(2)' EQ '').NCLABEL                              07260020
         AIF   (&LABEL(2) EQ &LT).NCLABEL                               07320020
         AGO   .CCC                                                     07380020
.AAA     AIF   ('&LABEL(2)' EQ '').BBB                                  07440020
         AIF   (&LABEL(2) EQ &LT).BBB                                   07500020
.CCC     ANOP                                                           07560020
&LT      SETA  &LABEL(2)                                                07620020
.BBB     DC    XL2'FFFF'                                                07680020
         DC    FL1'&LB'                                                 07740020
         DC    FL1'&LT'                                                 07800020
&NRENTS  SETA  &NRENTS+1                                                07860020
.NCLABEL ANOP                                                           07920020
         AIF   ('&SKIP' NE '').CONT                                     07980020
         AIF   (&TEST EQ 0).NOCHANG                                     08040020
&S       SETA  &LASTCC                                                  08100020
         AGO   .DEFAULT                                                 08160020
.CONT    ANOP                                                           08220020
         AIF   (&TEST EQ 0).AB                                          08280020
&S       SETA  &SKIP                                                    08340020
.DEFAULT ANOP                                                           08400020
         ORG   &CARRCON+3                                               08460020
         DC    AL.3(0),AL.2(&S),AL.3(1)                                 08520020
         ORG                                                            08580020
&LASTCC  SETA  &S                                                       08640020
&TEST    SETB  0                                                        08700020
         AGO   .DD                                                      08760020
.AB      AIF   (&SKIP EQ &LASTCC).NOCHANG                               08820020
A&SYSNDX DC    XL2'FFFB'                                                08880020
         DC    AL.3(0),AL.2(&SKIP),AL.3(1)                              08940020
         DC    FL1'9'                                                   09000020
&LASTCC  SETA  &SKIP                                                    09060020
&TEST    SETB  1                                                        09120020
&CARRCON SETC  'A&SYSNDX'                                               09180020
&NRENTS  SETA  &NRENTS+1                                                09240020
.DD      ANOP                                                           09300020
.NOCHANG ANOP                                                           09360020
         MEND                                                           09420020
         EJECT                                                          09480020
         MACRO                                                          09540020
         BIN   &LOGITEM,&SKIP$OR                                        09600020
.*                                 &LOGITEM - NAME OF ITEM IN LOG       09660020
.*                                      RECORD DSECT                    09720020
.*                                 &SKIP$OR - NBR OF SPACES TO SKIP ON  09780020
.*                                 MULTIPLE PRINT OR NBR OF SPACES      09840020
.*                                      TO BACK UP FROM COLUMN FOR      09900020
.*                                      START OF PRINTING. THE LETTER   09960020
.*                                      B PRECEEDS THE NUMBER (B5) IF   10020020
.*                                      THE LATTER IS DESIRED. OMISSION 10080020
.*                                      OF THIS OPERAND FORCES OPTION 2 10140020
.*                                      WITH BACKUP SPACING EQUAL TO    10200020
.*                                 THE NUMBER OF BITS IN THE ITEM       10260020
.*                                 DEFINITION (VIA MACRO 'DSGEN').      10320020
.*                            A LIMIT OF 15 BITS IS IMPOSED FOR THE     10380020
.*                                 TOTAL NUMBER OF BITS TO PRINT.       10440020
         GBLC  &ITEM(1500)                                              10500020
         GBLA  &ITEMNBR,&BITS(1500),&SHIFT(1500),&NRENTS                10560020
         GBLC  &DSECT                                                   10620020
         LCLB  &B                                                       10680020
         LCLA  &IBITS,&NDX,&ISHIFT,&K                                   10740020
         LCLA  &TEMP,&T                                                 10800020
         LCLC  &S                                                       10860020
.LOOP    ANOP                                                           10920020
&NDX     SETA  &NDX+1                                                   10980020
         AIF   ('&ITEM(&NDX)' EQ '&LOGITEM').FOUND                      11040020
         AIF   (&NDX LT &ITEMNBR).LOOP                                  11100020
         MNOTE *,'YOU BLEW IT'                                          11160020
         ROUTINE DUM,SKIPPER                                            11220020
         MEXIT                                                          11280020
.FOUND   ANOP                                                           11340020
&ISHIFT  SETA  &SHIFT(&NDX)                                             11400020
&IBITS   SETA  &BITS(&NDX)                                              11460020
&T       SETA  &IBITS-1                                                 11520020
&B       SETB  0                                                        11580020
         AIF   ('&SKIP$OR' EQ '').BACKUP                                11640020
&K       SETA  K'&SKIP$OR                                               11700020
&B       SETB  ('&SKIP$OR'(1,1) EQ 'B')                                 11760020
&S       SETC  '&SKIP$OR'(&B+1,&K-&B)                                   11820020
&T       SETA  &S                                                       11880020
.BACKUP  ANOP                                                           11940020
&NRENTS  SETA  &NRENTS+1                                                12000020
&TEMP    SETA  &ISHIFT                                                  12060020
A&SYSNDX DC    AL.12(&LOGITEM-&DSECT),XL.4'&TEMP'                       12120020
         AIF (&IBITS EQ 1 AND &B EQ 0).OUT                              12180020
         ORG   A&SYSNDX                                                 12240020
&B       SETB  1                                                        12300020
         DC    AL.12(&LOGITEM-&DSECT),XL.4'C'                           12360020
         DC    BL.1'&B',AL.7(&T),AL.4(&IBITS),FL.4'&ISHIFT'             12420020
.OUT     ANOP                                                           12480020
         MEND                                                           12540020
         EJECT                                                          12600020
         MACRO                                                 @YL02021 12660002
         HEX   ,                                               @YL02021 12690002
.*                            THIS MACRO FACILITATES GENERATION OF THE  12720002
.*                                 TABLE ENTRIES SPECIFYING HEX TYPE    12750002
.*                                 CONVERSIONS.                @YL02021 12780002
.*                            &SYSLIST(&OP,1) - NAME OF DATA ITEM TO BE 12810002
.*                                 CONVERTED                   @YL02021 12840002
.*                            &SYSLIST(&OP,2) - # OF SPACES TO SKIP ON  12870002
.*                                 MULTIPLE PRINT OR NBR OF SPACES TO   12900002
.*                                 BACK UP FROM COLUMN FOR START OF     12930002
.*                                 PRINTING. THE LETTER B PRECEEDS THE  12960002
.*                                 NUMBER (B8) IF THE LATTER OPTION IS  12990002
.*                                 DESIRED. OMISSION OF THIS OPERAND    13020002
.*                                 FORCES OPTION 2 WITH BACKUP SPACING  13050002
.*                                 EQUAL TO THE NUMBER OF BYTES SPECI-  13080002
.*                                 FIED BY THE NEXT OPERAND (TWICE THIS 13110002
.*                                 NUMBER IF SUFFIX IS EXPLICITLY OR    13140002
.*                                 IMPLICITLY B).              @YL02021 13170002
.*                            &SYSLIST(&OP,3) - NUMBER OF BYTES TO      13200002
.*                                 PROCESS. THIS NUMBER IS SUFFIXED     13230002
.*                                 WITH AN L,R, OR B TO STIPULATE       13260002
.*                                 PRINTING OF THE LEFT HALF, RIGHT     13290002
.*                                 HALF, OR BOTH HALVES OF THE BYTE     13320002
.*                                 RESPECTIVELY. OMISSION OF THIS       13350002
.*                                 OPERAND DEFAULTS TO B WITH THE NUM-  13380002
.*                                 BER OF BYTES EQUAL TO THE ITEM LNGTH 13410002
.*                            A NEW ENTRY IS GENERATED ONLY WHEN NECES- 13440002
.*                                 SARY. THIS MACRO MUST BE THE FIRST   13470002
.*                                 MACRO INSTRUCTION ISSUED FOR THE     13500002
.*                                 SUBJECT LINE(S).            @YL02021 13530002
         GBLC &ITEM(1500),&DSECT                               @YL02021 13560002
         GBLA &ITEMNBR,&BITS(1500),&SHIFT(1500),&NRENTS        @YL02021 13590002
         GBLA  &INITIAL                                        @YL02021 13620002
         GBLB  &DIAGTST,&DIAGSET,&DIAGLIN                      @YL02021 13650002
         LCLB  &B,&SUB                                         @YL02021 13680002
         LCLA  &IBITS,&NDX,&ISHIFT,&K,&T,&CODE,&BYTESP,&OP     @YL02021 13710002
         LCLC  &S,&LETTER                                      @YL02021 13740002
         AIF   (&DIAGTST EQ 0).NOMSG                           @YL02021 13770002
         AIF   (&INITIAL EQ 0).NOMSG                           @YL02021 13800002
         AIF   (&DIAGLIN EQ 1).NOMSG                           @YL02021 13830002
         MNOTE '**NO LINE ASSOCIATED WITH THIS DATA**'         @YL02021 13860002
*****THE FOLLOWING GENERATED FOR TEST PURPOSES*****            @YL02021 13890002
*        LINE                                                  @YL02021 13920002
         LINE                                                  @YL02021 13950002
.NOMSG   ANOP                                                  @YL02021 13980002
         AIF   (K'&SYSLIST(1) EQ K'&SYSLIST(1,1)).LOOP         @YL02021 14010002
&SUB     SETB  1                                               @YL02021 14040002
.LOOP    ANOP                                                  @YL02021 14070002
&OP      SETA  &OP+1                                           @YL02021 14100002
.LOOP1   ANOP                                                  @YL02021 14130002
&NDX     SETA  &NDX+1                                          @YL02021 14160002
         AIF   ('&ITEM(&NDX)' EQ '&SYSLIST(&OP,1)').FOUND      @YL02021 14190002
         AIF   (&NDX LT &ITEMNBR).LOOP1                        @YL02021 14220002
         MNOTE '**LOG ITEM NOT DEFINED**'                      @YL02021 14250002
         MEXIT                                                 @YL02021 14280002
.FOUND   ANOP                                                  @YL02021 14310002
&ISHIFT  SETA  &SHIFT(&NDX)                                    @YL02021 14340002
&IBITS   SETA  &BITS(&NDX)                                     @YL02021 14370002
         AIF   (&ISHIFT EQ 0 OR &ISHIFT EQ 4).ALIGN            @YL02021 14400002
.ERR     MNOTE '**DATA NOT ALLIGNED ON HEX OR BYTE BOUNDARY**' @YL02021 14430002
         MEXIT                                                 @YL02021 14460002
.ALIGN   ANOP                                                  @YL02021 14490002
&CODE    SETA  1                                               @YL02021 14520002
         AIF   (&SUB EQ 1).SUBA                                @YL02021 14550002
&K       SETA  K'&SYSLIST(3)                                   @YL02021 14580002
         AGO   .SUBB                                           @YL02021 14610002
.SUBA    ANOP                                                  @YL02021 14640002
&K       SETA  K'&SYSLIST(&OP,3)                               @YL02021 14670002
.SUBB    ANOP                                                  @YL02021 14700002
         AIF   (&IBITS-(&IBITS/4)*4 EQ 0).OK                   @YL02021 14730002
         MNOTE '**BIT LENGTH SPECIFICATION INCONSISTENT WITH CONTROL**' 14760002
         MEXIT                                                 @YL02021 14790002
.OK      AIF   (&SUB EQ 1).SUBC                                @YL02021 14820002
         AIF   ('&SYSLIST(3)' EQ '').USEDEFN                   @YL02021 14850002
&LETTER  SETC  '&SYSLIST(3)'(&K,1)                             @YL02021 14880002
&S       SETC  '&SYSLIST(3)'(1,&K-1)                           @YL02021 14910002
         AGO   .SUBD                                           @YL02021 14940002
.SUBC    ANOP                                                  @YL02021 14970002
         AIF   ('&SYSLIST(&OP,3)' EQ '').USEDEFN               @YL02021 15000002
&LETTER  SETC  '&SYSLIST(&OP,3)'(&K,1)                         @YL02021 15030002
&S       SETC  '&SYSLIST(&OP,3)'(1,&K-1)                       @YL02021 15060002
.SUBD    ANOP                                                  @YL02021 15090002
&BYTESP  SETA  &S                                              @YL02021 15120002
         AIF   ('&LETTER' EQ 'B').BOTH                         @YL02021 15150002
         AIF   ('&LETTER' EQ 'L').LEFT                         @YL02021 15180002
&CODE    SETA  2                                               @YL02021 15210002
         AGO   .L                                              @YL02021 15240002
.LEFT    ANOP                                                  @YL02021 15270002
&CODE    SETA  1                                               @YL02021 15300002
         AGO   .L                                              @YL02021 15330002
.USEDEFN ANOP                                                  @YL02021 15360002
&BYTESP  SETA  &IBITS/8                                        @YL02021 15390002
.BOTH    AIF   (&ISHIFT NE 0).ERR                              @YL02021 15420002
&CODE    SETA  3                                               @YL02021 15450002
&T       SETA  &BYTESP*2-1                                     @YL02021 15480002
&S       SETC  '&T'                                            @YL02021 15510002
.OK2     AIF   (&ISHIFT EQ 0).L                                @YL02021 15540002
&CODE    SETA  2                                               @YL02021 15570002
.L       ANOP                                                  @YL02021 15600002
&B       SETB  1                                               @YL02021 15630002
         AIF   (&SUB EQ 1).SUBE                                @YL02021 15660002
         AIF   ('&SYSLIST(2)' EQ '').BACKUP                    @YL02021 15690002
&K       SETA  K'&SYSLIST(2)                                   @YL02021 15720002
&B       SETB  ('&SYSLIST(2)'(1,1) EQ 'B')                     @YL02021 15750002

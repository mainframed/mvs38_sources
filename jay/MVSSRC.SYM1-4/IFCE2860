*                              SET ACTIVE  I/O UNITS                    34020020
SETAIO   STM   R5,R8,XSAVE              SAVE REGS                       34080020
         LR    R5,R3                    SET R5 TO BUFFER                34140020
         SH    R5,H43                   POINT R5 TO BEGGIN OF DATA      34200020
         SR    R7,R7                                                    34260020
         LA    R6,L8                    LOOP COUNT                      34320020
SETAIOLP CLC   L0(L4,R5),CZEROS         TEST FOR ALL ZEROS              34380020
         BE    BLANKOUT                 BE-TO BLANK OUT ZEROS           34440020
SETAIOL1 LA    R5,L5(R5)                BUMP COUNT                      34500020
         BCT   R6,SETAIOLP              LOOP TILL 8 TESTS               34560020
         CH    R7,H8                    ARE ALL ACTIVE IO ZERO          34620020
         BNE   RETURN                   NO RETURN                       34680020
         LR    R5,R3                    YES                             34740020
         SH    R5,H43                   PUT IN NONE                     34800020
         MVC   L0(L4,R5),NONE           INTO PRINT BUFF                 34860020
         B     RETURN                                                   34920020
BLANKOUT MVC   L0(L4,R5),BLANKS         MOVE IN BLANKS                  34980020
         LA    R7,L1(R7)                COUNT # OF ZERO FLDS            35040020
         B     SETAIOL1                 LOOP                            35100020
RETURN   LM    R5,R8,XSAVE                                              35160020
         BR    R14                                                      35220020

         IC    R7,L0(R4)           GET CHAR FROM RECORD                 72480002
SETHEX1  LA    R8,CHARS            PTR TO CHARACTERS HEX                72540002
         SLL   R7,L24              MOVE IT TO HI OF R7                  72600002
         SR    R6,R6               ZERO R6                              72660002
         SLDL  R6,L4               MOVE 4 BITS TO R6                    72720002
         AR    R6,R8               COMPUTE OFFSET                       72780002
         MVC   L0(L1,R3),L0(R6)    MOVE CHAR TO PRT BUFF                72840002
         SR    R6,R6               ZERO R6                              72900002
         SLDL  R6,L4               MOVE NEXT 4 BITS TO R6               72960002
         AR    R6,R8               COMPUTE OFFSET                       73020002
         MVC   L1(L1,R3),L0(R6)    MOVE CHAR TO PRT BUFF+1              73080002
         B     RETURN              EXIT                                 73140002

               &X25,&X26,&X27,&X28,&X29,&X30,&X31,&X32                  11175002
.*                            THIS MACRO GENERATES ONE DSECT WORD       11250002
.*                                 FORMATTED FOR THE SUBJECT LOG RECORD 11325002
.*                                 EACH OPERAND HAS TWO SUB LISTS. THE  11400002
.*                                 FIRST IS THE NAME OF THE LOG ITEM    11475002
.*                                 WHILE THE SECOND IS THE # OF BITS TO 11550002
.*                                 ALLOCATE FOR THIS ITEM. ITEMS MUST   11625002
.*                                 BE DEFINED CONTIGUOUSLY WITHOUT GAPS 11700002
.*                                 UNUSED BIT GROUPS MUST BE GIVEN      11775002
.*                                 DUMMY NAMES. THE NAMES PROVIDED FOR  11850002
.*                                 THE LOG ITEMS MAY THEN BE USED IN    11925002
.*                                 THE CONVERSION MACRO INSTRUCTIONS.   12000002
         GBLC &ITEM(1000)                                               12075002
         GBLA &ITEMNBR,&BITS(1000),&SHIFT(1000)                         12150002
         LCLC  &SYMBOL                                                  12225002
         LCLA  &OP,&COUNT,&BYTE                                         12300002
.LOOP    ANOP                                                           12375002
&OP      SETA  &OP+1                                                    12450002
&ITEMNBR SETA  &ITEMNBR+1                                               12525002
&SYMBOL  SETC  '&SYSLIST(&OP,1)'                                        12600002
         AIF   ('&SYSLIST(&OP,2)' NE '').OK1                            12675002
         MNOTE *,'NULL BIT COUNT FIELD'                                 12750002
         MEXIT                                                          12825002
.OK1     ANOP                                                           12900002
         AIF   ('&SYMBOL' NE '').OK                                     12975002
         MNOTE 'LOG ITEM SYMBOL NOT PROVIDED'                           13050002
         AGO   .CNT                                                     13125002
.OK      ANOP                                                           13200002
         AIF   ('&SYMBOL'(1,1) EQ '''').CNT                *TEMP*       13275002
&SYMBOL  EQU   *+&BYTE                                                  13350002
&ITEM(&ITEMNBR)  SETC '&SYSLIST(&OP,1)'                                 13425002
&BITS(&ITEMNBR) SETA &SYSLIST(&OP,2)                                    13500002
&SHIFT(&ITEMNBR) SETA &COUNT-&BYTE*8                                    13575002
.CNT     ANOP                                                           13650002
&COUNT   SETA  &COUNT+&SYSLIST(&OP,2)                                   13725002
&BYTE    SETA  &COUNT/8                                                 13800002
         AIF (&OP LT N'&SYSLIST).LOOP                                   13875002
         AIF   (&COUNT NE 32).NOT32                                     13950002
&NAME    DS    4C                                                       14025002
         MEXIT                                                          14100002
.NOT32   AIF   (&COUNT NE 64).ERR                                       14175002
&NAME    DS    8C                                                       14250002
         MEXIT                                                          14325002
.ERR     MNOTE '**BIT COUNT FIELDS DO NOT TOTAL 32 OR 64**'             14400002
         MEND                                                           14475002
         MACRO                                                          14550002
         LINEND                                                         14625002
.*                            THIS MACRO IS USED ONLY BY THE LINE       14700002

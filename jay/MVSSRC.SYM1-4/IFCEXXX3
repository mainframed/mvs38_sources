         AIF (&OP LT N'&SYSLIST).NOTLAST                                18450002
&MARGIN  SETC  '$'                                                      18525002
.NOTLAST ANOP                                                           18600002
&NRCHAR  SETA  8                                                        18675002
&TACK    SETC  ''                                                       18750002
.LOOPIN  ANOP                                                           18825002
&REMAIN  SETA  &K-&START                                                18900002
         AIF (&REMAIN GE 8).MORE                                        18975002
&TACK    SETC  '&MARGIN'                                                19050002
&NRCHAR  SETA  &REMAIN                                                  19125002
.MORE    ANOP                                                           19200002
&TEXT    SETC  ''                                                       19275002
         AIF (&REMAIN EQ 0).ENDTEXT                                     19350002
&TEXT    SETC  '&SYSLIST(&OP)'(&START,&NRCHAR)                          19425002
.ENDTEXT ANOP                                                           19500002
&LABELNR SETA  &LABELNR+1                                               19575002
&LABELS(&LABELNR) SETC '&TEXT'.'&TACK'                                  19650002
&START   SETA  &START+8                                                 19725002
         AIF ('&TACK' EQ '').LOOPIN                                     19800002
         AIF ('&TACK' EQ '*').LOOPOUT                                   19875002
         MEND                                                           19950002
         SPACE 2                                                        20025002
         MACRO                                                          20100002
         LSTART                                                         20175002
.*                            THIS MACRO IS ISSUED WHEN THE LABEL AREA  20250002

*  BIN MACRO     BIN MACRO     BIN MACRO     BIN MACRO     BIN MACRO  * 21750002
         SPACE 2                                                        21825002
         MACRO                                                          21900002
         BIN   &LOGITEM,&SKIP$OR                                        21975002
.*                                 &LOGITEM - NAME OF ITEM IN LOG       22050002
.*                                      RECORD DSECT                    22125002
.*                                 &SKIP$OR - NBR OF SPACES TO SKIP ON  22200002
.*                                 MULTIPLE PRINT OR NBR OF SPACES      22275002
.*                                      TO BACK UP FROM COLUMN FOR      22350002
.*                                      START OF PRINTING. THE LETTER   22425002
.*                                      B PRECEEDS THE NUMBER (B5) IF   22500002
.*                                      THE LATTER IS DESIRED. OMISSION 22575002
.*                                      OF THIS OPERAND FORCES OPTION 2 22650002
.*                                      WITH BACKUP SPACING EQUAL TO    22725002
.*                                 THE NUMBER OF BITS IN THE ITEM       22800002
.*                                 DEFINITION (VIA MACRO 'DSGEN').      22875002
.*                            A LIMIT OF 15 BITS IS IMPOSED FOR THE     22950002
.*                                 TOTAL NUMBER OF BITS TO PRINT.       23025002
         GBLC &ITEM(1000)                                               23100002
         GBLA &ITEMNBR,&BITS(1000),&SHIFT(1000),&NRENTS                 23175002
         GBLC  &DSECT                                                   23250002
         LCLB  &B                                                       23325002
         LCLA  &IBITS,&NDX,&ISHIFT,&K                                   23400002
         LCLA  &TEMP,&T                                                 23475002
         LCLC  &S                                                       23550002
.LOOP    ANOP                                                           23625002
&NDX     SETA  &NDX+1                                                   23700002
         AIF   ('&ITEM(&NDX)' EQ '&LOGITEM').FOUND                      23775002
         AIF   (&NDX LT &ITEMNBR).LOOP                                  23850002
         MNOTE *,'YOU BLEW IT'                                          23925002
         ROUTINE DUM,SKIPPER                                            24000002
         MEXIT                                                          24075002
.FOUND   ANOP                                                           24150002
&ISHIFT  SETA  &SHIFT(&NDX)                                             24225002
&IBITS   SETA  &BITS(&NDX)                                              24300002
&T       SETA  &IBITS-1                                                 24375002
&B       SETB  0                                                        24450002
         AIF   ('&SKIP$OR' EQ '').BACKUP                                24525002
&K       SETA  K'&SKIP$OR                                               24600002
&B       SETB  ('&SKIP$OR'(1,1) EQ 'B')                                 24675002
&S       SETC  '&SKIP$OR'(&B+1,&K-&B)                                   24750002
&T       SETA  &S                                                       24825002
.BACKUP  ANOP                                                           24900002
&NRENTS  SETA  &NRENTS+1                                                24975002
&TEMP    SETA  &ISHIFT                                                  25050002
A&SYSNDX DC    AL.12(&LOGITEM-&DSECT),XL.4'&TEMP'                       25125002
         AIF (&IBITS EQ 1 AND &B EQ 0).OUT                              25200002
         ORG   A&SYSNDX                                                 25275002
&B       SETB  1                                                        25350002
         DC    AL.12(&LOGITEM-&DSECT),XL.4'C'                           25425002
         DC    BL.1'&B',AL.7(&T),AL.4(&IBITS),FL.4'&ISHIFT'             25500002
.OUT     ANOP                                                           25575002
         MEND                                                           25650002
         EJECT                                                          25725002
*  HEX MACRO     HEX MACRO     HEX MACRO    HEXN MACRO     HEX MACRO  * 25800002
         SPACE 2                                                        25875002
         MACRO                                                          25950002
         HEX   &LOGITEM,&SKIP$OR,&BYTES                                 26025002
.*                                 CONVERSIONS.                         26100002
.*                            &LOGITEM - NAME OF DATA ITEM TO BE        26175002
.*                                 CONVERTED                            26250002
.*                            &SKIP$OR - NUMBER OF SPACES TO SKIP ON    26325002
.*                                 MULTIPLE PRINT OR NBR OF SPACES TO   26400002
.*                                 BACK UP FROM COLUMN FOR START OF     26475002
.*                                 PRINTING. THE LETTER B PRECEEDS THE  26550002
.*                                 NUMBER (B8) IF THE LATTER OPTION IS  26625002
.*                                 DESIRED. OMISSION OF THIS OPERAND    26700002
.*                                 FORCES OPTION 2 WITH BACKUP SPACING  26775002
.*                                 EQUAL TO THE NUMBER OF BYTES SPECI-  26850002
.*                                 FIED BY THE NEXT OPERAND (TWICE THIS 26925002
.*                                 NUMBER IF SUFFIX IS EXPLICITLY OR    27000002
.*                                 IMPLICITLY B).                       27075002
.*                            &BYTES - NUMBER OF TOTAL INPUT BYTES TO   27150002
.*                                 PROCESS. THIS NUMBER IS SUFFIXED     27225002
.*                                 WITH AN L,R, OR B TO STIPULATE       27300002
.*                                 PRINTING OF THE LEFT HALF, RIGHT     27375002
.*                                 HALF, OR BOTH HALVES OF THE BYTE     27450002
.*                                 RESPECTIVELY. OMISSION OF THIS       27525002
.*                                 OPERAND DEFAULTS TO B WITH THE NUM-  27600002
.*                                 BER OF BYTES EQUAL TO THE ITEM LNGTH 27675002
.*                            A NEW ENTRY IS GENERATED ONLY WHEN NECES- 27750002
.*                                 SARY. THIS MACRO MUST BE THE FIRST   27825002
.*                                 MACRO INSTRUCTION ISSUED FOR THE     27900002
.*                                 SUBJECT LINE(S).                     27975002
         GBLC &ITEM(1000),&DSECT                                        28050002
         GBLA &ITEMNBR,&BITS(1000),&SHIFT(1000),&NRENTS                 28125002
         LCLB  &B                                                       28200002
         LCLA  &IBITS,&NDX,&ISHIFT,&K,&T,&CODE,&BYTESP                  28275002
         LCLC  &S,&LETTER                                               28350002
.LOOP    ANOP                                                           28425002
&NDX     SETA  &NDX+1                                                   28500002
         AIF   ('&ITEM(&NDX)' EQ '&LOGITEM').FOUND                      28575002
         AIF   (&NDX LT &ITEMNBR).LOOP                                  28650002
         MNOTE *,'LOG ITEM NOT DEFINED'                                 28725002
         MEXIT                                                          28800002
.FOUND   ANOP                                                           28875002
&ISHIFT  SETA  &SHIFT(&NDX)                                             28950002
&IBITS   SETA  &BITS(&NDX)                                              29025002
&CODE    SETA  1                                                        29100002
&K       SETA  K'&BYTES                                                 29175002
         AIF   (&IBITS-(&IBITS/4)*4 EQ 0).OK                            29250002
         MNOTE *,'BIT LENGTH SPECIFICATION INCONSISTENT WITH CONTROL'   29325002
         MEXIT                                                          29400002
.OK      AIF   ('&BYTES' EQ '').USEDEFN                                 29475002
&LETTER  SETC  '&BYTES'(&K,1)                                           29550002
&S       SETC  '&BYTES'(1,&K-1)                                         29625002
&BYTESP  SETA  &S                                                       29700002
         AIF   ('&LETTER' EQ 'B').BOTH                                  29775002
         AIF   (&ISHIFT EQ 0 OR &ISHIFT EQ 4).OK2                       29850002
.ERR     MNOTE *,'DATA NOT ALLIGNED ON HEX OR BYTE BOUNDARY'            29925002
         MEXIT                                                          30000002
.USEDEFN ANOP                                                           30075002
&BYTESP  SETA  &IBITS/8                                                 30150002
.BOTH    AIF   (&ISHIFT NE 0).ERR                                       30225002
&CODE    SETA  3                                                        30300002
&T       SETA  &BYTESP*2-1                                              30375002
.OK2     AIF   (&ISHIFT EQ 0).L                                         30450002
&CODE    SETA  2                                                        30525002
.L       ANOP                                                           30600002
&S       SETC  '&T'                                                     30675002
&B       SETB  1                                                        30750002
         AIF   ('&SKIP$OR' EQ '').BACKUP                                30825002
&K       SETA  K'&SKIP$OR                                               30900002
&B       SETB  ('&SKIP$OR'(1,1) EQ 'B')                                 30975002
&S       SETC  '&SKIP$OR'(&B+1,&K-&B)                                   31050002
.BACKUP  ANOP                                                           31125002
&NRENTS  SETA  &NRENTS+1                                                31200002
         DC    AL.12(&LOGITEM-&DSECT),XL.4'A'                           31275002
         DC    BL.1'&B',FL.7'&S',FL.4'&BYTESP',FL.4'&CODE'              31350002
.OUT     ANOP                                                           31425002
         MEND                                                           31500002
         MACRO                                                          31575002
         SPECIAL  &ROUTINE,&PARAM,&SPACE=                               31650002
.*                            THIS MACRO GENERATES A SPECIAL ROUTINE    31725002
.*                                 ENTRY TO BE PROCESSED BY THE SUM. M. 31800002
.*                            &ROUTINE - SYMBOL ON SPECIAL ROUTINE EWT. 31875002
.*                            &PARAM - AN 8 BIT BINARY REPRESENTATION   31950002
.*                                 TO OCCUPY THE 'MASK' FIELD BUT TO BE 32025002
.*                                 INTERPRETED AS AN ARGUMENT OF THE    32100002
.*                                 SPECIAL ROUTINE.                     32175002
.*                            &SPACE - THE NUMBER OF 2BYTE COUNTERS TO  32250002
.*                                 ALLOCATE FOR REQUIRED PROCESSING.    32325002
         GBLC  &CSECT                                                   32400002
         GBLA  &COUNTR                                                  32475002
         LCLA  &ADD                                                     32550002
         DC    AL1(128)                                                 32625002
         DC    BL.8'&PARAM'                                             32700002
         DC    AL2(&ROUTINE-&CSECT)                                     32775002
         DC    AL2(&COUNTR)                                             32850002
&ADD     SETA  1                                                        32925002
         AIF   ('&SPACE' EQ '').NORM                                    33000002
&ADD     SETA  &SPACE                                                   33075002
.NORM    ANOP                                                           33150002
&COUNTR  SETA  &COUNTR+&ADD                                             33225002
         MEND                                                           33300002
         MACRO                                                          33375002
         SUM   &LOGITEM,&MASK                                           33450002
.*                            THIS MACRO GENERATES A NORMAL EDIT ENTRY  33525002
.*                                 FOR PROCESSING BY THE SUMMARY MODULE 33600002
.*                            &LOGITEM - NAME OF DATA ITEM TO BE SUMMED 33675002
.*                            &MASK - BYTE MASK FOR BITS TO BE TESTED.  33750002
.*                                 THIS OPERAND SHOULD BE OMITTED FOR   33825002
.*                                 SINGLE BIT ITEMS IN WHICH CASE THE   33900002
.*                                 MASK WILL BE SET ACCORDING TO THE    33975002
.*                                 OFF BYTE BIT DISPLACEMENT OF THE     34050002
.*                                ITEM AS DEFINED BY THE MACRO 'DSGEN'  34125002
         GBLA  &COUNTR,&ITEMNBR,&BITS(1000),&SHIFT(1000)                34200002
         GBLC  &ITEM(1000),&DSECT                                       34275002
         LCLA  &IBITS,&NDX,&ISHIFT,&M,&BITNBR                           34350002
.LOOP    ANOP                                                           34425002
&NDX     SETA  &NDX+1                                                   34500002
         AIF   ('&ITEM(&NDX)' EQ '&LOGITEM').FOUND                      34575002
         AIF   (&NDX LT &ITEMNBR).LOOP                                  34650002
         MNOTE *,'LOGITEM NOT FOUND'                                    34725002
         MEXIT                                                          34800002
.FOUND   ANOP                                                           34875002
         DC    AL1(0)                                                   34950002
&ISHIFT  SETA  &SHIFT(&NDX)                                             35025002
&IBITS   SETA  &BITS(&NDX)                                              35100002
         AIF   ('&MASK' EQ '').FORM                                     35175002
         DC    B'&MASK'                                                 35250002
         AGO   .ITEM                                                    35325002
.FORM    ANOP                                                           35400002
&BITNBR  SETA  &ISHIFT-7                                                35475002
&BITNBR  SETA  0-&BITNBR                                                35550002
&M       SETA  1                                                        35625002
.LOOP2   ANOP                                                           35700002
         AIF   (&BITNBR EQ 0).GOTIT                                     35775002
&M       SETA  &M*2                                                     35850002
&BITNBR  SETA  &BITNBR-1                                                35925002
         AGO   .LOOP2                                                   36000002
.GOTIT   ANOP                                                           36075002
         DC    AL1(&M)                                                  36150002
.ITEM    DC    AL2(&LOGITEM-&DSECT)                                     36225002
         DC    AL2(&COUNTR)                                             36300002
&COUNTR  SETA  &COUNTR+1                                                36375002
         MEND                                                           36450002
         GBLC  &DSECT,&CSECT                                            36525002

*****THE FOLLOWING GENERATED FOR TEST PURPOSES*****                     21780002
*        LINE                                                           21830002
         LINE                                                           21880002
.NOMSG   ANOP                                                           21930002
         AIF   (K'&SYSLIST(1) EQ K'&SYSLIST(1,1)).LOOP                  21980002
&SUB     SETB  1                                                        22030002
.LOOP    ANOP                                                           22080002
&OP      SETA  &OP+1                                                    22130002
.LOOP1   ANOP                                                           22180002
&NDX     SETA  &NDX+1                                                   22230002
         AIF   ('&ITEM(&NDX)' EQ '&SYSLIST(&OP,1)').FOUND               22280002
         AIF   (&NDX LT &ITEMNBR).LOOP1                                 22330002
         MNOTE '**LOG ITEM NOT DEFINED**'                               22380002
         MEXIT                                                          22430002
.FOUND   ANOP                                                           22480002
&ISHIFT  SETA  &SHIFT(&NDX)                                             22530002
&IBITS   SETA  &BITS(&NDX)                                              22580002
         AIF   (&ISHIFT EQ 0 OR &ISHIFT EQ 4).ALIGN                     22630002
.ERR     MNOTE '**DATA NOT ALLIGNED ON HEX OR BYTE BOUNDARY**'          22680002
         MEXIT                                                          22730002
.ALIGN   ANOP                                                           22780002
&CODE    SETA  1                                                        22830002
         AIF   (&SUB EQ 1).SUBA                                         22880002
&K       SETA  K'&SYSLIST(3)                                            22930002
         AGO   .SUBB                                                    22980002

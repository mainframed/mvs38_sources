         MEXIT                                                          23280002
.OK      AIF   (&SUB EQ 1).SUBC                                         23330002
         AIF   ('&SYSLIST(3)' EQ '').USEDEFN                            23380002
&LETTER  SETC  '&SYSLIST(3)'(&K,1)                                      23430002
&S       SETC  '&SYSLIST(3)'(1,&K-1)                                    23480002
         AGO   .SUBD                                                    23530002
.SUBC    ANOP                                                           23580002
         AIF   ('&SYSLIST(&OP,3)' EQ '').USEDEFN                        23630002
&LETTER  SETC  '&SYSLIST(&OP,3)'(&K,1)                                  23680002
&S       SETC  '&SYSLIST(&OP,3)'(1,&K-1)                                23730002
.SUBD    ANOP                                                           23780002
&BYTESP  SETA  &S                                                       23830002
         AIF   ('&LETTER' EQ 'B').BOTH                                  23880002

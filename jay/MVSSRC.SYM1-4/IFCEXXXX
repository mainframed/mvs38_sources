         SPACE 2                                                        00380002
         MACRO                                                          00430002
&NAME    DSGEN                                                          00480002
.*                            THIS MACRO GENERATES ONE DSECT WORD       00530002
.*                                 FORMATTED FOR THE SUBJECT LOG RECORD 00580002
.*                                 EACH OPERAND HAS TWO SUB LISTS. THE  00630002
.*                                 FIRST IS THE NAME OF THE LOG ITEM    00680002
.*                                 WHILE THE SECOND IS THE # OF BITS TO 00730002
.*                                 ALLOCATE FOR THIS ITEM. ITEMS MUST   00780002
.*                                 BE DEFINED CONTIGUOUSLY WITHOUT GAPS 00830002
.*                                 UNUSED BIT GROUPS MUST BE GIVEN      00880002
.*                                 DUMMY NAMES. THE NAMES PROVIDED FOR  00930002
.*                                 THE LOG ITEMS MAY THEN BE USED IN    00980002
.*                                 THE CONVERSION MACRO INSTRUCTIONS.   01030002
         GBLC &ITEM(1000)                                               01080002
         GBLA &ITEMNBR,&BITS(1000),&SHIFT(1000)                         01130002
         LCLB  &ONLY1,&CTIND                                            01180002
         LCLA  &START1,&START2,&END1,&END2                              01230002
         LCLC  &SYMBOL,&EQUSYM                                          01280002
         LCLA  &OP,&COUNT,&BYTE,&EQUCNT                                 01330002
         ACTR  100000                                                   01380002
.START   AIF   (N'&SYSLIST(&OP+1) LT 3).LOOP                            01430002
         AIF   ('&SYSLIST(&OP+1,3)' EQ 'EQU').LOOP     TEST FOR EQU     01480002
  AIF   ('&SYSLIST(&OP+1,3)' EQ '' OR '&SYSLIST(&OP+1,4)' EQ '').ERR1   01530002
         AIF   ('&SYSLIST(&OP+1,3)' GT '&SYSLIST(&OP+1,4)').ERR3        01580002
&START1  SETA  &SYSLIST(&OP+1,3)        GET START COUNT OF 1            01630002
&START2  SETA  &SYSLIST(&OP+1,4)        GET FIRST END COUNT             01680002
         AGO   .SET2A                   BRANCH AROUND                   01730002
.ERR1    MNOTE '**FIRST SYMBOL CONCATENATION INCOMPLETE**'              01780002
         MEXIT                                                          01830002
.SET2A   AIF   ('&SYSLIST(&OP+1,5)' EQ ''AND '&SYSLIST(&OP+1,6)' EQ '' X01880002
               ).SET2                   TEST FOR NO SECOND              01930002
  AIF   ('&SYSLIST(&OP+1,5)' EQ '' OR '&SYSLIST(&OP+1,6)' EQ '').ERR2   01980002
         AIF   ('&SYSLIST(&OP+1,5)' GT '&SYSLIST(&OP+1,6)').ERR3        02030002
&END1    SETA  &SYSLIST(&OP+1,5)        GET START COUNT OF 2            02080002
&END2    SETA  &SYSLIST(&OP+1,6)        GET END COUNT OF 2              02130002
         AGO   .LOOP                                                    02180002
.SET2    ANOP                                                           02230002
&ONLY1   SETB  1                        INDICATE ONLY ONE CONCATENATION 02280002
         AGO   .LOOP                                                    02330002
.ERR2    MNOTE '**SECOND SYMBOL CONCATENATION INCOMPLETE**'             02380002
         MEXIT                                                          02430002
.ERR3    MNOTE '**START GREATER THAN END CONCATENATION**'               02480002
         MEXIT                                                          02530002
.LOOP    ANOP                                                           02580002
&OP      SETA  &OP+1                                                    02630002
         AIF   (&OP GT N'&SYSLIST).DONE      TEST IF ALL DONE           02680002
&ITEMNBR SETA  &ITEMNBR+1                                               02730002
         AIF   ('&SYSLIST(&OP,2)' NE '').OK1                            02780002
         MNOTE '**NULL BIT COUNT FIELD**'                               02830002
         MEXIT                                                          02880002
.OK1     ANOP                                                           02930002
&SYMBOL  SETC  '&SYSLIST(&OP,1)'                                        02980002
         AIF   ('&SYMBOL' NE '').OK                                     03030002
*              *****     LOG ITEM SYMBOL NOT PROVIDED     *****         03080002
         AIF   ('&SYSLIST(&OP,3)' NE 'EQU').CNT                         03130002
         AGO   .START                   GET NEXT                        03180002
.OK      AIF   (N'&SYSLIST(&OP) LT 3).NO3    TEST FOR OLD FORMAT        03230002
         AIF   ('&SYSLIST(&OP,3)' EQ 'EQU').EQU   TEST FOR EQU          03280002
         AIF   (&ONLY1).OK2             TEST FOR ONLY ONE CONCAT        03330002
&SYMBOL  SETC  '&SYMBOL.&START1.&END1'       GET 1 AND 2 CONCAT         03380002
         AGO   .NO3                     BRANCH AROUND                   03430002
.OK2     ANOP                                                           03480002
&SYMBOL  SETC  '&SYMBOL.&START1'        GET 1 CONCATENATED              03530002
.NO3     ANOP                                                           03580002
&SYMBOL  EQU   *+&BYTE                                                  03630002
&ITEM(&ITEMNBR) SETC '&SYMBOL'          GET LABEL IN GLOBAL             03680002
&BITS(&ITEMNBR) SETA &SYSLIST(&OP,2)                                    03730002
&SHIFT(&ITEMNBR) SETA &COUNT-&BYTE*8                                    03780002
.CNT     ANOP                                                           03830002
&CTIND   SETB  1                        SET COUNT INDICATOR             03880002
&COUNT   SETA  &COUNT+&SYSLIST(&OP,2)                                   03930002
&BYTE    SETA  &COUNT/8                                                 03980002
         AIF   (N'&SYSLIST(&OP) LT 3).START  TEST FOR OLD FORMAT        04030002
         AIF   (&ONLY1).TSTST           TEST FOR ONLY ONE CONCAT        04080002
         AIF   (&END1 EQ &END2).TSTST   TEST FOR SECOND END             04130002
&END1    SETA  &END1+1                  INCREMENT COUNT                 04180002
&OP      SETA  &OP-1                    DECREMENT INCREMENT             04230002
         AGO   .LOOP                    GENERATE NEXT                   04280002
.TSTST   ANOP                                                           04330002
         AIF   (&START1 EQ &START2).LPDONE   TEST FOR END               04380002
         AIF   (&ONLY1).NSET1           TEST FOR ONLY ONE CONCAT        04430002
&END1    SETA  &SYSLIST(&OP,5)          RE-INIT SECOND START COUNT      04480002
.NSET1   ANOP                                                           04530002
&OP      SETA  &OP-1                    DECREMENT INDEX                 04580002
&START1  SETA  &START1+1                INCREMENT FIRST COUNT           04630002
         AGO   .LOOP                    GO DO NEXT                      04680002
.LPDONE  ANOP                                                           04730002
&ONLY1   SETB  0                        CLEAR INDICATOR                 04780002
         AGO   .START                   GO TEST FOR MORE SUB-LISTS      04830002
.DONE    AIF   (&COUNT-&COUNT/8*8 NE 0).ERR  TEST FOR MULTIPLE OF EIGHT 04880002
         AIF   (&CTIND EQ 0).END        NO DS IF NO COUNT               04930002
&COUNT   SETA  &COUNT/8                 GET NUMBER OF BYTES             04980002
&NAME    DS    &COUNT.C                                                 05030002
.END     MEXIT                                                          05080002
.ERR     MNOTE '**BIT COUNT FLDS DO NOT TOTAL MULTIPLE OF 8**'          05130002
         MEXIT                                                          05180002
.EQU     AIF   ('&SYSLIST(&OP,4)' NE '').EQU1     TEST FOR NULL LABEL   05230002
.ERR4    MNOTE '**EQU OPERAND OMITTED OR NOT FOUND**'                   05280002
         MEXIT                                                          05330002
.EQU1    AIF   (&SYSLIST(&OP,2) LT 16).EQU2  COUNT OVER FIFTEEN         05380002
*****               THE FOLLOWING STATEMENT                *****        05430002
*****     NUMBER OF BITS EXCEED 15 - DO NOT USE AS BIN     *****        05480002
.EQU2    ANOP                                                           05530002
&EQUCNT  SETA  0                        CLEAR COUNTER                   05580002
&EQUSYM  SETC  '&SYSLIST(&OP,4)'        GET EQU OPERAND                 05630002
.EQULOP  ANOP                                                           05680002
&EQUCNT  SETA  &EQUCNT+1                INCREMENT INDEX                 05730002
         AIF   ('&ITEM(&EQUCNT)' EQ '&EQUSYM').EQUOUT  LABEL FOUND?     05780002
         AIF   (&EQUCNT EQ &ITEMNBR).ERR4    ALL TESTED?                05830002
         AGO   .EQULOP                  NO-GET NEXT                     05880002
.EQUOUT  ANOP                                                           05930002
&ITEM(&ITEMNBR) SETC '&SYMBOL'          GET LABEL NAME                  05980002
&BITS(&ITEMNBR) SETA &SYSLIST(&OP,2)    GET NUMBER OF BITS              06030002
&SHIFT(&ITEMNBR) SETA &SHIFT(&EQUCNT)   GET SHIFT COUNT                 06080002
&SYMBOL  EQU   &EQUSYM                  THIS SYMBOL EQUATED             06130002
         AGO   .START                   RETURN FOR NEXT                 06180002
         MEND                                                           06230002
         EJECT                                                          06280002
         MACRO                                                          06330002
         LINEND                                                         06380002
.*                            THIS MACRO IS USED ONLY BY THE LINE       06430002
.*                                 MACRO TO SET THE NUMBER OF ENTRIES   06480002
.*                                 ASSOCIATED WITH EACH LINE.           06530002
         GBLA  &NRENTS                                                  06580002
         GBLC &NRENTSX                                                  06630002
         AIF   (&NRENTS EQ 0).NORENTS   TEST FOR NO COUNT               06680002
         ORG   &NRENTSX                                                 06730002
         DC    FL1'&NRENTS'                                             06780002
         ORG                                                            06830002
&NRENTS  SETA  0                                                        06880002
.NORENTS ANOP                                                           06930002
B&SYSNDX EQU   *                                                        06980002
&NRENTSX SETC  'B&SYSNDX'                                               07030002
         MEND                                                           07080002
         EJECT                                                          07130002
         MACRO                                                          07180002
         BIN                                                            07230002
.*                                 &SYSLIST(&OP,1) - NAME OF LOG ITEM   07280002
.*                                      RECORD DSECT                    07330002
.*                                 $SYSLIST(&OP,2) - NBR OF SPACES      07380002
.*                                      TO SKIP ON MULTIPLE             07430002
.*                                      PRINT OR NBR OF SPACES          07480002
.*                                      TO BACK UP FROM COLUMN FOR      07530002
.*                                      START OF PRINTING. THE LETTER   07580002
.*                                      B PRECEEDS THE NUMBER (B5) IF   07630002
.*                                      THE LATTER IS DESIRED. OMISSION 07680002
.*                                      OF THIS OPERAND FORCES OPTION 2 07730002
.*                                      WITH BACKUP SPACING EQUAL TO    07780002
.*                                 THE NUMBER OF BITS IN THE ITEM       07830002
.*                                 DEFINITION (VIA MACRO 'DSGEN').      07880002
.*                            A LIMIT OF 15 BITS IS IMPOSED FOR THE     07930002
.*                                 TOTAL NUMBER OF BITS TO PRINT.       07980002
         GBLA  &INITIAL                                                 08030002
         GBLB  &DIAGTST,&DIAGSET,&DIAGLIN                               08080002
         GBLC &ITEM(1000)                                               08130002
         GBLA &ITEMNBR,&BITS(1000),&SHIFT(1000),&NRENTS                 08180002
         GBLC  &DSECT                                                   08230002
         LCLB  &B,&SUB                                                  08280002
         LCLA  &IBITS,&NDX,&ISHIFT,&K                                   08330002
         LCLA  &TEMP,&T,&OP                                             08380002
         LCLC  &S                                                       08430002
         AIF   (&DIAGTST EQ 0).NOMSG                                    08480002
         AIF   (&INITIAL EQ 0).NOMSG                                    08530002
         AIF   (&DIAGLIN EQ 1).NOMSG                                    08580002
         MNOTE '**NO LINE ASSOCIATED WITH THIS DATA**'                  08630002
*****THE FOLLOWING GENERATED FOR TEST PURPOSES*****                     08680002
*        LINE                                                           08730002
         LINE                                                           08780002
.NOMSG   ANOP                                                           08830002
         AIF   (K'&SYSLIST(1) EQ K'&SYSLIST(1,1)).LOOP                  08880002
&SUB     SETB  1                                                        08930002
.LOOP    ANOP                                                           08980002
&OP      SETA  &OP+1                                                    09030002
.LOOP1   ANOP                                                           09080002
&NDX     SETA  &NDX+1                                                   09130002
         AIF   ('&ITEM(&NDX)' EQ '&SYSLIST(&OP,1)').FOUND               09180002
         AIF   (&NDX LT &ITEMNBR).LOOP1                                 09230002
         MNOTE '**LOG ITEM NOT DEFINED**'                               09280002
         MEXIT                                                          09330002
.FOUND   ANOP                                                           09380002
&ISHIFT  SETA  &SHIFT(&NDX)                                             09430002
&IBITS   SETA  &BITS(&NDX)                                              09480002
&T       SETA  &IBITS-1                                                 09530002
&B       SETB  0                                                        09580002
         AIF   (&IBITS GT 0 AND &IBITS LT 16).SUB0                      09630002
         MNOTE '**NO. OF BITS OUTSIDE RANGE OF 1-15**'                  09680002
         MEXIT                                                          09730002
.SUB0    AIF   (&SUB EQ 1).SUBA                                         09780002
         AIF   ('&SYSLIST(2)' EQ '').BACKUP                             09830002
&K       SETA  K'&SYSLIST(2)                                            09880002
&B       SETB  ('&SYSLIST(2)'(1,1) EQ 'B')                              09930002
&S       SETC  '&SYSLIST(2)'(&B+1,&K-&B)                                09980002
         AGO   .SUBB                                                    10030002
.SUBA    ANOP                                                           10080002
         AIF   ('&SYSLIST(&OP,2)' EQ '').BACKUP                         10130002
&K       SETA  K'&SYSLIST(&OP,2)                                        10180002
&B       SETB  ('&SYSLIST(&OP,2)'(1,1) EQ 'B')                          10230002
&S       SETC  '&SYSLIST(&OP,2)'(&B+1,&K-&B)                            10280002
.SUBB    ANOP                                                           10330002
&T       SETA  &S                                                       10380002
.BACKUP  ANOP                                                           10430002
&NRENTS  SETA  &NRENTS+1                                                10480002
&TEMP    SETA  &ISHIFT                                                  10530002
         AIF   (&IBITS NE 1 OR &B NE 0).BIN2                            10580002
         DC    AL.12(&SYSLIST(&OP,1)-&DSECT),XL.4'&TEMP'                10630002
         AGO   .OUT                                                     10680002
.BIN2    AIF   ('&SYSLIST(&OP,2)' NE '').BIN3                           10730002
&B       SETB  1                                                        10780002
.BIN3    ANOP                                                           10830002
         DC    AL.12(&SYSLIST(&OP,1)-&DSECT),XL.4'C'                    10880002
         DC    BL.1'&B',AL.7(&T),AL.4(&IBITS),FL.4'&ISHIFT'             10930002
.OUT     ANOP                                                           10980002
&NDX     SETA  0                                                        11030002
         AIF   (&SUB EQ 0).OUTA                                         11080002
         AIF   (&OP LT N'&SYSLIST).LOOP                                 11130002
.OUTA    ANOP                                                           11180002
         MEND                                                           11230002
         EJECT                                                          11280002
         MACRO                                                          11330002
         LSTART                                                         11380002
.*                            THIS MACRO IS ISSUED WHEN THE LABEL AREA  11430002
.*                                 OF THE TABLE IS TO BEGIN.            11480002
         GBLA  &LABELNR                                                 11530002
         GBLC  &LBL(100)                                                11580002
         GBLC &LABELS(2000)                                             11630002
         GBLA  &LBLCTR(100)                                             11680002
         GBLB  &DIAGTST,&DIAGSET,&DIAGLIN                               11730002
         LCLA  &NDX,&CTR                                                11780002
         AIF   (&DIAGTST EQ 0).NODIAG                                   11830002
         AIF   (&DIAGSET EQ 0).NODIAG                                   11880002
         MNOTE '**NO LABEL ASSOCIATED WITH LAST SET**'                  11930002
***** THE FOLLOWING GENERATED FOR TEST PURPOSES *****                   11980002
*        LABEL '%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%'       12030002
               LABEL '%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%' 12080002
.NODIAG  ANOP                                                           12130002
         LINEND                                                         12180002
&CTR     SETA  1                                                        12230002
.LOOP    ANOP                                                           12280002
&NDX     SETA  &NDX+1                                                   12330002
         AIF   (&LBLCTR(&CTR) NE &NDX).NOLBL                            12380002
&LBL(&CTR) DC  C'&LABELS(&NDX)'                                         12430002
&CTR     SETA  &CTR+1                                                   12480002
         AGO   .LOOPCTL                                                 12530002
.NOLBL   ANOP                                                           12580002
         DC    C'&LABELS(&NDX)'                                         12630002
.LOOPCTL AIF   (&NDX LT &LABELNR).LOOP                                  12680002
         MEND                                                           12730002
         EJECT                                                          12780002
         MACRO                                                          12830002
&NAME    ROUTINE &LOGITEM,&RNAME                                        12880002
         GBLA  &NRENTS                                                  12930002
         GBLC  &DSECT,&CSECT                                            12980002
         GBLA  &INITIAL                                                 13030002
         GBLB  &DIAGTST,&DIAGSET,&DIAGLIN                               13080002
         AIF   (&DIAGTST EQ 0).NOMSG                                    13130002
         AIF   (&INITIAL EQ 0).NOMSG                                    13180002
         AIF   (&DIAGLIN EQ 1).NOMSG                                    13230002
         MNOTE '**NO LINE ASSOCIATED WITH THIS ROUTINE**'               13280002
*****THE FOLLOWING GENERATED FOR TEST PURPOSES*****                     13330002
*        LINE                                                           13380002
         LINE                                                           13430002
.NOMSG   ANOP                                                           13480002
&NAME    DC    AL.12(&LOGITEM-&DSECT),XL.4'D'                           13530002
         DC    AL2(&RNAME-&CSECT)                                       13580002
&NRENTS  SETA  &NRENTS+1                                                13630002
         MEND                                                           13680002
         EJECT                                                          13730002
         MACRO                                                          13780002
&NAME    LINE  &DATA,&LABEL,&SKIP=                                      13830002
.*                            THIS MACRO ESTABLISHES THE MARGINS OR     13880002
.*                                 COLUMN SETTINGS FOR EACH LINE. IT    13930002
.*                                 GENERATES NECESSARY CARRIAGE CONTROL 13980002
.*                                 ENTRIES. LASTLY IT DETERMINES THE    14030002
.*                                 NUMBER OF ENTRIES ASSOCIATED WITH    14080002
.*                                 THE PREVIOUS LINE AND STORES THAT #  14130002
.*                                 INTO THE APPROPRIATE BYTE.           14180002
.*                            &DATA(1) - CHARACTER POSITION TO BEGIN    14230002
.*                                 DATA.                                14280002
.*                            &DATA(2) - # OF SPACES BETWEEN SUCCESSIVE 14330002
.*                                 DATA COLUMN STARTING CHARACTERS.     14380002
.*                            &LABEL(1) - CHAR. POS. TO BEGIN LABELS.   14430002
.*                            &LABEL(2) - # SPACES BETWEEN LINE'S LABEL 14480002
.*                                 STARTING CHARACTERS.                 14530002
.*                            &SKIP= - # OF LINES TO ADVANCE CARRIAGE   14580002
.*                                 TO BEGIN NEXT PRINTED LINE.          14630002
.*                            NEW ENTRIES ARE GENERATED ONLY WHEN       14680002
.*                                 NECESSARY. OMISSION OF ITEMS MEANS   14730002
.*                                 THEY HAVE NOT CHANGED SINCE PREVIOUS 14780002
.*                                 SETTINGS. A LINE MACRO IS REQUIRED   14830002
.*                                 FOR EACH PRINTED LINE. IF NO SETTING 14880002
.*                                 CHANGES ARE REQUIRED ALL OPERANDS    14930002
.*                                 ARE OMITTED IN WHICH CASE ONLY THE   14980002
.*                                 # OF ENTRIES FOR THE PREVIOUS LINE   15030002
.*                                 IS COMPUTED. THIS MACRO MUST BE THE  15080002
.*                                 FIRST MACRO INSTRUCTION ISSUED FOR   15130002
.*                                 THE SUBJECT LINE                     15180002
         GBLA  &DB,&DT,&LB,&LT,&NRENTS                                  15230002
         GBLA  &LASTCC,&INITIAL                                         15280002
         GBLC  &NRENTSX                                                 15330002
         GBLC  &CARRCON                                                 15380002
         GBLB  &TEST                                                    15430002
         GBLB  &DIAGTST,&DIAGSET,&DIAGLIN                               15480002
         LCLA  &S                                                       15530002
         AIF   (&INITIAL EQ 0 OR &DIAGTST EQ 0).DIAGSET                 15580002
         AIF   (&DIAGSET EQ 0).DIAGSET                                  15630002
         MNOTE '**NO LABEL ASSOCIATED WITH LAST SET**'                  15680002
***** THE FOLLOWING GENERATED FOR TEST PURPOSES *****                   15730002
*        LABEL '%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%'       15780002
               LABEL '%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%' 15830002
.DIAGSET ANOP                                                           15880002
&DIAGSET SETB  1                                                        15930002
&DIAGLIN SETB  1                                                        15980002
&NAME    DS    0C                                                       16030002
         AIF   (&INITIAL EQ 0).FIRSTLN                                  16080002
         LINEND                                                         16130002
         AGO   .NTFIRST                                                 16180002
.FIRSTLN ANOP                                                           16230002
&NRENTSX SETC  'N&SYSNDX'                                               16280002
&INITIAL SETA  1                                                        16330002
N&SYSNDX EQU   *                                                        16380002
.NTFIRST ANOP                                                           16430002
         DC    AL1(0)                                                   16480002
         AIF   ('&DATA(1)' EQ '').CKDTABS                               16530002
         AIF   (&DATA(1) EQ &DB).CKDTABS                                16580002
&DB      SETA  &DATA(1)                                                 16630002
         AGO   .AA                                                      16680002
.CKDTABS AIF   ('&DATA(2)' EQ '').NCDATA                                16730002
         AIF   (&DATA(2) EQ &DT).NCDATA                                 16780002
         AGO   .CC                                                      16830002
.AA      AIF   ('&DATA(2)' EQ '').BB                                    16880002
         AIF   (&DATA(2) EQ &DT).BB                                     16930002
.CC      ANOP                                                           16980002
&DT      SETA  &DATA(2)                                                 17030002
.BB      DC    XL2'FFFE'                                                17080002
         DC    FL1'&DB'                                                 17130002
         DC    FL1'&DT'                                                 17180002
&NRENTS  SETA  &NRENTS+1                                                17230002
.NCDATA  ANOP                                                           17280002
         AIF   ('&LABEL(1)' EQ '').CKLTABS                              17330002
         AIF   (&LABEL(1) EQ &LB).CKLTABS                               17380002
&LB      SETA  &LABEL(1)                                                17430002
         AGO   .AAA                                                     17480002
.CKLTABS AIF   ('&LABEL(2)' EQ '').NCLABEL                              17530002
         AIF   (&LABEL(2) EQ &LT).NCLABEL                               17580002
         AGO   .CCC                                                     17630002
.AAA     AIF   ('&LABEL(2)' EQ '').BBB                                  17680002
         AIF   (&LABEL(2) EQ &LT).BBB                                   17730002
.CCC     ANOP                                                           17780002
&LT      SETA  &LABEL(2)                                                17830002
.BBB     DC    XL2'FFFF'                                                17880002
         DC    FL1'&LB'                                                 17930002
         DC    FL1'&LT'                                                 17980002
&NRENTS  SETA  &NRENTS+1                                                18030002
.NCLABEL ANOP                                                           18080002
         AIF   ('&SKIP' NE '').CONT                                     18130002
         AIF   (&TEST EQ 0).NOCHANG                                     18180002
         AGO   .DEFALTA                                                 18230002
.CONT    ANOP                                                           18280002
         AIF   (&TEST EQ 0).AB                                          18330002
         AIF   ('&SKIP' EQ 'E').AB                                      18380002
&S       SETA  &SKIP                                                    18430002
.DEFAULT ANOP                                                           18480002
         AIF   (&SKIP EQ &LASTCC).DEFALTA                               18530002
         ORG   &CARRCON+3                                               18580002
         DC    AL.3(0),AL.2(&S),AL.3(1)                                 18630002
         ORG                                                            18680002
&LASTCC  SETA  &S                                                       18730002
.DEFALTA ANOP                                                           18780002
&TEST    SETB  0                                                        18830002
         AGO   .DD                                                      18880002
.AB      AIF   ('&SKIP' NE 'E').ABB                                     18930002
A&SYSNDX DC    X'FFFB8B00'              EJECT                           18980002
         AGO   .ABC                                                     19030002
.ABB     AIF   (&SKIP EQ &LASTCC).NOCHANG                               19080002
A&SYSNDX DC    XL2'FFFB'                                                19130002
         DC    AL.3(0),AL.2(&SKIP),AL.3(1)                              19180002
         DC    AL.3(0),AL.2(&SKIP),AL.3(1)                              19230002
&LASTCC  SETA  &SKIP                                                    19280002
&TEST    SETB  1                                                        19330002
.ABC     ANOP                                                           19380002
&CARRCON SETC  'A&SYSNDX'                                               19430002
&NRENTS  SETA  &NRENTS+1                                                19480002
.DD      ANOP                                                           19530002
.NOCHANG ANOP                                                           19580002
         MEND                                                           19630002
         EJECT                                                          19680002
         MACRO                                                          19730002
         HEX                                                            19780002
.*                            THIS MACRO FACILITATES GENERATION OF THE  19830002
.*                                 TABLE ENTRIES SPECIFYING HEX TYPE    19880002
.*                                 CONVERSIONS.                         19930002
.*                            &SYSLIST(&OP,1) - NAME OF DATA ITEM TO BE 19980002
.*                                 CONVERTED                            20030002
.*                            &SYSLIST(&OP,2) - # OF SPACES TO SKIP ON  20080002
.*                                 MULTIPLE PRINT OR NBR OF SPACES TO   20130002
.*                                 BACK UP FROM COLUMN FOR START OF     20180002
.*                                 PRINTING. THE LETTER B PRECEEDS THE  20230002
.*                                 NUMBER (B8) IF THE LATTER OPTION IS  20280002
.*                                 DESIRED. OMISSION OF THIS OPERAND    20330002
.*                                 FORCES OPTION 2 WITH BACKUP SPACING  20380002
.*                                 EQUAL TO THE NUMBER OF BYTES SPECI-  20430002
.*                                 FIED BY THE NEXT OPERAND (TWICE THIS 20480002
.*                                 NUMBER IF SUFFIX IS EXPLICITLY OR    20530002
.*                                 IMPLICITLY B).                       20580002
.*                            &SYSLIST(&OP,3) - NUMBER OF BYTES TO      20630002
.*                                 PROCESS. THIS NUMBER IS SUFFIXED     20680002
.*                                 WITH AN L,R, OR B TO STIPULATE       20730002
.*                                 PRINTING OF THE LEFT HALF, RIGHT     20780002
.*                                 HALF, OR BOTH HALVES OF THE BYTE     20830002
.*                                 RESPECTIVELY. OMISSION OF THIS       20880002
.*                                 OPERAND DEFAULTS TO B WITH THE NUM-  20930002
.*                                 BER OF BYTES EQUAL TO THE ITEM LNGTH 20980002
.*                            A NEW ENTRY IS GENERATED ONLY WHEN NECES- 21030002
.*                                 SARY. THIS MACRO MUST BE THE FIRST   21080002
.*                                 MACRO INSTRUCTION ISSUED FOR THE     21130002
.*                                 SUBJECT LINE(S).                     21180002

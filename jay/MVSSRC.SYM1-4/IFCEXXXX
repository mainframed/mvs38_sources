*                                                                       53550002
*        SET HEX -PRINTS 1 CHAR HEX                                     53640002
*                                                                       53730002
SETHEX   STM   R5,R8,XSAVE             SAVE REGS                        53820002
         LA    R8,CHARS                CHARACTER TABLE POINTER          53910002
         IC    R7,L0(R4)               PICK UP CHARACTER                54000002
         SLL   R7,L24                  MOVE TO HI R7                    54090002
         SR    R6,R6                   CLEAR REGS                       54180002
         SLDL  R6,L4                   MOVE FOUR BITS                   54270002
         AR    R6,R8                   ADD OFFSET AND POINTER           54360002
         MVC   L0(L1,R3),L0(R6)        MOVE CHARACTER TO LINE           54450002
         SR    R6,R6                   CLEAR AGAIN                      54540002
         SLDL  R6,L4                   MOVE LAST 4 BITS                 54630002
         AR    R6,R8                   COMPUTE OFFSET                   54720002
         MVC   L1(L1,R3),L0(R6)        MOVE LAST CHARACTER              54810002
         B     RETURN                                                   54900002
*                                                                       54990002
*        TEST TO SEE IF RECORD IS IN AR-149 FORMAT                      55080002
*                                                                       55170002
SETCONV  STM   R5,R8,XSAVE             SAVE REGS                 S21926 55260002
         TM    L0(R4),HEX02            HAS RECORD BEEN CONVERTED S21926 55350002
         BO    SETCONV1                                          S21926 55440002
         SR    R15,R15                 NO,CLEAR PRINT LINE       S21926 55530002
         B     RETURN                                            S21926 55620002
SETCONV1 IC    R5,SBCNTL               GET SENSE BYTE COUNT      S21926 55710002
         STC   R5,SBCNT                                          S21926 55800002
         MVC   SNB01(L2),SNB01+L17      MOVE SENSE OVER SDR      P4954  55890002
         B     RETURN                                            S21926 55980002
SBCNTL   DC    X'02'                                             S21926 56070002
ON       EQU   1                                                        56160002
HEX02    EQU   X'02'                                                    56250002
*                                                                       56340002
*        NO OP RETURN                                                   56430002
*                                                                       56520002
SKIPPER  BR    R14                                                      56610002
*                                                                       56700002
*        TO ADD SPACES ON LINE                                          56790002
*                                                                       56880002
SPACER1  LA    R3,L1(R3)                                                56970002
         BR    R14                                                      57060002
SPACER2  LA    R3,L2(R3)                                                57150002
         BR    14                                                       57240002
SPACER19 LA    R3,L19(R3)                                               57330002
         BR    R14                                                      57420002
*                                                                       57510002
*        DEFINE THE MODE IN WHICH TCAM RECORD WAS RECORDED              57600002
*                                                                       57690002
RECMODE  TM    L0(R4),HE0              TEST FOR UNRECOVERABLE           57780002
         BNZ   INTEST                  NO                               57870002
         MVC   L0(L15,R3),UNREC        MOVE IN MODE                     57960002
         BR    R14                     RETURN                           58050002
INTEST   TM    L0(R4),H80              TEST FOR UNINTENSIVE             58140002
         BNO   OVERTST                 NO                               58230002
         MVC   L0(L15,R3),INTEN        MOVE IN MODE                     58320002
         BR    R14                     RETURN                           58410002
OVERTST  TM    L0(R4),H40              TEST FOR OVERFLOW                58500002
         BNO   EODTST                  NO                               58590002
         MVC   L0(L15,R3),OVERF        MOVE IN MODE                     58680002
         BR    R14                     RETURN                           58770002
EODTST   MVC   L0(L15,R3),EOD          MOVE IN MODE                     58860002
         BR    R14                                                      58950002
*                                                                       59040002
*        CONVERT COUNTERS FROM HEX TO DECIMAL READY FOR PRINT           59130002
*                                                                       59220002
CONV1    STM   R5,R8,XSAVE             SAVE REGISTERS                   59310002
         XC    READYP(L8),READYP       CLEAR UNPACK BUFFER              59400002
         SR    R6,R6                   ZERO REG                         59490002
         MVC   READYP(L2),L0(R4)        ALIGN DATA             @YA02789 59580002
         LH    R6,READYP                LOAD DATA TO BE CONVERTED       59610002
         XC    READYP(L8),READYP        CLEAR BUCKET           @YA02789 59640002
         CVD   R6,READYP               CONVERT ERROR COUNTER TO DECI    59670002
         UNPK  READYP(L5),READYP+L5(L3)  UNPACK DECI COUNT              59760002
         OI    READYP+L4,CLRSGN        CLEAR SIGN                       59850002
         MVC   L0(L5,R3),READYP         MOVE COUNTER READY TO PRINT     59940002
         SR    R7,R7                                                    60030002
         IC    R7,L2(R4)                                                60120002
         CVD   R7,READYP                                                60210002
         UNPK  READYP(L5),READYP+L5(L3)                                 60300002
         OI      READYP+L4,CLRSGN                                       60390002
         MVC   L49(L4,R3),READYP+L1                                     60480002
         B     RETURN                                                   60570002
*                                                                       60660002
*        TEST FOR INITIAL SELECTION ERROR  ENTER 1 IF YES, 0 IF NO      60750002
*                                                                       60840002
RECMODE1 TM    L0(R4),H10              IS INITIAL SELECTION ON          60930002
         BZ    NOTON                   NO                               61020002
         MVC   L0(L1,R3),ONE           YES MOVE IN ONE                  61110002
         BR    R14                     RETURN                           61200002
NOTON    MVC   L0(L1,R3),ZERO          NO MOVE IN ZERO                  61290002
         BR    R14                     RETURN                           61380002
*                                                                       61470002
*        TEST FOR A GRAPHIC ERROR FROM A TERMINAL, OR AN END OF DAY OR  61560002
*        COUNTER OVER FLOW RECORD. IF YES LINE AND LABEL POINTERS GET   61650002
*        SET PAST THOSE THAT PRINT CCW AND SENSE.                       61740002
*                                                                       61830002
CHECKMOD STM   R5,R8,XSAVE                                              61920002
         TM    L1(R4),H21              IS THIS A GRAPHIC ERROR          62010002
         BO    RIGHTON                 YES                              62100002
         TM    L0(R4),NORCE            NO- IS THE EOD OR CNTR OVERFLOW  62190002
         BNZ   RIGHTON                 YES                              62280002
         B     RETURN                                                   62370002
*                                                                       62460002
RIGHTON  LA    R5,LINES1                                                62550002
         SH    R5,FOUR                 SUBTRACT FOUR FROM ADDRESS TO PR 62640002
         ST    R5,XR9PTR                                                62730002
         LA    R5,LSTLINE                                               62820002
         ST    R5,XLABELP                                               62910002
         B     RETURN                                                   63000002
*                                                                       63090002
*        TEST FOR GRAPHIC ERROR. IF YES FIND THE TYPE AND PRINT.        63180002
*        IF NO SKIP THAT PRINT LINE.                                    63270002
*                                                                       63360002
CHKGRAP  TM    L0(R4),H21              IS THIS A GRAPHIC ERROR          63450002
         BZ    NOTGRAP                 NO-DONT PRINT THIS LINE          63540002
         TM    L0(R4),H23              YES - IS IT THE CHARACTER /      63630002
         BNO   CH1                     NO                               63720002
         MVC   L0(L26,R3),TERM1        YES-MOVE IN ERROR INDICATION     63810002
         BR    R14                                                      63900002
CH1      TM    L0(R4),H25              IS IT THE CHARACTER S            63990002
         BNO   CH2                     NO                               64080002
         MVC   L0(L23,R3),TERM2        YES-MOVE IN ERROR INDICATION     64170002
         BR    R14                     RETURN                           64260002
CH2      TM    L0(R4),H29              IS IT THE CHARACTER U            64350002
         BNO   CH3                     NO                               64440002
         MVC   L0(L23,R3),TERM3        YES-MOVE IN ERROR INDICATION     64530002
         BR    R14                     RETURN                           64620002
CH3      MVC   L0(L24,R3),TERM4                                         64710002
         BR    R14                     RETURN                           64800002
NOTGRAP  SR    R15,R15                 ZERO REG 15 TO INDICATE NO PRINT 64890002
         BR    R14                     RETURN                           64980002
*                                                                       65070002
*                                                                       65160002
*                                                                       65250002
*        GET COMMUNICATION ADAPTER TYPE AND MOVE IN READY FOR PRINT     65340002
*                                                                       65430002
*                                                                       65520002
*                                                                       65610002
SETADPT  STM   R5,R8,XSAVE             SAVE REGISTERS                   65700002
         SR    R6,R6                   ZERO REG                         65790002
         CLI   L0(R4),H40              COMPARE FOR COMUNICATIONS TYPE   65880002
         BNE   NOPRTRET                NO-FIX TO NO PRINT               65970002
         IC    R6,L1(R4)               YES-PICK UP CHARACTER DEVICE TYP 66060002
         SRL   R6,L4                   DROP OFF LOW 4 BITS              66150002
         MH    R6,HALF13               COMPUTE INDEX                    66240002
         LA    R5,ADPTYPE              PICK UP TABLE ADDRESS            66330002
         AR    R5,R6                   SET ADDRESS TO LABEL             66420002
         MVC   L0(L13,R3),L0(R5)       MOVE IN TYPE DEVICE              66510002
         B     RETURN                                                   66600002
NOPRTRET SR    R15,R15                                                  66690002
         B     RETURN                                                   66780002
*                                                                       66870002
*        GET TERMINAL TYPE AND MOVE IN READY FOR PRINT                  66960002
*                                                                       67050002
SETTERM  STM   R5,R8,XSAVE             SAVE REGISTERS                   67140002
         CLI   L2(R4),H40              COMPARE FOR COMUNICATIONS TYPE   67230002
         BNE   NOPRTRET                NO-FIX TO NO PRINT OF TERM TYPE  67320002
         SR    R6,R6                   YES-CLEAR REGS                   67410002
         SR    R7,R7                                                    67500002
         IC    R7,L3(R4)               PICK UP CHARS FROM DEVICE TYPE   67590002
         IC    R6,L0(R4)               TO DETERMINE TERMINAL TYPE       67680002
         SRL   R7,L4                   DROP OFF LOW 4 BITS              67770002
         SLL   R7,L4                                                    67860002
         SLL   R6,L8                   MOVE IT OVER ONE BYTE            67950002
         OR    R6,R7                   SET UP KEY HALF                  68040002
         LA    R7,L15                  SET UP LOOP COUNT                68130002
         LA    R8,COMMTAB              SET PTR TO TERM TYPE TAB         68220002
COMLOOP  CH    R6,L0(R8)               IS IT THIS TYPE                  68310002
         BE    COMFND                  BR-YES                           68400002
         LA    R8,L10(R8)              UP POINTER TO NEXT               68490002
         BCT   R7,COMLOOP              LOOP                             68580002
         MVC   L0(L6,R3),ADPTYPE       MOVE IN NONE                     68670002
         B     RETURN                                                   68760002
COMFND   MVC   L0(L8,R3),L2(R8)        MOVE IN TERM NAME                68850002
         B     RETURN                                                   68940002
         DS    0F                                                       69030002

         GBLA &ITEMNBR,&BITS(1000),&SHIFT(1000),&NRENTS                 21280002
         GBLA  &INITIAL                                                 21330002
         GBLB  &DIAGTST,&DIAGSET,&DIAGLIN                               21380002
         LCLB  &B,&SUB                                                  21430002
         LCLA  &IBITS,&NDX,&ISHIFT,&K,&T,&CODE,&BYTESP,&OP              21480002
         LCLC  &S,&LETTER                                               21530002
         AIF   (&DIAGTST EQ 0).NOMSG                                    21580002
         AIF   (&INITIAL EQ 0).NOMSG                                    21630002
         AIF   (&DIAGLIN EQ 1).NOMSG                                    21680002

&CODE    SETA  2                                                        23980002
         AGO   .L                                                       24030002
.LEFT    ANOP                                                           24080002
&CODE    SETA  1                                                        24130002
         AGO   .L                                                       24180002
.USEDEFN ANOP                                                           24230002
&BYTESP  SETA  &IBITS/8                                                 24280002
.BOTH    AIF   (&ISHIFT NE 0).ERR                                       24330002
&CODE    SETA  3                                                        24380002
&T       SETA  &BYTESP*2-1                                              24430002
&S       SETC  '&T'                                                     24480002
.OK2     AIF   (&ISHIFT EQ 0).L                                         24530002
&CODE    SETA  2                                                        24580002
.L       ANOP                                                           24630002
&B       SETB  1                                                        24680002
         AIF   (&SUB EQ 1).SUBE                                         24730002
         AIF   ('&SYSLIST(2)' EQ '').BACKUP                             24780002
&K       SETA  K'&SYSLIST(2)                                            24830002
&B       SETB  ('&SYSLIST(2)'(1,1) EQ 'B')                              24880002
&S       SETC  '&SYSLIST(2)'(&B+1,&K-&B)                                24930002
         AGO   .SUBF                                                    24980002
.SUBE    ANOP                                                           25030002
         AIF   ('&SYSLIST(&OP,2)' EQ '').BACKUP                         25080002
&K       SETA  K'&SYSLIST(&OP,2)                                        25130002
&B       SETB  ('&SYSLIST(&OP,2)'(1,1) EQ 'B')                          25180002
&S       SETC  '&SYSLIST(&OP,2)'(&B+1,&K-&B)                            25230002
.SUBF    ANOP                                                           25280002
.BACKUP  ANOP                                                           25330002
&NRENTS  SETA  &NRENTS+1                                                25380002
         AIF   (&BYTESP GT 0 AND &BYTESP LT 16).SUB0                    25430002
         MNOTE '**NO. OF BYTES OUTSIDE RANGE OF 1-15**'                 25480002
         MEXIT                                                          25530002
.SUB0    AIF   (&SUB EQ 1).SUBG                                         25580002

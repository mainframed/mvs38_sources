.OUT     ANOP                                                           25930002
&NDX     SETA  0                                                        25980002
         AIF   (&SUB EQ 0).OUTA                                         26030002
         AIF   (&OP LT N'&SYSLIST).LOOP                                 26080002
.OUTA    ANOP                                                           26130002

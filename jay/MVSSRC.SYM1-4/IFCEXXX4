*  BIN MACRO     BIN MACRO     BIN MACRO     BIN MACRO     BIN MACRO  * 23120002
         SPACE 2                                                        23200002
         MACRO                                                          23280002
         BIN   &LOGITEM,&SKIP$OR                                        23360002
.*                                 &LOGITEM - NAME OF ITEM IN LOG       23440002
.*                                      RECORD DSECT                    23520002
.*                                 &SKIP$OR - NBR OF SPACES TO SKIP ON  23600002
.*                                 MULTIPLE PRINT OR NBR OF SPACES      23680002
.*                                      TO BACK UP FROM COLUMN FOR      23760002
.*                                      START OF PRINTING. THE LETTER   23840002
.*                                      B PRECEEDS THE NUMBER (B5) IF   23920002
.*                                      THE LATTER IS DESIRED. OMISSION 24000002
.*                                      OF THIS OPERAND FORCES OPTION 2 24080002
.*                                      WITH BACKUP SPACING EQUAL TO    24160002
.*                                 THE NUMBER OF BITS IN THE ITEM       24240002
.*                                 DEFINITION (VIA MACRO 'DSGEN').      24320002
.*                            A LIMIT OF 15 BITS IS IMPOSED FOR THE     24400002
.*                                 TOTAL NUMBER OF BITS TO PRINT.       24480002
         GBLC &ITEM(1000)                                               24560002
         GBLA &ITEMNBR,&BITS(1000),&SHIFT(1000),&NRENTS                 24640002
         GBLC  &DSECT                                                   24720002
         LCLB  &B                                                       24800002
         LCLA  &IBITS,&NDX,&ISHIFT,&K                                   24880002
         LCLA  &TEMP,&T                                                 24960002
         LCLC  &S                                                       25040002
.LOOP    ANOP                                                           25120002
&NDX     SETA  &NDX+1                                                   25200002
         AIF   ('&ITEM(&NDX)' EQ '&LOGITEM').FOUND                      25280002
         AIF   (&NDX LT &ITEMNBR).LOOP                                  25360002
         MNOTE *,'YOU BLEW IT'                                          25440002
         ROUTINE DUM,SKIPPER                                            25520002
         MEXIT                                                          25600002
.FOUND   ANOP                                                           25680002
&ISHIFT  SETA  &SHIFT(&NDX)                                             25760002
&IBITS   SETA  &BITS(&NDX)                                              25840002
&T       SETA  &IBITS-1                                                 25920002
&B       SETB  0                                                        26000002
         AIF   ('&SKIP$OR' EQ '').BACKUP                                26080002
&K       SETA  K'&SKIP$OR                                               26160002
&B       SETB  ('&SKIP$OR'(1,1) EQ 'B')                                 26240002
&S       SETC  '&SKIP$OR'(&B+1,&K-&B)                                   26320002
&T       SETA  &S                                                       26400002
.BACKUP  ANOP                                                           26480002
&NRENTS  SETA  &NRENTS+1                                                26560002
&TEMP    SETA  &ISHIFT                                                  26640002
A&SYSNDX DC    AL.12(&LOGITEM-&DSECT),XL.4'&TEMP'                       26720002
         AIF (&IBITS EQ 1 AND &B EQ 0).OUT                              26800002
         ORG   A&SYSNDX                                                 26880002
&B       SETB  1                                                        26960002
         DC    AL.12(&LOGITEM-&DSECT),XL.4'C'                           27040002
         DC    BL.1'&B',AL.7(&T),AL.4(&IBITS),FL.4'&ISHIFT'             27120002
.OUT     ANOP                                                           27200002
         MEND                                                           27280002
         EJECT                                                          27360002
*  HEX MACRO     HEX MACRO     HEX MACRO    HEXN MACRO     HEX MACRO  * 27440002
         SPACE 2                                                        27520002
         MACRO                                                          27600002
         HEX   &LOGITEM,&SKIP$OR,&BYTES                                 27680002
.*                            THIS MACRO FACILITATES GENERATION OF THE  27760002
.*                                 TABLE ENTRIES SPECIFYING HEX TYPE    27840002
.*                                 CONVERSIONS.                         27920002
.*                            &LOGITEM - NAME OF DATA ITEM TO BE        28000002
.*                                 CONVERTED                            28080002
.*                            &SKIP$OR - NUMBER OF SPACES TO SKIP ON    28160002
.*                                 MULTIPLE PRINT OR NBR OF SPACES TO   28240002
.*                                 BACK UP FROM COLUMN FOR START OF     28320002
.*                                 PRINTING. THE LETTER B PRECEEDS THE  28400002
.*                                 NUMBER (B8) IF THE LATTER OPTION IS  28480002
.*                                 DESIRED. OMISSION OF THIS OPERAND    28560002
.*                                 FORCES OPTION 2 WITH BACKUP SPACING  28640002
.*                                 EQUAL TO THE NUMBER OF BYTES SPECI-  28720002
.*                                 FIED BY THE NEXT OPERAND (TWICE THIS 28800002
.*                                 NUMBER IF SUFFIX IS EXPLICITLY OR    28880002
.*                                 IMPLICITLY B).                       28960002
.*                            &BYTES - NUMBER OF TOTAL INPUT BYTES TO   29040002
.*                                 PROCESS. THIS NUMBER IS SUFFIXED     29120002
.*                                 WITH AN L,R, OR B TO STIPULATE       29200002
.*                                 PRINTING OF THE LEFT HALF, RIGHT     29280002
.*                                 HALF, OR BOTH HALVES OF THE BYTE     29360002
.*                                 RESPECTIVELY. OMISSION OF THIS       29440002
.*                                 OPERAND DEFAULTS TO B WITH THE NUM-  29520002
.*                                 BER OF BYTES EQUAL TO THE ITEM LNGTH 29600002
.*                            A NEW ENTRY IS GENERATED ONLY WHEN NECES- 29680002
.*                                 SARY. THIS MACRO MUST BE THE FIRST   29760002
.*                                 MACRO INSTRUCTION ISSUED FOR THE     29840002
.*                                 SUBJECT LINE(S).                     29920002
         GBLC &ITEM(1000),&DSECT                                        30000002
         GBLA &ITEMNBR,&BITS(1000),&SHIFT(1000),&NRENTS                 30080002
         LCLB  &B                                                       30160002
         LCLA  &IBITS,&NDX,&ISHIFT,&K,&T,&CODE,&BYTESP                  30240002
         LCLC  &S,&LETTER                                               30320002
.LOOP    ANOP                                                           30400002
&NDX     SETA  &NDX+1                                                   30480002
         AIF   ('&ITEM(&NDX)' EQ '&LOGITEM').FOUND                      30560002
         AIF   (&NDX LT &ITEMNBR).LOOP                                  30640002
         MNOTE *,'LOG ITEM NOT DEFINED'                                 30720002
         MEXIT                                                          30800002
.FOUND   ANOP                                                           30880002
&ISHIFT  SETA  &SHIFT(&NDX)                                             30960002
&IBITS   SETA  &BITS(&NDX)                                              31040002
&CODE    SETA  1                                                        31120002
&K       SETA  K'&BYTES                                                 31200002
         AIF   (&IBITS-(&IBITS/4)*4 EQ 0).OK                            31280002
         MNOTE *,'BIT LENGTH SPECIFICATION INCONSISTENT WITH CONTROL'   31360002
         MEXIT                                                          31440002
.OK      AIF   ('&BYTES' EQ '').USEDEFN                                 31520002
&LETTER  SETC  '&BYTES'(&K,1)                                           31600002
&S       SETC  '&BYTES'(1,&K-1)                                         31680002
&BYTESP  SETA  &S                                                       31760002
         AIF   ('&LETTER' EQ 'B').BOTH                                  31840002
         AIF   (&ISHIFT EQ 0 OR &ISHIFT EQ 4).OK2                       31920002
.ERR     MNOTE *,'DATA NOT ALLIGNED ON HEX OR BYTE BOUNDARY'            32000002
         MEXIT                                                          32080002
.USEDEFN ANOP                                                           32160002
&BYTESP  SETA  &IBITS/8                                                 32240002
.BOTH    AIF   (&ISHIFT NE 0).ERR                                       32320002
&CODE    SETA  3                                                        32400002
&T       SETA  &BYTESP*2-1                                              32480002
&S       SETC  '&T'                                                     32560002
.OK2     AIF   (&ISHIFT EQ 0).L                                         32640002
&CODE    SETA  2                                                        32720002
.L       ANOP                                                           32800002
&B       SETB  1                                                        32880002
         AIF   ('&SKIP$OR' EQ '').BACKUP                                32960002
&K       SETA  K'&SKIP$OR                                               33040002
&B       SETB  ('&SKIP$OR'(1,1) EQ 'B')                                 33120002
&S       SETC  '&SKIP$OR'(&B+1,&K-&B)                                   33200002
.BACKUP  ANOP                                                           33280002
&NRENTS  SETA  &NRENTS+1                                                33360002
         DC    AL.12(&LOGITEM-&DSECT),XL.4'A'                           33440002
         DC    BL.1'&B',FL.7'&S',FL.4'&BYTESP',FL.4'&CODE'              33520002
.OUT     ANOP                                                           33600002
         MEND                                                           33680002
         MACRO                                                          33760002
         SPECIAL  &ROUTINE,&PARAM,&SPACE=                               33840002
.*                            THIS MACRO GENERATES A SPECIAL ROUTINE    33920002
.*                                 ENTRY TO BE PROCESSED BY THE SUM. M. 34000002
.*                            &ROUTINE - SYMBOL ON SPECIAL ROUTINE EWT. 34080002
.*                            &PARAM - AN 8 BIT BINARY REPRESENTATION   34160002
.*                                 TO OCCUPY THE 'MASK' FIELD BUT TO BE 34240002
.*                                 INTERPRETED AS AN ARGUMENT OF THE    34320002
.*                                 SPECIAL ROUTINE.                     34400002
.*                            &SPACE - THE NUMBER OF 2BYTE COUNTERS TO  34480002
.*                                 ALLOCATE FOR REQUIRED PROCESSING.    34560002
         GBLC  &CSECT                                                   34640002
         GBLA  &COUNTR                                                  34720002
         LCLA  &ADD                                                     34800002
         DC    AL1(128)                                                 34880002
         DC    BL.8'&PARAM'                                             34960002
         DC    AL2(&ROUTINE-&CSECT)                                     35040002
         DC    AL2(&COUNTR)                                             35120002
&ADD     SETA  1                                                        35200002
         AIF   ('&SPACE' EQ '').NORM                                    35280002
&ADD     SETA  &SPACE                                                   35360002
.NORM    ANOP                                                           35440002
&COUNTR  SETA  &COUNTR+&ADD                                             35520002
         MEND                                                           35600002
         MACRO                                                          35680002
         SUM   &LOGITEM,&MASK                                           35760002
.*                            THIS MACRO GENERATES A NORMAL EDIT ENTRY  35840002
.*                                 FOR PROCESSING BY THE SUMMARY MODULE 35920002
.*                            &LOGITEM - NAME OF DATA ITEM TO BE SUMMED 36000002
.*                            &MASK - BYTE MASK FOR BITS TO BE TESTED.  36080002
.*                                 THIS OPERAND SHOULD BE OMITTED FOR   36160002
.*                                 SINGLE BIT ITEMS IN WHICH CASE THE   36240002
.*                                 MASK WILL BE SET ACCORDING TO THE    36320002
.*                                 OFF BYTE BIT DISPLACEMENT OF THE     36400002
.*                                ITEM AS DEFINED BY THE MACRO 'DSGEN'  36480002
         GBLA  &COUNTR,&ITEMNBR,&BITS(1000),&SHIFT(1000)                36560002
         GBLC  &ITEM(1000),&DSECT                                       36640002
         LCLA  &IBITS,&NDX,&ISHIFT,&M,&BITNBR                           36720002
.LOOP    ANOP                                                           36800002
&NDX     SETA  &NDX+1                                                   36880002
         AIF   ('&ITEM(&NDX)' EQ '&LOGITEM').FOUND                      36960002
         AIF   (&NDX LT &ITEMNBR).LOOP                                  37040002
         MNOTE *,'LOGITEM NOT FOUND'                                    37120002
         MEXIT                                                          37200002
.FOUND   ANOP                                                           37280002
         DC    AL1(0)                                                   37360002
&ISHIFT  SETA  &SHIFT(&NDX)                                             37440002
&IBITS   SETA  &BITS(&NDX)                                              37520002
         AIF   ('&MASK' EQ '').FORM                                     37600002
         DC    B'&MASK'                                                 37680002
         AGO   .ITEM                                                    37760002
.FORM    ANOP                                                           37840002
&BITNBR  SETA  &ISHIFT-7                                                37920002
&BITNBR  SETA  0-&BITNBR                                                38000002
&M       SETA  1                                                        38080002
.LOOP2   ANOP                                                           38160002
         AIF   (&BITNBR EQ 0).GOTIT                                     38240002
&M       SETA  &M*2                                                     38320002
&BITNBR  SETA  &BITNBR-1                                                38400002
         AGO   .LOOP2                                                   38480002
.GOTIT   ANOP                                                           38560002
         DC    AL1(&M)                                                  38640002
.ITEM    DC    AL2(&LOGITEM-&DSECT)                                     38720002
         DC    AL2(&COUNTR)                                             38800002
&COUNTR  SETA  &COUNTR+1                                                38880002
         MEND                                                           38960002
         GBLC  &DSECT,&CSECT                                            39040002

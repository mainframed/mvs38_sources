         MNOTE 'LOG ITEM SYMBOL NOT PROVIDED'                           13840002
         AGO   .CNT                                                     13920002
.OK      ANOP                                                           14000002
         AIF   ('&SYMBOL'(1,1) EQ '''').CNT                *TEMP*       14080002
&SYMBOL  EQU   *+&BYTE                                                  14160002
&ITEM(&ITEMNBR)  SETC '&SYSLIST(&OP,1)'                                 14240002
&BITS(&ITEMNBR) SETA &SYSLIST(&OP,2)                                    14320002
&SHIFT(&ITEMNBR) SETA &COUNT-&BYTE*8                                    14400002
.CNT     ANOP                                                           14480002
&COUNT   SETA  &COUNT+&SYSLIST(&OP,2)                                   14560002
&BYTE    SETA  &COUNT/8                                                 14640002
         AIF (&OP LT N'&SYSLIST).LOOP                                   14720002
         AIF   (&COUNT NE 32).NOT32                                     14800002
&NAME    DS    4C                                                       14880002
         MEXIT                                                          14960002
.NOT32   AIF   (&COUNT NE 64).ERR                                       15040002
&NAME    DS    8C                                                       15120002
         MEXIT                                                          15200002
.ERR     MNOTE '**BIT COUNT FIELDS DO NOT TOTAL 32 OR 64**'             15280002
         MEND                                                           15360002
         MACRO                                                          15440002
         LINEND                                                         15520002

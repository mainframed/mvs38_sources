CONVREL  STM   R5,R8,XSAVE              SAVE WORK REGS                  53400002
         SR    R6,R6                    CLEAR R6                        53500002
         IC    R6,L0(R4)                PICK UP REL & SYS VALUES        53600002
         SRDL  R6,L5                    SAVE REL NO IN R7               53700002
        SLL   R6,L2                    MULTIPLY SYS VALUE BY 4          53800002
         L     R8,XLABELP               PICK UP LABEL POINTER           53900002
         LA    R6,RELTAB(R6)            INDEX INTO TABLE OF SYSTEMS     54000002
         MVC   L0(L4,R8),L0(R6)         MOVE SYSTEM NAME INTO LABEL     54100002
         SR    R6,R6                    CLEAR R6                        54200002
         SLDL  R6,L5                    RETREIVE REL NO.                54300002
         LTR   R6,R6                    IS IT ZERO                      54400002
         BZ    NOREL                    BR-YES                          54500002
         CVD   R6,WORK                  CONVERT IT                      54600002
         UNPK  L0(L2,R3),WORK+L6(L2)    UNPACK IT INTO PRINT BUFF       54700002
         MVZ   L1(L1,R3),L0(R3)         MOVE ON PROPER ZONE             54800002
RETURN   LM    R5,R8,XSAVE         RESTORE WORK REGISTERS               54900002
         BR    R14                 RETURN                               55000002

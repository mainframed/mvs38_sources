*  DSGEN MACRO     DSGEN MACRO     DSGEN MACRO     DSGEN MACRO        * 00100002
         SPACE 2                                                        00140002
         MACRO                                                          00180002
&NAME    DSGEN                                                          00220002
.*                            THIS MACRO GENERATES ONE DSECT WORD       00260002
.*                                 FORMATTED FOR THE SUBJECT LOG RECORD 00300002
.*                                 EACH OPERAND HAS TWO SUB LISTS. THE  00340002
.*                                 FIRST IS THE NAME OF THE LOG ITEM    00380002
.*                                 WHILE THE SECOND IS THE # OF BITS TO 00420002
.*                                 ALLOCATE FOR THIS ITEM. ITEMS MUST   00460002
.*                                 BE DEFINED CONTIGUOUSLY WITHOUT GAPS 00500002
.*                                 UNUSED BIT GROUPS MUST BE GIVEN      00540002
.*                                 DUMMY NAMES. THE NAMES PROVIDED FOR  00580002
.*                                 THE LOG ITEMS MAY THEN BE USED IN    00620002
.*                                 THE CONVERSION MACRO INSTRUCTIONS.   00660002
         GBLC &ITEM(1000)                                               00700002
         GBLA &ITEMNBR,&BITS(1000),&SHIFT(1000)                         00740002
         LCLB  &ONLY1,&CTIND                                            00780002
         LCLA  &START1,&START2,&END1,&END2                              00820002
         LCLC  &SYMBOL,&EQUSYM                                          00860002
         LCLA  &OP,&COUNT,&BYTE,&EQUCNT                                 00900002
         ACTR  100000                                                   00940002
.START   AIF   (N'&SYSLIST(&OP+1) LT 3).LOOP                            00980002
         AIF   ('&SYSLIST(&OP+1,3)' EQ 'EQU').LOOP     TEST FOR EQU     01020002
  AIF   ('&SYSLIST(&OP+1,3)' EQ '' AND '&SYSLIST(&OP+1,4)' EQ '').ERR1  01060002
  AIF   ('&SYSLIST(&OP+1,3)' EQ '' OR '&SYSLIST(&OP+1,4)' EQ '').ERR1   01100002
         AIF   ('&SYSLIST(&OP+1,3)' GT '&SYSLIST(&OP+1,4)').ERR3        01140002
&START1  SETA  &SYSLIST(&OP+1,3)        GET START COUNT OF 1            01180002
&START2  SETA  &SYSLIST(&OP+1,4)        GET FIRST END COUNT             01220002
         AGO   .SET2A                   BRANCH AROUND                   01260002
.ERR1    MNOTE '**FIRST SYMBOL CONCATENATION INCOMPLETE**'              01300002
         MEXIT                                                          01340002
.SET2A   AIF   ('&SYSLIST(&OP+1,5)' EQ ''AND '&SYSLIST(&OP+1,6)' EQ '' X01380002
               ).SET2                   TEST FOR NO SECOND              01420002
  AIF   ('&SYSLIST(&OP+1,5)' EQ '' OR '&SYSLIST(&OP+1,6)' EQ '').ERR2   01460002
         AIF   ('&SYSLIST(&OP+1,5)' GT '&SYSLIST(&OP+1,6)').ERR3        01500002
&END1    SETA  &SYSLIST(&OP+1,5)        GET START COUNT OF 2            01540002
&END2    SETA  &SYSLIST(&OP+1,6)        GET END COUNT OF 2              01580002
         AGO   .LOOP                                                    01620002
.SET2    ANOP                                                           01660002
&ONLY1   SETB  1                        INDICATE ONLY ONE CONCATENATION 01700002
         AGO   .LOOP                                                    01740002
.ERR2    MNOTE '**SECOND SYMBOL CONCATENATION INCOMPLETE**'             01780002
         MEXIT                                                          01820002
.ERR3    MNOTE '**START GREATER THAN END CONCATENATION**'               01860002
         MEXIT                                                          01900002
.LOOP    ANOP                                                           01940002
&OP      SETA  &OP+1                                                    01980002
         AIF   (&OP GT N'&SYSLIST).DONE      TEST IF ALL DONE           02020002
&ITEMNBR SETA  &ITEMNBR+1                                               02060002
         AIF   ('&SYSLIST(&OP,2)' NE '').OK1                            02100002
         MNOTE '**NULL BIT COUNT FIELD**'                               02140002
         MEXIT                                                          02180002
.OK1     ANOP                                                           02220002
&SYMBOL  SETC  '&SYSLIST(&OP,1)'                                        02260002
         AIF   ('&SYMBOL' NE '').OK                                     02300002
*              *****     LOG ITEM SYMBOL NOT PROVIDED     *****         02340002
         AIF   ('&SYSLIST(&OP,3)' NE 'EQU').CNT                         02380002
         AGO   .START                   GET NEXT                        02420002
.OK      AIF   (N'&SYSLIST(&OP) LT 3).NO3    TEST FOR OLD FORMAT        02460002
         AIF   ('&SYSLIST(&OP,3)' EQ 'EQU').EQU   TEST FOR EQU          02500002
         AIF   (&ONLY1).OK2             TEST FOR ONLY ONE CONCAT        02540002
&SYMBOL  SETC  '&SYMBOL.&START1.&END1'       GET 1 AND 2 CONCAT         02580002
         AGO   .NO3                     BRANCH AROUND                   02620002
.OK2     ANOP                                                           02660002
&SYMBOL  SETC  '&SYMBOL.&START1'        GET 1 CONCATENATED              02700002
.NO3     ANOP                                                           02740002
&SYMBOL  EQU   *+&BYTE                                                  02780002
&ITEM(&ITEMNBR) SETC '&SYMBOL'          GET LABEL IN GLOBAL             02820002
&BITS(&ITEMNBR) SETA &SYSLIST(&OP,2)                                    02860002
&SHIFT(&ITEMNBR) SETA &COUNT-&BYTE*8                                    02900002
.CNT     ANOP                                                           02940002
&CTIND   SETB  1                        SET COUNT INDICATOR             02980002
&COUNT   SETA  &COUNT+&SYSLIST(&OP,2)                                   03020002
&BYTE    SETA  &COUNT/8                                                 03060002
         AIF   (N'&SYSLIST(&OP) LT 3).START  TEST FOR OLD FORMAT        03100002
         AIF   (&ONLY1).TSTST           TEST FOR ONLY ONE CONCAT        03140002
         AIF   (&END1 EQ &END2).TSTST   TEST FOR SECOND END             03180002
&END1    SETA  &END1+1                  INCREMENT COUNT                 03220002
&OP      SETA  &OP-1                    DECREMENT INCREMENT             03260002
         AGO   .LOOP                    GENERATE NEXT                   03300002
.TSTST   ANOP                                                           03340002
         AIF   (&START1 EQ &START2).LPDONE   TEST FOR END               03380002
         AIF   (&ONLY1).NSET1           TEST FOR ONLY ONE CONCAT        03420002
&END1    SETA  &SYSLIST(&OP,5)          RE-INIT SECOND START COUNT      03460002
.NSET1   ANOP                                                           03500002
&OP      SETA  &OP-1                    DECREMENT INDEX                 03540002
&START1  SETA  &START1+1                INCREMENT FIRST COUNT           03580002
         AGO   .LOOP                    GO DO NEXT                      03620002
.LPDONE  ANOP                                                           03660002
&ONLY1   SETB  0                        CLEAR INDICATOR                 03700002
         AGO   .START                   GO TEST FOR MORE SUB-LISTS      03740002
.DONE    AIF   (&COUNT-&COUNT/8*8 NE 0).ERR  TEST FOR MULTIPLE OF EIGHT 03780002
         AIF   (&CTIND EQ 0).END        NO DS IF NO COUNT               03820002
&COUNT   SETA  &COUNT/8                 GET NUMBER OF BYTES             03860002
&NAME    DS    &COUNT.C                                                 03900002
.END     MEXIT                                                          03940002
.ERR     MNOTE '**BIT COUNT FLDS DO NOT TOTAL MULTIPLE OF 8**'          03980002
         MEXIT                                                          04020002
.EQU     AIF   ('&SYSLIST(&OP,4)' NE '').EQU1     TEST FOR NULL LABEL   04060002
.ERR4    MNOTE '**EQU OPERAND OMITTED OR NOT FOUND**'                   04100002
         MEXIT                                                          04140002
.EQU1    AIF   (&SYSLIST(&OP,2) LT 16).EQU2  COUNT OVER FIFTEEN         04180002
*****               THE FOLLOWING STATEMENT                *****        04220002
*****     NUMBER OF BITS EXCEED 15 - DO NOT USE AS BIN     *****        04260002
.EQU2    ANOP                                                           04300002
&EQUCNT  SETA  0                        CLEAR COUNTER                   04340002
&EQUSYM  SETC  '&SYSLIST(&OP,4)'        GET EQU OPERAND                 04380002
.EQULOP  ANOP                                                           04420002
&EQUCNT  SETA  &EQUCNT+1                INCREMENT INDEX                 04460002
         AIF   ('&ITEM(&EQUCNT)' EQ '&EQUSYM').EQUOUT  LABEL FOUND?     04500002
         AIF   (&EQUCNT EQ &ITEMNBR).ERR4    ALL TESTED?                04540002
         AGO   .EQULOP                  NO-GET NEXT                     04580002
.EQUOUT  ANOP                                                           04620002
&ITEM(&ITEMNBR) SETC '&SYMBOL'          GET LABEL NAME                  04660002
&BITS(&ITEMNBR) SETA &SYSLIST(&OP,2)    GET NUMBER OF BITS              04700002
&SHIFT(&ITEMNBR) SETA &SHIFT(&EQUCNT)   GET SHIFT COUNT                 04740002
&SYMBOL  EQU   &EQUSYM                  THIS SYMBOL EQUATED             04780002
         AGO   .START                   RETURN FOR NEXT                 04820002
         MEND                                                           04860002
         EJECT                                                          05100002
*  LINE MACRO     LINE MACRO     LINE MACRO     LINE MACRO            * 05200002
         SPACE 2                                                        05300002
         MACRO                                                          05400002
&NAME    LINE  &DATA,&LABEL,&SKIP=                                      05500002
.*                            THIS MACRO ESTABLISHES THE MARGINS OR     05600002
.*                                 COLUMN SETTINGS FOR EACH LINE. IT    05700002
.*                                 GENERATES NECESSARY CARRIAGE CONTROL 05800002
.*                                 ENTRIES. LASTLY IT DETERMINES THE    05900002
.*                                 NUMBER OF ENTRIES ASSOCIATED WITH    06000002
.*                                 THE PREVIOUS LINE AND STORES THAT #  06100002
.*                                 INTO THE APPROPRIATE BYTE.           06200002
.*                            &DATA(1) - CHARACTER POSITION TO BEGIN    06300002
.*                                 DATA.                                06400002
.*                            &DATA(2) - # OF SPACES BETWEEN SUCCESSIVE 06500002
.*                                 DATA COLUMN STARTING CHARACTERS.     06600002
.*                            &LABEL(1) - CHAR. POS. TO BEGIN LABELS.   06700002
.*                            &LABEL(2) - # SPACES BETWEEN LINE'S LABEL 06800002
.*                                 STARTING CHARACTERS.                 06900002
.*                            &SKIP= - # OF LINES TO ADVANCE CARRIAGE   07000002
.*                                 TO BEGIN NEXT PRINTED LINE.          07100002
.*                            NEW ENTRIES ARE GENERATED ONLY WHEN       07200002
.*                                 NECESSARY. OMISSION OF ITEMS MEANS   07300002
.*                                 THEY HAVE NOT CHANGED SINCE PREVIOUS 07400002
.*                                 SETTINGS. A LINE MACRO IS REQUIRED   07500002
.*                                 FOR EACH PRINTED LINE. IF NO SETTING 07600002
.*                                 CHANGES ARE REQUIRED ALL OPERANDS    07700002
.*                                 ARE OMITTED IN WHICH CASE ONLY THE   07800002
.*                                 # OF ENTRIES FOR THE PREVIOUS LINE   07900002
.*                                 IS COMPUTED. THIS MACRO MUST BE THE  08000002
.*                                 FIRST MACRO INSTRUCTION ISSUED FOR   08100002
.*                                 THE SUBJECT LINE                     08200002
         GBLA  &DB,&DT,&LB,&LT,&NRENTS                                  08300002
         GBLA  &LASTCC,&INITIAL                                         08400002
         GBLC  &NRENTSX                                                 08500002
         GBLC  &CARRCON                                                 08600002
         GBLB  &TEST                                                    08700002
         LCLA  &S                                                       08800002
&NAME    DS    0C                                                       08900002
         AIF   (&INITIAL EQ 0).FIRSTLN                                  09000002
         LINEND                                                         09100002
         AGO   .NTFIRST                                                 09200002
.FIRSTLN ANOP                                                           09300002
&NRENTSX SETC  'N&SYSNDX'                                               09400002
&INITIAL SETA  1                                                        09500002
N&SYSNDX EQU   *                                                        09600002
.NTFIRST ANOP                                                           09700002
         DC    AL1(0)                                                   09800002
         AIF   ('&DATA(1)' EQ '').CKDTABS                               09900002
         AIF   (&DATA(1) EQ &DB).CKDTABS                                10000002
&DB      SETA  &DATA(1)                                                 10100002
         AGO   .AA                                                      10200002
.CKDTABS AIF   ('&DATA(2)' EQ '').NCDATA                                10300002
         AIF   (&DATA(2) EQ &DT).NCDATA                                 10400002
         AGO   .CC                                                      10500002
.AA      AIF   ('&DATA(2)' EQ '').BB                                    10600002
         AIF   (&DATA(2) EQ &DT).BB                                     10700002
.CC      ANOP                                                           10800002
&DT      SETA  &DATA(2)                                                 10900002
.BB      DC    XL2'FFFE'                                                11000002
         DC    FL1'&DB'                                                 11100002
         DC    FL1'&DT'                                                 11200002
&NRENTS  SETA  &NRENTS+1                                                11300002
.NCDATA  ANOP                                                           11400002
         AIF   ('&LABEL(1)' EQ '').CKLTABS                              11500002
         AIF   (&LABEL(1) EQ &LB).CKLTABS                               11600002
&LB      SETA  &LABEL(1)                                                11700002
         AGO   .AAA                                                     11800002
.CKLTABS AIF   ('&LABEL(2)' EQ '').NCLABEL                              11900002
         AIF   (&LABEL(2) EQ &LT).NCLABEL                               12000002
         AGO   .CCC                                                     12100002
.AAA     AIF   ('&LABEL(2)' EQ '').BBB                                  12200002
         AIF   (&LABEL(2) EQ &LT).BBB                                   12300002
.CCC     ANOP                                                           12400002
&LT      SETA  &LABEL(2)                                                12500002
.BBB     DC    XL2'FFFF'                                                12600002
         DC    FL1'&LB'                                                 12700002
         DC    FL1'&LT'                                                 12800002
&NRENTS  SETA  &NRENTS+1                                                12900002
.NCLABEL ANOP                                                           13000002
         AIF   ('&SKIP' NE '').CONT                                     13100002
         AIF   (&TEST EQ 0).NOCHANG                                     13200002
&S       SETA  &LASTCC                                                  13300002
         AGO   .DEFAULT                                                 13400002
.CONT    ANOP                                                           13500002
         AIF   (&TEST EQ 0).AB                                          13600002
&S       SETA  &SKIP                                                    13700002
.DEFAULT ANOP                                                           13800002
         ORG   &CARRCON+3                                               13900002
         DC    AL.3(0),AL.2(&S),AL.3(1)                                 14000002
         ORG                                                            14100002
&LASTCC  SETA  &S                                                       14200002
&TEST    SETB  0                                                        14300002
         AGO   .DD                                                      14400002
.AB      AIF   (&SKIP EQ &LASTCC).NOCHANG                               14500002
A&SYSNDX DC    XL2'FFFB'                                                14600002
         DC    AL.3(0),AL.2(&SKIP),AL.3(1)                              14700002
         DC    FL1'9'                                                   14800002
&LASTCC  SETA  &SKIP                                                    14900002
&TEST    SETB  1                                                        15000002
&CARRCON SETC  'A&SYSNDX'                                               15100002
&NRENTS  SETA  &NRENTS+1                                                15200002
.DD      ANOP                                                           15300002
.NOCHANG ANOP                                                           15400002
         MEND                                                           15500002
         EJECT                                                          15600002
*  BIN MACRO     BIN MACRO     BIN MACRO     BIN MACRO     BIN MACRO  * 15700002
         SPACE 2                                                        15800002
         MACRO                                                          15900002
         BIN   &LOGITEM,&SKIP$OR                                        16000002
.*                                 &LOGITEM - NAME OF ITEM IN LOG       16100002
.*                                      RECORD DSECT                    16200002
.*                                 &SKIP$OR - NBR OF SPACES TO SKIP ON  16300002
.*                                 MULTIPLE PRINT OR NBR OF SPACES      16400002
.*                                      TO BACK UP FROM COLUMN FOR      16500002
.*                                      START OF PRINTING. THE LETTER   16600002
.*                                      B PRECEEDS THE NUMBER (B5) IF   16700002
.*                                      THE LATTER IS DESIRED. OMISSION 16800002
.*                                      OF THIS OPERAND FORCES OPTION 2 16900002
.*                                      WITH BACKUP SPACING EQUAL TO    17000002
.*                                 THE NUMBER OF BITS IN THE ITEM       17100002
.*                                 DEFINITION (VIA MACRO 'DSGEN').      17200002
.*                            A LIMIT OF 15 BITS IS IMPOSED FOR THE     17300002
.*                                 TOTAL NUMBER OF BITS TO PRINT.       17400002
         GBLC &ITEM(1000)                                               17500002
         GBLA &ITEMNBR,&BITS(1000),&SHIFT(1000),&NRENTS                 17600002
         GBLC  &DSECT                                                   17700002
         LCLB  &B                                                       17800002
         LCLA  &IBITS,&NDX,&ISHIFT,&K                                   17900002
         LCLA  &TEMP,&T                                                 18000002
         LCLC  &S                                                       18100002
.LOOP    ANOP                                                           18200002
&NDX     SETA  &NDX+1                                                   18300002
         AIF   ('&ITEM(&NDX)' EQ '&LOGITEM').FOUND                      18400002
         AIF   (&NDX LT &ITEMNBR).LOOP                                  18500002
         MNOTE *,'YOU BLEW IT'                                          18600002
         ROUTINE DUM,SKIPPER                                            18700002
         MEXIT                                                          18800002
.FOUND   ANOP                                                           18900002
&ISHIFT  SETA  &SHIFT(&NDX)                                             19000002
&IBITS   SETA  &BITS(&NDX)                                              19100002
&T       SETA  &IBITS-1                                                 19200002
&B       SETB  0                                                        19300002
         AIF   ('&SKIP$OR' EQ '').BACKUP                                19400002
&K       SETA  K'&SKIP$OR                                               19500002
&B       SETB  ('&SKIP$OR'(1,1) EQ 'B')                                 19600002
&S       SETC  '&SKIP$OR'(&B+1,&K-&B)                                   19700002
&T       SETA  &S                                                       19800002
.BACKUP  ANOP                                                           19900002
&NRENTS  SETA  &NRENTS+1                                                20000002
&TEMP    SETA  &ISHIFT                                                  20100002
A&SYSNDX DC    AL.12(&LOGITEM-&DSECT),XL.4'&TEMP'                       20200002
         AIF (&IBITS EQ 1 AND &B EQ 0).OUT                              20300002
         ORG   A&SYSNDX                                                 20400002
&B       SETB  1                                                        20500002
         DC    AL.12(&LOGITEM-&DSECT),XL.4'C'                           20600002
         DC    BL.1'&B',AL.7(&T),AL.4(&IBITS),FL.4'&ISHIFT'             20700002
.OUT     ANOP                                                           20800002
         MEND                                                           20900002
         EJECT                                                          21000002
*  HEX MACRO     HEX MACRO     HEX MACRO    HEXN MACRO     HEX MACRO  * 21100002
         SPACE 2                                                        21200002
         MACRO                                                          21300002
         HEX   &LOGITEM,&SKIP$OR,&BYTES                                 21400002
.*                            THIS MACRO FACILITATES GENERATION OF THE  21500002
.*                                 TABLE ENTRIES SPECIFYING HEX TYPE    21600002
.*                                 CONVERSIONS.                         21700002
.*                            &LOGITEM - NAME OF DATA ITEM TO BE        21800002
.*                                 CONVERTED                            21900002
.*                            &SKIP$OR - NUMBER OF SPACES TO SKIP ON    22000002
.*                                 MULTIPLE PRINT OR NBR OF SPACES TO   22100002
.*                                 BACK UP FROM COLUMN FOR START OF     22200002
.*                                 PRINTING. THE LETTER B PRECEEDS THE  22300002
.*                                 NUMBER (B8) IF THE LATTER OPTION IS  22400002
.*                                 DESIRED. OMISSION OF THIS OPERAND    22500002
.*                                 FORCES OPTION 2 WITH BACKUP SPACING  22600002
.*                                 EQUAL TO THE NUMBER OF BYTES SPECI-  22700002
.*                                 FIED BY THE NEXT OPERAND (TWICE THIS 22800002
.*                                 NUMBER IF SUFFIX IS EXPLICITLY OR    22900002
.*                                 IMPLICITLY B).                       23000002
.*                            &BYTES - NUMBER OF TOTAL INPUT BYTES TO   23100002
.*                                 PROCESS. THIS NUMBER IS SUFFIXED     23200002
.*                                 WITH AN L,R, OR B TO STIPULATE       23300002
.*                                 PRINTING OF THE LEFT HALF, RIGHT     23400002
.*                                 HALF, OR BOTH HALVES OF THE BYTE     23500002
.*                                 RESPECTIVELY. OMISSION OF THIS       23600002
.*                                 OPERAND DEFAULTS TO B WITH THE NUM-  23700002
.*                                 BER OF BYTES EQUAL TO THE ITEM LNGTH 23800002
.*                            A NEW ENTRY IS GENERATED ONLY WHEN NECES- 23900002
.*                                 SARY. THIS MACRO MUST BE THE FIRST   24000002
.*                                 MACRO INSTRUCTION ISSUED FOR THE     24100002
.*                                 SUBJECT LINE(S).                     24200002
         GBLC &ITEM(1000),&DSECT                                        24300002
         GBLA &ITEMNBR,&BITS(1000),&SHIFT(1000),&NRENTS                 24400002
         LCLB  &B                                                       24500002
         LCLA  &IBITS,&NDX,&ISHIFT,&K,&T,&CODE,&BYTESP                  24600002
         LCLC  &S,&LETTER                                               24700002
.LOOP    ANOP                                                           24800002
&NDX     SETA  &NDX+1                                                   24900002
         AIF   ('&ITEM(&NDX)' EQ '&LOGITEM').FOUND                      25000002
         AIF   (&NDX LT &ITEMNBR).LOOP                                  25100002
         MNOTE *,'LOG ITEM NOT DEFINED'                                 25200002
         MEXIT                                                          25300002
.FOUND   ANOP                                                           25400002
&ISHIFT  SETA  &SHIFT(&NDX)                                             25500002
&IBITS   SETA  &BITS(&NDX)                                              25600002
&CODE    SETA  1                                                        25700002
&K       SETA  K'&BYTES                                                 25800002
         AIF   (&IBITS-(&IBITS/4)*4 EQ 0).OK                            25900002
         MNOTE *,'BIT LENGTH SPECIFICATION INCONSISTENT WITH CONTROL'   26000002
         MEXIT                                                          26100002
.OK      AIF   ('&BYTES' EQ '').USEDEFN                                 26200002
&LETTER  SETC  '&BYTES'(&K,1)                                           26300002
&S       SETC  '&BYTES'(1,&K-1)                                         26400002
&BYTESP  SETA  &S                                                       26500002
         AIF   ('&LETTER' EQ 'B').BOTH                                  26600002
         AIF   (&ISHIFT EQ 0 OR &ISHIFT EQ 4).OK2                       26700002
.ERR     MNOTE *,'DATA NOT ALLIGNED ON HEX OR BYTE BOUNDARY'            26800002
         MEXIT                                                          26900002
.USEDEFN ANOP                                                           27000002
&BYTESP  SETA  &IBITS/8                                                 27100002
.BOTH    AIF   (&ISHIFT NE 0).ERR                                       27200002
&CODE    SETA  3                                                        27300002
&T       SETA  &BYTESP*2-1                                              27400002
&S       SETC  '&T'                                                     27500002
.OK2     AIF   (&ISHIFT EQ 0).L                                         27600002
&CODE    SETA  2                                                        27700002
.L       ANOP                                                           27800002
&B       SETB  1                                                        27900002
         AIF   ('&SKIP$OR' EQ '').BACKUP                                28000002
&K       SETA  K'&SKIP$OR                                               28100002
&B       SETB  ('&SKIP$OR'(1,1) EQ 'B')                                 28200002
&S       SETC  '&SKIP$OR'(&B+1,&K-&B)                                   28300002
.BACKUP  ANOP                                                           28400002
&NRENTS  SETA  &NRENTS+1                                                28500002
         DC    AL.12(&LOGITEM-&DSECT),XL.4'A'                           28600002
         DC    BL.1'&B',FL.7'&S',FL.4'&BYTESP',FL.4'&CODE'              28700002
.OUT     ANOP                                                           28800002
         MEND                                                           28900002
         SPACE 2                                                        29000002
*  ROUTINE MACRO     ROUTINE MACRO     ROUTINE MACRO     ROUTINE MACRO* 29100002
         SPACE 2                                                        29200002
         MACRO                                                          29300002
&NAME    ROUTINE &LOGITEM,&RNAME                                        29400002
         GBLC  &DSECT,&CSECT                                            29500002
         GBLC  &ITEM(1000)                                              29600002
         GBLA  &SHIFT(1000),&ITEMNBR                                    29700002
         GBLA  &NRENTS                                                  29800002
         LCLA  &ISHIFT,&NDX                                             29900002
.LOOP    ANOP                                                           30000002
&NDX     SETA  &NDX+1                                                   30100002
         AIF   ('&ITEM(&NDX)' EQ '&LOGITEM').FOUND                      30200002
         AIF   (&NDX LT &ITEMNBR).LOOP                                  30300002
         MNOTE *,'LOG ITEM NOT DEFINED'                                 30400002
         MEXIT                                                          30500002
.FOUND   ANOP                                                           30600002
&ISHIFT  SETA  &SHIFT(&NDX)                                             30700002
&NAME    DC    AL.12(&LOGITEM-&DSECT),XL.4'D'                           30800002
         DC    AL.4(&ISHIFT),AL.12(&RNAME-&CSECT)                       30900002
&NRENTS  SETA  &NRENTS+1                                                31000002
         MEND                                                           31100002
         EJECT                                                          31200002
*  LABEL MACRO     LABEL MACRO     LABEL MACRO     LABEL MACRO        * 31300002
         SPACE 2                                                        31400002
         MACRO                                                          31500002
&NAME    LABEL &LABEL1,&LABEL2,&LABEL3,&LABEL4,&LABEL5,&LABEL6,        C31600002
               &LABEL7,&LABEL8                                          31700002
.*                            THIS MACRO ALLOWS UP TO ACHT LABELS TO    31800002
.*                                 BE SPECIFIED FOR A GIVEN LINE. THE   31900002
.*                                 LABELS SHOULD BE ENCLOSED IN QUOTA-  32000002
.*                                 TION MARKS. THE COLUMN AND LINE END  32100002
.*                                 CONTROLS WILL BE INSERTED BY THE     32200002
.*                                 MACRO. OMITTED OPERANDS MEAN NO      32300002
.*                                 LABEL REQUIRED FOR RESPECTIVE        32400002
.*                                 COLUMN. THIS MACRO WILL ALSO ALERT   32500002
.*                                 THE PROGRAMMER TO EXACTLY WHERE THE  32600002
.*                                 TABLE MUST BE BROKEN IF NECESSARY    32700002
.*                                 TO AVOID EXCEEDING THE MAXIMUM LIMIT 32800002
         GBLC &LABELS(2000)                                             32900002
         GBLC  &LBL(100)                                                33000002
         GBLA  &LBLCTR(100)                                             33100002
         GBLA  &LABELNR                                                 33200002
         GBLA  &NRENTS,&LPTR                                            33300002
         LCLC  &MARGIN,&TACK                                            33400002
         LCLC  &TEXT                                                    33500002
         LCLA  &OP,&K,&START,&NRCHAR,&REMAIN                            33600002
&MARGIN  SETC  '*'                                                      33700002
         AIF   ('&NAME' EQ '').LOOPOUT                                  33800002
&LPTR    SETA  &LPTR+1                                                  33900002
&LBL(&LPTR) SETC '&NAME'                                                34000002

*  DSGEN MACRO     DSGEN MACRO     DSGEN MACRO     DSGEN MACRO        * 00060002
         SPACE 2                                                        00120002
         MACRO                                                          00180002
&NAME    DSGEN &X1,&X2,&X3,&X4,&X5,&X6,&X7,&X8,&X9,&X10,&X11,&X12,&X13,X00240002
               &X14,&X15,&X16,&X17,&X18,&X19,&X20,&X21,&X22,&X23,&X24, X00300002
               &X25,&X26,&X27,&X28,&X29,&X30,&X31,&X32                  00360002
.*                            THIS MACRO GENERATES ONE DSECT WORD       00420002
.*                                 FORMATTED FOR THE SUBJECT LOG RECORD 00480002
.*                                 EACH OPERAND HAS TWO SUB LISTS. THE  00540002
.*                                 FIRST IS THE NAME OF THE LOG ITEM    00600002
.*                                 WHILE THE SECOND IS THE # OF BITS TO 00660002
.*                                 ALLOCATE FOR THIS ITEM. ITEMS MUST   00720002
.*                                 BE DEFINED CONTIGUOUSLY WITHOUT GAPS 00780002
.*                                 UNUSED BIT GROUPS MUST BE GIVEN      00840002
.*                                 DUMMY NAMES. THE NAMES PROVIDED FOR  00900002
.*                                 THE LOG ITEMS MAY THEN BE USED IN    00960002
.*                                 THE CONVERSION MACRO INSTRUCTIONS.   01020002
         GBLC &ITEM(1000)                                               01080002
         GBLA &ITEMNBR,&BITS(1000),&SHIFT(1000)                         01140002
         LCLC  &SYMBOL                                                  01200002
         LCLA  &OP,&COUNT,&BYTE                                         01260002
.LOOP    ANOP                                                           01320002
&OP      SETA  &OP+1                                                    01380002
&ITEMNBR SETA  &ITEMNBR+1                                               01440002
&SYMBOL  SETC  '&SYSLIST(&OP,1)'                                        01500002
         AIF   ('&SYSLIST(&OP,2)' NE '').OK1                            01560002
         MNOTE *,'NULL BIT COUNT FIELD'                                 01620002
         MEXIT                                                          01680002
.OK1     ANOP                                                           01740002
         AIF   ('&SYMBOL' NE '').OK                                     01800002
         MNOTE 'LOG ITEM SYMBOL NOT PROVIDED'                           01860002
         AGO   .CNT                                                     01920002
.OK      ANOP                                                           01980002
         AIF   ('&SYMBOL'(1,1) EQ '''').CNT                *TEMP*       02040002
&SYMBOL  EQU   *+&BYTE                                                  02100002
&ITEM(&ITEMNBR)  SETC '&SYSLIST(&OP,1)'                                 02160002
&BITS(&ITEMNBR) SETA &SYSLIST(&OP,2)                                    02220002
&SHIFT(&ITEMNBR) SETA &COUNT-&BYTE*8                                    02280002
.CNT     ANOP                                                           02340002
&COUNT   SETA  &COUNT+&SYSLIST(&OP,2)                                   02400002
&BYTE    SETA  &COUNT/8                                                 02460002
         AIF (&OP LT N'&SYSLIST).LOOP                                   02520002
         AIF   (&COUNT NE 32).NOT32                                     02580002
&NAME    DS    4C                                                       02640002
         MEXIT                                                          02700002
.NOT32   AIF   (&COUNT NE 64).ERR                                       02760002
&NAME    DS    8C                                                       02820002
         MEXIT                                                          02880002
.ERR     MNOTE '**BIT COUNT FIELDS DO NOT TOTAL 32 OR 64**'             02940002
         MEND                                                           03000002
         EJECT                                                          03060002
*  LINE MACRO     LINE MACRO     LINE MACRO     LINE MACRO            * 03120002
         SPACE 2                                                        03180002
         MACRO                                                          03240002
&NAME    LINE  &DATA,&LABEL,&SKIP=                                      03300002
.*                            THIS MACRO ESTABLISHES THE MARGINS OR     03360002
.*                                 COLUMN SETTINGS FOR EACH LINE. IT    03420002
.*                                 GENERATES NECESSARY CARRIAGE CONTROL 03480002
.*                                 ENTRIES. LASTLY IT DETERMINES THE    03540002
.*                                 NUMBER OF ENTRIES ASSOCIATED WITH    03600002
.*                                 THE PREVIOUS LINE AND STORES THAT #  03660002
.*                                 INTO THE APPROPRIATE BYTE.           03720002
.*                            &DATA(1) - CHARACTER POSITION TO BEGIN    03780002
.*                                 DATA.                                03840002
.*                            &DATA(2) - # OF SPACES BETWEEN SUCCESSIVE 03900002
.*                                 DATA COLUMN STARTING CHARACTERS.     03960002
.*                            &LABEL(1) - CHAR. POS. TO BEGIN LABELS.   04020002
.*                            &LABEL(2) - # SPACES BETWEEN LINE'S LABEL 04080002
.*                                 STARTING CHARACTERS.                 04140002
.*                            &SKIP= - # OF LINES TO ADVANCE CARRIAGE   04200002
.*                                 TO BEGIN NEXT PRINTED LINE.          04260002
.*                            NEW ENTRIES ARE GENERATED ONLY WHEN       04320002
.*                                 NECESSARY. OMISSION OF ITEMS MEANS   04380002
.*                                 THEY HAVE NOT CHANGED SINCE PREVIOUS 04440002
.*                                 SETTINGS. A LINE MACRO IS REQUIRED   04500002
.*                                 FOR EACH PRINTED LINE. IF NO SETTING 04560002
.*                                 CHANGES ARE REQUIRED ALL OPERANDS    04620002
.*                                 ARE OMITTED IN WHICH CASE ONLY THE   04680002
.*                                 # OF ENTRIES FOR THE PREVIOUS LINE   04740002
.*                                 IS COMPUTED. THIS MACRO MUST BE THE  04800002
.*                                 FIRST MACRO INSTRUCTION ISSUED FOR   04860002
.*                                 THE SUBJECT LINE                     04920002
         GBLA  &DB,&DT,&LB,&LT,&NRENTS                                  04980002
         GBLA  &LASTCC,&INITIAL                                         05040002
         GBLC  &NRENTSX                                                 05100002
         GBLC  &CARRCON                                                 05160002
         GBLB  &TEST                                                    05220002
         LCLA  &S                                                       05280002
&NAME    DS    0C                                                       05340002
         AIF   (&INITIAL EQ 0).FIRSTLN                                  05400002
         LINEND                                                         05460002
         AGO   .NTFIRST                                                 05520002
.FIRSTLN ANOP                                                           05580002
&NRENTSX SETC  'N&SYSNDX'                                               05640002
&INITIAL SETA  1                                                        05700002
N&SYSNDX EQU   *                                                        05760002
.NTFIRST ANOP                                                           05820002
         DC    AL1(0)                                                   05880002
         AIF   ('&DATA(1)' EQ '').CKDTABS                               05940002
         AIF   (&DATA(1) EQ &DB).CKDTABS                                06000002
&DB      SETA  &DATA(1)                                                 06060002
         AGO   .AA                                                      06120002
.CKDTABS AIF   ('&DATA(2)' EQ '').NCDATA                                06180002
         AIF   (&DATA(2) EQ &DT).NCDATA                                 06240002
         AGO   .CC                                                      06300002
.AA      AIF   ('&DATA(2)' EQ '').BB                                    06360002
         AIF   (&DATA(2) EQ &DT).BB                                     06420002
.CC      ANOP                                                           06480002
&DT      SETA  &DATA(2)                                                 06540002
.BB      DC    XL2'FFFE'                                                06600002
         DC    FL1'&DB'                                                 06660002
         DC    FL1'&DT'                                                 06720002
&NRENTS  SETA  &NRENTS+1                                                06780002
.NCDATA  ANOP                                                           06840002
         AIF   ('&LABEL(1)' EQ '').CKLTABS                              06900002
         AIF   (&LABEL(1) EQ &LB).CKLTABS                               06960002
&LB      SETA  &LABEL(1)                                                07020002
         AGO   .AAA                                                     07080002
.CKLTABS AIF   ('&LABEL(2)' EQ '').NCLABEL                              07140002
         AIF   (&LABEL(2) EQ &LT).NCLABEL                               07200002
         AGO   .CCC                                                     07260002
.AAA     AIF   ('&LABEL(2)' EQ '').BBB                                  07320002
         AIF   (&LABEL(2) EQ &LT).BBB                                   07380002
.CCC     ANOP                                                           07440002
&LT      SETA  &LABEL(2)                                                07500002
.BBB     DC    XL2'FFFF'                                                07560002
         DC    FL1'&LB'                                                 07620002
         DC    FL1'&LT'                                                 07680002
&NRENTS  SETA  &NRENTS+1                                                07740002
.NCLABEL ANOP                                                           07800002
         AIF   ('&SKIP' NE '').CONT                                     07860002
         AIF   (&TEST EQ 0).NOCHANG                                     07920002
&S       SETA  &LASTCC                                                  07980002
         AGO   .DEFAULT                                                 08040002
.CONT    ANOP                                                           08100002
         AIF   (&TEST EQ 0).AB                                          08160002
&S       SETA  &SKIP                                                    08220002
.DEFAULT ANOP                                                           08280002
         ORG   &CARRCON+3                                               08340002
         DC    AL.3(0),AL.2(&S),AL.3(1)                                 08400002
         ORG                                                            08460002
&LASTCC  SETA  &S                                                       08520002
&TEST    SETB  0                                                        08580002
         AGO   .DD                                                      08640002
.AB      AIF   (&SKIP EQ &LASTCC).NOCHANG                               08700002
A&SYSNDX DC    XL2'FFFB'                                                08760002
         DC    AL.3(0),AL.2(&SKIP),AL.3(1)                              08820002
         DC    FL1'9'                                                   08880002
&LASTCC  SETA  &SKIP                                                    08940002
&TEST    SETB  1                                                        09000002
&CARRCON SETC  'A&SYSNDX'                                               09060002
&NRENTS  SETA  &NRENTS+1                                                09120002
.DD      ANOP                                                           09180002
.NOCHANG ANOP                                                           09240002
         MEND                                                           09300002
         EJECT                                                          09360002
*  BIN MACRO     BIN MACRO     BIN MACRO     BIN MACRO     BIN MACRO  * 09420002
         SPACE 2                                                        09480002
         MACRO                                                          09540002
         BIN   &LOGITEM,&SKIP$OR                                        09600002
.*                                 &LOGITEM - NAME OF ITEM IN LOG       09660002
.*                                      RECORD DSECT                    09720002
.*                                 &SKIP$OR - NBR OF SPACES TO SKIP ON  09780002
.*                                 MULTIPLE PRINT OR NBR OF SPACES      09840002
.*                                      TO BACK UP FROM COLUMN FOR      09900002
.*                                      START OF PRINTING. THE LETTER   09960002
.*                                      B PRECEEDS THE NUMBER (B5) IF   10020002
.*                                      THE LATTER IS DESIRED. OMISSION 10080002
.*                                      OF THIS OPERAND FORCES OPTION 2 10140002
.*                                      WITH BACKUP SPACING EQUAL TO    10200002
.*                                 THE NUMBER OF BITS IN THE ITEM       10260002
.*                                 DEFINITION (VIA MACRO 'DSGEN').      10320002
.*                            A LIMIT OF 15 BITS IS IMPOSED FOR THE     10380002
.*                                 TOTAL NUMBER OF BITS TO PRINT.       10440002
         GBLC &ITEM(1000)                                               10500002
         GBLA &ITEMNBR,&BITS(1000),&SHIFT(1000),&NRENTS                 10560002
         GBLC  &DSECT                                                   10620002
         LCLB  &B                                                       10680002
         LCLA  &IBITS,&NDX,&ISHIFT,&K                                   10740002
         LCLA  &TEMP,&T                                                 10800002
         LCLC  &S                                                       10860002
.LOOP    ANOP                                                           10920002
&NDX     SETA  &NDX+1                                                   10980002
         AIF   ('&ITEM(&NDX)' EQ '&LOGITEM').FOUND                      11040002
         AIF   (&NDX LT &ITEMNBR).LOOP                                  11100002
         MNOTE *,'YOU BLEW IT'                                          11160002
         ROUTINE DUM,SKIPPER                                            11220002
         MEXIT                                                          11280002
.FOUND   ANOP                                                           11340002
&ISHIFT  SETA  &SHIFT(&NDX)                                             11400002
&IBITS   SETA  &BITS(&NDX)                                              11460002
&T       SETA  &IBITS-1                                                 11520002
&B       SETB  0                                                        11580002
         AIF   ('&SKIP$OR' EQ '').BACKUP                                11640002
&K       SETA  K'&SKIP$OR                                               11700002
&B       SETB  ('&SKIP$OR'(1,1) EQ 'B')                                 11760002
&S       SETC  '&SKIP$OR'(&B+1,&K-&B)                                   11820002
&T       SETA  &S                                                       11880002
.BACKUP  ANOP                                                           11940002
&NRENTS  SETA  &NRENTS+1                                                12000002
&TEMP    SETA  &ISHIFT                                                  12060002
A&SYSNDX DC    AL.12(&LOGITEM-&DSECT),XL.4'&TEMP'                       12120002
         AIF (&IBITS EQ 1 AND &B EQ 0).OUT                              12180002
         ORG   A&SYSNDX                                                 12240002
&B       SETB  1                                                        12300002
         DC    AL.12(&LOGITEM-&DSECT),XL.4'C'                           12360002
         DC    BL.1'&B',AL.7(&T),AL.4(&IBITS),FL.4'&ISHIFT'             12420002
.OUT     ANOP                                                           12480002
         MEND                                                           12540002
         EJECT                                                          12600002
*  HEX MACRO     HEX MACRO     HEX MACRO    HEXN MACRO     HEX MACRO  * 12660002
         SPACE 2                                                        12720002
         MACRO                                                          12780002
         HEX   &LOGITEM,&SKIP$OR,&BYTES                                 12840002
.*                            THIS MACRO FACILITATES GENERATION OF THE  12900002
.*                                 TABLE ENTRIES SPECIFYING HEX TYPE    12960002
.*                                 CONVERSIONS.                         13020002
.*                            &LOGITEM - NAME OF DATA ITEM TO BE        13080002
.*                                 CONVERTED                            13140002
.*                            &SKIP$OR - NUMBER OF SPACES TO SKIP ON    13200002
.*                                 MULTIPLE PRINT OR NBR OF SPACES TO   13260002
.*                                 BACK UP FROM COLUMN FOR START OF     13320002
.*                                 PRINTING. THE LETTER B PRECEEDS THE  13380002
.*                                 NUMBER (B8) IF THE LATTER OPTION IS  13440002
.*                                 DESIRED. OMISSION OF THIS OPERAND    13500002
.*                                 FORCES OPTION 2 WITH BACKUP SPACING  13560002
.*                                 EQUAL TO THE NUMBER OF BYTES SPECI-  13620002
.*                                 FIED BY THE NEXT OPERAND (TWICE THIS 13680002
.*                                 NUMBER IF SUFFIX IS EXPLICITLY OR    13740002
.*                                 IMPLICITLY B).                       13800002
.*                            &BYTES - NUMBER OF TOTAL INPUT BYTES TO   13860002
.*                                 PROCESS. THIS NUMBER IS SUFFIXED     13920002
.*                                 WITH AN L,R, OR B TO STIPULATE       13980002
.*                                 PRINTING OF THE LEFT HALF, RIGHT     14040002
.*                                 HALF, OR BOTH HALVES OF THE BYTE     14100002
.*                                 RESPECTIVELY. OMISSION OF THIS       14160002
.*                                 OPERAND DEFAULTS TO B WITH THE NUM-  14220002
.*                                 BER OF BYTES EQUAL TO THE ITEM LNGTH 14280002
.*                            A NEW ENTRY IS GENERATED ONLY WHEN NECES- 14340002
.*                                 SARY. THIS MACRO MUST BE THE FIRST   14400002
.*                                 MACRO INSTRUCTION ISSUED FOR THE     14460002
.*                                 SUBJECT LINE(S).                     14520002
         GBLC &ITEM(1000),&DSECT                                        14580002
         GBLA &ITEMNBR,&BITS(1000),&SHIFT(1000),&NRENTS                 14640002
         LCLB  &B                                                       14700002
         LCLA  &IBITS,&NDX,&ISHIFT,&K,&T,&CODE,&BYTESP                  14760002
         LCLC  &S,&LETTER                                               14820002
.LOOP    ANOP                                                           14880002
&NDX     SETA  &NDX+1                                                   14940002
         AIF   ('&ITEM(&NDX)' EQ '&LOGITEM').FOUND                      15000002
         AIF   (&NDX LT &ITEMNBR).LOOP                                  15060002
         MNOTE *,'LOG ITEM NOT DEFINED'                                 15120002
         MEXIT                                                          15180002
.FOUND   ANOP                                                           15240002
&ISHIFT  SETA  &SHIFT(&NDX)                                             15300002
&IBITS   SETA  &BITS(&NDX)                                              15360002
&CODE    SETA  1                                                        15420002
&K       SETA  K'&BYTES                                                 15480002
         AIF   (&IBITS-(&IBITS/4)*4 EQ 0).OK                            15540002
         MNOTE *,'BIT LENGTH SPECIFICATION INCONSISTENT WITH CONTROL'   15600002
         MEXIT                                                          15660002
.OK      AIF   ('&BYTES' EQ '').USEDEFN                                 15720002
&LETTER  SETC  '&BYTES'(&K,1)                                           15780002
&S       SETC  '&BYTES'(1,&K-1)                                         15840002
&BYTESP  SETA  &S                                                       15900002
         AIF   ('&LETTER' EQ 'B').BOTH                                  15960002
         AIF   (&ISHIFT EQ 0 OR &ISHIFT EQ 4).OK2                       16020002
.ERR     MNOTE *,'DATA NOT ALLIGNED ON HEX OR BYTE BOUNDARY'            16080002
         MEXIT                                                          16140002
.USEDEFN ANOP                                                           16200002
&BYTESP  SETA  &IBITS/8                                                 16260002
.BOTH    AIF   (&ISHIFT NE 0).ERR                                       16320002
&CODE    SETA  3                                                        16380002
&T       SETA  &BYTESP*2-1                                              16440002
&S       SETC  '&T'                                                     16500002
.OK2     AIF   (&ISHIFT EQ 0).L                                         16560002
&CODE    SETA  2                                                        16620002
.L       ANOP                                                           16680002
&B       SETB  1                                                        16740002
         AIF   ('&SKIP$OR' EQ '').BACKUP                                16800002
&K       SETA  K'&SKIP$OR                                               16860002
&B       SETB  ('&SKIP$OR'(1,1) EQ 'B')                                 16920002
&S       SETC  '&SKIP$OR'(&B+1,&K-&B)                                   16980002
.BACKUP  ANOP                                                           17040002
&NRENTS  SETA  &NRENTS+1                                                17100002
         DC    AL.12(&LOGITEM-&DSECT),XL.4'A'                           17160002
         DC    BL.1'&B',FL.7'&S',FL.4'&BYTESP',FL.4'&CODE'              17220002
.OUT     ANOP                                                           17280002
         MEND                                                           17340002
         SPACE 2                                                        17400002

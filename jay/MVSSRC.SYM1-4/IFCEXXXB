         AIF (&REMAIN EQ 0).ENDTEXT                                     21660002
&TEXT    SETC  '&SYSLIST(&OP)'(&START,&NRCHAR)                          21720002
.ENDTEXT ANOP                                                           21780002
&LABELNR SETA  &LABELNR+1                                               21840002
&LABELS(&LABELNR) SETC '&TEXT'.'&TACK'                                  21900002
&START   SETA  &START+8                                                 21960002
         AIF ('&TACK' EQ '').LOOPIN                                     22020002
         AIF ('&TACK' EQ '*').LOOPOUT                                   22080002
         MEND                                                           22140002
         SPACE 2                                                        22200002
         MACRO                                                          22260002
         LSTART                                                         22320002

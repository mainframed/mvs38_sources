         LA    R8,STLBLIST              POINT TO FIRST LABEL ENTRS21021 67440002
STLBCOMP CLC   XDEVT2(L2),0(R8)         IS THIS THE DEVICE TYPE? S21021 67520002
         BE    STLBLTFD                 YES, GO MOVE LABELS      S21021 67600002
         LA    R8,L18(R8)               BUMP POINTER                    67680002
         BCT   R7,STLBCOMP              TRY AGAIN                       67760002
         LA    R8,STLBDFLT              USE DEFAULT                     67840002
STLBLTFD DS    0H                  LABELS FOUND                  S21021 67920002
         LA    R8,L2(R8)                USE LABELS               S21021 68000002
         LA    R7,L16(R8)               DETERMINE END POINT      S21021 68080002
         ST    R7,STLBLEND              SAVE FOR COMPARE         S21021 68160002

.MORE    ANOP                                                           20480002
&TEXT    SETC  ''                                                       20560002
         AIF (&REMAIN EQ 0).ENDTEXT                                     20640002
&TEXT    SETC  '&SYSLIST(&OP)'(&START,&NRCHAR)                          20720002
.ENDTEXT ANOP                                                           20800002

*     DO;                           /* YES                   @ZM20856*/ 02290000
*       SAVECHAR=HEXFF;             /* FLAG ERROR            @ZM20856*/ 02291000
         MVI   SAVECHAR(SAVEPTR),X'FF'                             0827 02292000
*       RETURN;                     /*                       @ZM20856*/ 02293000

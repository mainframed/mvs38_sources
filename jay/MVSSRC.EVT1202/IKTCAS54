EDITIT   ST    @14,@SA00010                                        0600 01521000
*                                                                  0601 01522000
*/*  *****************************************************************/ 01523000
*/*                                                                  */ 01524000
*/*    THIS SUBROUTINE TAKES A KEYWORD NAME, ITS VALUE, PLACES THEM  */ 01525000
*/*    INTO AN EDITTED FORMAT, AND PRINTS THEM.  UPON ENTRY,         */ 01526000
*/*    'TABLEPTR' POINTS AT THE TABLE ENTRY, AND 'WORKPARM' CONTAINS */ 01527000
*/*    THE PARAMETER VALUE, WITH THE EXCEPTION OF THE RCFBDUMP       */ 01528000
*/*    KEYWORD, WHICH SETS UP 'WORKPARM' HERE.                       */ 01529000
*/*  *****************************************************************/ 01530000
*                                                                  0601 01531000
*   EDITFLD='';                     /* CLEAR WORK FIELD              */ 01532000
         MVI   EDITFLD,C' '                                        0601 01533000
         MVC   EDITFLD+1(19),EDITFLD                               0601 01534000
*   SCANPTR=TABLEPTR;               /* SETUP PTR                     */ 01535000
         LR    SCANPTR,TABLEPTR                                    0602 01536000
*   DO I=1 TO 8 WHILE(WORKCHAR^=' ');/* MOVE IN KEYWORD NAME         */ 01537000
         LA    I,1                                                 0603 01538000
@DL00603 CLI   WORKCHAR(SCANPTR),C' '                              0603 01539000
         BE    @DC00603                                            0603 01540000
*     EDFIELD(I)=WORKCHAR;                                         0604 01541000
         LA    @04,EDFIELD-1(I)                                    0604 01542000
         MVC   0(1,@04),WORKCHAR(SCANPTR)                          0604 01543000
*     SCANPTR=SCANPTR+1;                                           0605 01544000

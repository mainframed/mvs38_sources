*       PRNTDATA='CONSOLE VALUES ENTERED:';/* HEADER LINE            */ 00287000
         MVI   PRNTDATA+23,C' '                                    0195 00288000
         MVC   PRNTDATA+24(108),PRNTDATA+23                        0195 00289000
         MVC   PRNTDATA(23),@CC01526                               0195 00290000
*       CALL PRINTIT;               /* PRINT THE HEADER              */ 00291000

*                                                                  0280 00802000
*   RFY                                                            0280 00803000
*    (REG0,                                                        0280 00804000
*     REG1,                                                        0280 00805000
*     REG2,                                                        0280 00806000
*     REG3) RSTD;                   /* RESTRICT FOR MVCL             */ 00807000
*   REG0=ADDR(TVCSDATA);            /* TARGET ADDRESS FOR MVCL       */ 00808000
         LA    REG0,TVCSDATA(,@14)                                 0281 00809000
*   REG1=XSAPRMSZ;                  /* TARGET SIZE FOR MVCL          */ 00810000
         LR    REG1,@08                                            0282 00811000
*   REG2=XSABFRAD;                  /* SOURCE ADDRESS FOR MVCL       */ 00812000
         L     REG2,XSABFRAD-1(,RBSECPTR)                          0283 00813000
         LA    REG2,0(,REG2)                                       0283 00814000
*   REG3=REG1;                      /* SOURCE SIZE FOR MVCL          */ 00815000
         LR    REG3,REG1                                           0284 00816000
*   IF XSAAUTH='0'B THEN            /* CALLER AUTHORIZED ?           */ 00817000
         TM    XSAAUTH(RBSECPTR),B'00000010'                       0285 00818000
         BNZ   @RF00285                                            0285 00819000
*     DO;                           /* NO                            */ 00820000
*       TVCSDATA(1:1)=PLUSSIGN;     /* INSERT '+'                    */ 00821000
         MVI   TVCSDATA(@14),C'+'                                  0287 00822000
*       TVCSDASZ=TVCSDASZ+1;        /* ADJUST SIZE OF TPUT           */ 00823000
         LA    @04,1                                               0288 00824000
         ALR   @08,@04                                             0288 00825000
         STH   @08,TVCSDASZ(,@14)                                  0288 00826000
*       REG0=REG0+1;                /* ADJUST TARGET ADDRESS OF MVCL    00827000
*                                      TO POINT PAST '+'             */ 00828000
         ALR   REG0,@04                                            0289 00829000
*     END;                                                         0290 00830000
*   MVCL(REG0,REG2);                /* MOVE DATA INTO TVCS           */ 00831000
@RF00285 MVCL  REG0,REG2                                           0291 00832000
*   RFY                                                            0292 00833000
*    (REG0,                                                        0292 00834000
*     REG1,                                                        0292 00835000
*     REG2,                                                        0292 00836000
*     REG3) UNRSTD;                                                0292 00837000
*   RFY                                                            0293 00838000
*     RTNREG RSTD;                                                 0293 00839000
*   RTNREG=XSASAVEA;                /* RESTORE RETURN ADDRESS        */ 00840000
         L     RTNREG,XSASAVEA(,RBSECPTR)                          0294 00841000
*   RETURN;                                                        0295 00842000
@EL00003 DS    0H                                                  0295 00843000
@EF00003 DS    0H                                                  0295 00844000
@ER00003 BR    @14                                                 0295 00845000
*   RFY                                                            0296 00846000
*     RTNREG UNRSTD;                                               0296 00847000
*   END INITTVCS;                                                  0297 00848000
*INITSRB:                                                          0298 00849000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0298 00850000
INITSRB  DS    0H                                                  0299 00851000
*                                                                  0299 00852000
*/********************************************************************/ 00853000
*/*                                                                  */ 00854000
*/*         INITSRB IS AN INTERNAL PROCEDURE THAT MOVES THE          */ 00855000
*/*         PROPER INFORMATION INTO THE SRB CONTROL BLOCK            */ 00856000
*/*         THAT IS USED TO SCHEDULE IKTXMTPT.                       */ 00857000
*/*      ****INITSRB SAVES RETURN ADDRESS IN FIRST LEVEL             */ 00858000
*/*          'SAVE WORD' - XSASAVEA                                  */ 00859000
*/*                                                                  */ 00860000
*/********************************************************************/ 00861000
*                                                                  0299 00862000
*   RFY                                                            0299 00863000
*     RTNREG RSTD;                                                 0299 00864000
*   XSASAVEA=RTNREG;                /* SAVE RETURN ADDRESS           */ 00865000
         ST    RTNREG,XSASAVEA(,RBSECPTR)                          0300 00866000
*   RFY                                                            0301 00867000
*     RTNREG UNRSTD;                                               0301 00868000
*   SRB=''B;                        /* ZERO OUT SRB                  */ 00869000
         L     @14,XSATVCSP-1(,RBSECPTR)                           0302 00870000
         LA    @14,0(,@14)                                         0302 00871000
         L     @08,TVCSSRBA-1(,@14)                                0302 00872000
         LA    @08,0(,@08)                                         0302 00873000
         XC    SRB(44,@08),SRB(@08)                                0302 00874000
*   SRBID='SRB ';                                                  0303 00875000
         MVC   SRBID(4,@08),@CC02306                               0303 00876000
*   SRBASCB=XSATASCB;               /* ADDRESS OF TARGET ASCB        */ 00877000
         L     @04,XSATASCB(,RBSECPTR)                             0304 00878000
         ST    @04,SRBASCB(,@08)                                   0304 00879000
*   SRBEP=ADDR(IKTXMTPT);           /* ADDRESS OF SRB ROUTINE        */ 00880000
         L     @04,@CV02267                                        0305 00881000
         ST    @04,SRBEP(,@08)                                     0305 00882000
*   SRBRMTR=ADDR(TCASBR14);         /* ADDRESS OF DUMMY RMTR. CLEANUP   00883000
*                                      IS DONE IN IKTAY8 AFTER PURGE */ 00884000
         L     @04,CVTPTR                                          0306 00885000
         L     @04,TCASTPTR(,@04)                                  0306 00886000
         LA    @04,TCASBR14(,@04)                                  0306 00887000
         ST    @04,SRBRMTR(,@08)                                   0306 00888000
*   SRBPARM=XSATVCSP;               /* SET FOR REG1=ADDR(TVCS) AT  0307 00889000
*                                      ENTRY TO IKTXMTPT             */ 00890000
         ST    @14,SRBPARM(,@08)                                   0307 00891000
*   SRBPASID=XSAASID;               /* PURGEDQ ASID IDENTIFIER       */ 00892000
         LH    @14,XSAASID(,RBSECPTR)                              0308 00893000
         STH   @14,SRBPASID(,@08)                                  0308 00894000
*   RFY                                                            0309 00895000
*     RTNREG RSTD;                                                 0309 00896000
*   RTNREG=XSASAVEA;                /* RESTORE RETURN ADDRESS        */ 00897000
         L     @08,XSASAVEA(,RBSECPTR)                             0310 00898000
         LR    RTNREG,@08                                          0310 00899000
*   RFY                                                            0311 00900000
*     RTNREG UNRSTD;                                               0311 00901000
*   END INITSRB;                                                   0312 00902000
@EL00004 DS    0H                                                  0312 00903000
@EF00004 DS    0H                                                  0312 00904000
@ER00004 BR    @14                                                 0312 00905000
*DATAEDIT:                                                         0313 00906000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0313 00907000
*                                                                  0313 00908000
DATAEDIT DS    0H                                                  0314 00909000
*/*                                                                  */ 00910000
*/********************************************************************/ 00911000
*/*                                                                  */ 00912000
*/*      DATA EDIT IS AN INTERNAL PROCEDURE THAT SCANS THE           */ 00913000
*/*      CALLER'S DATA AND REMOVES TRAILING BLANKS ADJUSTING         */ 00914000
*/*      THE BYTE COUNT ACCORDINGLY.                                 */ 00915000
*/*    **** DATAEDIT SAVES RETURN ADDRESS INTO THE LEVEL 'A'         */ 00916000
*/*         RETURN 'SAVE WORD' - XSASAVEA.                           */ 00917000
*/*                                                                  */ 00918000
*/********************************************************************/ 00919000
*/*                                                                  */ 00920000
*                                                                  0314 00921000
*   DCL                                                            0314 00922000
*     I REG(1);                                                    0314 00923000
*   DCL                                                            0315 00924000
*     BLANK CHAR(1) CONSTANT(' ');                                 0315 00925000
*   RFY                                                            0316 00926000
*     RTNREG RSTD;                                                 0316 00927000
*   XSASAVEA=RTNREG;                                               0317 00928000
         ST    RTNREG,XSASAVEA(,RBSECPTR)                          0317 00929000
*   RFY                                                            0318 00930000
*     RTNREG UNRSTD;                                               0318 00931000
*   IF XSAEDITO^='00'B THEN                                        0319 00932000
         TM    XSAEDITO(RBSECPTR),B'00000011'                      0319 00933000
         BNZ   @RT00319                                            0319 00934000
*     ;                             /* IF NOT EDIT, NO-OP            */ 00935000
*   ELSE                                                           0321 00936000
*     DO;                           /* EDIT OPTION-REMOVE TRAILING 0321 00937000
*                                      BLANKS                        */ 00938000
*       XSADMOVE='1'B;              /* SET DATA MOVE INDICATOR       */ 00939000
         OI    XSADMOVE(RBSECPTR),B'01000000'                      0322 00940000
*       RFY                                                        0323 00941000
*         REG2 RSTD;                                               0323 00942000
*       REG2=RBLINKB->RBOPSWB2;     /* POINT TO CALLERS KEY          */ 00943000
         L     @14,RBLINKB-1(,RBSECPTR)                            0324 00944000
         SLR   REG2,REG2                                           0324 00945000
         IC    REG2,RBOPSWB2(,@14)                                 0324 00946000
*       GEN SETS(REG2);                                            0325 00947000
        MODESET KEYADDR=(2),WORKREG=2                                   00948000
*       RFY                                                        0326 00949000
*         REG2 UNRSTD;                                             0326 00950000
*       RFY                                                        0327 00951000
*         I RSTD;                                                  0327 00952000
*       DO I=XSAPRMSZ TO 2 BY-1     /* SCAN FROM END OF DATA         */ 00953000
*             WHILE TPUTDATA(I)=BLANK;/* CONTINUE WHILE BLANK        */ 00954000
         LH    I,XSAPRMSZ(,RBSECPTR)                               0328 00955000
         B     @DE00328                                            0328 00956000
@DL00328 L     @14,XSABFRAD-1(,RBSECPTR)                           0328 00957000
         ALR   @14,I                                               0328 00958000
         BCTR  @14,0                                               0328 00959000
         CLI   TPUTDATA(@14),C' '                                  0328 00960000
         BNE   @DC00328                                            0328 00961000
*       END;                                                       0329 00962000
         BCTR  I,0                                                 0329 00963000
@DE00328 C     I,@CF00072                                          0329 00964000
         BNL   @DL00328                                            0329 00965000
@DC00328 DS    0H                                                  0330 00966000
*       RFY                                                        0330 00967000
*         REG2 RSTD;                                               0330 00968000
*       GEN SETS(REG2) REFS(RBLINK,RBOPSW,RBBASIC,TCBRBP);         0331 00969000
        MODESET EXTKEY=ZERO GET BACK INTO KEY 0                         00970000
*       RFY                                                        0332 00971000
*         REG2 UNRSTD;                                             0332 00972000
*       XSADMOVE='0'B;              /* CLEAR DATA MOVE INDICATOR     */ 00973000
         NI    XSADMOVE(RBSECPTR),B'10111111'                      0333 00974000
*       XSAPRMSZ=I;                 /* UPDATE TPUT SIZE. MIN VALUE IS   00975000
*                                      1, EVEN IF WHOLE TPUT IS BLANK*/ 00976000
         STH   I,XSAPRMSZ(,RBSECPTR)                               0334 00977000
*       RFY                                                        0335 00978000
*         I UNRSTD;                                                0335 00979000
*     END;                          /* END OF EDIT DO GROUP          */ 00980000
*   RFY                                                            0337 00981000
*     RTNREG RSTD;                                                 0337 00982000
@RT00319 DS    0H                                                  0338 00983000
*   RTNREG=XSASAVEA;                /* RESTORE RETURN ADDRESS        */ 00984000
         L     RTNREG,XSASAVEA(,RBSECPTR)                          0338 00985000
*   RETURN;                                                        0339 00986000
@EL00005 DS    0H                                                  0339 00987000
@EF00005 DS    0H                                                  0339 00988000
@ER00005 BR    @14                                                 0339 00989000
*   RFY                                                            0340 00990000
*     RTNREG UNRSTD;                                               0340 00991000
*   END DATAEDIT;                                                  0341 00992000
*VERIFY:                                                           0342 00993000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0342 00994000
*                                                                  0342 00995000
VERIFY   DS    0H                                                  0343 00996000
*/*                                                                  */ 00997000
*/********************************************************************/ 00998000
*/*                                                                  */ 00999000
*/*      THIS TESTS IF CALLER'S DATA ADDRESS IS IN THE CALLER'S      */ 01000000
*/*      KEY. IT DOES SO BY REFERENCING THE FIRST BYTE OF EACH       */ 01001000
*/*      PAGE REQUIRED FOR THE TPUT.                                 */ 01002000
*/*    ***VERIFY SAVES RETURN ADDRESS INTO THE LEVEL 'B' RETURN      */ 01003000
*/*       'SAVE WORD' - XSASAVEB.                                    */ 01004000
*/*                                                                  */ 01005000
*/********************************************************************/ 01006000
*/*                                                                  */ 01007000
*                                                                  0343 01008000
*   DCL                                                            0343 01009000
*     APAGE FIXED(15) CONSTANT(4096);/* PAGE SIZE                    */ 01010000
*   DCL                                                            0344 01011000
*     MODULUS FIXED(8) CONSTANT(12);/* MODULUS OF A VS2 PAGE IS    0344 01012000
*                                      2**12=4096                    */ 01013000
*   RFY                                                            0345 01014000
*     RTNREG RSTD;                                                 0345 01015000
*   XSASAVEB=RTNREG;                /* SAVE RETURN ADDRESS           */ 01016000
         ST    RTNREG,XSASAVEB(,RBSECPTR)                          0346 01017000
*   RFY                                                            0347 01018000
*     RTNREG UNRSTD;                                               0347 01019000
*   XSADMOVE='1'B;                  /* INDICATE 'DATA VERIFICATION 0348 01020000
*                                      UNDERWAY'                     */ 01021000
         OI    XSADMOVE(RBSECPTR),B'01000000'                      0348 01022000
*/********************************************************************/ 01023000
*/*    GET INTO CALLER'S KEY TO VALIDATE DATA                        */ 01024000
*/********************************************************************/ 01025000
*                                                                  0349 01026000
*   RFY                                                            0349 01027000
*     REG2 RSTD;                                                   0349 01028000
*   REG2=RBLINKB->RBOPSWB2;         /* POINT TO KEY OF CALLER        */ 01029000
         L     @14,RBLINKB-1(,RBSECPTR)                            0350 01030000
         SLR   REG2,REG2                                           0350 01031000
         IC    REG2,RBOPSWB2(,@14)                                 0350 01032000
*   GEN SETS(REG2);                                                0351 01033000
        MODESET KEYADDR=(2),WORKREG=2                                   01034000
*   RFY                                                            0352 01035000
*     REG2 UNRSTD;                                                 0352 01036000
*   RFY                                                            0353 01037000
*     WORK1REG RSTD;                                               0353 01038000
*   WORK1REG=XSABFRAD;              /* GET INPUT BUFFER ADDRESS      */ 01039000
         L     WORK1REG,XSABFRAD-1(,RBSECPTR)                      0354 01040000
         LA    WORK1REG,0(,WORK1REG)                               0354 01041000
*   SRL(WORK1REG,MODULUS);          /* SHIFT OFF PARTIAL PAGE        */ 01042000
         SRL   WORK1REG,12                                         0355 01043000
*   SLL(WORK1REG,MODULUS);          /* SHIFT IN ZEROS                */ 01044000
         SLL   WORK1REG,12                                         0356 01045000
*/********************************************************************/ 01046000
*/*       LOOP THRU EACH PAGE, MOVING FIRST BYTE TO ITSELF           */ 01047000
*/*       ABEND WILL CAUSE IKT93EST TO GAIN CONTROL AND CHANGE       */ 01048000
*/*       CODE TO 15D X.                                             */ 01049000
*/********************************************************************/ 01050000
*                                                                  0357 01051000
*   RFY                                                            0357 01052000
*     WORK2REG RSTD;                                               0357 01053000
*   WORK2REG=XSABFRAD+XSAPRMSZ;     /* SET UP LOOP UPPER LIMIT       */ 01054000
         L     WORK2REG,XSABFRAD-1(,RBSECPTR)                      0358 01055000
         LA    WORK2REG,0(,WORK2REG)                               0358 01056000
         AH    WORK2REG,XSAPRMSZ(,RBSECPTR)                        0358 01057000
*   DO WHILE WORK1REG<WORK2REG;                                    0359 01058000
         B     @DE00359                                            0359 01059000
@DL00359 DS    0H                                                  0360 01060000
*     GEN REFS(WORK1REG)(CLC 0(1,WORK1REG),0(WORK1REG));/* REFERENCE    01061000
*                                      PAGE                          */ 01062000
         CLC 0(1,WORK1REG),0(WORK1REG)                                  01063000
*     WORK1REG=WORK1REG+APAGE;      /* INCREMENT BY PAGE SIZE        */ 01064000
         AL    WORK1REG,@CF02319                                   0361 01065000
*   END;                                                           0362 01066000
@DE00359 CR    WORK1REG,WORK2REG                                   0362 01067000
         BL    @DL00359                                            0362 01068000
*   RFY                                                            0363 01069000
*    (WORK1REG,                                                    0363 01070000
*     WORK2REG) UNRSTD;                                            0363 01071000
*                                                                  0363 01072000
*/********************************************************************/ 01073000
*/*         GET BACK INTO KEY 0                                      */ 01074000
*/********************************************************************/ 01075000
*                                                                  0364 01076000
*   GEN(MODESET EXTKEY=ZERO);                                      0364 01077000
         MODESET EXTKEY=ZERO                                            01078000
*   XSADMOVE='0'B;                  /* CLEAR INDICATOR THAT DATA   0365 01079000
*                                      VERIFICATION WAS GOING ON     */ 01080000
         NI    XSADMOVE(RBSECPTR),B'10111111'                      0365 01081000
*   RFY                                                            0366 01082000
*     RTNREG RSTD;                                                 0366 01083000
*   RTNREG=XSASAVEB;                                               0367 01084000
         L     RTNREG,XSASAVEB(,RBSECPTR)                          0367 01085000
*   RETURN;                                                        0368 01086000
@EL00006 DS    0H                                                  0368 01087000
@EF00006 DS    0H                                                  0368 01088000
@ER00006 BR    @14                                                 0368 01089000
*   RFY                                                            0369 01090000
*     RTNREG UNRSTD;                                               0369 01091000
*   END VERIFY;                                                    0370 01092000
*   END IKTASTPT                                                   0371 01093000
*                                                                  0371 01094000
*/* THE FOLLOWING INCLUDE STATEMENTS WERE FOUND IN THIS PROGRAM.     */ 01095000

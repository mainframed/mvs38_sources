*             DO;                                                  0089 00158000
*               MLCMACRO(1:MACLEN)=MACOP(1:MACLEN);/* MOVE IN MACRO  */ 00159000
         LR    @07,MACLEN                                          0090 00160000
         BCTR  @07,0                                               0090 00161000
         EX    @07,@SM00193                                        0090 00162000
*               ENTCOUNT=COUNT;     /* PUT # OF ENTRIES IN CTR       */ 00163000
         SLR   ENTCOUNT,ENTCOUNT                                   0091 00164000

*   DATREG=ADDR(MLCMHDAT);                                         0035 00016000
         LA    DATREG,MLCMHDAT(,MLCAPTR)                           0035 00017000
*   SCANPTR=PARMR;                                                 0036 00018000
         LR    SCANPTR,PARMR                                       0036 00019000
*   PARMSAVE=PARMR;                 /* SAVE PARM REG                 */ 00020000
         LR    PARMSAVE,PARMR                                      0037 00021000
*   TABLEPTR=TABPTR;                                               0038 00022000
         LR    TABLEPTR,TABPTR                                     0038 00023000
*   MLCNAME=BLANK;                  /* BLANK MLCNAME                 */ 00024000
         MVI   MLCNAME+1(MLCAPTR),C' '                             0039 00025000
         MVC   MLCNAME+2(6,MLCAPTR),MLCNAME+1(MLCAPTR)             0039 00026000
         MVI   MLCNAME(MLCAPTR),C' '                               0039 00027000
*   MLCMACRO=BLANK;                 /* BLANK MLCMACRO                */ 00028000
         MVI   MLCMACRO+1(MLCAPTR),C' '                            0040 00029000
         MVC   MLCMACRO+2(6,MLCAPTR),MLCMACRO+1(MLCAPTR)           0040 00030000
         MVI   MLCMACRO(MLCAPTR),C' '                              0040 00031000
*   NAMELEN=0;                      /* SET CTR TO 0                  */ 00032000
         SLR   NAMELEN,NAMELEN                                     0041 00033000
*/********************************************************************/ 00034000
*/* IF THERE IS NO NAME ON THE MACRO SET UP THE MLCNAME AS BLANKS    */ 00035000
*/* OTHERWISE MOVE IN THE NAME FROM THE MACRO. HTIS NAME WILL BE USED*/ 00036000
*/* TO TELL THAT THIS MACRO CAUSED AN ERROR IF ONE IS DETECTED       */ 00037000
*/********************************************************************/ 00038000
*                                                                  0042 00039000
*   IF CHARCOMP^=BLANK THEN         /* 1ST BUFFER CHAR A BLANK?      */ 00040000
         CLI   CHARCOMP(SCANPTR),C' '                              0042 00041000
         BE    @RF00042                                            0042 00042000
*     DO;                                                          0043 00043000
*       DO WHILE CHARCOMP^=BLANK;   /* NO,FIND BLANK & COUNT CHAR    */ 00044000
         B     @DE00044                                            0044 00045000
@DL00044 DS    0H                                                  0045 00046000
*         NAMELEN=NAMELEN+1;        /* COUNT CHARACTERS              */ 00047000
         LA    @08,1                                               0045 00048000
         ALR   NAMELEN,@08                                         0045 00049000
*         SCANPTR=SCANPTR+1;        /* STEP UP PTR                   */ 00050000
         ALR   SCANPTR,@08                                         0046 00051000
*       END;                                                       0047 00052000
@DE00044 CLI   CHARCOMP(SCANPTR),C' '                              0047 00053000
         BNE   @DL00044                                            0047 00054000
*       SCANPTR=PARMSAVE;           /* RESTORE POINTER               */ 00055000
         LR    SCANPTR,PARMSAVE                                    0048 00056000
*       IF NAMELEN<=8 THEN          /* NAME LONGER THAN 8            */ 00057000
         CH    NAMELEN,@CH00051                                    0049 00058000
         BH    @RF00049                                            0049 00059000
*         DO;                                                      0050 00060000
*           MLCNAME(1:NAMELEN)=TEXTBUF(1:NAMELEN);/* MOVE IN NAME    */ 00061000
         LR    @07,NAMELEN                                         0051 00062000
         BCTR  @07,0                                               0051 00063000
         EX    @07,@SM00191                                        0051 00064000
*           PARMSAVE=PARMR;         /* SAVE REGS 1 & 2 OVER TRT      */ 00065000
         LR    PARMSAVE,PARMR                                      0052 00066000
*           TWOSAVE=R2;                                            0053 00067000
         LR    TWOSAVE,R2                                          0053 00068000
*           PARMR=0;                /* SET REGS 1 & 2 TO 0           */ 00069000
         SLR   PARMR,PARMR                                         0054 00070000
*           R2=0;                                                  0055 00071000
         SLR   R2,R2                                               0055 00072000
*           TRT(CHARCOMP,TTABLE1);  /* TRT FIRST CHARACTER           */ 00073000
         TRT   CHARCOMP(1,SCANPTR),TTABLE1                         0056 00074000
*           IF PARMR=0 THEN         /* FIRST CHAR VALID?             */ 00075000
         LTR   PARMR,PARMR                                         0057 00076000
         BNZ   @RF00057                                            0057 00077000
*             DO;                                                  0058 00078000
*               SCANPTR=SCANPTR+1;  /* YES, ADVANCE PTR              */ 00079000
         AH    SCANPTR,@CH00039                                    0059 00080000
*               TRT(TEXTBUF,TTABLE2);/* TRT REST OF NAME             */ 00081000
         TRT   TEXTBUF(256,SCANPTR),TTABLE2                        0060 00082000
*               IF R2^=1 THEN       /* TRT STOPPED BY BLANK?         */ 00083000
         CH    R2,@CH00039                                         0061 00084000
         BE    @RF00061                                            0061 00085000
*                 DO;                                              0062 00086000
*                   RCSAVE=8;       /* NO, ERROR CONDITION           */ 00087000
         LA    RCSAVE,8                                            0063 00088000
*                 END;                                             0064 00089000
*               ELSE                /* R2 = 1, GO ON                 */ 00090000
*                 RCSAVE=0;         /* INDICATE NO ERROR             */ 00091000
         B     @RC00061                                            0065 00092000
@RF00061 SLR   RCSAVE,RCSAVE                                       0065 00093000
*             END;                                                 0066 00094000
*           ELSE                                                   0067 00095000
*             RCSAVE=8;             /* PARMR NOT = 0                 */ 00096000
         B     @RC00057                                            0067 00097000
@RF00057 LA    RCSAVE,8                                            0067 00098000
*         END;                                                     0068 00099000
*       ELSE                        /* NAME > 8                      */ 00100000
*         DO;                                                      0069 00101000
         B     @RC00049                                            0069 00102000
@RF00049 DS    0H                                                  0070 00103000
*           MLCNAME(1:8)=TEXTBUF(1:8);/* MOVE IN 1ST 8 CHAR          */ 00104000
         MVC   MLCNAME(8,MLCAPTR),TEXTBUF(SCANPTR)                 0070 00105000
*           RCSAVE=8;               /* ERROR CONDITION               */ 00106000
         LA    RCSAVE,8                                            0071 00107000
*         END;                                                     0072 00108000
*     END;                                                         0073 00109000
*   ELSE                            /* FIRST CHAR WAS BLANK          */ 00110000
*     DO;                                                          0074 00111000
         B     @RC00042                                            0074 00112000
@RF00042 DS    0H                                                  0075 00113000
*       RCSAVE=0;                   /* SIGNIFY NO ERROR              */ 00114000
         SLR   RCSAVE,RCSAVE                                       0075 00115000
*     END;                                                         0076 00116000
*   IF RCSAVE=0 THEN                                               0077 00117000
@RC00042 LTR   RCSAVE,RCSAVE                                       0077 00118000
         BNZ   @RF00077                                            0077 00119000
*     DO;                                                          0078 00120000
*                                                                  0078 00121000
*/********************************************************************/ 00122000
*/* NOW SCAN THE INPUT FOR A MACRO NAME.  WHEN IT IS FOUND BEGIN A   */ 00123000
*/* SCAN OF THE MACRO NAME TABLE AND COMPARE THE NAME OF THE MACRO   */ 00124000
*/* THAT IS ON THE INPUT TO THE NAME IN THE TABLE IF THE NAMES ARE   */ 00125000
*/* NOT THE SAME CONTINUE. IF THE NAME IS FOUND THEN PICK UP THE     */ 00126000
*/* ADDRESS OF THE ROUTINE ASSOCIATED WITH THE MACRO AND PUT IT IN   */ 00127000
*/* R0 TO RETURN TO THE CALLER                                       */ 00128000
*/********************************************************************/ 00129000
*                                                                  0079 00130000
*       SCANPTR=PARMSAVE+NAMELEN+1; /* RESET PTR                     */ 00131000
         LR    SCANPTR,PARMSAVE                                    0079 00132000
         ALR   SCANPTR,NAMELEN                                     0079 00133000
         AH    SCANPTR,@CH00039                                    0079 00134000
*       IF CHARCOMP^=BLANK THEN                                    0080 00135000
         CLI   CHARCOMP(SCANPTR),C' '                              0080 00136000

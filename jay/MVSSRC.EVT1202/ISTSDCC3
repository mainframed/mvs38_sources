         B     @DE00094                                            0094 00173000
@DL00094 DS    0H                                                  0095 00174000
*                 IF MLCMACRO=MHTMACRO THEN                        0095 00175000
         CLC   MLCMACRO(8,MLCAPTR),MHTMACRO(MHTEPTR)               0095 00176000
         BNE   @RF00095                                            0095 00177000
*                   DO;                                            0096 00178000
*                     TABPTR=MHTVALUE;/* PUT PTR TO MACRO IN R0      */ 00179000
         L     TABPTR,MHTVALUE(,MHTEPTR)                           0097 00180000
*                     RCSAVE=0;     /* INDICATE NO ERROR             */ 00181000
         SLR   RCSAVE,RCSAVE                                       0098 00182000
*                   END;                                           0099 00183000
*                 ELSE              /* MLCMACRO^=MHTMACRO,GO ON      */ 00184000
*                   DO;                                            0100 00185000
         B     @RC00095                                            0100 00186000
@RF00095 DS    0H                                                  0101 00187000
*                     ENTCOUNT=ENTCOUNT-1;/* STEP DOWN CTR           */ 00188000
         BCTR  ENTCOUNT,0                                          0101 00189000
*                     MHTEPTR=MHTEPTR+12;/* STEP UP PTR              */ 00190000
         AH    MHTEPTR,@CH00163                                    0102 00191000
*                   END;                                           0103 00192000
*               END;                                               0104 00193000
@RC00095 DS    0H                                                  0104 00194000
@DE00094 LTR   ENTCOUNT,ENTCOUNT                                   0104 00195000
         BZ    @DC00094                                            0104 00196000
         CH    RCSAVE,@CH00041                                     0104 00197000
         BE    @DL00094                                            0104 00198000
@DC00094 DS    0H                                                  0105 00199000
*             END;                                                 0105 00200000
*           ELSE                    /* MACLEN > 8                    */ 00201000
*             DO;                                                  0106 00202000
         B     @RC00088                                            0106 00203000
@RF00088 DS    0H                                                  0107 00204000
*               MLCMACRO(1:8)=MACOP(1:8);/* MOVE IN FIRST 8 CHAR     */ 00205000

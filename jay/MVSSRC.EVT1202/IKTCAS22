         TITLE '*** IKTCAS22 - TERMINAL HANDLING ***'                   00001000
IKTCAS22 CSECT ,                                                   0001 00002000
@MAINENT DS    0H                                                  0001 00003000
         USING *,@15                                               0001 00004000
         B     @PROLOG                                             0001 00005000
         DC    AL1(16)                                             0001 00006000
         DC    C'IKTCAS22  78.336'                                 0001 00007000
         DROP  @15                                                      00008000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00009000
         BALR  @12,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@12                                         0001 00012000
         ST    @13,@SA00001+4                                      0001 00013000
         LA    @14,@SA00001                                        0001 00014000
         ST    @14,8(,@13)                                         0001 00015000
         LR    @13,@14                                             0001 00016000
*   R1=WEVRPL;                      /* GET ADDRESS OF RPL FOR CLSDST    00017000
*                                                            @ZA32361*/ 00018000
         L     @14,WEVRPL(,R10)                                    0033 00019000
         LR    R1,@14                                              0033 00020000
*   LOGWEPTR=R1-LENGTH(WESTD);      /* COMPUTE ADDRESS OF LOGON WORK    00021000
*                                      ELEMENT FROM A(RPL)   @ZA32361*/ 00022000
         LR    LOGWEPTR,R1                                         0034 00023000
         SL    LOGWEPTR,@CF00518                                   0034 00024000
*   LOGWEPTR->WEUASCB=WEUASCB;      /* COPY ASCB ADDRESS TO LOGON  0035 00025000
*                                      WORK ELEMENT          @ZA32361*/ 00026000
         L     @04,WEUASCB(,R10)                                   0035 00027000
         ST    @04,WEUASCB(,LOGWEPTR)                              0035 00028000
*   LOGWEPTR->WEUECB=WEUECB;        /* COPY ECB ADDRESS TO LOGON WORK   00029000
*                                      ELEMENT               @ZA32361*/ 00030000
         L     @04,WEUECB(,R10)                                    0036 00031000
         ST    @04,WEUECB(,LOGWEPTR)                               0036 00032000
*   LBUFPTR=ADDR(LOGWEPTR->WELBUF->LBUFFER);/* GET ADDRESS OF LOGON     00033000
*                                      BUFFER IN LOGON WORK ELEMENT     00034000
*                                                            @ZA32361*/ 00035000
         L     LBUFPTR,WELBUF(,LOGWEPTR)                           0037 00036000
         LA    LBUFPTR,LBUFFER(,LBUFPTR)                           0037 00037000
*   R15=ADDR(IKTCASCX);             /* GET CLSDST EXIT ADDRESS     0038 00038000
*                                                            @ZA32361*/ 00039000
         L     R15,@CV00508                                        0038 00040000
*   IF WECODE1F=FCVITT THEN         /* IS THIS TERM TRANSFER REQ     */ 00041000
         CLI   WECODE1F(R10),X'21'                                 0039 00042000
         BNE   @RF00039                                            0039 00043000
*     DO;                           /* YES ISSUE CLSDST PASS         */ 00044000
*       R0=LBUFPTR;                 /* GET LOGON BUFFER ADDRESS    0041 00045000
*                                                            @ZA32361*/ 00046000
         LR    R0,LBUFPTR                                          0041 00047000
*       WSCLSPAS='1'B;              /* SET PASS INDICATOR            */ 00048000
*       WSCLSREL='0'B;              /* SET RELEASE BIT OFF           */ 00049000
         AL    @14,@CF00519                                        0043 00050000
         OI    WSCLSPAS(@14),B'00010000'                           0043 00051000
         NI    WSCLSREL(@14),B'11011111'                           0043 00052000
*       GEN REFS(R0,R1,R15);                                       0044 00053000
*                                  /* ISSUE CLSDST PASS     @ZA32361*/  00054000
         CLSDST RPL=(1),AAREA=(0),EXIT=(15),OPTCD=(ASY,PASS)  @ZA32361  00055000
*     END;                                                         0045 00056000
*   ELSE                                                           0046 00057000
*     DO;                           /* NOT TERM PASS THEN RELEASE    */ 00058000
         B     @RC00039                                            0046 00059000
@RF00039 DS    0H                                                  0047 00060000
*       WSCLSREL='1'B;              /* SET RELEASE BIT ON            */ 00061000
*       WSCLSPAS='0'B;              /* SET PASS BIT OFF              */ 00062000
         LA    @14,112                                             0048 00063000
         AL    @14,WEVRPL(,R10)                                    0048 00064000
         OI    WSCLSREL(@14),B'00100000'                           0048 00065000
         NI    WSCLSPAS(@14),B'11101111'                           0048 00066000
*       GEN;                                                       0049 00067000
         CLSDST RPL=(1),EXIT=(15),OPTCD=(ASY,RELEASE)         @ZA32361  00068000
*     END;                                                         0050 00069000
*   IF R5=0 THEN                    /* IS REG 5 0? IF SO,    @ZA19531*/ 00070000
@RC00039 LTR   R5,R5                                               0051 00071000
         BNZ   @RF00051                                            0051 00072000
*     DO;                           /* FREE THE LOGON WORK ELEMENT 0052 00073000
*                                                            @ZA19531*/ 00074000
*       R0=WELEN;                   /* GET WORK ELEMENT LENGTH     0053 00075000
*                                                            @ZA32361*/ 00076000

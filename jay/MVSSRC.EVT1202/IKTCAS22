*       WA1=R10;                    /* GET WORK ELEMENT ADDRESS    0054 00078000
*                                                            @ZM42700*/ 00079000
         ST    R10,WA1                                             0054 00080000
*       GEN REFS(R0,WA1) SETS(R1);                                 0055 00081000
*                                     /* FREE WORK ELEMENT  @ZM42700*/  00082000
         FREEMAIN E,LV=(0),A=WA1,SP=231  FREEMAIN FOR LOGON W.E.        00083000
*     END;                          /*                       @ZA19531*/ 00084000
*   ELSE                                                           0057 00085000
*     ;                             /* DO NOT FREE W.E.      @ZA19531*/ 00086000
@RF00051 DS    0H                                                  0058 00087000

*   CRPRPHPT=OCWRPH;                /* RESTORE RPH ADD               */ 06123000
         L     CRPRPHPT,OCWRPH(,CRPOCWPT)                       01S0663 06124000
*   CRPRTPT=CRPR14SV;               /* RESTORE RET @                 */ 06125000
         LA    @11,180                                          01S0664 06126000
         ALR   @11,CRPOCWPT                                     01S0664 06127000
         L     CRPRTPT,CRPR14SV(,@11)                           01S0664 06128000
*   CRPSVPT=CRPR13SV;               /* RESTORE SAVE AREA @           */ 06129000
         L     CRPSVPT,CRPR13SV(,@11)                           01S0665 06130000
*   RETURN;                                                     01S0666 06131000
@EL00002 DS    0H                                               01S0666 06132000
@EF00002 DS    0H                                               01S0666 06133000
@ER00002 BR    @14                                              01S0666 06134000
*   RFY                                                         01S0667 06135000
*     CRPRTPT UNRSTD;                                           01S0667 06136000
*   END CRPMSG;                                                 01S0668 06137000
         EJECT                                                          06138000
*CRPEDIT:                                                       01S0669 06139000
*   PROC OPTIONS(NOSAVE NOSAVEAREA);                            01S0669 06140000
CRPEDIT  DS    0H                                               01S0670 06141000
*   RFY                                                         01S0670 06142000
*     CRPRTPT RSTD;                                             01S0670 06143000
*   CRPR14SV=CRPRTPT;                                           01S0671 06144000
         LA    @15,180                                          01S0671 06145000

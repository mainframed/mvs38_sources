         ST    CRPRTPT,CRPR14SV(,@15)                           01S0671 06147000
*   RFY                                                         01S0672 06148000
*     CRPRTPT UNRSTD;                                           01S0672 06149000
*   RFY                                                         01S0673 06150000
*    (CRPBASE,                                                  01S0673 06151000
*     CRPPTRA) RSTD;                                            01S0673 06152000
*   RFY                                                         01S0674 06153000
*     CRPCHARA BASED(CRPPTRA);                                  01S0674 06154000
*   RFY                                                         01S0675 06155000
*     CRPCHARB BASED(CRPBASE);                                  01S0675 06156000
*   RFY                                                         01S0676 06157000
*     CRPCHARC BASED(CRPSTRNG);     /* THIS WILL BE THE ACTUAL TEXT     06158000
*                                      PTR                           */ 06159000
*/*                                               AFTER IT IS EDITED */ 06160000
*                                                               01S0677 06161000
*   CRPPTRA=CRPSTRNG+TWO;           /* SET TO SECOND CHAR IN TEXT    */ 06162000
         LA    CRPPTRA,2                                        01S0677 06163000
         ALR   CRPPTRA,CRPSTRNG                                 01S0677 06164000
*   CRPBASE=CRPSTRNG+ONE;           /* SET TO FIRST CHAR IN TEXT     */ 06165000
         LA    CRPBASE,1                                        01S0678 06166000
         ALR   CRPBASE,CRPSTRNG                                 01S0678 06167000
*   CRPSTRNG=CRPBASE;               /* SET TO FIRST CHAR IN TEXT     */ 06168000
         LR    CRPSTRNG,CRPBASE                                 01S0679 06169000
*   CRPFND=OFF;                     /* INIT BIT                      */ 06170000
*   CRPSAME=OFF;                    /* SIGNALS THAT WE HAVE FOUND TWO   06171000
*                                      CONSECUTIVE QUOTES            */ 06172000
         LA    @14,180                                          01S0681 06173000
         ALR   @14,CRPOCWPT                                     01S0681 06174000
         NI    CRPFND(@14),B'01011111'                          01S0681 06175000
*   CRPBUFLN=ZERO;                                              01S0682 06176000
*                                                               01S0682 06177000
         SLR   @11,@11                                          01S0682 06178000
         ST    @11,CRPBUFLN(,@14)                               01S0682 06179000
*   /*****************************************************************/ 06180000
*   /*                                                               */ 06181000
*   /* THE FOLLOWING LOOP WILL USE THREE POINTERS: TWO TO TELL US    */ 06182000
*   /* WHEN WE HAVE HIT UPON A DOUBLE QUOTE COMBIN-                  */ 06183000
*   /* NATION(CHARA,CHARB) AND THE THIRD TO KEEP TRACK OF WHERE WE   */ 06184000
*   /* ARE IN ACTUAL TEXT. THERE WILL ALSO BE A A TOTAL TEXT CTR SO  */ 06185000
*   /* THAT WE DONT EXCEED 126 CHARS                                 */ 06186000
*   /*                                                               */ 06187000
*   /*****************************************************************/ 06188000
*                                                               01S0683 06189000
*   DO WHILE(CRPFND=OFF&CRPBUFLN<=RPLRLEN);                     01S0683 06190000
         B     @DE00683                                         01S0683 06191000
@DL00683 DS    0H                                               01S0684 06192000
*     IF(CRPCHARA=QUOTE&CRPCHARB=QUOTE) THEN                    01S0684 06193000
         CLI   CRPCHARA(CRPPTRA),X'7D'                          01S0684 06194000
         BNE   @RF00684                                         01S0684 06195000
         CLI   CRPCHARB(CRPBASE),X'7D'                          01S0684 06196000
         BNE   @RF00684                                         01S0684 06197000
*       DO;                         /* WE HAVE A DOUBLE QUOTE TO BE     06198000
*                                      REDUCED TO A SINGLE QUOTE     */ 06199000
*         CRPSAME=ON;               /* SIGNAL FOR DOUBLE QUOTE       */ 06200000
         OI    CRPSAME+180(CRPOCWPT),B'00100000'                01S0686 06201000
*         CRPPTRA=CRPPTRA+ONE;      /* ADVANCE PTR                   */ 06202000
         LA    @14,1                                            01S0687 06203000
         ALR   CRPPTRA,@14                                      01S0687 06204000
*         CRPBASE=CRPBASE+ONE;      /* ADVANCE PTR                   */ 06205000
         ALR   CRPBASE,@14                                      01S0688 06206000
*       END;                                                    01S0689 06207000
*     ELSE                                                      01S0690 06208000
*       ;                                                       01S0690 06209000
@RF00684 DS    0H                                               01S0691 06210000
*     CRPCHARC=CRPCHARB;            /* IF THEY DIFFER(IE WE HAVE HIT    06211000
*                                      DOU. QUOTE) THEN THIS WILL       06212000
*                                      EDIT BUFFER CORRECTLY         */ 06213000
         MVC   CRPCHARC(1,CRPSTRNG),CRPCHARB(CRPBASE)           01S0691 06214000
*     IF CRPSAME=OFF&CRPCHARC=QUOTE /* END OF BUFFER TEXT?           */ 06215000
*       THEN                                                    01S0692 06216000
         LA    @14,180                                          01S0692 06217000
         ALR   @14,CRPOCWPT                                     01S0692 06218000
         TM    CRPSAME(@14),B'00100000'                         01S0692 06219000
         BNZ   @RF00692                                         01S0692 06220000
         CLI   CRPCHARC(CRPSTRNG),X'7D'                         01S0692 06221000
         BNE   @RF00692                                         01S0692 06222000
*       CRPFND=ON;                  /* WE WANT TO EXIT LOOP          */ 06223000
         OI    CRPFND(@14),B'10000000'                          01S0693 06224000
*     ELSE                                                      01S0694 06225000
*       DO;                         /* ADVANCE ALL POINTERS AND 01S0694 06226000
*                                      REINIT FLAGS                  */ 06227000
         B     @RC00692                                         01S0694 06228000
@RF00692 DS    0H                                               01S0695 06229000
*         CRPSAME=OFF;                                          01S0695 06230000
         NI    CRPSAME+180(CRPOCWPT),B'11011111'                01S0695 06231000
*         CRPPTRA=CRPPTRA+ONE;                                  01S0696 06232000
         LA    @14,1                                            01S0696 06233000
         ALR   CRPPTRA,@14                                      01S0696 06234000
*         CRPBASE=CRPBASE+ONE;                                  01S0697 06235000
         ALR   CRPBASE,@14                                      01S0697 06236000
*         CRPSTRNG=CRPSTRNG+ONE;                                01S0698 06237000
         ALR   CRPSTRNG,@14                                     01S0698 06238000
*       END;                                                    01S0699 06239000
*     CRPBUFLN=((CRPBASE-CRPADBUF)+ONE);/* COMPUTE TOTAL LENGTH SO      06240000
*                                      FAR                           */ 06241000
@RC00692 LR    @14,CRPBASE                                      01S0700 06242000
         SLR   @14,CRPADBUF                                     01S0700 06243000
         AH    @14,@CH00061                                     01S0700 06244000
         ST    @14,CRPBUFLN+180(,CRPOCWPT)                      01S0700 06245000
*   END;                                                        01S0701 06246000
@DE00683 LA    @14,180                                          01S0701 06247000
         ALR   @14,CRPOCWPT                                     01S0701 06248000
         TM    CRPFND(@14),B'10000000'                          01S0701 06249000
         BNZ   @DC00683                                         01S0701 06250000
         L     @11,OCWRPL(,CRPOCWPT)                            01S0701 06251000
         L     @14,CRPBUFLN(,@14)                               01S0701 06252000
         C     @14,RPLRLEN(,@11)                                01S0701 06253000
         BNH   @DL00683                                         01S0701 06254000
@DC00683 DS    0H                                               01S0702 06255000
*   CRPBUFLN=(CRPSTRNG-CRPADBUF)-(CRPBGN-ONE);/* ACTUAL LEN OF TEXT  */ 06256000
         LA    @14,180                                          01S0702 06257000
         ALR   @14,CRPOCWPT                                     01S0702 06258000
         LR    @11,CRPSTRNG                                     01S0702 06259000
         SLR   @11,CRPADBUF                                     01S0702 06260000
         L     @09,CRPBGN(,@14)                                 01S0702 06261000
         BCTR  @09,0                                            01S0702 06262000
         SLR   @11,@09                                          01S0702 06263000
         ST    @11,CRPBUFLN(,@14)                               01S0702 06264000
*   RFY                                                         01S0703 06265000
*    (CRPBASE,                                                  01S0703 06266000
*     CRPPTRA) UNRSTD;                                          01S0703 06267000
*   RFY                                                         01S0704 06268000
*     CRPRTPT RSTD;                                             01S0704 06269000
*   CRPRTPT=CRPR14SV;               /* RESTORE RETURN @              */ 06270000
         LA    @11,180                                          01S0705 06271000
         ALR   @11,CRPOCWPT                                     01S0705 06272000
         L     CRPRTPT,CRPR14SV(,@11)                           01S0705 06273000
*   RETURN;                                                     01S0706 06274000
@EL00003 DS    0H                                               01S0706 06275000
@EF00003 DS    0H                                               01S0706 06276000
@ER00003 BR    @14                                              01S0706 06277000
*   RFY                                                         01S0707 06278000
*     CRPRTPT UNRSTD;                                           01S0707 06279000
*   END CRPEDIT;                                                01S0708 06280000
         EJECT                                                          06281000
*CRPCBSH:                                                       01S0709 06282000
*   PROC OPTIONS(NOSAVE NOSAVEAREA);                            01S0709 06283000
*                                                               01S0709 06284000
CRPCBSH  DS    0H                                               01S0710 06285000
*   /*****************************************************************/ 06286000
*   /*                                                               */ 06287000
*   /* THIS SUBROUTINE SEARCHES FOR A PROCB WITH A MATCHING REPLY    */ 06288000
*   /* NUMBER                                                        */ 06289000
*   /*                                                               */ 06290000
*   /*****************************************************************/ 06291000
*                                                               01S0710 06292000
*   RFY                                                         01S0710 06293000
*     CRPRTPT RSTD;                                             01S0710 06294000
*   CRPR14SV=CRPRTPT;                                           01S0711 06295000
         LA    @15,180                                          01S0711 06296000
         ALR   @15,CRPOCWPT                                     01S0711 06297000
         ST    CRPRTPT,CRPR14SV(,@15)                           01S0711 06298000
*   RFY                                                         01S0712 06299000
*     CRPRTPT UNRSTD;                                           01S0712 06300000
*   RFY                                                         01S0713 06301000
*     CRPBASE RSTD;                                             01S0713 06302000
*   IF POCAWTRQ=ZERO                /* NO AWAITING REPLIES           */ 06303000
*     THEN                                                      01S0714 06304000
         ICM   @11,7,POCAWTRQ(CRPPOCB)                          01S0714 06305000
         BNZ   @RF00714                                         01S0714 06306000
*     CRPFND=OFF;                   /* IT WAS NOT FOUND              */ 06307000
         NI    CRPFND(@15),B'01111111'                          01S0715 06308000
*   ELSE                                                        01S0716 06309000
*     DO;                                                       01S0716 06310000
         B     @RC00714                                         01S0716 06311000
@RF00714 DS    0H                                               01S0717 06312000
*       CRPPTR=POCAWTRQ;            /* SET PTR TO AWAIT. REP. QUEUE  */ 06313000
         SLR   CRPPTR,CRPPTR                                    01S0717 06314000
         ICM   CRPPTR,7,POCAWTRQ(CRPPOCB)                       01S0717 06315000
*       CRPBASE=CRPPTR;                                         01S0718 06316000
         LR    CRPBASE,CRPPTR                                   01S0718 06317000
*       RFY                                                     01S0719 06318000
*         ISTPORCB BASED(CRPPTR);                               01S0719 06319000
*                                                               01S0719 06320000
*       /*************************************************************/ 06321000
*       /*                                                           */ 06322000
*       /* THIS LOOP SEARCHES QUEUE FOR MATCH IN REP ID IF MATCH IS  */ 06323000
*       /* FOUND, PORCB ADDRESS WILL BE IN CRPPTR                    */ 06324000
*       /*                                                           */ 06325000
*       /*************************************************************/ 06326000
*                                                               01S0720 06327000
*       DO UNTIL(PORFWD=ZERO|CRPFND=ON);                        01S0720 06328000
@DL00720 DS    0H                                               01S0721 06329000
*         CRPPTR=CRPBASE;                                       01S0721 06330000
         LR    CRPPTR,CRPBASE                                   01S0721 06331000
*         IF CRPNUM=POREPID         /* IF THE REP IDS MATCH          */ 06332000
*           THEN                                                01S0722 06333000
         LA    @11,180                                          01S0722 06334000
         ALR   @11,CRPOCWPT                                     01S0722 06335000
         CLC   CRPNUM(2,@11),POREPID(CRPPTR)                    01S0722 06336000
         BNE   @RF00722                                         01S0722 06337000
*           CRPFND=ON;              /* WE WANT TO EXIT LOOP          */ 06338000
         OI    CRPFND(@11),B'10000000'                          01S0723 06339000
*         ELSE                                                  01S0724 06340000
*           CRPBASE=PORFWD;         /* POINT TO NEXT PORCB           */ 06341000
         B     @RC00722                                         01S0724 06342000
@RF00722 SLR   CRPBASE,CRPBASE                                  01S0724 06343000
         ICM   CRPBASE,7,PORFWD(CRPPTR)                         01S0724 06344000
*       END;                                                    01S0725 06345000
@RC00722 DS    0H                                               01S0725 06346000
@DE00720 ICM   @11,7,PORFWD(CRPPTR)                             01S0725 06347000
         BZ    @DC00720                                         01S0725 06348000
         TM    CRPFND+180(CRPOCWPT),B'10000000'                 01S0725 06349000
         BNO   @DL00720                                         01S0725 06350000
@DC00720 DS    0H                                               01S0726 06351000
*     END;                                                      01S0726 06352000
*   RFY                                                         01S0727 06353000
*     CRPBASE UNRSTD;                                           01S0727 06354000
@RC00714 DS    0H                                               01S0728 06355000
*   RFY                                                         01S0728 06356000
*     CRPRTPT RSTD;                                             01S0728 06357000
*   CRPRTPT=CRPR14SV;               /* RESTORE RET @                 */ 06358000
         L     CRPRTPT,CRPR14SV+180(,CRPOCWPT)                  01S0729 06359000
*   RETURN;                                                     01S0730 06360000
@EL00004 DS    0H                                               01S0730 06361000
@EF00004 DS    0H                                               01S0730 06362000
@ER00004 BR    @14                                              01S0730 06363000
*   RFY                                                         01S0731 06364000
*     CRPRTPT UNRSTD;                                           01S0731 06365000
*   END CRPCBSH;                                                01S0732 06366000
@PB00004 DS    0H                                               01S0733 06367000
*   DO;                             /* TPEPILOG                      */ 06368000
*     GEN(PRINT OFF);                                           01S0734 06369000
         PRINT OFF                                                      06370000
*     DO;                                                       01S0735 06371000
*       DCL                                                     01S0736 06372000
*         GPR00F FIXED(31) REG(00),                             01S0736 06373000
*         GPR00P PTR(31) REG(00),                               01S0736 06374000
*         GPR01F FIXED(31) REG(01),                             01S0736 06375000
*         GPR01P PTR(31) REG(01),                               01S0736 06376000
*         GPR02F FIXED(31) REG(02),                             01S0736 06377000
*         GPR02P PTR(31) REG(02),                               01S0736 06378000

         MVI   RPLRTNCD(@11),X'08'                              01S0617 06009000
*         RPLFDB2='6E'X;            /* RPL ERROR CODE                */ 06010000
         MVI   RPLFDB2(@11),X'6E'                               01S0618 06011000
*         POPMSGID='194I';          /* SET MSG ID FOR TPMSG          */ 06012000
         MVC   POPMSGID+360(4,CRPOCWPT),@CC03410                01S0619 06013000
*         CALL CRPMSG;                                          01S0620 06014000
         BAL   @14,CRPMSG                                       01S0620 06015000
*       END;                                                    01S0621 06016000
         EJECT                                                          06017000
*CRPMSG:                                                        01S0622 06018000
*   PROC OPTIONS(NOSAVE NOSAVEAREA);                            01S0622 06019000
*                                                               01S0622 06020000
         B     @PB00002                                         01S0622 06021000
CRPMSG   DS    0H                                               01S0623 06022000
*   /*****************************************************************/ 06023000
*   /*                                                               */ 06024000
*   /* THIS ROUTINE SETS UP AREAS TO ISSUE A TPMSG. AREAS ARE        */ 06025000
*   /* INITIALIZED IN POPWA                                          */ 06026000
*   /*                                                               */ 06027000
*   /*****************************************************************/ 06028000
*                                                               01S0623 06029000
*   RFY                                                         01S0623 06030000
*    (CRPRTPT,                                                  01S0623 06031000
*     CRPSVPT) RSTD;                                            01S0623 06032000
*   CRPR14SV=CRPRTPT;               /* REG 14 IS SAVED FOR RETURN @  */ 06033000
         LA    @15,180                                          01S0624 06034000
         ALR   @15,CRPOCWPT                                     01S0624 06035000
         ST    CRPRTPT,CRPR14SV(,@15)                           01S0624 06036000
*   CRPR13SV=CRPSVPT;               /* REG 13 SAVE AREA @ IS SAVED   */ 06037000
         ST    CRPSVPT,CRPR13SV(,@15)                           01S0625 06038000
*   RFY                                                         01S0626 06039000
*     CRPRTPT UNRSTD;                                           01S0626 06040000
*   CRPSVPT=ADDR(POPSAVE);          /* PROVIDES SAVE AREA FOR TPMSG  */ 06041000
         LA    @11,360                                          01S0627 06042000
         ALR   @11,CRPOCWPT                                     01S0627 06043000
         LA    CRPSVPT,POPSAVE(,@11)                            01S0627 06044000
*   RFY                                                         01S0628 06045000
*    (CRPMSGID,                                                 01S0628 06046000
*     CRPVARDT) RSTD;                                           01S0628 06047000
*   CRPMSGID=ADDR(POPMSGID);        /* SET POINTER TO MSG ID         */ 06048000
         LA    CRPMSGID,POPMSGID(,@11)                          01S0629 06049000
*   CRPVARDT=ADDR(CRPNUM);          /* SET PTR TO VAR DATA           */ 06050000
         LA    CRPVARDT,CRPNUM(,@15)                            01S0630 06051000
*   DO;                             /* TPMSG MSGID((CRPMSGID))MSG((CR   06052000
*                                      PVARDT))OPTIONS(POPTPOPT)PLARE   06053000
*                                      A(POPPLA)                     */ 06054000
*     DO;                                                       01S0632 06055000
*       RFY                                                     01S0633 06056000
*        (GPR01P,                                               01S0633 06057000
*         GPR13P) RSTD;                                         01S0633 06058000
*     END;                                                      01S0634 06059000
*     DCL                                                       01S0635 06060000
*       1 * BASED(GPR01P),                                      01S0635 06061000
*        2 CMZID086 PTR(31),                                    01S0635 06062000
*        2 *,                                                   01S0635 06063000

         MVI   RPLFDB2(@11),X'6E'                               01S0611 05995000
*             POPMSGID='194I';      /* SET MSGID FOR TPMSG           */ 05996000
         MVC   POPMSGID+360(4,CRPOCWPT),@CC03410                01S0612 05997000
*             CALL CRPMSG;                                      01S0613 05998000
         BAL   @14,CRPMSG                                       01S0613 05999000
*           END;                                                01S0614 06000000
*       END;                                                    01S0615 06001000
*     ELSE                                                      01S0616 06002000
*       DO;                         /* THE REPLY MUST HAVE ALREADY      06003000
*                                      BEEN SATISFIED                */ 06004000
         B     @RC00600                                         01S0616 06005000
@RF00600 DS    0H                                               01S0617 06006000
*         RPLRTNCD='08'X;           /* RPL ERROR CODE                */ 06007000

*     DO;                                                       01S0648 06096000
*       RFY                                                     01S0649 06097000
*        (GPR15P) RSTD;                                         01S0649 06098000
*     END;                                                      01S0650 06099000
*     DO;                           /* LISATCVT                 01S0651 06100000
*                                      CVTBASE(PAGEABLE,(GPR15P))    */ 06101000
*       GPR15P=ATCLCPTR;            /* SET UP ATCVT POINTER          */ 06102000
         L     GPR15P,ATCLCPTR                                  01S0652 06103000
*     END;                          /* LISATCVT                      */ 06104000

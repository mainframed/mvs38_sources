*       GPR01P=ADDR(POPPLA);        /* SET RREG SYMBOL TO ADDRESS OF    06072000
*                                      PPARM OPERAND                 */ 06073000
         LA    GPR01P,POPPLA(,@11)                              01S0637 06074000
*     END;                          /* XREGSET                       */ 06075000
*     CMZID086=0;                                               01S0639 06076000
         SLR   @09,@09                                          01S0639 06077000
         ST    @09,CMZID086(,GPR01P)                            01S0639 06078000
*     CMZRL086=0;                                               01S0640 06079000
         MVI   CMZRL086(GPR01P),X'00'                           01S0640 06080000
*     CMZRA086=0;                                               01S0641 06081000
         STCM  @09,7,CMZRA086(GPR01P)                           01S0641 06082000
*     CMZEA086=0;                                               01S0642 06083000
         ST    @09,CMZEA086(,GPR01P)                            01S0642 06084000
*     CMZVP086=0;                                               01S0643 06085000
         ST    @09,CMZVP086(,GPR01P)                            01S0643 06086000
*     CMZID086=CRPMSGID;                                        01S0644 06087000
